
<section>
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Escrituração</li>
        <li class="breadcrumb-item active">Regras para Pagamento ao Provedor</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i></i> Regras para Pagamento ao Provedor
        </h1>
    </div>

    <!-- #region Campos de Pesquisa -->

    <div class="panel">
        <div class="panel-container">
            <div id="knockoutPesquisaRegraPagamentoProvedor" class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i> Pesquisar Regras para Pagamento ao Provedor
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6 col-lg-4" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3 col-lg-4" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3 col-lg-4" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6 col-lg-4" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6 col-lg-4" data-bind="visible: Aprovador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Aprovador.text, attr: { for: Aprovador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Aprovador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Aprovador.maxlength, id : Aprovador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Aprovador.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex justify-content-end mb-3">
                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>

    <!-- #endregion -->

    <!-- #region Campos de Cadastro -->

    <div class="panel" id="knockoutCadastroRegraPagamentoProvedor">
        <div class="panel-container">
            <div class="panel-content">
                <ul class="nav nav-tabs" id="myTab">
                    <li class="nav-item active">
                        <a href="#tabRegra" class="nav-link active" data-bs-toggle="tab"><span class="hidden-mobile hidden-tablet">Regra</span></a>
                    </li>
                    <!-- ko foreach: { data: Alcadas.val, as: 'Aba' }  -->
                    <li class="nav-item">
                        <a data-bind="attr: { href: '#tabRegras' + Aba.prop }, click: function(){ $root.Alcadas.changeAba(Aba.prop) }" class="nav-link" data-bs-toggle="tab"><span data-bind="text: Aba.descricao" class="hidden-mobile hidden-tablet"></span></a>
                    </li>
                    <!-- /ko -->
                </ul>
                <div class="tab-content border border-top-0 p-3" id="myTabContent">
                    <div id="tabRegra" class="tab-pane fade active show">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: PrioridadeAprovacao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: PrioridadeAprovacao.text, attr: { for: PrioridadeAprovacao.id }"></label>
                                        <select class="form-control" data-bind="options: PrioridadeAprovacao.options, optionsText: 'text', optionsValue: 'value', value: PrioridadeAprovacao.val, attr: { id : PrioridadeAprovacao.id}"></select>
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: Vigencia.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Vigencia.text, attr: { for: Vigencia.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Vigencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Vigencia.maxlength, id : Vigencia.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: NumeroAprovadores.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroAprovadores.text, attr: { for: NumeroAprovadores.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: NumeroAprovadores.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroAprovadores.maxlength, id : NumeroAprovadores.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                        <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                    </div>
                                </div>
                                <div class="col-12 col-md-12" data-bind="visible: ValidarTodosCamposAuditoriaDocumentoProvedor.visible">
                                    <div class="form-group">
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" data-bind="checked: ValidarTodosCamposAuditoriaDocumentoProvedor.val, attr: { maxlength: ValidarTodosCamposAuditoriaDocumentoProvedor.maxlength, name: ValidarTodosCamposAuditoriaDocumentoProvedor.id, id: ValidarTodosCamposAuditoriaDocumentoProvedor.id }">
                                            <label class="custom-control-label" data-bind="text: ValidarTodosCamposAuditoriaDocumentoProvedor.text, attr: { for: ValidarTodosCamposAuditoriaDocumentoProvedor.id }"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-12" data-bind="visible: BloquearPagamentoMultiplosCTe.visible">
                                    <div class="form-group">
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" data-bind="checked: BloquearPagamentoMultiplosCTe.val, attr: { maxlength: BloquearPagamentoMultiplosCTe.maxlength, name: BloquearPagamentoMultiplosCTe.id, id: BloquearPagamentoMultiplosCTe.id }">
                                            <label class="custom-control-label" data-bind="text: BloquearPagamentoMultiplosCTe.text, attr: { for: BloquearPagamentoMultiplosCTe.id }"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-12" data-bind="visible: Observacoes.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Observacoes.text, attr: { for: Observacoes.id }"></label>
                                        <textarea class="form-control" rows="4" data-bind="value: Observacoes.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacoes.maxlength, id : Observacoes.id ,class: Observacoes.requiredClass}"></textarea>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="card" data-bind="visible: Aprovadores.visible">
                                        <div class="card-header d-flex align-items-center justify-content-between">
                                            <h3 class="card-title">Aprovador</h3>
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: AdicionarAprovador.idBtnSearch, text: AdicionarAprovador.text }">
                                                <i class="fal fa-plus"></i><span data-bind="text: AdicionarAprovador.text"></span>
                                            </button>
                                        </div>
                                        <div class="card-body">
                                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Aprovadores.idGrid}" cellspacing="0"></table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <!-- ko foreach: { data: Alcadas.val, as: 'Aba' }  -->
                    <div data-bind="attr: { id: 'tabRegras' + Aba.prop }" class="tab-pane fade">
                        <div>
                            <div class="custom-control custom-switch mb-3">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: $root['UsarRegraPor' + Aba.prop].val, attr: { name: $root['UsarRegraPor' + Aba.prop].id, id: $root['UsarRegraPor' + Aba.prop].id }">
                                <label class="custom-control-label" data-bind="text: $root['UsarRegraPor' + Aba.prop].text, attr: { for: $root['UsarRegraPor' + Aba.prop].id }"></label>
                            </div>
                            <hr />
                            <div class="row exchange" data-bind="id: $root['UsarRegraPor' + Aba.prop].idFade, visible: $root['UsarRegraPor' + Aba.prop].val">
                                <div class="col-12 col-md-6">
                                    <div class="panel">
                                        <div class="panel-container">
                                            <div class="panel-content">
                                                <form>
                                                    <div class="row">
                                                        <div class="col-12 col-md-6">
                                                            <div class="form-group">
                                                                <label class="form-label" data-bind="text: $root['Condicao' + Aba.prop].text"></label>
                                                                <select class="form-control" data-bind="options: $root['Condicao' + Aba.prop].options, optionsText: 'text', optionsValue: 'value', value: $root['Condicao' + Aba.prop].val, attr: { id : $root['Condicao' + Aba.prop].id}"></select>
                                                            </div>
                                                        </div>
                                                        <div class="col-12 col-md-6">
                                                            <div class="form-group">
                                                                <label class="form-label" data-bind="text: $root['Juncao' + Aba.prop].text"></label>
                                                                <select class="form-control" data-bind="options: $root['Juncao' + Aba.prop].options, optionsText: 'text', optionsValue: 'value', value: $root['Juncao' + Aba.prop].val, attr: { id : $root['Juncao' + Aba.prop].id}"></select>
                                                            </div>
                                                        </div>
                                                        <!-- ko if: (Aba.tipoPropriedadeAlcada === EnumTipoPropriedadeAlcada.Entidade || Aba.tipoPropriedadeAlcada === EnumTipoPropriedadeAlcada.MultiplasEntidades) -->
                                                        <div class="col-12 col-md-12" data-bind="visible:  $root[Aba.prop].visible">
                                                            <div class="form-group">
                                                                <label class="form-label" data-bind="text:  $root[Aba.prop].text, attr: { for:  $root[Aba.prop].id }"></label>
                                                                <div class="input-group">
                                                                    <input type="text" class="form-control" data-bind="css:  $root[Aba.prop].requiredClass, value:  $root[Aba.prop].val, valueUpdate: 'afterkeydown', attr: { maxlength:  $root[Aba.prop].maxlength, id :  $root[Aba.prop].id}" />
                                                                    <div class="input-group-append">
                                                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id:  $root[Aba.prop].idBtnSearch}" type="button" aria-label="Buscar">
                                                                            <i class="fal fa-search"></i>
                                                                        </button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- /ko -->
                                                        <!-- ko if: (Aba.tipoPropriedadeAlcada === EnumTipoPropriedadeAlcada.Inteiro || Aba.tipoPropriedadeAlcada === EnumTipoPropriedadeAlcada.Decimal) -->
                                                        <div class="col-12 col-md-6" data-bind="visible: $root[Aba.prop].visible">
                                                            <div class="form-group">
                                                                <label class="form-label" data-bind="text: $root[Aba.prop].text, attr: { for: $root[Aba.prop].id }"></label>
                                                                <input type="text" class="form-control" data-bind="class: $root[Aba.prop].requiredClass, value: $root[Aba.prop].val, valueUpdate: 'afterkeydown', attr: { maxlength: $root[Aba.prop].maxlength, id : $root[Aba.prop].id}" />
                                                            </div>
                                                        </div>
                                                        <!-- /ko -->
                                                        <!-- ko if: (Aba.tipoPropriedadeAlcada === EnumTipoPropriedadeAlcada.Enum) -->
                                                        <div class="col-12 col-md-6" data-bind="visible: $root[Aba.prop].visible">
                                                            <div class="form-group">
                                                                <label class="form-label" data-bind="text: $root[Aba.prop].text, attr: { for: $root[Aba.prop].id }"></label>
                                                                <select class="form-control" data-bind="class: $root[Aba.prop].requiredClass, options: $root[Aba.prop].options, optionsText: 'text', optionsValue: 'value', value: $root[Aba.prop].val, attr: { maxlength: $root[Aba.prop].maxlength, id : $root[Aba.prop].id}"></select>
                                                            </div>
                                                        </div>
                                                        <!-- /ko -->
                                                    </div>
                                                </form>
                                                <div class="d-flex justify-content-end">
                                                    <button class="btn btn-default ms-1" type="button" data-bind="click: function(data, evt){ $root.Cancelar.eventClick(Aba.prop, data, evt) }, visible: $root.Cancelar.visible, attr: { value: $root.Cancelar.text, id : $root.Cancelar.id}">
                                                        <span data-bind="text: $root.Cancelar.text"></span>
                                                    </button>
                                                    <button class="btn btn-danger ms-1" type="button" data-bind="click: function(data, evt){ $root.Excluir.eventClick(Aba.prop, data, evt) }, visible: $root.Excluir.visible, attr: { value: $root.Excluir.text, id : $root.Excluir.id}">
                                                        <span data-bind="text: $root.Excluir.text"></span>
                                                    </button>
                                                    <button class="btn btn-success ms-1" data-bind="click: function(data, evt){ $root.Atualizar.eventClick(Aba.prop, data, evt) }, visible: $root.Atualizar.visible, attr: { value: $root.Atualizar.text, id : $root.Atualizar.id}">
                                                        <span data-bind="text: $root.Atualizar.text"></span>
                                                    </button>
                                                    <button class="btn btn-success ms-1" type="button" data-loading-text="Aguarde..." data-bind="click: function(data, evt){ $root.Adicionar.eventClick(Aba.prop, data, evt) }, visible: $root.Adicionar.visible, attr: { value: $root.Adicionar.text, id : $root.Adicionar.id}">
                                                        <span data-bind="text: $root.Adicionar.text"></span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-6">
                                    <div class="panel">
                                        <div class="panel-container">
                                            <div class="panel-hdr">
                                                <h3 class="form-label" data-bind="text: Aba.descricao"></h3>
                                            </div>
                                            <div class="panel-content">
                                                <div data-bind="attr: { id: $root['Alcadas' + Aba.prop].idGrid}"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /ko -->
                    <footer class="d-flex justify-content-end mt-3">
                        <button class="btn btn-success ms-1" type="button" data-bind="click: CRUDAtualizar.eventClick, visible: CRUDAtualizar.visible, attr: { value: CRUDAtualizar.text, id : CRUDAtualizar.id}">
                            <span data-bind="text: CRUDAtualizar.text"></span>
                        </button>
                        <button class="btn btn-success ms-1" type="button" data-bind="click: CRUDAdicionar.eventClick, visible: CRUDAdicionar.visible, attr: { value: CRUDAdicionar.text, id : CRUDAdicionar.id}">
                            <span data-bind="text: CRUDAdicionar.text"></span>
                        </button>
                        <button class="btn btn-danger ms-1" type="button" data-bind="click: CRUDExcluir.eventClick, visible: CRUDExcluir.visible, attr: { value: CRUDExcluir.text, id : CRUDExcluir.id}">
                            <span data-bind="text: CRUDExcluir.text"></span>
                        </button>
                        <button class="btn btn-default ms-1" type="button" data-bind="click: CRUDCancelar.eventClick, visible: CRUDCancelar.visible, attr: { value: CRUDCancelar.text, id : CRUDCancelar.id}">
                            <span data-bind="text:CRUDCancelar.text"></span>
                        </button>
                    </footer>
                </div>
            </div>
        </div>
    </div>

    <!-- #endregion -->

</section>


<script src="~/scripts/regraPagamentoProvedor"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRegrasAutorizacao();
    });

</script>