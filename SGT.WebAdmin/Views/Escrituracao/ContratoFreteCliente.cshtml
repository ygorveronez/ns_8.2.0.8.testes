<ol class="breadcrumb page-breadcrumb">
	<li class="breadcrumb-item">Escrituração</li>
	<li class="breadcrumb-item active">Contrato de Frete Cliente</li>
</ol>
<div class="subheader">
	<h1 class="subheader-title">
		Contrato de Frete Cliente
	</h1>
</div>

<div id="knockoutPesquisaContrato">
	<div class="panel">
		<div class="panel-container">
			<div class="panel-content">
				<button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
					<i class="fal fa-search"></i>
					Pesquisar Contrato de Frete Cliente
				</button>
				<div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
					<form>
						<div class="row">
							<div class="col-12 col-md-3" data-bind="visible: NumeroContrato.visible">
								<div class="form-group">
									<label class="form-label" data-bind="text: NumeroContrato.text"></label>
									<input type="text" class="form-control" data-bind="class: NumeroContrato.requiredClass, value: NumeroContrato.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroContrato.maxlength, id : NumeroContrato.id}" />
								</div>
							</div>
							<div class="col-12 col-md-3" data-bind="visible: Descricao.visible">
								<div class="form-group">
									<label class="form-label" data-bind="text: Descricao.text"></label>
									<input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
								</div>
							</div>
							<div class="col-12 col-md-3">
								<div class="form-group">
									<label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
									<div class="input-group">
										<input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
										<div class="input-group-append">
											<button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
												<i class="fal fa-search"></i>
											</button>
										</div>
									</div>
								</div>
							</div>
							<div class="col-12 col-md-3">
								<div class="form-group">
									<label class="form-label" data-bind="text: Cliente.text, attr: { for: Cliente.id }"></label>
									<div class="input-group">
										<input type="text" class="form-control" data-bind="css: Cliente.requiredClass, value: Cliente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Cliente.maxlength, id : Cliente.id}" />
										<div class="input-group-append">
											<button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Cliente.idBtnSearch}" type="button" aria-label="Buscar">
												<i class="fal fa-search"></i>
											</button>
										</div>
									</div>
								</div>
							</div>
						</div>
					</form>
					<div class="d-flex align-items-center justify-content-end mb-3">
						<button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
							<i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
						</button>
					</div>
					<table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="panel">
	<div class="panel-container">
		<div class="panel-hdr">
			<h3>
				<i class="fal fa-edit"></i>
				Cadastrar Contrato de Frete Por Cliente
			</h3>
		</div>
		<div class="panel-content">
			<form>
				<div class="row" id="knockoutContrato">
					<div class="col-12 col-md-4" data-bind="visible: NumeroContrato.visible">
						<div class="form-group">
							<label class="form-label" data-bind="text: NumeroContrato.text"></label>
							<input type="text" class="form-control" data-bind="class: NumeroContrato.requiredClass, enable: NumeroContrato.enable, value: NumeroContrato.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroContrato.maxlength, id : NumeroContrato.id}" />
						</div>
					</div>
					<div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
						<div class="form-group">
							<label class="form-label" data-bind="text: Descricao.text"></label>
							<input type="text" class="form-control" data-bind="class: Descricao.requiredClass, enable: Descricao.enable, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
						</div>
					</div>
					<div class="col-12 col-md-2" data-bind="visible: Status.visible">
						<div class="form-group">
							<label class="form-label" data-bind="text: Status.text"></label>
							<input type="text" class="form-control" data-bind="class: Status.requiredClass, enable: Status.enable, value: Status.val, valueUpdate: 'afterkeydown', attr: { maxlength: Status.maxlength, id : Status.id}" />
						</div>
					</div>
					<div class="col-12 col-md-4" data-bind="visible: ValorContrato.visible">
						<div class="form-group">
							<label class="form-label" data-bind="text: ValorContrato.text, attr: { for: ValorContrato.id }"></label>
							<input type="text" class="form-control" data-bind="class: ValorContrato.requiredClass, enable: ValorContrato.enable, value: ValorContrato.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorContrato.maxlength, id : ValorContrato.id}" />
						</div>
					</div>
					<div class="col-12 col-md-4">
						<div class="form-group">
							<label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
							<div class="input-group">
								<input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, enable: TipoOperacao.enable, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
								<div class="input-group-append">
									<button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}, enable: TipoOperacao.enable" type="button" aria-label="Buscar">
										<i class="fal fa-search"></i>
									</button>
								</div>
							</div>
						</div>
					</div>
					<div class="col-12 col-md-4">
						<div class="form-group">
							<label class="form-label" data-bind="text: Cliente.text, attr: { for: Cliente.id }"></label>
							<div class="input-group">
								<input type="text" class="form-control" data-bind="css: Cliente.requiredClass, enable: Cliente.enable, value: Cliente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Cliente.maxlength, id : Cliente.id}" />
								<div class="input-group-append">
									<button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Cliente.idBtnSearch}, enable: Cliente.enable" type="button" aria-label="Buscar">
										<i class="fal fa-search"></i>
									</button>
								</div>
							</div>
						</div>
					</div>
					<div class="col-12 col-md-2" data-bind="visible: DataInicioContrato.visible">
						<div class="form-group">
							<label class="form-label" data-bind="text: DataInicioContrato.text"></label>
							<input type="text" class="form-control" data-bind="css: DataInicioContrato.requiredClass, value: DataInicioContrato.val, enable: DataInicioContrato.enable, attr: { maxlength: DataInicioContrato.maxlength, id : DataInicioContrato.id} " />
						</div>
					</div>
					<div class="col-12 col-md-2" data-bind="visible: DataFimContrato.visible">
						<div class="form-group">
							<label class="form-label" data-bind="text: DataFimContrato.text"></label>
							<input type="text" class="form-control" data-bind="css: DataInicioContrato.requiredClass, value: DataFimContrato.val, enable: DataFimContrato.enable, attr: { maxlength: DataFimContrato.maxlength, id : DataFimContrato.id} " />
						</div>
					</div>
				</div>
			</form>
			<footer id="knockoutCRUDContrato">
				<div class="row">
					<div class="col-6">
						<button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
							<i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
						</button>
					</div>
					<div class="col-6 d-flex justify-content-end">
						<button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
							<i class="fal fa-check"></i><span data-bind="text: Atualizar.text"></span>
						</button>
						<button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="visible: Adicionar.visible, click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
							<span data-bind="text: Adicionar.text"></span>
						</button>
					</div>
				</div>
			</footer>
		</div>
	</div>
</div>
<script src="~/scripts/ContratoFreteCliente"></script>
<script type="text/javascript">

	pageSetUp();

	$(document).ready(function () {
		_PermissoesPersonalizadas = @Html.Raw(ViewBag.PermissoesPersonalizadas);
		LoadLocalizationResources(["Escrituracao.ContratoFreteCliente"]).then(loadContrato);
	});

</script>
