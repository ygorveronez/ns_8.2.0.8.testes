
<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">MDFE</li>
        <li class="breadcrumb-item">Encerramento</li>

    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Encerramento de MDF-e
        </h1>
    </div>
    <div id="knockoutPesquisaMDFe" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>

                    Pesquisar MDF-es
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroInicial.text, attr: { for: NumeroInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroInicial.maxlength, id : NumeroInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroFinal.text, attr: { for: NumeroFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroFinal.maxlength, id : NumeroFinal.id}" />
                                </div>
                            </div>
                        </div>
                        <div class="row" data-bind="id: BuscaAvancada.idFade, fadeVisible: BuscaAvancada.visibleFade">
                            <div class="col-12 col-md-6" data-bind="visible: EstadoCarregamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EstadoCarregamento.text, attr: { for: EstadoCarregamento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: EstadoCarregamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoCarregamento.maxlength, id : EstadoCarregamento.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: EstadoCarregamento.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: EstadoDescarregamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EstadoDescarregamento.text, attr: { for: EstadoDescarregamento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: EstadoDescarregamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoDescarregamento.maxlength, id : EstadoDescarregamento.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: EstadoDescarregamento.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="d-flex justify-content-end mt-2">
                    <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                        <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                    </button>
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                    </button>
                </div>

            </div>
            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
        </div>
    </div>
</section>

<div class="modal fade" id="divModalCancelamento" role="dialog" aria-hidden="true">
    <div id="knockoutCancelamento" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Cancelar MFD-e</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="col-12 col-md-3" data-bind="visible: DataHora.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: DataHora.text, attr: { for: DataHora.id }"></label>
                        <input type="text" class="form-control" data-bind="value: DataHora.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataHora.maxlength, id : DataHora.id}" />
                    </div>
                </div>
                <div class="form-group" data-bind="visible: Justificativa.visible">
                    <label class="form-label" data-bind="text: Justificativa.text, attr: { for: Justificativa.id }"></label>
                    <textarea class="form-control" rows="5" type="text" data-bind="value: Justificativa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Justificativa.maxlength, id : Justificativa.id}"></textarea>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-danger" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                        <i class="fal fa-times"></i><span data-bind="text: Cancelar.text"></span>
                    </button>                
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalEdiFiscal" role="dialog" aria-hidden="true">
    <div id="knockoutEdiFiscal" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">EDI Fiscal MDF-e</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group" data-bind="visible: Lacre.visible">
                    <label class="form-label" data-bind="text: Lacre.text, attr: { for: Lacre.id }"></label>
                    <textarea class="form-control" rows="5" type="text" data-bind="value: Lacre.val, valueUpdate: 'afterkeydown', attr: { maxlength: Lacre.maxlength, id : Lacre.id}"></textarea>
                </div>
                <div class="d-flex justify-content-end mt-2">
                    <button class="btn btn-default" type="button" data-bind="click: GerarEDIFiscal.eventClick, attr: { id : GerarEDIFiscal.id}">
                        <i class="fal fa-times"></i><span data-bind="text: GerarEDIFiscal.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="knockoutInclusaoMotoristaMDFe" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Inclusão de Motorista no MDF-e</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-4 col-md-6">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataEvento.text, attr: { for: DataEvento.id }"></label>
                            <input type="text" class="form-control" data-bind="css: DataEvento.requiredClass, value: DataEvento.val, attr: { maxlength: DataEvento.maxlength, id : DataEvento.id}" />
                        </div>
                    </div>
                    <div class="col-md-6" data-bind="visible: Motorista.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, enable: Motorista.enable, attr: { maxlength: Motorista.maxlength, id: Motorista.id}, event: { blur: Motorista.eventChange }, valueUpdate: 'afterkeydown'" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : Motorista.enable, attr: { id: Motorista.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-2">
                    <button class="btn btn-primary" type="button" data-bind="click: Adicionar.eventClick, attr: { id: Adicionar.id}">
                        <i class="fal fa-plus"></i><span data-bind="text: Adicionar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/encerramentoMDFe"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadEncerramentoMDFe();
    });

</script>

