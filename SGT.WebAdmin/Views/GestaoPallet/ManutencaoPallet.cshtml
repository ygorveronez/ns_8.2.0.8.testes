<section id="widget-grid">
    <div class="container-box">
        <div class="contain-actions">
            <div class="w-100">
                <ol class="breadcrumb page-breadcrumb">
                    <li class="breadcrumb-item">Gestão de Pallets</li>
                    <li class="breadcrumb-item active">Manutenção de Pallet</li>
                </ol>

                <div class="subheader">
                    <h1 class="subheader-title">Manutenção de Pallet</h1>
                </div>
            </div>
            <div class="header-search">
                <div class="layout-and-filters">
                    <div class="box-search" style="margin-right:0px">
                        <div class="button-filter" onclick="exibirFiltrosManutencaoPallet()"></div>
                    </div>
                </div>

                <div id="carousel-filters" class="carousel" data-ride="carousel" data-bs-interval="false">
                    <div class="carousel-inner">
                        <div class="carousel-item active" id="knoutCabecalhoManutencaoPallet">
                            <div class="bar-view-config">
                                <span>Situações</span>

                                <div class="view-selection" id="view-select-cards">
                                    <div id="view-1" class="view-select-button active">
                                        <div class="icon-number" data-bind="text: EmManutencao.val"></div>
                                        <span data-bind="text: EmManutencao.text"></span>
                                    </div>
                                    <div class="vr"></div>
                                    <div id="view-2" class="view-select-button">
                                        <div class="icon-number" data-bind="text: Disponivel.val"></div>
                                        <span data-bind="text: Disponivel.text"></span>
                                    </div>
                                    <div class="vr"></div>
                                    <div id="view-3" class="view-select-button">
                                        <div class="icon-number" data-bind="text: Descarte.val"></div>
                                        <span data-bind="text: Descarte.text"></span>
                                    </div>
                                    <div class="vr"></div>
                                    <div id="view-4" class="view-select-button">
                                        <div class="icon-number" data-bind="text: Sucata.val"></div>
                                        <span data-bind="text: Sucata.text"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="grid-panel panel mt-4">
                <div class="panel-container">
                    <div class="panel-container">
                        <div class="panel-content">
                            <div class="d-flex justify-content-end mt-3" style="gap:8px;">
                                <button class="btn btn-primary" type="button" onclick="exibirModalAdicionarManutencaoPallet()">
                                    <span>Adicionar Manutenção Pallet</span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <div class="grid-panel panel mt-4">
                <div class="panel-container">
                    <div class="panel-container">
                        <div class="panel-content">
                            <table width="100%" class="table table-bordered table-hover" id="grid-gestao-pallet-manutencao-pallet" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
            </div>

            <div id="divModalFiltroPesquisaManutencaoPallet" class="modal fade" role="dialog" aria-hidden="true">
                <div id="knockoutPesquisaManutencaoPallet" class="modal-dialog modal-xl modal-dialog-centered">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title">Filtrar</h4>
                            <button type="button" class="btn-close" data-bs-dismiss="modal">
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="border p-4">
                                <div class="row">
                                    <div class="col-12 col-md-2" data-bind="visible: Carga.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id: Carga.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: DataInicialMovimentacao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataInicialMovimentacao.text, attr: { for: DataInicialMovimentacao.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: DataInicialMovimentacao.requiredClass, value: DataInicialMovimentacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialMovimentacao.maxlength, id: DataInicialMovimentacao.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: DataFinalMovimentacao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataFinalMovimentacao.text, attr: { for: DataFinalMovimentacao.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: DataFinalMovimentacao.requiredClass, value: DataFinalMovimentacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalMovimentacao.maxlength, id: DataFinalMovimentacao.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: TipoMovimentacao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: TipoMovimentacao.text, attr: { for: TipoMovimentacao.id }"></label>
                                            <select class="form-control" data-bind="class: TipoMovimentacao.requiredClass, options: TipoMovimentacao.options, optionsText: 'text', optionsValue: 'value', value: TipoMovimentacao.val, attr: { maxlength: TipoMovimentacao.maxlength, id: TipoMovimentacao.id}"></select>
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: TipoManutencaoPallet.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: TipoManutencaoPallet.text, attr: { for: TipoManutencaoPallet.id }"></label>
                                            <select class="form-control" data-bind="class: TipoManutencaoPallet.requiredClass, options: TipoManutencaoPallet.options, optionsText: 'text', optionsValue: 'value', value: TipoManutencaoPallet.val, attr: { maxlength: TipoManutencaoPallet.maxlength, id: TipoManutencaoPallet.id}"></select>
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id: Filial.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: Cliente.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Cliente.text, attr: { for: Cliente.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: Cliente.requiredClass, value: Cliente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Cliente.maxlength, id : Cliente.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Cliente.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="d-flex justify-content-end mt-3" style="gap:8px;">
                                <button class="btn btn-outline-primary waves-effect waves-themed ghost-button" type="button" data-bind="click: CarregarFiltrosPesquisa.eventClick, attr: { id: CarregarFiltrosPesquisa.id}">
                                    <span data-bind="text: CarregarFiltrosPesquisa.text"></span>
                                </button>
                                <button class="btn btn-outline-primary waves-effect waves-themed" type="button" data-bind="click: ConfiguracaoModeloFiltroPesquisa.eventClick, attr: { id: ConfiguracaoModeloFiltroPesquisa.id}">
                                    <span data-bind="text: ConfiguracaoModeloFiltroPesquisa.text"></span>
                                </button>
                                <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                    <span data-bind="text: Pesquisar.text"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal fade" id="divModalAdicionarManutencaoPallet" role="dialog" aria-hidden="true" tabindex="-1">
                <div class="modal-dialog modal-lg modal-dialog-centered">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h1>Adicionar Manutenção Pallet</h1>

                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true">
                                <i class="fal fa-times" onclick="fecharModalAdicionarManutencaoPallet()"></i>
                            </button>
                        </div>

                        <div class="modal-body pt-0">
                            <div class="tab-content p-2">
                                <div id="knoutAdicionarManutencaoPallet" class="tab-pane active">
                                    <div class="modal-body">
                                        <div class="border p-4">
                                            <div class="row">

                                                <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id: Filial.id}" />
                                                            <div class="input-group-append">
                                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                                    <i class="fal fa-search"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-12 col-md-6" data-bind="visible: TipoEntradaSaida.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: TipoEntradaSaida.text, attr: { for: TipoEntradaSaida.id }"></label>
                                                        <select class="form-control" data-bind="class: TipoEntradaSaida.requiredClass, options: TipoEntradaSaida.options, optionsText: 'text', optionsValue: 'value', value: TipoEntradaSaida.val, attr: { maxlength: TipoEntradaSaida.maxlength, id : TipoEntradaSaida.id}"></select>
                                                    </div>
                                                </div>

                                                <div class="col-12 col-md-6" data-bind="visible: QuantidadePallets.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: QuantidadePallets.text"></label>
                                                        <input type="text" class="form-control" data-bind="class: QuantidadePallets.requiredClass, value: QuantidadePallets.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadePallets.maxlength, id: QuantidadePallets.id}" />
                                                    </div>
                                                </div>

                                                <div class="col-12 col-md-6" data-bind="visible: TipoManutencaoPallet.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: TipoManutencaoPallet.text, attr: { for: TipoManutencaoPallet.id }"></label>
                                                        <select class="form-control" data-bind="class: TipoManutencaoPallet.requiredClass, options: TipoManutencaoPallet.options, optionsText: 'text', optionsValue: 'value', value: TipoManutencaoPallet.val, attr: { maxlength: TipoManutencaoPallet.maxlength, id : TipoManutencaoPallet.id}"></select>
                                                    </div>
                                                </div>

                                                <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                                                        <textarea class="form-control" rows="4" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id: Observacao.id }"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="d-flex justify-content-end mt-3" style="gap:8px;">
                                            <button class="btn btn-outline-primary waves-effect waves-themed ghost-button" type="button" data-bind="click: Cancelar.eventClick, attr: { id: Cancelar.id}">
                                                <span data-bind="text: Cancelar.text"></span>
                                            </button>
                                            <button class="btn btn-primary" type="button" data-bind="click: Adicionar.eventClick, attr: { id: Adicionar.id}">
                                                <span data-bind="text: Adicionar.text"></span>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

