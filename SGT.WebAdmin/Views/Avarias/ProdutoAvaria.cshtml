
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Avarias</li>
    <li class="breadcrumb-item">Configurações</li>
    <li class="breadcrumb-item active">Produtos Avaria</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Produtos Avaria
    </h1>
</div>
<div id="knockoutPesquisaProdutoAvaria">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Produtos
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Produto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Produto.text, attr: { for: Produto.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Produto.requiredClass, value: Produto.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Produto.text, maxlength: Produto.maxlength, id: Produto.id, name: Produto.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Produto.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: GrupoProduto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoProduto.text, attr: { for: GrupoProduto.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: GrupoProduto.requiredClass, value: GrupoProduto.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': GrupoProduto.text, maxlength: GrupoProduto.maxlength, id: GrupoProduto.id, name: GrupoProduto.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoProduto.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFim.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Adicionar Produto Avaria
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutProdutoAvaria">
                <div class="row">
                    <div class="col-12 col-md-4" data-bind="visible: Produto.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Produto.text, attr: { for: Produto.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Produto.requiredClass, value: Produto.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Produto.text, maxlength: Produto.maxlength, id: Produto.id, name: Produto.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Produto.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: QuantidadeCaixa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeCaixa.text"></label>
                            <input type="text" class="form-control" data-bind="value: QuantidadeCaixa.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeCaixa.maxlength, id : QuantidadeCaixa.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CaixasPallet.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CaixasPallet.text"></label>
                            <input type="text" class="form-control" data-bind="value: CaixasPallet.val, valueUpdate: 'afterkeydown', attr: { maxlength: CaixasPallet.maxlength, id : CaixasPallet.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: PesoUnitario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PesoUnitario.text"></label>
                            <input type="text" class="form-control" data-bind="value: PesoUnitario.val, valueUpdate: 'afterkeydown', attr: { maxlength: PesoUnitario.maxlength, id : PesoUnitario.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: PesoCaixa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PesoCaixa.text"></label>
                            <input type="text" class="form-control" data-bind="value: PesoCaixa.val, valueUpdate: 'afterkeydown', attr: { maxlength: PesoCaixa.maxlength, id : PesoCaixa.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: ValorEstorno.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ValorEstorno.text"></label>
                            <input type="text" class="form-control" data-bind="value: ValorEstorno.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorEstorno.maxlength, id : ValorEstorno.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: ValorProducao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ValorProducao.text"></label>
                            <input type="text" class="form-control" data-bind="value: ValorProducao.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorProducao.maxlength, id : ValorProducao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: PrecoTransferencia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PrecoTransferencia.text"></label>
                            <input type="text" class="form-control" data-bind="value: PrecoTransferencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: PrecoTransferencia.maxlength, id : PrecoTransferencia.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: CustoPrimario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CustoPrimario.text"></label>
                            <input type="text" class="form-control" data-bind="value: CustoPrimario.val, valueUpdate: 'afterkeydown', attr: { maxlength: CustoPrimario.maxlength, id : CustoPrimario.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: CustoSecundario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CustoSecundario.text"></label>
                            <input type="text" class="form-control" data-bind="value: CustoSecundario.val, valueUpdate: 'afterkeydown', attr: { maxlength: CustoSecundario.maxlength, id : CustoSecundario.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Observacao.text"></label>
                            <textarea type="text" rows="5" class="form-control" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}"><</textarea>
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end">
                    <span class="ms-1" data-bind='component: {name: "import-file", params: { ko: Importar }}'></span>
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/produtoAvaria"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadProdutoAvaria();
    });

</script>

