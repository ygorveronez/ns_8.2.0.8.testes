                                <label class="custom-control-label" data-bind="text: DesabilitarBotaoTermo.text, attr: { for: DesabilitarBotaoTermo.id }"></label>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <label class="label"><b data-bind="text: Observacao.text"></b></label>
                        <textarea class="form-control" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                    </div>
