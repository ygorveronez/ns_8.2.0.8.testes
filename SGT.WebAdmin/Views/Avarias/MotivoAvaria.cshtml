                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: ContaContabil.idBtnSearch}">
                                        <i class="fa fa-search"></i> &nbsp; Buscar &nbsp;
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="mt-3 col-12 col-md-4" data-bind="visible: Finalidade.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text:  Finalidade.text, attr: { for: Finalidade.id }"></label>
