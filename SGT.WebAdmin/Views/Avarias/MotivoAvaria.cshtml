                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: ObrigarInformarValorParaLiberarOcorrencia.val,enable: ObrigarInformarValorParaLiberarOcorrencia.enable, attr: { maxlength: ObrigarInformarValorParaLiberarOcorrencia.maxlength, name: ObrigarInformarValorParaLiberarOcorrencia.id, id: ObrigarInformarValorParaLiberarOcorrencia.id }">
