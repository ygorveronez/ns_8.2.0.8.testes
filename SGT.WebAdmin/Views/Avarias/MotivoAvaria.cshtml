                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: CalcularOcorrenciaPorTabelaFrete.val,enable: CalcularOcorrenciaPorTabelaFrete.enable, attr: { maxlength: CalcularOcorrenciaPorTabelaFrete.maxlength, name: CalcularOcorrenciaPorTabelaFrete.id, id: CalcularOcorrenciaPorTabelaFrete.id }">
                                <label class="custom-control-label" data-bind="text: CalcularOcorrenciaPorTabelaFrete.text, attr: { for: CalcularOcorrenciaPorTabelaFrete.id }"></label>
