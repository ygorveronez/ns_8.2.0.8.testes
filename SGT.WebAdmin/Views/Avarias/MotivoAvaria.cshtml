                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoOcorrencia.idBtnSearch}">
                                        <i class="fa fa-search"></i> &nbsp; Buscar &nbsp;
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="clearfix"></div>

                    <div class="col-12 col-md-6" data-bind="visible: GerarOcorrenciaAutomaticamente.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: GerarOcorrenciaAutomaticamente.val,enable: GerarOcorrenciaAutomaticamente.enable, attr: { maxlength: GerarOcorrenciaAutomaticamente.maxlength, name: GerarOcorrenciaAutomaticamente.id, id: GerarOcorrenciaAutomaticamente.id }">
                                <label class="custom-control-label" data-bind="text: GerarOcorrenciaAutomaticamente.text, attr: { for: GerarOcorrenciaAutomaticamente.id }"></label>
