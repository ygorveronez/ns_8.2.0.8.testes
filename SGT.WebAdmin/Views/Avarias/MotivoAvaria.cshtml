                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: GerarCTeValorAnteriorTratativaReentrega.val,enable: GerarCTeValorAnteriorTratativaReentrega.enable, attr: { maxlength: GerarCTeValorAnteriorTratativaReentrega.maxlength, name: GerarCTeValorAnteriorTratativaReentrega.id, id: GerarCTeValorAnteriorTratativaReentrega.id }">
