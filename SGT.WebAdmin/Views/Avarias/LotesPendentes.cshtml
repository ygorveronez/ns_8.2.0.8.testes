
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Avarias</li>
    <li class="breadcrumb-item active">Lotes Pendentes</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Consulta de Lotes
    </h1>
    <div class="subheader-help">
        <button type="button" onclick="iniciarAjuda(949)" data-action="toggle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Abrir a ajuda da página" aria-label="Abrir a ajuda da página"><i class="fal fa-question-circle"></i></button>
    </div>
</div>
<div id="knockoutPesquisaLotes">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h2>Pesquisar Lotes</h2>
            </div>
            <div class="panel-content">
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroLote.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroLote.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroLote.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroLote.maxlength, id : NumeroLote.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Transportador.text, maxlength: Transportador.maxlength, id: Transportador.id, name: Transportador.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Transportador.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Etapa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Etapa.text"></label>
                                    <select class="form-control" data-bind="options: Etapa.options, optionsText: 'text', optionsValue: 'value', value: Etapa.val, attr: { id : Etapa.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Lotes Pendentes
                </h3>
            </div>
            <div class="panel-content">
                <div class="d-flex justify-content-end mb-2">
                    <button class="btn btn-primary" type="button" data-bind="click: SelecionarTodos.eventClick, text: SelecionarTodos.text, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible"></button>
                </div>
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                <div class="d-flex justify-content-end mt-3">
                    <button type="button"
                            class="btn btn-success me-2"
                            data-loading-text="Aguarde..."
                            data-bind="click: Finalizar.eventClick, visible: Finalizar.visible, text: Finalizar.text, attr: { id : Finalizar.id}">
                    </button>
                    <button class="btn btn-primary me-2" type="button" data-bind="visible: Delegar.visible, click: Delegar.eventClick, attr: { id: Delegar.id}">
                        <span data-bind="text: Delegar.text"></span>
                    </button>
                    <button class="btn btn-success" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id: AprovarTodas.id }, visible: AprovarTodas.visible">
                        <span data-bind="text: AprovarTodas.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalLote" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Informações do Lote</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="panel">
                    <div class="panel-container">
                        <div class="panel-content">
                            <ul class="nav nav-tabs">
                                <li class="nav-item">
                                    <a href="#knockoutLote" class="nav-link active" data-bs-toggle="tab"><i class="fal fa-info-circle"></i><span class="hidden-mobile hidden-tablet">Lote</span></a>
                                </li>
                                <li class="nav-item" id="liAvarias" style="display:none;">
                                    <a href="#knockoutAvarias" class="nav-link" data-bs-toggle="tab"><i class="fal fa-list-alt"></i><span class="hidden-mobile hidden-tablet">Avarias</span></a>
                                </li>
                                <li class="nav-item" id="liAnexos" style="display:none;">
                                    <a href="#knockoutAnexos" class="nav-link" data-bs-toggle="tab"> <i class="fal fa-file"></i><span class="hidden-mobile hidden-tablet">Anexos</span></a>
                                </li>
                                <li class="nav-item" id="liAutorizacao" style="display:none;">
                                    <a href="#knockoutAutorizacao" class="nav-link" data-bs-toggle="tab"><i class="fal fa-check"></i><span class="hidden-mobile hidden-tablet">Autorização</span></a>
                                </li>
                                <li class="nav-item" id="liDelegar">
                                    <a href="#knockoutDelegar" class="nav-link" data-bs-toggle="tab"><i class="fal fa-exchange"></i><span class="hidden-mobile hidden-tablet">Delegar</span></a>
                                </li>
                            </ul>
                            <div class="tab-content border border-top-0 p-3">
                                <!--  Detalhe Lote  -->
                                <div id="knockoutLote" class="tab-pane fade active show">
                                    <div class="row mb-2">
                                        <h4>Detalhes do Lote</h4>
                                        <hr />
                                    </div>
                                    <form>
                                        <div class="row">
                                            <div class="col-12 col-md-4" data-bind="visible: NumeroLote.visible">
                                                <div class="form-group">
                                                    <label class="form-label"><b data-bind="text: NumeroLote.text"></b><span data-bind="text: NumeroLote.val"></span></label>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-4" data-bind="visible: ValorLote.visible">
                                                <div class="form-group">
                                                    <label class="form-label"><b data-bind="text: ValorLote.text"></b><span data-bind="text: ValorLote.val"></span></label>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-4" data-bind="visible: DataGeracao.visible">
                                                <div class="form-group">
                                                    <label class="form-label"><b data-bind="text: DataGeracao.text"></b><span data-bind="text: DataGeracao.val"></span></label>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-4" data-bind="visible: SituacaoLote.visible">
                                                <div class="form-group">
                                                    <label class="form-label"><b data-bind="text: SituacaoLote.text"></b><span data-bind="text: SituacaoLote.val"></span></label>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                                <div class="form-group">
                                                    <label class="form-label"><b data-bind="text: Transportador.text"></b><span data-bind="text: Transportador.val"></span></label>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-4" data-bind="visible: Responsavel.visible">
                                                <div class="form-group">
                                                    <label class="form-label"><b data-bind="text: Responsavel.text"></b><span data-bind="text: Responsavel.val"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <!-- Anexos -->
                                <div id="knockoutAnexos" class="tab-pane">
                                    <div class="row mb-2">
                                        <h4>Anexos</h4>
                                        <hr />
                                    </div>
                                    <form>
                                        <div class="row">
                                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id : Anexos.idGrid}"></table>
                                        </div>
                                    </form>
                                </div>
                                <!-- Autorização -->
                                <div id="knockoutAutorizacao" class="tab-pane fade">
                                    <div class="row mb-2">
                                        <h4>Autorização</h4>
                                        <hr />
                                    </div>
                                    <form>
                                        <div class="row">
                                            <div class="col-12 d-flex justify-content-end">
                                                <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Aceitar.eventClick, attr: { value: Aceitar.text, id : Aceitar.id}">
                                                    <span data-bind="text: Aceitar.text"></span>
                                                </button>
                                                <button type="button" class="btn btn-labeled btn-warning ms-1" data-bind="click: Rejeitar.eventClick, attr: { value: Rejeitar.text, id : Rejeitar.id}">
                                                    <span data-bind="text: Rejeitar.text"></span>
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <!-- Delegar -->
                                <div id="knockoutDelegar" class="tab-pane fade">
                                    <div class="row mb-2">
                                        <h4>Delegar</h4>
                                        <hr />
                                    </div>
                                    <form>
                                        <div class="row">
                                            <div class="col-12 col-md-12" data-bind="visible: Usuario.visible">
                                                <div class="form-group">
                                                    <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                                    <div class="input-group">
                                                        <input type="text" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Usuario.text, maxlength: Usuario.maxlength, id: Usuario.id, name: Usuario.id }" />
                                                        <div class="input-group-append">
                                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Usuario.idBtnSearch}">
                                                                <i class="fal fa-search"></i>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <footer>
                                                <div class="row">
                                                    <div class="col-12 d-flex justify-content-end">
                                                        <button class="btn btn-success" type="button" data-bind="click: Delegar.eventClick, attr: { id: Delegar.id}">
                                                            <span data-bind="text: Delegar.text"></span>
                                                        </button>
                                                    </div>
                                                </div>
                                            </footer>
                                        </div>
                                    </form>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalDelegarLote" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" id="knockoutDelegarLote" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Delegar Cargas</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="row">
                        <div class="col-12 col-md-12" data-bind="visible: Usuario.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Usuario.text, maxlength: Usuario.maxlength, id: Usuario.id, name: Usuario.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Usuario.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" data-bind="click: Cancelar.eventClick, attr: { value: Cancelar.text, id : Cancelar.id}">
                    <span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-success" data-bind="click: Delegar.eventClick, attr: { value: Delegar.text, id : Delegar.id}">
                    <span data-bind="text: Delegar.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

