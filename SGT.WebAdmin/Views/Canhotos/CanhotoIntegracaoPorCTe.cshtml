
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Canhotos</li>
    <li class="breadcrumb-item active">Canhoto Integração Por CT-e</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Integração de Canhotos Por CT-e
    </h1>
</div>

<div id="knockoutPesquisaCanhotoPorCTe">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h2>Filtrar Imagens</h2>
            </div>
            <div class="panel-content">
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroDocumento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroDocumento.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroDocumento.maxlength, id : NumeroDocumento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFim.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-5" data-bind="visible: Emitente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Emitente.text, attr: { for: Emitente.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Emitente.requiredClass, value: Emitente.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Emitente.text, maxlength: Emitente.maxlength, id: Emitente.id, name: Emitente.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Emitente.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-5" data-bind="visible: CodigoCargaEmbarcador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text, attr: { for: CodigoCargaEmbarcador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: CodigoCargaEmbarcador.requiredClass, value: CodigoCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CodigoCargaEmbarcador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: TipoRegistro.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRegistro.text, attr: { for: TipoRegistro.id }"></label>
                                    <select class="form-control" data-bind="class: TipoRegistro.requiredClass, options: TipoRegistro.options, optionsText: 'text', optionsValue: 'value', value: TipoRegistro.val, attr: { maxlength: TipoRegistro.maxlength, id : TipoRegistro.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEmissaoNFeInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoNFeInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoNFeInicial.requiredClass, value: DataEmissaoNFeInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoNFeInicial.maxlength, id : DataEmissaoNFeInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEmissaoNFeFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoNFeFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoNFeFinal.requiredClass, value: DataEmissaoNFeFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoNFeFinal.maxlength, id : DataEmissaoNFeFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEntregaInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEntregaInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEntregaInicial.requiredClass, value: DataEntregaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEntregaInicial.maxlength, id : DataEntregaInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEntregaFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEntregaFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEntregaFinal.requiredClass, value: DataEntregaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEntregaFinal.maxlength, id : DataEntregaFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataDigitalizacaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataDigitalizacaoInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataDigitalizacaoInicial.requiredClass, value: DataDigitalizacaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataDigitalizacaoInicial.maxlength, id : DataDigitalizacaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataDigitalizacaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataDigitalizacaoFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataDigitalizacaoFinal.requiredClass, value: DataDigitalizacaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataDigitalizacaoFinal.maxlength, id : DataDigitalizacaoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataAprovacaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAprovacaoInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataAprovacaoInicial.requiredClass, value: DataAprovacaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAprovacaoInicial.maxlength, id : DataAprovacaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataAprovacaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAprovacaoFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataAprovacaoFinal.requiredClass, value: DataAprovacaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAprovacaoFinal.maxlength, id : DataAprovacaoFinal.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Integrações
                </h3>
            </div>
            <div class="row col-12">
                <div class="mt-2 ms-3">
                    <span><b>Legendas:</b> </span>
                    <div class="badge legend yellow">
                        <b>Aguardando retorno</b>
                    </div>
                    <div class="badge legend blue">
                        <b>Aguardando Integrações</b>
                    </div>
                    <div class="badge legend green">
                        <b>Integrado</b>
                    </div>
                    <div class="badge legend red">
                        <b>Falha Integração</b>
                    </div>
                </div>
                <div class="d-flex justify-content-end">
                    <button class="btn btn-primary" data-bind="click: ReenviarTodos.eventClick, attr: { id: ReenviarTodos.id}" type="button">
                        <span data-bind="text: ReenviarTodos.text"></span>
                    </button>
                </div>
            </div>
            <div class="panel-content">
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalHistoricoIntegracaoPorCTe" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Histórico de Integração</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table width="100%" class="table table-bordered table-hover" cellspacing="0" id="tabHistoricoIntegracaoPorCte"></table>
            </div>
        </div>
    </div>
</div>

<style>
    .img-canhoto {
        border: 1px solid #d0d0d0;
        border-radius: 3px;
    }
</style>

<script src="~/scripts/canhotoIntegracaoPorCTe"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadCanhotoIntegracaoPorCTe();
    });

</script>