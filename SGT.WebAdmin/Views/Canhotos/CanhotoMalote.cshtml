
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Canhotos</li>
    <li class="breadcrumb-item active">Canhoto Malote</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Enviar Canhotos em Malote
    </h1>
</div>

<div id="knockoutPesquisaCanhotos">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h2>Pesquisar Canhotos</h2>
            </div>
            <div class="panel-content">

                <form>
                    <div class="row">
                        <div class="col-12 col-md-3" data-bind="visible: Numero.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Numero.text"></label>
                                <input type="text" class="form-control" data-bind="value: Numero.val, valueUpdate: 'afterkeydown', attr: { maxlength: Numero.maxlength, id : Numero.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicio.text"></label>
                                <input type="text" class="form-control" data-bind="value: DataInicio.val, attr: { maxlength: DataInicio.maxlength, id : DataInicio.id} " />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataFim.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFim.text"></label>
                                <input type="text" class="form-control" data-bind="value: DataFim.val, attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: TipoCanhoto.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoCanhoto.text"></label>
                                <select class="form-control" data-bind="options: TipoCanhoto.options, optionsText: 'text', optionsValue: 'value', value: TipoCanhoto.val, attr: { id : TipoCanhoto.id}, event : {change : TipoCanhoto.eventChange}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: SituacaoCanhoto.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: SituacaoCanhoto.text"></label>
                                <select class="form-control" data-bind="options: SituacaoCanhoto.options, optionsText: 'text', optionsValue: 'value', value: SituacaoCanhoto.val, attr: { id : SituacaoCanhoto.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: SituacaoDigitalizacaoCanhoto.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: SituacaoDigitalizacaoCanhoto.text"></label>
                                <select class="form-control" data-bind="options: SituacaoDigitalizacaoCanhoto.options, optionsText: 'text', optionsValue: 'value', value: SituacaoDigitalizacaoCanhoto.val, attr: { id : SituacaoDigitalizacaoCanhoto.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CodigoCargaEmbarcador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text"></label>
                                <input type="text" class="form-control" data-bind="value: CodigoCargaEmbarcador.val,valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CodigosCargaEmbarcador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigosCargaEmbarcador.text, attr: { for: CodigosCargaEmbarcador.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: CodigosCargaEmbarcador.requiredClass, value: CodigosCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigosCargaEmbarcador.maxlength, id : CodigosCargaEmbarcador.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CodigosCargaEmbarcador.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Transportador.text, maxlength: Transportador.maxlength, id: Transportador.id, name: Transportador.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Transportador.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Motorista.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Motorista.text, maxlength: Motorista.maxlength, id: Motorista.id, name: Motorista.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Motorista.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Emitente.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Emitente.text, attr: { for: Emitente.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: Emitente.requiredClass, value: Emitente.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Emitente.text, maxlength: Emitente.maxlength, id: Emitente.id, name: Emitente.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Emitente.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Filial.text, maxlength: Filial.maxlength, id: Filial.id, name: Filial.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Filial.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="d-flex align-items-center justify-content-end mb-3">
                    <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                    </button>
                </div>

            </div>
        </div>
    </div>

    <div class="panel" data-bind="visible: Salvar.visible">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Canhotos
                </h3>
            </div>
            <div class="panel-content">
                <div class="d-flex justify-content-end mb-2">
                    <button class="btn btn-primary" type="button" data-bind="click: SelecionarTodos.eventClick, text: SelecionarTodos.text, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible"></button>
                </div>
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>

                <div class="d-flex justify-content-between mt-3">
                    <div class="d-flex">
                        <button class="btn btn-default me-2" type="button" data-bind="click: Cancelar.eventClick, attr: { id: Cancelar.id }, visible: Cancelar.visible">
                            <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
                        </button>
                    </div>
                    <div class="d-flex justify-content-end">
                        <button class="btn btn-success" type="button" data-bind="click: Salvar.eventClick, attr: { id: Salvar.id }, visible: Salvar.visible">
                            <span data-bind="text: Salvar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="ModalDivDetalhesCanhoto" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Canhoto</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="divDetalhesCanhoto">

            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalMalote" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Salvar Malote</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutMalote">
                <div class="row">
                    <div class="col-12 col-md-4" data-bind="visible: QuantidadeCanhotos.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeCanhotos.text"></label>
                            <input type="text" class="form-control" data-bind="value: QuantidadeCanhotos.val, enable: false, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeCanhotos.maxlength, id : QuantidadeCanhotos.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: ProtocoloMalote.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ProtocoloMalote.text"></label>
                            <input type="text" class="form-control" data-bind="value: ProtocoloMalote.val, enable: false, valueUpdate: 'afterkeydown', attr: { maxlength: ProtocoloMalote.maxlength, id : ProtocoloMalote.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: DataMalote.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataMalote.text"></label>
                            <input type="text" class="form-control" data-bind="value: DataMalote.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataMalote.maxlength, id : DataMalote.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: OperadorManualMalote.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: OperadorManualMalote.text, attr: { for: OperadorManualMalote.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: OperadorManualMalote.requiredClass, value: OperadorManualMalote.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': OperadorManualMalote.text, maxlength: OperadorManualMalote.maxlength, id: OperadorManualMalote.id, name: OperadorManualMalote.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: OperadorMalote.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: OrigemMalote.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: OrigemMalote.text"></label>
                            <input type="text" class="form-control" data-bind="value: OrigemMalote.val, enable: false, valueUpdate: 'afterkeydown', attr: { maxlength: OrigemMalote.maxlength, id : OrigemMalote.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: DestinoMalote.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DestinoMalote.text, attr: { for: DestinoMalote.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: DestinoMalote.requiredClass, value: DestinoMalote.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': DestinoMalote.text, maxlength: DestinoMalote.maxlength, id: DestinoMalote.id, name: DestinoMalote.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: DestinoMalote.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 d-flex justify-content-end">
                        <button type="button" class="btn btn-success" data-bind="click: GerarMalote.eventClick,visible: GerarMalote.visible, attr: { value: GerarMalote.text, id : GerarMalote.id}">
                            <span data-bind="text: GerarMalote.text"></span>
                        </button>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/canhotoMalote"></script>

