
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Canhotos</li>
    <li class="breadcrumb-item active">Malote</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Malotes
    </h1>
</div>

<div id="knockoutPesquisaMalote">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Malotes
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: Protocolo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Protocolo.text, attr: { for: Protocolo.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Protocolo.requiredClass, value: Protocolo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Protocolo.maxlength, id : Protocolo.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroCanhoto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCanhoto.text, attr: { for: NumeroCanhoto.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroCanhoto.requiredClass, value: NumeroCanhoto.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCanhoto.maxlength, id : NumeroCanhoto.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Emitente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Emitente.text, attr: { for: Emitente.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Emitente.requiredClass, value: Emitente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Emitente.maxlength, id : Emitente.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Emitente.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Operador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Operador.text, attr: { for: Operador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Operador.requiredClass, value: Operador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Operador.maxlength, id : Operador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Operador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel mt-3" id="divCadastroMalote" style="display: none">
    <div id="myTab" class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs">
                <li class="nav-item">
                    <a href="#tabMaloteDados" class="nav-link active" data-bs-toggle="tab">
                        <span class="hidden-mobile hidden-tablet">Dados Malote</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#tabCanhotos" class="nav-link" data-bs-toggle="tab">
                        <span class="hidden-mobile hidden-tablet">Canhotos</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3" id="knockoutMalote">
                <div class="tab-pane fade active show"  role="tabpanel" id="tabMaloteDados">
                    <form novalidate>
                        <div class="panel-heading"><h4>Cadastro</h4></div>
                        <div class="row" id="divNovoMalote" style="display:none">
                            <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: FilialCadastro.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: FilialCadastro.text, attr: { for: FilialCadastro.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: FilialCadastro.requiredClass, value: FilialCadastro.val, valueUpdate: 'afterkeydown', attr: { maxlength: FilialCadastro.maxlength, id : FilialCadastro.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: FilialCadastro.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                            <div class="row col-12" id="divDadosMaloteEditar">
                                <div class="row col-md-4">
                                    <label data-bind="visible: Protocolo.visible"><b><span data-bind="text: Protocolo.text"></span></b> <span data-bind="text: Protocolo.val"></span></label>
                                    <label data-bind="visible: Quantidade.visible"><b><span data-bind="text: Quantidade.text"></span></b> <span data-bind="text: Quantidade.val"></span></label>
                                    <label data-bind="visible: Data.visible"><b><span data-bind="text: Data.text"></span></b> <span data-bind="text: Data.val"></span></label>
                                </div>
                                <div class="row col-md-4">
                                    <label data-bind="visible: Filial.visible"><b><span data-bind="text: Filial.text"></span></b> <span data-bind="text: Filial.val"></span></label>
                                    <label data-bind="visible: Operador.visible"><b><span data-bind="text: Operador.text"></span></b> <span data-bind="text: Operador.val"></span></label>
                                    <label data-bind="visible: Origem.visible"><b><span data-bind="text: Origem.text"></span></b> <span data-bind="text: Origem.val"></span></label>
                                </div>
                                <div class="row col-md-4">
                                    <label data-bind="visible: Destino.visible"><b><span data-bind="text: Destino.text"></span></b> <span data-bind="text: Destino.val"></span></label>
                                    <label data-bind="visible: tpOperacao.visible"><b><span data-bind="text: tpOperacao.text"></span></b> <span data-bind="text: tpOperacao.val"></span></label>
                                    <label data-bind="visible: Motivo.val() != ''"><b><span data-bind="text: Motivo.text"></span></b> <span data-bind="text: Motivo.val"></span></label>
                                </div>
                            </div>
                    </form>
                </div>

                <div class="tab-pane fade" id="tabCanhotos" role="tabpanel">
                    <div class="panel-heading"><h4>Canhotos</h4></div>
                    <form novalidate>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Codigo.idGrid}" cellspacing="0"></table>
                        <div class="row mt-3">
                            <div class="col-12 col-md-10" data-bind="visible: DadosCanhoto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DadosCanhoto.text, attr: { for: DadosCanhoto.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="enable : DadosCanhoto.enable, css: DadosCanhoto.requiredClass, value: DadosCanhoto.val, valueUpdate: 'afterkeydown', attr: { maxlength: DadosCanhoto.maxlength, id : DadosCanhoto.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-success waves-effect waves-themed" data-bind="attr: { id: DadosCanhoto.idBtnSearch}, click: DadosCanhoto.eventClick, enable : DadosCanhoto.enable" type="button" aria-label="Buscar">
                                                <i class="fal fa-arrow-right"></i>  Enviar
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" style="margin-top:27px" data-bind="visible : Confirmar.visible">
                                <button class="btn btn-default" type="button" data-bind="enable : Confirmar.enable, click: Confirmar.eventClick, attr: { id: Confirmar.id}">
                                    <span data-bind="text: Confirmar.text"></span>
                                </button>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="d-flex justify-content-between mt-3">
                    <div class="d-flex">
                        <button class="btn btn-success" type="button" data-bind="visible: Conferencia.visible, click: Conferencia.eventClick, attr: { id: Conferencia.id}">
                            <span data-bind="text: Conferencia.text"></span>
                        </button>
                    </div>
                    <div class="d-flex justify-content-end">
                        <button class="btn btn-default me-2" type="button" data-bind="visible: Imprimir.visible, click: Imprimir.eventClick, attr: { id: Imprimir.id}">
                            <span data-bind="text: Imprimir.text"></span>
                        </button>
                        <button class="btn btn-danger me-2" type="button" data-bind="visible: Cancelar.visible, click: Cancelar.eventClick, attr: { id: Cancelar.id}">
                            <span data-bind="text: Cancelar.text"></span>
                        </button>
                        <button class="btn btn-success me-2" type="button" data-bind="visible: GerarMalote.visible, click: GerarMalote.eventClick, attr: { id: GerarMalote.id}">
                            <span data-bind="text: GerarMalote.text"></span>
                        </button>
                        <button class="btn btn-default" type="button" data-bind="visible: Limpar.visible, click: Limpar.eventClick, attr: { id: Limpar.id}">
                            <span data-bind="text: Limpar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="ModalDivDetalhesCanhoto" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel">Canhoto</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="divDetalhesCanhoto">

            </div>
        </div>
    </div>
</div>

<div id="ModalDivInconsistencia" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Inconsistência no Malote</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="KnoutInconsistencia">
                <div class="col-12 col-md-12" data-bind="visible: Motivo.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                        <textarea class="form-control" rows="4" data-bind="value: Motivo.val, class: Motivo.requiredClass, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id }"></textarea>
                    </div>
                </div>
                <div class="col-12 d-flex justify-content-end">
                    <button class="btn btn-success" type="button" data-bind="visible: Salvar.visible, click: Salvar.eventClick, attr: { id: Salvar.id}">
                        <span data-bind="text: Salvar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="ModalDivConferencia" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Conferência</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutConferencia">
                <div class="row container-imagens">
                    <!-- ko foreach: { data: Imagens.val, as: 'imagem' } -->
                    <div class="col col-sm-6 col-md-6 col-lg-6">
                        <div data-bind="css: { 'container-img': true, 'possui-imagem': imagem.Miniatura != null, 'sem-imagem': imagem.Miniatura == null }">
                            <strong>Canhoto: </strong><span data-bind="text: imagem.Numero"></span>
                            <!-- ko if: imagem.Miniatura == null -->
                            <div class="img-canhoto"></div>
                            <!-- /ko -->
                            <!-- ko if: imagem.Miniatura != null -->
                            <div class="img-canhoto container-drag">
                                <img class="draggable-img" data-bind="attr: { src:  'data:image/png;base64,' + imagem.Miniatura }" />
                            </div>
                            <!-- /ko -->
                            <input type="button" data-bind="click: function(e){ $parent.Rejeitar.eventClick(e) }, visible: !imagem.Rejeitado, attr: { value: 'Rejeitar' }" class="btnPesquisarFiltroPesquisa btn btn-labeled btn-danger btn-xs" />
                            <input type="button" data-bind="click: function(e){ $parent.Validar.eventClick(e) }, visible: imagem.Rejeitado, attr: { value: 'Validar' }" class="btnPesquisarFiltroPesquisa btn btn-labeled btn-success btn-xs" />
                        </div>
                    </div>
                    <!-- /ko -->
                </div>
            </div>
        </div>
    </div>
</div>

<div id="ModalDivConferenciaDetalhes" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Imagem Detalhada</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="well">
                    <img style="width: 100%" class="img-fluid d-block img-canhoto" src="" />
                </div>
            </div>
        </div>
    </div>
</div>

<div id="divModalMotivoInconsistenciaDigitacao" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Adicionar Motivo da Inconsistência</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutMotivoInconsistenciaDigitacao">
                <div class="col-12 col-md-12" data-bind="visible: Motivo.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                        <div class="input-group">
                            <input type="text" class="form-control" data-bind="enable : Motivo.enable, css: Motivo.requiredClass, value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}" />
                            <div class="input-group-append">
                                <button class="btn btn-primary waves-effect waves-themed" data-bind="enable : Motivo.enable, attr: { id: Motivo.idBtnSearch}" type="button" aria-label="Buscar">
                                    <i class="fal fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-12 d-flex justify-content-end">
                    <button class="btn btn-success" type="button" data-bind="click: Adicionar.eventClick, attr: { id: Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

