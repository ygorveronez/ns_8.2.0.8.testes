
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Patrimonio</li>
    <li class="breadcrumb-item active">Baixa Bem</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Baixa de Patrimônios
    </h1>
</div>

<div id="knockoutPesquisaBaixaBem">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Baixa de Patrimônios
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Bem.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Bem.text, attr: { for: Bem.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Bem.requiredClass, value: Bem.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Bem.text, maxlength: Bem.maxlength, id: Bem.id, name: Bem.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Bem.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Baixa de Patrimônio
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroBaixaBem">
                    <div class="col-12 col-md-6" data-bind="visible: Bem.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Bem.text, attr: { for: Bem.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Bem.requiredClass, value: Bem.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Bem.text, maxlength: Bem.maxlength, id: Bem.id, name: Bem.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Bem.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Data.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Data.text"></label>
                            <input type="text" class="form-control" data-bind="css: Data.requiredClass, value: Data.val, valueUpdate: 'afterkeydown', attr: { maxlength: Data.maxlength, id : Data.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: ValorVenda.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ValorVenda.text"></label>
                            <input type="text" class="form-control" data-bind="css: ValorVenda.requiredClass, value: ValorVenda.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorVenda.maxlength, id : ValorVenda.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Funcionario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Funcionario.requiredClass, value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Funcionario.text, maxlength: Funcionario.maxlength, id: Funcionario.id, name: Funcionario.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Funcionario.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: NotaFiscal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NotaFiscal.text, attr: { for: NotaFiscal.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: NotaFiscal.requiredClass, value: NotaFiscal.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': NotaFiscal.text, maxlength: NotaFiscal.maxlength, id: NotaFiscal.id, name: NotaFiscal.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: NotaFiscal.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDBaixaBem">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger me-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success  me-2" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-primary" data-loading-text="Aguarde..." data-bind="click: TermoBaixa.eventClick,visible: TermoBaixa.visible, attr: { value: TermoBaixa.text, id : TermoBaixa.id}">
                                <span data-bind="text: TermoBaixa.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/baixaBem"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadBaixaBem();
    });

</script>

