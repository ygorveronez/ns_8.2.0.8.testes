                                            <label class="form-label" data-bind="text: Responsavel.text, attr: { for: Responsavel.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: Responsavel.requiredClass, value: Responsavel.val, valueUpdate: 'afterkeydown', attr: { maxlength: Responsavel.maxlength, id : Responsavel.id}" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--/ Lancamento -->
                    <!-- Devolucao -->
                    <div class="tab-pane fade " id="tabDevolucao">
                        <div id="knockoutDevolucao">
                            <div class="card mb-3">
                                <div class="card-header">
                                    <h3 class="card-title">Devolução Vale Pallet</h3>
                                </div>
                                <div class="card-body">
                                    <div data-bind="visible: Pendente.val">
                                        <div class="alert alert-warning">
                                            <strong>O Vale Pallet está pendente de devolução.</strong>
                                        </div>
                                    </div>
                                    <div data-bind="visible: Cancelado.val">
                                        <div class="alert alert-danger">
                                            <strong>A devolução do Vale Pallet foi cancelada.</strong>
                                        </div>
                                    </div>
                                    <div data-bind="visible: Devolvido.val">
                                        <div class="alert alert-success">
                                            <strong>O Vale Pallet foi devolvido.</strong>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--/ Devolucao -->
                    <!-- Recolhimento -->
                    <div class="tab-pane fade " id="tabRecolhimento">
                        <div id="knockoutRecolhimento">
                            <div class="card mb-3">
                                <div class="card-header">
                                    <h3 class="card-title">Recolhimento Vale Pallet</h3>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-12 col-md-2" data-bind="visible: Data.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: Data.text, attr: { for: Data.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: Data.requiredClass,enable: Data.enable, value: Data.val, valueUpdate: 'afterkeydown', attr: { maxlength: Data.maxlength, id : Data.id}" />
                                            </div>
                                        </div>
                                        <div class="col-12 col-md-2" data-bind="visible: NFe.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: NFe.text, attr: { for: NFe.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: NFe.requiredClass, enable: NFe.enable, value: NFe.val, valueUpdate: 'afterkeydown', attr: { maxlength: NFe.maxlength, id : NFe.id}" />
                                            </div>
                                        </div>
                                        <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, enable: Transportador.enable, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
