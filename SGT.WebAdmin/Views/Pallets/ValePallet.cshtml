                                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}, enable: Transportador.enable" type="button" aria-label="Buscar">
                                                            <i class="fal fa-search"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12 col-md-2" data-bind="visible: Quantidade.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: Quantidade.text, attr: { for: Quantidade.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: Quantidade.requiredClass, enable: Quantidade.enable, value: Quantidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Quantidade.maxlength, id : Quantidade.id}" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--/ Recolhimento -->
                </div>
                <footer id="knockoutCRUDValePallet">
                    <div class="row">
                        <div class="d-flex align-items-center justify-content-end mb-3 mt-3">
                            <button type="button" class="btn btn-success ms-1" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">

                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                            <button type="button" class="btn btn-success ms-1" data-bind="click: Limpar.eventClick, visible: Limpar.visible, attr: { id : Limpar.id}">

                                <span data-bind="text: Limpar.text"></span>
                            </button>
                            <button type="button" class="btn btn-success ms-1" data-bind="click: Finalizar.eventClick, visible: Finalizar.visible, attr: { id : Finalizar.id}">

                                <span data-bind="text: Finalizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-success ms-1" data-bind="click: Imprimir.eventClick, visible: Imprimir.visible, attr: { id : Imprimir.id}">

                                <span data-bind="text: Imprimir.text"></span>
                            </button>
                            <button type="button" class="btn btn-success ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">

                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button type="button" class="btn btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">

                                <span data-bind="text: Excluir.text"></span>
                            </button>
                        </div>
                        <div>
                        </div>
                    </div>
                </footer>
            </div>
        </div>
    </div>
</section>

