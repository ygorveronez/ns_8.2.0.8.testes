
<section id="widget-grid">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Pallets</li>
        <li class="breadcrumb-item active">Vale Pallet</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Vale Pallet
        </h1>
    </div>
    <div id="knockoutPesquisaValePallet" class="panel">
