                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Representante.idBtnSearch}, enable: Representante.enable" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-2" data-bind="visible: (Responsavel.codEntity() > 0).visible">
