    .imposto-linha {
        display: grid;
        grid-template-columns: 200px 200px 200px 200px; /* Ajuste conforme necessário */
        gap: 2rem;
        margin-bottom: 1rem;
    }

    .imposto-item {
        display: flex;
        flex-direction: column;
    }

    .imposto-item h5 {
        font-weight: 600;
        margin-top:10px;
    }

    .imposto-item h7 {
        margin: 0;
        color: #E5E5E5;
        font-weight: 600;
    }

    .imposto-item span {
        font-size: 1rem;
    }


    .dt-toolbar {
        display: none;
    }

    #knotPesquisa header {
        border: none;
    }

    #knotPesquisa .container-no-padding {
        border: none;
        border-top: 1px solid rgb(194, 194, 194);
    }

    #knotPesquisa .botao-atualizar-cargas {
        background-color: transparent;
        border: none;
        height: 34px;
        padding: 0;
        position: absolute;
        right: 0;
        text-align: center;
        top: 0;
        user-select: none;
        vertical-align: middle;
        width: 34px;
        z-index: 1;
    }

        #knotPesquisa .botao-atualizar-cargas i {
            color: #269917;
            font-size: 20px;
        }

        #knotPesquisa .botao-atualizar-cargas:hover i {
            color: #1c6f11;
        }

    #fdsCargas {
        background-color: transparent;
    }

    #fdsCargas-footer {
        background-color: transparent;
        border: none;
        padding: 0 0 10px 0;
    }

        #fdsCargas-footer .pesquisa-carga-sem-registros {
            color: #016f65;
            font-size: 16px;
            margin-top: 15px;
        }

    .rastreador-on {
        color: #33cc33;
    }

    .rastreador-off {
        color: #e74c3c;
    }

    #termo-aceite {
        height: 385px;
        background: #f5f5f5;
        margin: 0 15px 20px 15px;
        overflow: auto;
        border: 1px solid #a09a9a;
        padding: 5px;
    }
