

<style>
    .content-no-padding fieldset {
        padding: 20px 0 0 0 !important;
    }

    .content-no-padding header {
        margin: 0;
        padding: 6px 0 8px 0;
    }
</style>

<section id="widget-grid" class="">
    <div class="row">
        <div class="col col-lg-12">
            <h2 class="page-title txt-color-blueDark">
                Responsável entrega
            </h2>
        </div>
    </div>

    <div class="row">
        <article class="col-sm-12 col-md-12 col-lg-12">
            <div id="knockoutPesquisaCarga">
                <div class="jarviswidget" data-widget-colorbutton="false" data-widget-sortable="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-togglebutton="false">
                    <header class="headerCursor" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <span class="widget-icon"> <i class="glyphicon glyphicon-search"></i> </span>
                        <h2>Pesquisar Cargas</h2>
                    </header>
                    <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                        <div>
                            <div class="smart-form">
                                <section>
                                    <div class="well">
                                        <fieldset>
                                            <div class="row">
                                                <section class="col col-xs-4 col-md-3">
                                                    <label class="label"><b data-bind="text: CodigoCargaEmbarcador.text"></b></label>
                                                    <label data-bind="attr: { class: CodigoCargaEmbarcador.requiredClass}">
                                                        <input type="text" data-bind="value: CodigoCargaEmbarcador.val, attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id} , valueUpdate: 'afterkeydown'" />
                                                    </label>
                                                </section>
                                                <section class="col col-xs-4 col-md-3">
                                                    <label class="label"><b data-bind="text: DataInicio.text"></b></label>
                                                    <label data-bind="attr: { class: DataInicio.requiredClass}">
                                                        <input type="text" data-bind="value: DataInicio.val, attr: { maxlength: DataInicio.maxlength, id : DataInicio.id} " />
                                                    </label>
                                                </section>
                                                <section class="col col-xs-4 col-md-3">
                                                    <label class="label"><b data-bind="text: DataLimite.text"></b></label>
                                                    <label data-bind="attr: { class: DataLimite.requiredClass}">
                                                        <input type="text" data-bind="value: DataLimite.val, attr: { maxlength: DataLimite.maxlength, id : DataLimite.id}" />
                                                    </label>
                                                </section>
                                                <section class="col col-xs-6 col-md-3">
                                                    <label class="label"><b data-bind="text: SituacaoResponsavel.text"></b></label>
                                                    <label class="select">
                                                        <select data-bind="options: SituacaoResponsavel.options, optionsText: 'text', optionsValue: 'value', value: SituacaoResponsavel.val, attr: { id : SituacaoResponsavel.id}, event: {change: SituacaoResponsavel.eventChange}"></select>
                                                        <i></i>
                                                    </label>
                                                </section>
                                                <section class="col col-xs-6" data-bind="visible: Filial.visible">
                                                    <div class="input-group">
                                                        <label class="label"><b data-bind="text: Filial.text"></b></label>
                                                        <label data-bind="attr: { class: Filial.requiredClass}">
                                                            <input type="text" data-bind="value: Filial.val, enable: Filial.enable, attr: { maxlength: Filial.maxlength, id: Filial.id}, event: { blur: Filial.eventChange }, valueUpdate: 'afterkeydown'" />
                                                        </label>
                                                        <div class="input-group-btn">
                                                            <button data-bind="enable : Filial.enable, attr: { id: Filial.idBtnSearch}" class="btn btn-default btn-primary botaoBusca" type="button">
                                                                <i class="fa fa-search"></i> &nbsp; Buscar &nbsp;
                                                            </button>
                                                        </div>
                                                    </div>
                                                </section>
                                                <section data-bind="visible: ResponsavelEntrega.visible" class="col col-xs-12 col-md-6">
                                                    <div class="input-group">
                                                        <label class="label"><b data-bind="text: ResponsavelEntrega.text"></b></label>
                                                        <label data-bind="attr: { class: ResponsavelEntrega.requiredClass}">
                                                            <input type="text" data-bind="value: ResponsavelEntrega.val, valueUpdate: 'afterkeydown', attr: { maxlength: ResponsavelEntrega.maxlength, id : ResponsavelEntrega.id}" />
                                                        </label>
                                                        <div class="input-group-btn">
                                                            <button data-bind="attr: { id: ResponsavelEntrega.idBtnSearch}" class="btn btn-default btn-primary botaoBusca" type="button">
                                                                <i class="fa fa-search"></i> &nbsp; Buscar &nbsp;
                                                            </button>
                                                        </div>
                                                    </div>
                                                </section>
                                                <div class="clearfix"></div>
                                            </div>
                                        </fieldset>
                                        <footer class="no-padding">
                                            <button class="btn btn-default btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                                <i class="fa fa-search"></i>&nbsp;<span data-bind="text: Pesquisar.text"></span>
                                            </button>
                                        </footer>
                                    </div>
                                </section>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="widget-body" role="content">
                    <div class="smart-form">
                        <section class="col col-xs-12 col-sm-12 col-md-12 col-lg-12" style="background-color: #fff">
                            <div class="row">
                                <header>
                                    <b style="font-size:15px; margin-left:5px;">Cargas</b>
                                </header>
                                <fieldset>
                                    <button data-bind="click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible" class="btn btn-labeled btn-default botaoAdiconarMultiplosRegistros" style="float:right; margin-top:-7px;" type="button">
                                        &nbsp;<span data-bind="text: SelecionarTodos.text"></span>&nbsp;
                                    </button>
                                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                                </fieldset>

                                <footer>
                                    <button data-bind="click: AtribuirResponsavel.eventClick, attr: { id: AtribuirResponsavel.id }, visible: AtribuirResponsavel.visible" class="btn btn-success" type="button" style="float:right">
                                        &nbsp;<span data-bind="text: AtribuirResponsavel.text"></span>&nbsp;
                                    </button>
                                    <button data-bind="click: RemoverResponsavel.eventClick, attr: { id: RemoverResponsavel.id }, visible: RemoverResponsavel.visible" class="btn btn-danger" type="button" style="float:right">
                                        &nbsp;<span data-bind="text: RemoverResponsavel.text"></span>&nbsp;
                                    </button>

                                </footer>
                            </div>

                        </section>
                    </div>
                </div>
            </div>
        </article>
    </div>
</section>



<div class="modal fade" id="divModalResponsavelEntrega" role="dialog" aria-hidden="true">
    <div class="modal-dialog" id="knockoutResponsavelEntrega">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true"> ×</button>
                <h4 class="modal-title">Tratativa do alerta</h4>
            </div>
            <div class="modal-body">
                <div class="smart-form">
                    <fieldset>
                        <section data-bind="visible: ResponsavelEntrega.visible" class="col col-12">
                            <div class="input-group">
                                <label class="label"><b data-bind="text: ResponsavelEntrega.text"></b></label>
                                <label data-bind="attr: { class: ResponsavelEntrega.requiredClass}">
                                    <input type="text" data-bind="value: ResponsavelEntrega.val, valueUpdate: 'afterkeydown', attr: { maxlength: ResponsavelEntrega.maxlength, id : ResponsavelEntrega.id}" />
                                </label>
                                <div class="input-group-btn">
                                    <button data-bind="attr: { id: ResponsavelEntrega.idBtnSearch}" class="btn btn-default btn-primary botaoBusca" type="button">
                                        <i class="fa fa-search"></i> &nbsp; Buscar &nbsp;
                                    </button>
                                </div>
                            </div>
                        </section>
                    </fieldset>
                </div>
            </div>
            <div class="modal-footer">
                <footer class="text-right">
                    <input type="button" data-bind="click: Cancelar.eventClick, attr: { value: Cancelar.text, id : Cancelar.id}" class="btn btn-labeled btn-default botaoDentroSmartAdmimForm">
                    <input type="button" data-bind="click: Confirmar.eventClick, attr: { value: Confirmar.text, id : Confirmar.id}" class="btn btn-labeled btn-success botaoDentroSmartAdmimForm">
                </footer>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/responsavelEntrega"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadResponsavelEntrega();
    });

</script>