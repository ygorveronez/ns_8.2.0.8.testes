<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Cargas</li>
    <li class="breadcrumb-item active">Autorização Carregamento</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Autorização de Carregamento
    </h1>
</div>

<div id="knockoutPesquisaCarregamento">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h2>Pesquisar Carregamentos</h2>
            </div>
            <div class="panel-content">
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroCarregamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCarregamento.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroCarregamento.val, attr: { maxlength: NumeroCarregamento.maxlength, id : NumeroCarregamento.id} , valueUpdate: 'afterkeydown'" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicio.val, attr: { maxlength: DataInicio.maxlength, id : DataInicio.id} " />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataLimite.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataLimite.val, attr: { maxlength: DataLimite.maxlength, id : DataLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}, event: {change: Situacao.eventChange}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Filial.text, maxlength: Filial.maxlength, id: Filial.id, name: Filial.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Filial.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: ModeloVeicularCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ModeloVeicularCarga.text, attr: { for: ModeloVeicularCarga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: ModeloVeicularCarga.requiredClass, value: ModeloVeicularCarga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': ModeloVeicularCarga.text, maxlength: ModeloVeicularCarga.maxlength, id: ModeloVeicularCarga.id, name: ModeloVeicularCarga.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: ModeloVeicularCarga.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoCarga.text, attr: { for: TipoCarga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: TipoCarga.requiredClass, value: TipoCarga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoCarga.text, maxlength: TipoCarga.maxlength, id: TipoCarga.id, name: TipoCarga.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoCarga.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Usuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Usuario.text, maxlength: Usuario.maxlength, id: Usuario.id, name: Usuario.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Usuario.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Carregamentos
                </h3>
            </div>
            <div class="panel-content">
                <div class="d-flex justify-content-end mb-2">
                    <button class="btn btn-primary" type="button" data-bind="click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible">
                        <span data-bind="text: SelecionarTodos.text"></span>
                    </button>

                </div>
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>

                <div class="d-flex justify-content-between mt-3">
                    <div class="d-flex">
                        <button class="btn btn-danger me-2" type="button" data-bind="click: RejeitarTodos.eventClick, attr: { id: RejeitarTodos.id }, visible: RejeitarTodos.visible">
                            <i class="fal fa-times"></i><span data-bind="text: RejeitarTodos.text"></span>
                        </button>
                        <button class="btn btn-primary me-2" type="button" data-bind="click: DelegarTodos.eventClick, attr: { id: DelegarTodos.id }, visible: DelegarTodos.visible">
                            <i class="fal fa-times"></i><span data-bind="text: DelegarTodos.text"></span>
                        </button>
                    </div>
                    <div class="d-flex justify-content-end">
                        <button class="btn btn-success" type="button" data-bind="click: AprovarTodos.eventClick, attr: { id: AprovarTodos.id }, visible: AprovarTodos.visible">
                            <i class="fal fa-check"></i><span data-bind="text: AprovarTodos.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="divModalCarregamento" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">
                    Informações do Carregamento
                </h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="panel">
                    <div class="panel-container">
                        <div class="panel-content">
                            <ul class="nav nav-tabs" id="myTab">
                                <li class="nav-item">
                                    <a href="#knockoutCarregamento" class="nav-link active" data-bs-toggle="tab"><span class="hidden-mobile hidden-tablet">Carregamento</span></a>
                                </li>
                                <li class="nav-item" id="liAutorizacao">
                                    <a href="#knockoutAutorizacao" class="nav-link" data-bs-toggle="tab"><span class="hidden-mobile hidden-tablet">Autorização</span></a>
                                </li>
                                <li class="nav-item" id="liDelegar" style="display:none;">
                                    <a href="#knockoutDelegar" class="nav-link" data-bs-toggle="tab"><span class="hidden-mobile hidden-tablet">Delegar</span></a>
                                </li>
                            </ul>
                            <div class="tab-content border border-top-0 p-3">

                                <!-- Carregamento -->
                                <div id="knockoutCarregamento" class="tab-pane fade active show">
                                    <div class="row mb-2">
                                        <h4>Detalhes do Carregamento</h4>
                                        <hr />
                                    </div>
                                    <form>
                                        <div class="row">
                                            <div class="col-12 col-md-6">
                                                <div class="form-group">
                                                    <label data-bind="visible: NumeroCarregamento.visible"><b><span data-bind="text: NumeroCarregamento.text"></span></b> <span data-bind="text: NumeroCarregamento.val"></span></label>
                                                </div>
                                            </div>

                                            <div class="col-12 col-md-6">
                                                <div class="form-group">
                                                    <label data-bind="visible: DataCarregamento.visible"><b><span data-bind="text: DataCarregamento.text"></span></b> <span data-bind="text: DataCarregamento.val"></span></label>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-6">
                                                <div class="form-group">
                                                    <label data-bind="visible: TipoCarga.visible"><b><span data-bind="text: TipoCarga.text"></span></b> <span data-bind="text: TipoCarga.val"></span></label>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-6">
                                                <div class="form-group">
                                                    <label data-bind="visible: ModeloVeicularCarga.visible"><b><span data-bind="text: ModeloVeicularCarga.text"></span></b> <span data-bind="text: ModeloVeicularCarga.val"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <table class="table table-bordered table-hover table-condensed table-striped">
                                                <thead>
                                                    <tr>
                                                        <th style="width:30%">Medida</th>
                                                        <th style="text-align:right">Adicionado</th>
                                                        <th style="text-align:right">Capacidade</th>
                                                        <th style="text-align:right">Ocupação (%)</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr data-bind="visible: Peso.visible, attr: {id : Peso.id}">
                                                        <td data-bind="text: Peso.text"></td>
                                                        <td data-bind="text: Peso.val" style="text-align:right"></td>
                                                        <td data-bind="text: CapacidadePeso.val" style="text-align:right"></td>
                                                        <td data-bind="text: LotacaoPeso.val" style="text-align:right"></td>
                                                    </tr>
                                                    <tr data-bind="visible: Pallets.visible, attr: {id : Pallets.id}">
                                                        <td data-bind="text: Pallets.text"></td>
                                                        <td data-bind="text: Pallets.val" style="text-align:right"></td>
                                                        <td data-bind="text: CapacidadePallets.val" style="text-align:right"></td>
                                                        <td data-bind="text: LotacaoPallets.val" style="text-align:right"></td>
                                                    </tr>
                                                    <tr data-bind="visible: Cubagem.visible, attr: {id : Cubagem.id}">
                                                        <td data-bind="text: Cubagem.text"></td>
                                                        <td data-bind="text: Cubagem.val" style="text-align:right"></td>
                                                        <td data-bind="text: CapacidadeCubagem.val" style="text-align:right"></td>
                                                        <td data-bind="text: LotacaoCubagem.val" style="text-align:right"></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </form>
                                </div>

                                <!-- Autorização-->
                                <div id="knockoutAutorizacao" class="tab-pane fade">
                                    <div class="row mb-2">
                                        <h4>Regras</h4>
                                        <hr />
                                    </div>
                                    <form>
                                        <div class="row">
                                            <div class="col-12 col-md-12" data-bind="visible: Motivo.visible">
                                                <div class="form-group">
                                                    <label class="form-label" data-bind="text: Motivo.text"></label>
                                                    <textarea type="text" rows="5" class="form-control" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}"></textarea>
                                                </div>
                                            </div>

                                            <div data-bind="visible: Rejeitar.visible">
                                                <div class="row">
                                                    <div class="col-6">
                                                        <button class="btn btn-default me-2" type="button" data-bind="click: Cancelar.eventClick, attr: { value: Cancelar.text, id : Cancelar.id}">
                                                            <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
                                                        </button>
                                                        <button class="btn btn-danger me-2" type="button" data-bind="click: Rejeitar.eventClick, attr: { value: Rejeitar.text, id : Rejeitar.id}">
                                                            <i class="fal fa-times"></i><span data-bind="text: Rejeitar.text"></span>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id : Regras.idGrid}"></table>
                                            <footer data-bind="visible: AprovarTodas.visible">
                                                <div class="row">
                                                    <div class="col-12 d-flex justify-content-end">
                                                        <button class="btn btn-success me-2" type="button" data-bind="visible: AprovarTodas.visible, click: AprovarTodas.eventClick, attr: { id: AprovarTodas.id}">
                                                            <span data-bind="text: AprovarTodas.text"></span>
                                                        </button>
                                                    </div>
                                                </div>
                                            </footer>
                                        </div>
                                    </form>
                                </div>

                                <!-- Delegar -->
                                <div id="knockoutDelegar" class="tab-pane fade">
                                    <div class="row mb-2">
                                        <h4>Delegar</h4>
                                        <hr />
                                    </div>
                                    <form>
                                        <div class="row">
                                            <div class="col-12 col-md-12" data-bind="visible: UsuarioDelegado.visible">
                                                <div class="form-group">
                                                    <label class="form-label" data-bind="text: UsuarioDelegado.text, attr: { for: UsuarioDelegado.id }"></label>
                                                    <div class="input-group">
                                                        <input type="text" data-bind="css: UsuarioDelegado.requiredClass, value: UsuarioDelegado.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': UsuarioDelegado.text, maxlength: UsuarioDelegado.maxlength, id: UsuarioDelegado.id, name: UsuarioDelegado.id }" />
                                                        <div class="input-group-append">
                                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: UsuarioDelegado.idBtnSearch}">
                                                                <i class="fal fa-search"></i>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <footer>
                                                <div class="row">
                                                    <div class="col-12 d-flex justify-content-end">
                                                        <button class="btn btn-success me-2" type="button" data-bind="click: Delegar.eventClick, attr: { id: Delegar.id}">
                                                            <span data-bind="text: Delegar.text"></span>
                                                        </button>
                                                    </div>
                                                </div>
                                            </footer>
                                        </div>
                                    </form>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
        </div>
    </div>
</div>

<div class="modal fade" id="divModalRejeitarCarregamento" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" id="knockoutRejeicaoCarregamento" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Rejeitar</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="row">
                        <div class="col-12 col-md-12" data-bind="visible: Motivo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Motivo.text"></label>
                                <textarea type="text" rows="5" class="form-control" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}"></textarea>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" data-bind="click: Cancelar.eventClick, attr: { value: Cancelar.text, id : Cancelar.id}">
                    <i class="fal fa-times"></i><span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-danger" data-bind="click: Rejeitar.eventClick, attr: { value: Rejeitar.text, id : Rejeitar.id}">
                    <i class="fal fa-ban"></i><span data-bind="text: Rejeitar.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalDelegarSelecionados" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" id="knockoutDelegarSelecionados" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Delegar Cargas</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="row">
                        <div class="col-12 col-md-12" data-bind="visible: UsuarioDelegado.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: UsuarioDelegado.text, attr: { for: UsuarioDelegado.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: UsuarioDelegado.requiredClass, value: UsuarioDelegado.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': UsuarioDelegado.text, maxlength: UsuarioDelegado.maxlength, id: UsuarioDelegado.id, name: UsuarioDelegado.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: UsuarioDelegado.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" data-bind="click: Cancelar.eventClick, attr: { value: Cancelar.text, id : Cancelar.id}">
                    <i class="fal fa-times"></i><span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-success" data-bind="click: Delegar.eventClick, attr: { value: Delegar.text, id : Delegar.id}">
                    <span data-bind="text: Delegar.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/autorizacaoCarregamento"></script>

