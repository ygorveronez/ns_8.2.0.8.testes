
<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Cargas</li>
        <li class="breadcrumb-item active">Responsavel Carga</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Responsável das Cargas
        </h1>
    </div>

    <div id="knockoutPesquisaResponsavelCarga">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-content">
                    <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i>
                        Pesquisar de Responsável das Cargas
                    </button>
                    <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-4" data-bind="visible: Funcionario.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: Funcionario.requiredClass, value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Funcionario.text, maxlength: Funcionario.maxlength, id: Funcionario.id, name: Funcionario.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Funcionario.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-4" data-bind="visible: CategoriaResponsavel.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CategoriaResponsavel.text, attr: { for: CategoriaResponsavel.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: CategoriaResponsavel.requiredClass, value: CategoriaResponsavel.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': CategoriaResponsavel.text, maxlength: CategoriaResponsavel.maxlength, id: CategoriaResponsavel.id, name: CategoriaResponsavel.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CategoriaResponsavel.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: VigenciaInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: VigenciaInicial.text, attr: { for: VigenciaInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: VigenciaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: VigenciaInicial.maxlength, id : VigenciaInicial.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: VigenciaFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: VigenciaFinal.text, attr: { for: VigenciaFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: VigenciaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: VigenciaFinal.maxlength, id : VigenciaFinal.id}" />
                                    </div>
                                </div>
                            </div>
                        </form>

                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                        <div class="widget-body">
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel mt-3" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
        <div class="panel-container">
            <div class="panel-content">
                <ul class="nav nav-tabs">
                    <li class="nav-item active">
                        <a href="#tabResponsavelCargaDados" class="nav-link active" data-bs-toggle="tab">
                            <span class="hidden-mobile hidden-tablet">Dados</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#tabResponsavelCargaFilial" class="nav-link" data-bs-toggle="tab">
                            <span class="hidden-mobile hidden-tablet">Filial</span>
                        </a>
                    </li>
                </ul>
                <div class="tab-content border border-top-0 p-3">
                    <div class="tab-pane fade active show" id="tabResponsavelCargaDados" role="tabpanel">
                        <form novalidate id="knockoutResponsavelCarga">
                            <div class="row">
                                <div class="col-12 col-md-4" data-bind="visible: Funcionario.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: Funcionario.requiredClass, value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Funcionario.text, maxlength: Funcionario.maxlength, id: Funcionario.id, name: Funcionario.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Funcionario.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-4" data-bind="visible: CategoriaResponsavel.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CategoriaResponsavel.text, attr: { for: CategoriaResponsavel.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: CategoriaResponsavel.requiredClass, value: CategoriaResponsavel.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': CategoriaResponsavel.text, maxlength: CategoriaResponsavel.maxlength, id: CategoriaResponsavel.id, name: CategoriaResponsavel.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CategoriaResponsavel.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: VigenciaInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: VigenciaInicial.text, attr: { for: VigenciaInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: VigenciaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: VigenciaInicial.maxlength, id : VigenciaInicial.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: VigenciaFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: VigenciaFinal.text, attr: { for: VigenciaFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: VigenciaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: VigenciaFinal.maxlength, id : VigenciaFinal.id}" />
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>

                    <div class="tab-pane fade" id="tabResponsavelCargaFilial" role="tabpanel">

                        <div id="knockoutResponsavelCargaFilial">
                            <button type="button" class="btn btn-primary mb-2" data-bind="attr: { id: ListaFilial.idBtnSearch}">
                                <i class="fal fa-plus"></i><span data-bind="text: ListaFilial.text"></span>
                            </button>
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: ListaFilial.idGrid}" cellspacing="0"></table>
                        </div>
                    </div>

                    <footer id="knockoutCRUDResponsavelCarga">
                        <div class="row">
                            <div class="col-6">
                                <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, text: Cancelar.text, attr: { id : Cancelar.id}">
                                    <i class="fal fa-ban"></i>
                                </button>
                            </div>
                            <div class="col-6 d-flex justify-content-end">
                                <button type="button" class="btn btn-danger mx-1 mx-sm-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, text: Excluir.text, attr: { id : Excluir.id}">
                                    <i class="fal fa-trash"></i>
                                </button>
                                <button type="button" class="btn btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, text: Atualizar.text, attr: { id : Atualizar.id}">
                                    <i class="fal fa-check"></i>
                                </button>
                                <button type="button" class="btn btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                                    <i class="fal fa-spinner"></i>
                                </button>
                            </div>
                        </div>
                    </footer>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/scripts/responsavelCarga"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadResponsavelCarga();
    });

</script>