                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: NumeroCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCarga.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCarga.maxlength, id : NumeroCarga.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroCTe.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCTe.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroCTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCTe.maxlength, id : NumeroCTe.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroMDFe.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroMDFe.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroMDFe.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroMDFe.maxlength, id : NumeroMDFe.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Veiculo.text, maxlength: Veiculo.maxlength, id: Veiculo.id, name: Veiculo.id }" />
