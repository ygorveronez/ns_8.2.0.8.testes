
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Gestão de Cargas</li>
    <li class="breadcrumb-item">Cargas</li>
    <li class="breadcrumb-item active">Agendamento</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Agendamento
    </h1>
</div>

<div id="knockoutPesquisaAgendamentoEntrega">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Agendamentos
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: SenhaAgendamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SenhaAgendamento.text"></label>
                                    <input type="text" class="form-control" data-bind="value: SenhaAgendamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: SenhaAgendamento.maxlength, id : SenhaAgendamento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Placa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Placa.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Placa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Placa.maxlength, id : Placa.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Destinatario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Destinatario.text, attr: { for: Destinatario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Destinatario.requiredClass, value: Destinatario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Destinatario.text, maxlength: Destinatario.maxlength, id: Destinatario.id, name: Destinatario.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Destinatario.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Adicionar Agendamentos
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutAgendamentoEntrega">
                <div class="row">
                    <div class="col-12 col-md-2" data-bind="visible: SenhaAgendamento.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: SenhaAgendamento.text"></label>
                            <input type="text" class="form-control" data-bind="value: SenhaAgendamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: SenhaAgendamento.maxlength, id : SenhaAgendamento.id}, enable: SenhaAgendamento.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Senha.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Senha.text"></label>
                            <input type="text" class="form-control" data-bind="value: Senha.val, valueUpdate: 'afterkeydown', attr: { maxlength: Senha.maxlength, id : Senha.id}, enable: Senha.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Situacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Situacao.text"></label>
                            <input type="text" class="form-control" data-bind="value: Situacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Situacao.maxlength, id : Situacao.id}, enable: Situacao.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DataAgendamento.visible">
                        <div class="form-group">
                            <label class="form-label input" data-bind="text: DataAgendamento.text"></label>
                            <input type="text" class="form-control" data-bind="value: DataAgendamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAgendamento.maxlength, id : DataAgendamento.id}">
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: Destinatario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Destinatario.text, attr: { for: Destinatario.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Destinatario.requiredClass, value: Destinatario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Destinatario.text, maxlength: Destinatario.maxlength, id: Destinatario.id, name: Destinatario.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Destinatario.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-6" data-bind="visible: TipoDeCarga.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoDeCarga.text, attr: { for: TipoDeCarga.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: TipoDeCarga.requiredClass, value: TipoDeCarga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoDeCarga.text, maxlength: TipoDeCarga.maxlength, id: TipoDeCarga.id, name: TipoDeCarga.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoDeCarga.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: ModeloVeicularCarga.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ModeloVeicularCarga.text, attr: { for: ModeloVeicularCarga.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: ModeloVeicularCarga.requiredClass, value: ModeloVeicularCarga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': ModeloVeicularCarga.text, maxlength: ModeloVeicularCarga.maxlength, id: ModeloVeicularCarga.id, name: ModeloVeicularCarga.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: ModeloVeicularCarga.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-2" data-bind="visible: Placa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Placa.text"></label>
                            <input type="text" class="form-control" data-bind="value: Placa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Placa.maxlength, id : Placa.id}, enable: Placa.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Motorista.text"></label>
                            <input type="text" class="form-control" data-bind="value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}, enable: Motorista.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Transportador.text"></label>
                            <input type="text" class="form-control" data-bind="value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}, enable: Transportador.enable" />
                        </div>
                    </div>

                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Observacao.text"></label>
                            <textarea type="text" rows="5" class="form-control" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                        </div>
                    </div>
                </div>
                <footer>
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}" class="btn btn-labeled btn-default">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/agendamentoEntrega"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadAgendamentoEntrega();
    });

</script>

