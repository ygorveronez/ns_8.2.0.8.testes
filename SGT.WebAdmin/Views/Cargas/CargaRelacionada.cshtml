                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CanalEntrega.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-9" data-bind="visible: Transportador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Relacionada.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Relacionada.text, attr: { for: Relacionada.id }"></label>
                                <select class="form-control" data-bind="class: Relacionada.requiredClass, options: Relacionada.options, optionsText: 'text', optionsValue: 'value', value: Relacionada.val, attr: { maxlength: Relacionada.maxlength, id : Relacionada.id}"></select>
                            </div>
