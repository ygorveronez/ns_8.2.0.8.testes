                    </div>
                </form>
                <div class="d-flex align-items-center justify-content-end mb-3">
                    <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                    </button>
                </div>
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>

<div id="divModalJustificar" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutJustificar" class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Justificar</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-md-12" data-bind="visible: Justificativa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Justificativa.text, attr: { for: Justificativa.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Justificativa.requiredClass, value: Justificativa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Justificativa.maxlength, id : Justificativa.id}" />
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default me-2" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                        <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button class="btn btn-danger me-2" type="button" data-bind="click: Remover.eventClick, attr: { id : Remover.id}">
                        <i class="fal fa-times"></i><span data-bind="text: Remover.text"></span>
                    </button>
                    <button class="btn btn-success" type="button" data-bind="click: Adicionar.eventClick, attr: { id : Adicionar.id}">
                        <i class="fal fa-check"></i><span data-bind="text: Adicionar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="divModalRelacionarCarga" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutRelacionarCarga" class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Relacionar Carga</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-md-12" data-bind="visible: CargaRelacionada.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CargaRelacionada.text, attr: { for: CargaRelacionada.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: CargaRelacionada.requiredClass, value: CargaRelacionada.val, valueUpdate: 'afterkeydown', attr: { maxlength: CargaRelacionada.maxlength, id : CargaRelacionada.id}" />
