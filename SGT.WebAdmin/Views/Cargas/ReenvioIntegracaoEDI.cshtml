
<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Reenvio</li>
        <li class="breadcrumb-item active">ReenvioIntegracaoEDI</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Reenvio de Integração EDI
        </h1>
    </div>
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content" id="knockoutPesquisaReenvioIntegracaoEDI">
                <div data-widget-colorbutton="false" data-widget-sortable="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-togglebutton="false">
                    <button class="btn btn-primary" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i> Pesquisar Reenvios
                    </button>
                    <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade" class="mt-3">
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: LayoutEDI.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: LayoutEDI.text, attr: { for: LayoutEDI.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: LayoutEDI.requiredClass, value: LayoutEDI.val, valueUpdate: 'afterkeydown', attr: { maxlength: LayoutEDI.maxlength, id : LayoutEDI.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: LayoutEDI.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Usuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Usuario.maxlength, id : Usuario.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Usuario.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <footer class="d-flex justify-content-end mb-3">
                                <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                    <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                                </button>
                            </footer>
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="jarviswidget" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
        <div class="panel">
            <div class="panel-container" id="knockoutReenvioIntegracaoEDI">
                <div class="panel-hdr">
                    <h3 class="panel-title">
                        <i class="fal fa-edit"></i>
                        Reenvio de integração
                    </h3>
                </div>
                <div class="panel-content" data-bind="visible: Resumo.val">
                    <table class="table table-clean table-sm align-self-end">
                        <tbody>
                            <tr>
                                <td>
                                    <strong data-bind="text: Usuario.text"></strong>
                                    <span data-bind="text: Usuario.val"></span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <strong data-bind="text: DataEnvio.text"></strong>
                                    <span data-bind="text: DataEnvio.val"></span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="panel">
                    <div class="panel-container">
                        <div class="panel-hdr">
                            <h3 class="panel-title">
                                Layout EDI
                            </h3>
                        </div>
                        <div class="panel-content">
                            <button class="btn btn-primary ms-2 mb-3 " type="button" data-bind="click: AdicionarLayout.eventClick, visible: AdicionarLayout.visible, attr: { id: AdicionarLayout.idBtnSearch}">
                                <i class="fal fa-plus"></i><span data-bind="text: AdicionarLayout.text"></span>
                            </button>
                            <table width="100%" data-bind="attr: { id: LayoutsEDI.idGrid }" class="table table-bordered table-hover" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
                <div class="panel">
                    <div class="panel-container">
                        <div class="panel-hdr">
                            <h3 class="panel-title">
                                Cargas
                            </h3>
                        </div>
                        <div class="panel-content">
                            <button class="btn btn-primary ms-2 mb-3" type="button" data-bind="click: AdicionarCarga.eventClick, visible: AdicionarCarga.visible, attr: { id: AdicionarCarga.idBtnSearch}">
                                <i class="fal fa-plus"></i><span data-bind="text: AdicionarCarga.text"></span>
                            </button>
                            <table width="100%" data-bind="attr: { id: Cargas.idGrid }" class="table table-bordered table-hover" cellspacing="0"></table>
                            <footer class="d-flex justify-content-end mt-3">
                                <span data-bind='component: {name: "import-file", params: { ko: ImportarPlanilha }}' class="pull-left" style="margin-left: 5px;"></span>
                                <button class="btn btn-default ms-2" type="button" data-bind="click: Cancelar.eventClick, attr: { id: Cancelar.id}">
                                    <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
                                </button>
                                <button class="btn btn-success ms-2" type="button" data-bind="click: Enviar.eventClick, attr: { id: Enviar.id}">
                                    <i class="fal fa-check"></i><span data-bind="text: Enviar.text"></span>
                                </button>
                            </footer>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/scripts/reenvioIntegracaoEDI"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadReenvioIntegracaoEDI();
    });

</script>

