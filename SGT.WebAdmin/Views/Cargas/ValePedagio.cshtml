
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Cargas</li>
    <li class="breadcrumb-item active">Vale Pedágio</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Vale Pedágio
    </h1>
</div>

<div id="knockoutPesquisaValePedagio">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Vale Pedágio
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: CodigoCargaEmbarcador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataCargaInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCargaInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataCargaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCargaInicial.maxlength, id : DataCargaInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataCargaFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCargaFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataCargaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCargaFinal.maxlength, id : DataCargaFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroValePedagio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroValePedagio.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroValePedagio.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroValePedagio.maxlength, id : NumeroValePedagio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoIntegracao.text, attr: { for: TipoIntegracao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: TipoIntegracao.requiredClass, value: TipoIntegracao.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoIntegracao.text, maxlength: TipoIntegracao.maxlength, id: TipoIntegracao.id, name: TipoIntegracao.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoIntegracao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: SituacaoValePedagio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoValePedagio.text"></label>
                                    <select class="form-control" data-bind="options: SituacaoValePedagio.options, optionsText: 'text', optionsValue: 'value', value: SituacaoValePedagio.val, attr: { id : SituacaoValePedagio.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: SituacaoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoIntegracao.text"></label>
                                    <select class="form-control" data-bind="options: SituacaoIntegracao.options, optionsText: 'text', optionsValue: 'value', value: SituacaoIntegracao.val, attr: { id : SituacaoIntegracao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataIntegracaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataIntegracaoInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataIntegracaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataIntegracaoInicial.maxlength, id : DataIntegracaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataIntegracaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataIntegracaoFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataIntegracaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataIntegracaoFinal.maxlength, id : DataIntegracaoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroParcialCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroParcialCarga.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroParcialCarga.requiredClass, value: NumeroParcialCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroParcialCarga.maxlength, id : NumeroParcialCarga.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button type="button" class="btn btn-default waves-effect waves-themed me-3" data-bind="click: SelecionarTodos.eventClick, visible: SelecionarTodos.visible, attr: { value: SelecionarTodos.text, id : SelecionarTodos.id}">
                            <span data-bind="text: SelecionarTodos.text"></span>
                        </button>

                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <div class="widget-body">
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                    <div class="d-flex align-items-center justify-content-end my-2">
                        <button class="btn btn-success" type="button" data-bind="visible: RecomprarSelecionados.visible, click: RecomprarSelecionados.eventClick, attr: { id: RecomprarSelecionados.id}">
                            <i class="fal fa-check"></i><span data-bind="text: RecomprarSelecionados.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalHistoricoIntegracaoValePedagio" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Histórico de Integração</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table width="100%" class="table table-bordered table-hover" cellspacing="0" id="tblHistoricoIntegracaoValePedagio"></table>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/valePedagio"></script>
<script type="text/javascript">
    pageSetUp();
    $(document).ready(function () {
        loadValePedagio();
    });
</script>
