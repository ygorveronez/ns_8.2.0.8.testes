        $("#modal-carregamento").toggle(500, "swing", function () {
            //if ($('div.abrirJanelaPedidos a').hasClass('fa-chevron-right')) {
            //    $('div.abrirJanelaPedidos a').removeClass('fa-chevron-right');
            //    $('div.abrirJanelaPedidos a').addClass('fa-chevron-left');
            //} else {
            //    $('div.abrirJanelaPedidos a').addClass('fa-chevron-right');
            //    $('div.abrirJanelaPedidos a').removeClass('fa-chevron-left');
            //}
            if ($('div.abrirJanelaPedidos a').hasClass('fa-chevron-right')) {
                $('div.abrirJanelaPedidos a').removeClass('fa-chevron-right');
                $('div.abrirJanelaPedidos a').addClass('fa-chevron-left');
            } else {
                $('div.abrirJanelaPedidos a').addClass('fa-chevron-right');
                $('div.abrirJanelaPedidos a').removeClass('fa-chevron-left');
            }
