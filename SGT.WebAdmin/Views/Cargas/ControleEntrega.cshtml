<section id="widget-grid">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Cargas</li>
        <li class="breadcrumb-item active">Controle de Entregas</li>
    </ol>

    <div id="knockoutConfiguracaoExibicoesControleEntrega">
        <h3 class="mb-0 d-flex align-items-center">
            <i class="far fa-file-alt me-1"></i>
            Controle de Coleta e Entrega
        </h3>
        <div style="margin: 20px 0;"></div>
        <div class="row">
            <div class="col-12 col-md-6 d-flex align-items-center gap-2">
                <div class="w-100" style="max-width: 150px;">
                    <select class="form-control"
                            data-bind="options: PesquisaTermo.options,
                    optionsText: 'text',
                    optionsValue: 'value',
                    value: PesquisaTermo.val,
                    attr: { maxlength: PesquisaTermo.maxlength, id: PesquisaTermo.id }">
                    </select>
                </div>
                <div class="input-group w-100">
                    <input type="text"
                           class="form-control"
                           placeholder="Pesquisar por Carga, Nota Fiscal ou Pedido"
                           data-bind="value: PesquisarPor.val,
                  valueUpdate: 'afterkeydown',
                  attr: {
                      placeholder: PesquisaTermo.val() == '-1' ?
                          'Pesquisar por Carga, Nota Fiscal ou Pedido' :
                          'Pesquisa por ' + (ko.utils.arrayFirst(PesquisaTermo.options, function(item) {
                              return item.value == PesquisaTermo.val();
                          }) || {}).text || 'Pesquisar por...',
                      maxlength: PesquisarPor.maxlength,
                      id: PesquisarPor.id
                  }" />

                    <button class="btn btn-primary"
                            type="button"
                            data-bind="click: PesquisaRapida.eventClick, attr: { id: PesquisaRapida.id }">
                        <i class="fal fa-search"></i>
                    </button>

                    <button class="btn btn-outline-secondary" style="margin-left: 8px;"
                            type="button"
                            data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id"
                            title="Filtros">
                        <i class="fal fa-filter"></i> Filtros
                    </button>
                </div>
            </div>
            <div class="col-12 col-md-6 d-flex justify-content-end align-items-center gap-2">
                <button class="btn btn-secondary"
                        type="button"
                        data-bind="click: ExibirLegenda.eventClick"
                        title="Legenda">
                    Legenda
                </button>

                <button class="btn btn-outline-secondary"
                        type="button"
                        data-bind="click: ConfiguracaoWidget.eventClick"
                        title="Configurações">
                    <i class="fal fa-cog"></i>
                </button>
            </div>
        </div>
    </div>

    <div class="aviso-filtragem-dados" style="display:none;">
        <div>
            <i class="fal fa-filter"></i>
        </div>
        <span class="fs-1 info-title">{Localize::Cargas.ControleEntrega.MensagemFiltragem}</span>
    </div>

    <div id="controle-entrega-conteudo-container" style="display: none">
        <div class="panel controle-entrega-conteudo" style="margin-top: 8px;">
            <div class="panel-hdr">
                <h2>{Localize::Cargas.ControleEntrega.ControleDeColetaEntregas}</h2>
                <button class="btn btn-primary exibir-atentimento me-3 d-none" title="Exibir">
                    <i class="fal fa-users"></i>
                </button>
            </div>
            <div class="panel-container">
                <div class="panel-content">
                    <div id="knoutContainerControleEntrega">
                        <div class="d-flex justify-content-between mb-2">
                            <div class="d-flex">
                                <span data-bind='component: {name: "import-file", params: { ko: ImportacaoCargaFluvial }}'></span>
                                <button class="btn btn-dark mb-2" type="button" data-bind="visible: AssumirMonitoramentos.visible, click: AssumirMonitoramentos.eventClick">
                                    <span style="color: white" data-bind="text: AssumirMonitoramentos.text"></span>
                                </button>
                                <div class="d-flex">
                                    <button style="margin-right:5px;" class="btn btn-outline-dark ms-2" type="button" data-bind="visible: CancelarAcaoMonitoramentos.visible, text: CancelarAcaoMonitoramentos.text, click: CancelarAcaoMonitoramentos.eventClick">
                                    </button>
                                    <div class="dropdown" data-bind="visible: CancelarAcaoMonitoramentos.visible">
                                        <button class="btn btn-primary dropdown-toggle" data-bs-toggle="dropdown" aria-haspopup="true" type="button" data-bind="enable: DelegarCargas.enable" aria-expanded="false" id="dropdownMenuButton1">
                                            <i class="fal fa-download"></i>{Localize::Cargas.ControleEntrega.Acoes}<span class="caret"></span>
                                        </button>
                                        <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                                            <li>
                                                <a class="dropdown-item" data-bind="visible: AssumirCargas.visible, enable: AssumirCargas.enable, click: AssumirCargas.eventClick, attr: { id: AssumirCargas.id}">
                                                    <i class="fal fa-solid fa-file-code"></i><span class="ms-2" data-bind="text: AssumirCargas.text, enable: AssumirCargas.enable"></span>
                                                </a>
                                            </li>
                                            <li>
                                                <a class="dropdown-item" data-bind="visible: DelegarCargas.visible, enable: DelegarCargas.enable, click: DelegarCargas.eventClick, attr: { id: DelegarCargas.id}">
                                                    <i class="fal fa-solid fa-file-code"></i><span class="ms-2" data-bind="text: DelegarCargas.text, enable: DelegarCargas.enable"></span>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="my-3" data-bind="visible: SelecionarTodosCheckbox.visible">
                            <div class="custom-control custom-checkbox" style="margin-left: 12px; padding-left: 0px !important">
                                <input type="checkbox" name="checkbox-toggle" data-bind="checked: SelecionarTodosCheckbox.val, attr: { name: SelecionarTodosCheckbox.id, id: SelecionarTodosCheckbox.id }">
                                <label data-bind="attr: { for: SelecionarTodosCheckbox.id }" style="font-size: 14px; margin-left: 5px;">{Localize::Gerais.Geral.SelecionarTodos}</label>
                            </div>
                        </div>
                        <div class="mb-2">
                            <!-- ko foreach: { data: Entregas.val, as: 'Entrega' } -->
                            <!-- ko component: {name: "carga-entrega", params: Entrega } --><!-- /ko -->
                            <!-- /ko -->
                        </div>
                        <div id="paginacao-controle-entrega"></div>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel controle-entrega-atendimento d-none">
            <div class="panel-hdr">
                <h2>{Localize::Cargas.ControleEntrega.MotoristasAguardandoAtendimento}</h2>
            </div>
            <div class="panel-container">
                <div class="panel-content">
                    <div id="knoutContainerAtendimentos">
                        <div class="custom-control custom-checkbox">
                            <input type="checkbox" class="custom-control-input" name="checkbox-toggle" data-bind="checked: VisualizarAtendimentosOperadorResponsavel.val, attr: { name: VisualizarAtendimentosOperadorResponsavel.id, id: VisualizarAtendimentosOperadorResponsavel.id }">
                            <i data-swchon-text="Sim" data-swchoff-text="Não"></i>
                            <label class="custom-control-label" data-bind="html: VisualizarAtendimentosOperadorResponsavel.text, attr: { for: VisualizarAtendimentosOperadorResponsavel.id }"></label>
                        </div>

                        <div class="atendimentos-container">
                            <!-- ko foreach: { data: Atendimentos.view, as: 'Atendimento' } -->
                            <div class="row atendimento">
                                <div class="col col-xs-10 col-sm-10 container-chamado" data-bind="click: () => $parent.Atendimentos.eventClick(Atendimento)">
                                    <div class="row">
                                        <div class="col col-xs-3 col-sm-3 atendimento-date">
                                            <div class="date-container">
                                                <span data-bind="text: Atendimento.Hora"></span>
                                                <span data-bind="text: Atendimento.Data"></span>
                                                <div style="margin-top: 6px" data-bind="text: Atendimento.Numero"></div>
                                            </div>
                                        </div>
                                        <div class="col col-xs-9 col-sm-9 atendimento-name">
                                            <span data-bind="text: Atendimento.NomeMotorista"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col col-xs-2 col-sm-2 atendimento-message container-chat">
                                    <span class="icon" data-bind="click: () => $parent.Chat.eventClick(Atendimento)">
                                        <i class="fa fa-envelope" aria-hidden="true"></i>
                                    </span>
                                </div>
                            </div>
                            <!-- /ko -->
                            <!-- ko if: Atendimentos.val().length == 0 -->
                            <div class="atendimentos-vazio">
                                {Localize::Cargas.ControleEntrega.NenhumAtendimentoDisponivel}
                            </div>
                            <!-- /ko -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

