
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Cargas</li>
    <li class="breadcrumb-item active">Carga Agrupada</li>
</ol>


<div class="panel" id="knockoutCargaAgrupada">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                Carga Agrupada
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row">
                    <div class="col-12 col-md-12" data-bind="visible: Filial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </form>
            <div class="row" data-bind="fadeVisible: Carga.visibleFade">
                <div class="col-12 col-md-6">
                    <div class="card">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h3 class="card-title">Cargas para Agrupamento</h3>
                        </div>
                        <div class="card-body row" data-bind="attr: { id: NovaCarga.id}">
                            <div class="margin-bottom-15" id="divComponenteDivisaoCapacidade" style="display:none;"></div>
                            <div id="divGridCarga">
                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: NovaCarga.idGrid}" cellspacing="0"></table>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: TipoOperacaoCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacaoCarga.text, attr: { for: TipoOperacaoCarga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoOperacaoCarga.requiredClass, value: TipoOperacaoCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacaoCarga.maxlength, id : TipoOperacaoCarga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacaoCarga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: TransportadorCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TransportadorCarga.text, attr: { for: TransportadorCarga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TransportadorCarga.requiredClass, value: TransportadorCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: TransportadorCarga.maxlength, id : TransportadorCarga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TransportadorCarga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: RotaCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: RotaCarga.text, attr: { for: RotaCarga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: RotaCarga.requiredClass, value: RotaCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: RotaCarga.maxlength, id : RotaCarga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: RotaCarga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: ModeloVeicularCargaCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ModeloVeicularCargaCarga.text, attr: { for: ModeloVeicularCargaCarga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: ModeloVeicularCargaCarga.requiredClass, value: ModeloVeicularCargaCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: ModeloVeicularCargaCarga.maxlength, id : ModeloVeicularCargaCarga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ModeloVeicularCargaCarga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: CargaAgrupadaAlterar.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CargaAgrupadaAlterar.text, attr: { for: CargaAgrupadaAlterar.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: CargaAgrupadaAlterar.requiredClass, value: CargaAgrupadaAlterar.val, valueUpdate: 'afterkeydown', attr: { maxlength: CargaAgrupadaAlterar.maxlength, id : CargaAgrupadaAlterar.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CargaAgrupadaAlterar.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-footer" data-bind="visible: AgruparCarga.visible">
                            <div class="d-flex align-items-center justify-content-end">
                                <button class="btn btn-success" type="button" data-bind="click: AgruparCarga.eventClick, visible: AgruparCarga.visible, attr: {  id : AgruparCarga.id}">
                                    <span data-bind="text: AgruparCarga.text"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="card mt-3" id="divResumoCapacidade" style="display:none">
                        <div class="card-header">
                            <div class="card-title">Dados Capacidade</div>
                        </div>
                        <div class="card-body">
                            <div class="d-flex justify-content-center">
                                <h4 class="capacidade-titulo">Resumo capacidade veículo (Peso)</h4>
                            </div>
                            <div class="d-flex justify-content-between" id="divResumoCapacidadeDados">
                            </div>
                        </div>
                    </div>
                    <div class="col-12 mt-3">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: FilialCargaAgrupada.text"></label>
                            <select class="form-control" data-bind="options: FilialCargaAgrupada.options, optionsText: 'text', optionsValue: 'value', value: FilialCargaAgrupada.val, attr: { id : FilialCargaAgrupada.id }"></select>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-md-6">
                    <div class="card">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h3 class="card-title">Cargas</h3>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-12 col-md-4" data-bind="visible: CodigoCargaEmbarcador.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text"></label>
                                        <input type="text" class="form-control" data-bind="class: CodigoCargaEmbarcador.requiredClass, value: CodigoCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: DataInicio.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataInicio.text"></label>
                                        <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: DataFim.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataFim.text"></label>
                                        <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                    </div>
                                </div>
                            </div>
                            <div data-bind="id: BuscaAvancada.idFade, fadeVisible: BuscaAvancada.visibleFade" class="row">
                                <div class="col-12 col-md-12" data-bind="visible: RetornarCargaDocumentoEmitido.visible">
                                    <div class="form-group">
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" data-bind="checked: RetornarCargaDocumentoEmitido.val, attr: { maxlength: RetornarCargaDocumentoEmitido.maxlength, name: RetornarCargaDocumentoEmitido.id, id: RetornarCargaDocumentoEmitido.id }">
                                            <label class="custom-control-label" data-bind="text: RetornarCargaDocumentoEmitido.text, attr: { for: RetornarCargaDocumentoEmitido.id }"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-12" data-bind="visible: Empresa.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Empresa.maxlength, id : Empresa.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Empresa.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: Veiculo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: NumeroNF.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroNF.text"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroNF.requiredClass, value: NumeroNF.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroNF.maxlength, id : NumeroNF.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: PlacaDeAgrupamento.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: PlacaDeAgrupamento.text"></label>
                                        <input type="text" class="form-control" data-bind="class: PlacaDeAgrupamento.requiredClass, value: PlacaDeAgrupamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: PlacaDeAgrupamento.maxlength, id : PlacaDeAgrupamento.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: Destinatario.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Destinatario.text, attr: { for: Destinatario.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Destinatario.requiredClass, value: Destinatario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Destinatario.maxlength, id : Destinatario.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Destinatario.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: ZonaDeTransporte.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: ZonaDeTransporte.text, attr: { for: ZonaDeTransporte.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: ZonaDeTransporte.requiredClass, value: ZonaDeTransporte.val, valueUpdate: 'afterkeydown', attr: { maxlength: ZonaDeTransporte.maxlength, id : ZonaDeTransporte.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ZonaDeTransporte.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="d-flex align-items-center justify-content-between mb-3">
                                <div class="col-12 col-md-6">
                                    <button class="btn btn-default me-2 waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click:  SelecionarTodas.eventClick, attr: { id: SelecionarTodas.id}" type="button">
                                        <i class="fal fa-arrow-alt-left"></i><span data-bind="text: SelecionarTodas.text"></span>
                                    </button>
                                </div>
                                <div class="col-12 col-md-6 d-flex justify-content-end">
                                    <button class="btn btn-default ms-2 waves-effect waves-themed" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                                        <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                                    </button>
                                    <button class="btn btn-primary ms-2 waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: PesquisarFilho.eventClick, attr: { id: PesquisarFilho.id}" type="button">
                                        <i class="fal fa-search"></i><span data-bind="text: PesquisarFilho.text"></span>
                                    </button>
                                </div>
                            </div>
                            <div class="widget-body no-padding-bottom table-sem-paginacao mt-2" id="container-grid-carga-agrupada">
                                <table width="100%" class="table table-bordered table-hover table-condensed" id="grid-carga-agrupada" cellspacing="0"></table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="divModalEditarModeloVeicular" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Editar Modelo Veicular</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body row">
                <div class="row" id="knockoutEdicaoModeloVeicular">

                    <div class="col-12 col-md-12" data-bind="visible: ModeloVeicular.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ModeloVeicular.text, attr: { for: ModeloVeicular.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: ModeloVeicular.requiredClass, value: ModeloVeicular.val, valueUpdate: 'afterkeydown', attr: { maxlength: ModeloVeicular.maxlength, id : ModeloVeicular.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ModeloVeicular.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-3" id="knockoutCRUDEdicaoModeloVeicular">

                    <button class="btn btn-success" type="button" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: {id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="ModaisCarga"></div>


