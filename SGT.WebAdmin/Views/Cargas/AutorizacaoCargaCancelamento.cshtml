<div id="knockoutPesquisaCargaCancelamento">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h2>Pesquisar Carregamentos</h2>
            </div>
            <div class="panel-content">
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: CodigoCargaEmbarcador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoCargaEmbarcador.val, attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id} , valueUpdate: 'afterkeydown'" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicio.val, attr: { maxlength: DataInicio.maxlength, id : DataInicio.id} " />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataLimite.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataLimite.val, attr: { maxlength: DataLimite.maxlength, id : DataLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: SituacaoCargaCancelamentoSolicitacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoCargaCancelamentoSolicitacao.text"></label>
                                    <select class="form-control" data-bind="options: SituacaoCargaCancelamentoSolicitacao.options, optionsText: 'text', optionsValue: 'value', value: SituacaoCargaCancelamentoSolicitacao.val, attr: { id : SituacaoCargaCancelamentoSolicitacao.id}, event: {change: SituacaoCargaCancelamentoSolicitacao.eventChange}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Filial.text, maxlength: Filial.maxlength, id: Filial.id, name: Filial.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Filial.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoOperacao.text, maxlength: TipoOperacao.maxlength, id: TipoOperacao.id, name: TipoOperacao.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoOperacao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Usuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Usuario.text, maxlength: Usuario.maxlength, id: Usuario.id, name: Usuario.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Usuario.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!-- Campos Ocultos -->

                            <div id="camposMultimodalPesquisa">
                                <div class="row">
                                    <div class="col-12 col-md-4" data-bind="visible: Tomador.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Tomador.text, attr: { for: Tomador.id }"></label>
                                            <div class="input-group">
                                                <input type="text" data-bind="css: Tomador.requiredClass, value: Tomador.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Tomador.text, maxlength: Tomador.maxlength, id: Tomador.id, name: Tomador.id }" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Tomador.idBtnSearch}">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-4" data-bind="visible: PortoOrigem.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: PortoOrigem.text, attr: { for: PortoOrigem.id }"></label>
                                            <div class="input-group">
                                                <input type="text" data-bind="css: PortoOrigem.requiredClass, value: PortoOrigem.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': PortoOrigem.text, maxlength: PortoOrigem.maxlength, id: PortoOrigem.id, name: PortoOrigem.id }" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: PortoOrigem.idBtnSearch}">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-4" data-bind="visible: PortoDestino.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: PortoDestino.text, attr: { for: PortoDestino.id }"></label>
                                            <div class="input-group">
                                                <input type="text" data-bind="css: PortoDestino.requiredClass, value: PortoDestino.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': PortoDestino.text, maxlength: PortoDestino.maxlength, id: PortoDestino.id, name: PortoDestino.id }" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: PortoDestino.idBtnSearch}">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Cancelamento de Cargas
                </h3>
            </div>
            <div class="panel-content">
                <div class="d-flex justify-content-end mb-2">
                    <button class="btn btn-primary" type="button" data-bind="click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible">
                        <span data-bind="text: SelecionarTodos.text"></span>
                    </button>

                </div>
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>

                <div class="d-flex justify-content-between mt-3">
                    <div class="d-flex">
                        <button class="btn btn-warning me-2" type="button" data-bind="click: ReprocessarTodas.eventClick, attr: { id: ReprocessarTodas.id }, visible: ReprocessarTodas.visible">
                            <span data-bind="text: ReprocessarTodas.text"></span>
                        </button>
                        <button class="btn btn-danger me-2" type="button" data-bind="click: RejeitarTodas.eventClick, attr: { id: RejeitarTodas.id }, visible: RejeitarTodas.visible">
                            <i class="fal fa-times"></i><span data-bind="text: RejeitarTodas.text"></span>
                        </button>
                        <button class="btn btn-primary me-2" type="button" data-bind="click: DelegarTodas.eventClick, attr: { id: DelegarTodas.id }, visible: DelegarTodas.visible">
                            <span data-bind="text: DelegarTodas.text"></span>
                        </button>
                    </div>
                    <div class="d-flex justify-content-end">
                        <button class="btn btn-success" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id: AprovarTodas.id }, visible: AprovarTodas.visible">
                            <i class="fal fa-check"></i><span data-bind="text: AprovarTodas.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



