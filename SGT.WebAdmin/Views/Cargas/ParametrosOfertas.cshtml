    <div class="col-12 col-md-6" data-bind="visible: Propriedade.visible">
        <div class="form-group">
            <div class="custom-control custom-checkbox">
                <input type="checkbox" class="custom-control-input" data-bind="checked: Propriedade.val, attr: { maxlength: Propriedade.maxlength, name: Propriedade.id, id: Propriedade.id }">
                <label class="custom-control-label" data-bind="text: Propriedade.text, attr: { for: Propriedade.id }"></label>
            </div>
        </div>
    </div>
