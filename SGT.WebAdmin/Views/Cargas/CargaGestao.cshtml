                <div class="panel-container">
                    <div class="panel-hdr">
                        <h3>
                            <i class="fal fa-edit"></i>
                            Veículos
                        </h3>
                    </div>
                    <div class="panel-content">
                        <form>
                            <div class="row" id="knockoutPesquisaVeiculo">

                                <div class="col-12 col-md-12">
                                    <div class="card mb-3">

                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-12 col-md-4" data-bind="visible: Placa.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: Placa.text, attr: { for: Placa.id }"></label>
                                                        <input type="text" class="form-control inputReduzido" data-bind="class: Placa.requiredClass, value: Placa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Placa.maxlength, id : Placa.id}" />
                                                    </div>
                                                </div>

                                                <div class="col-12 col-md-8" data-bind="visible: Motorista.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
