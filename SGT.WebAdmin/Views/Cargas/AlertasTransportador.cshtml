

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Cargas</li>
    <li class="breadcrumb-item active">Alertas</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Alertas
    </h1>
</div>


<div class="panel">
    <div class="panel-container">
        <div class="panel-content">
            <form>
                <div id="knockoutPesquisaAlertasTransportador">
                    <div class="row">
                        <div class="col-12 col-md-3" data-bind="visible: TipoAlerta.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoAlerta.text, attr: { for: TipoAlerta.id }"></label>
                                <select class="form-control" data-bind="class: TipoAlerta.requiredClass, options: TipoAlerta.options, optionsText: 'text', optionsValue: 'value', value: TipoAlerta.val, attr: { maxlength: TipoAlerta.maxlength, id : TipoAlerta.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: AlertaMonitorStatus.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: AlertaMonitorStatus.text, attr: { for: AlertaMonitorStatus.id }"></label>
                                <select class="form-control" data-bind="class: AlertaMonitorStatus.requiredClass, options: AlertaMonitorStatus.options, optionsText: 'text', optionsValue: 'value', value: AlertaMonitorStatus.val, attr: { maxlength: AlertaMonitorStatus.maxlength, id : AlertaMonitorStatus.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CodigoCargaEmbarcador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text, attr: { for: CodigoCargaEmbarcador.id }"></label>
                                <input type="text" class="form-control" data-bind="class: CodigoCargaEmbarcador.requiredClass, value: CodigoCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Placa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Placa.text, attr: { for: Placa.id }"></label>
                                <input type="text" class="form-control" data-bind="class: Placa.requiredClass, value: Placa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Placa.maxlength, id : Placa.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Motorista.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3 mt-5" data-bind="visible: ExibirApenasComPosicaoTardia.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: ExibirApenasComPosicaoTardia.val, attr: { maxlength: ExibirApenasComPosicaoTardia.maxlength, name: ExibirApenasComPosicaoTardia.id, id: ExibirApenasComPosicaoTardia.id }">
                                    <label class="custom-control-label" data-bind="text: ExibirApenasComPosicaoTardia.text, attr: { for: ExibirApenasComPosicaoTardia.id }"></label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

<div id="knockoutAlertas">
    <div class="panel" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
        <div class="panel-container">
            <div class="panel-content">
                <div class="row">
                    <div class="widget-body no-padding-bottom" id="container-grid-alertas-transportador">
                        <table width="100%" class="table table-bordered table-hover table-condensed" id="grid-alertas-transportador" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalAlertasTransportador" role="dialog" aria-hidden="true">
    <div id="knockoutTratativasAlerta" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Alerta</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table>
                    <tbody>
                        <tr>
                            <td>
                                <b data-bind="text: Descricao.text"></b>
                            </td>
                            <td>
                                <span data-bind="text: Descricao.val"></span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <b data-bind="text: Data.text"></b>
                            </td>
                            <td>
                                <span data-bind="text: Data.val"></span>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <b data-bind="text: DataFim.text"></b>
                            </td>
                            <td>
                                <span data-bind="text: DataFim.val"></span>
                            </td>
                        </tr>
                        <tr data-bind="visible: ObservacaoMotorista.visible">
                            <td>
                                <b data-bind="text: ObservacaoMotorista.text"></b>
                            </td>
                            <td>
                                <span data-bind="text: ObservacaoMotorista.val"></span>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <form class="mt-3">
                    <div class="row">
                        <div class="col-12 col-md-6" data-bind="visible: Tratativa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Tratativa.text, attr: { for: Tratativa.id }"></label>
                                <select class="form-control" data-bind="enabled: Tratativa.enable,options: Tratativa.options, optionsText: 'text', optionsValue: 'value', value: Tratativa.val, attr: { id : Tratativa.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-6" data-bind="visible: Observacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                                <textarea rows="5" type="text" class="form-control" data-bind="enable: Observacao.enable, value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                            </div>
                        </div>
                        <div class="col-12">
                            <div id="mapaControleEntregaAlerta" style="width:100%;height:300px;"></div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button class="btn btn-default" type="button" data-bind="click: Fechar.eventClick, text: Fechar.text, attr: { id : Fechar.id}"></button>
                <button class="btn btn-success" type="button" data-bind="visible: Confirmar.visible, click: Confirmar.eventClick, text: Confirmar.text, attr: { id : Confirmar.id}"></button>
            </div>
        </div>
    </div>
</div>

<div id="ModaisTratativaAlerta"></div>

<script src="~/scripts/alertasTransportador"></script>
<script src="~/scripts/tratativaAlerta"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadAlertas();
    });

</script>
