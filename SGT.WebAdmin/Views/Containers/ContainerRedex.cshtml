
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Containers</li>
    <li class="breadcrumb-item active">Containers Redex</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Containers em áreas de Redex
    </h1>
</div>

<div id="knockoutPesquisaContainer">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Containers em áreas de Redex
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text"></label>
                                    <input class="form-control" data-bind="value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: AreaRedex.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: AreaRedex.text, attr: { for: AreaRedex.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: AreaRedex.requiredClass, value: AreaRedex.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': AreaRedex.text, maxlength: AreaRedex.maxlength, id: AreaRedex.id, name: AreaRedex.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: AreaRedex.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DiasPosse.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DiasPosse.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DiasPosse.val, valueUpdate: 'afterkeydown', attr: { maxlength: DiasPosse.maxlength, id : DiasPosse.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" style="margin-top:40px;" data-bind="visible: SomenteExcedentes.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: SomenteExcedentes.val, attr: { maxlength: SomenteExcedentes.maxlength, name: SomenteExcedentes.id, id: SomenteExcedentes.id }">
                                        <label class="custom-control-label" data-bind="text: SomenteExcedentes.text, attr: { for: SomenteExcedentes.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Containers em Redex
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="widget-body no-padding-bottom" id="container-grid-containers">
                    <table width="100%" class="table table-bordered table-hover table-condensed" id="grid-containers" cellspacing="0"></table>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalHistoricos" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Histórico Container</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true"><i class="fal fa-times"></i></span>
                </button>
            </div>
            <div class="modal-body">
                <table width="100%" class="table table-bordered table-hover" id="grid-historicos"></table>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalEmbarque" role="dialog" aria-hidden="true">
    <div id="knockoutInformarEmbarque" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Informar Embarque</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-md-4" data-bind="visible: DataEmbarque.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataEmbarque.text, attr: { for: DataEmbarque.id }"></label>
                            <input type="text" class="form-control" data-bind="css: DataEmbarque.requiredClass,value: DataEmbarque.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmbarque.maxlength, id : DataEmbarque.id}" />
                        </div>
                    </div>
                    <div class="form-group col-md-8" data-bind="visible: LocalEmbarque.visible">
                        <label class="form-label" data-bind="text: LocalEmbarque.text, attr: { for: LocalEmbarque.id }"></label>
                        <div class="input-group">
                            <input type="text" class="form-control" data-bind="css:LocalEmbarque.requiredClass,value: LocalEmbarque.val, valueUpdate: 'afterkeydown', attr: { maxlength: LocalEmbarque.maxlength, id : LocalEmbarque.id}" />
                            <div class="input-group-append">
                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: LocalEmbarque.idBtnSearch}">
                                    <i class="fal fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default ms-1" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button class="btn btn-success ms-1" type="button" data-bind="click: Confirmar.eventClick, attr: { id : Confirmar.id}">
                        <span data-bind="text: Confirmar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/containerRedex"></script>
<script src="~/scripts/canvg"></script>
<script src="~/scripts/d3js"></script>
<script src="~/scripts/chartjs"></script>
<script src="~/scripts/carga"></script>

<script type="text/javascript">
    pageSetUp();

    if (!$("body").hasClass("menu-on-top"))
        $("body").addClass("minified");

    $(document).ready(function () {
        loadContainerRedex();
    });

</script>
