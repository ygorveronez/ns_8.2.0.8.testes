
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Ocorrências</li>
    <li class="breadcrumb-item active">Log Leitura Arquivo Ocorrência</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Envio de arquivos para Ocorrência
    </h1>
</div>

<div id="knockoutLogLeituraArquivoOcorrencia">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Pesquisar Arquivo
                </h3>
            </div>
            <div class="panel-content">
                <div class="mt-3">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: Ocorrencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ocorrencia.text, attr: { for: Ocorrencia.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Ocorrencia.requiredClass, value: Ocorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Ocorrencia.maxlength, id : Ocorrencia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoEnvioArquivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoEnvioArquivo.text, attr: { for: TipoEnvioArquivo.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: TipoEnvioArquivo.requiredClass, value: TipoEnvioArquivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoEnvioArquivo.maxlength, id : TipoEnvioArquivo.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: NomeArquivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NomeArquivo.text, attr: { for: NomeArquivo.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NomeArquivo.requiredClass, value: NomeArquivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: NomeArquivo.maxlength, id : NomeArquivo.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <div class="custom-file-btn ms-2" data-bind="visible: Upload.visible">
                            <label class="btn btn-default waves-effect waves-themed" data-bind="attr: { for: Upload.idFile }">
                                <i data-bind="css: Upload.icon"></i><span data-bind="text: Upload.text"></span>
                            </label>
                            <input type="file" accept="text/plain" data-bind="event: {change: Upload.eventChange}, attr: { id : Upload.idFile, accept: Upload.accept, multiple: true }" />
                        </div>
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa ms-2" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/logLeituraArquivoOcorrencia"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLogLeituraArquivoOcorrencia();
    });

</script>

