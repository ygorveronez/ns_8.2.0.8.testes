                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="form-group col-12 col-md-6" data-bind="visible: Arquivo.visible">
                        <p class="form-label" data-bind="text: Arquivo.text"></p>
                        <div class="custom-file" data-bind="visible: Arquivo.visible">
                            <input type="file" class="custom-file-input" data-bind="value: Arquivo.val, attr: { id: Arquivo.id }" readonly />
                            <label class="custom-file-label" data-bind="text: Arquivo.val, attr: { for: Arquivo.id }"></label>
                        </div>
                    </div>
                    <footer class="d-flex align-items-center justify-content-end mb-3 mt-3">
                        <button class="btn btn-primary" type="button" data-bind="click: Adicionar.eventClick, attr: { id: Adicionar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Adicionar.text"></span>
                        </button>

                    </footer>
                </div>
                <div class="d-flex align-items-center justify-content-end mb-3 me-1">
                    <button type="button" data-bind="click: Fechar.eventClick, visible: Fechar.visible, attr: { id: Fechar.id}" class="btn btn-labeled btn-primary pull-left">
                        <i class="fal fa-times"></i>
                        <span data-bind="text: Fechar.text"></span>
                    </button>
                    <button type="button" data-bind="click: Finalizar.eventClick, visible: Finalizar.visible, attr: { id: Finalizar.id}" class="btn btn-labeled btn-success ms-2 ">
                        <i class="fal fa-check"></i>
                        <span data-bind="text: Finalizar.text"></span>
                    </button>
                </div>
            </div>

        </div>
    </div>
</div>

