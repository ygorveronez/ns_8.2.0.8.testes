
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Ocorrências</li>
    <li class="breadcrumb-item active">Aceite Débitos</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Aceite de Débito
    </h1>
</div>
<div id="knockoutPesquisaAceiteDebito">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    Pesquisar Aceites de Débito
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroOcorrencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroOcorrencia.text, attr: { for: NumeroOcorrencia.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroOcorrencia.requiredClass, value: NumeroOcorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroOcorrencia.maxlength, id : NumeroOcorrencia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCriacaoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCriacaoInicio.text, attr: { for: DataCriacaoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCriacaoInicio.requiredClass, value: DataCriacaoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCriacaoInicio.maxlength, id : DataCriacaoInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCriacaoFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCriacaoFim.text, attr: { for: DataCriacaoFim.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCriacaoFim.requiredClass, value: DataCriacaoFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCriacaoFim.maxlength, id : DataCriacaoFim.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                          <span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="d-none" id="wid-id-4">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content row">
                <div class="col-12 col-md-6">
                    <div class="card" id="knockoutAceiteDebito">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h3 class="card-title">Resposta</h3>
                        </div>
                        <div class="card-body row">
                            <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                                    <textarea class="form-control" rows="4" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id }"></textarea>
                                </div>
                            </div>
                            <div class="col-12 col-md-12 justify-content-between">
                                <button class="btn btn-success me-1" type="button" data-bind="click: Aprovar.eventClick, attr: { id: Aprovar.id}">
                                    <span data-bind="text: Aprovar.text"></span>
                                </button>
                                <button class="btn btn-danger" type="button" data-bind="click: Reprovar.eventClick, attr: { id: Reprovar.id}">
                                    <span data-bind="text: Reprovar.text"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-md-6">
                    <div class="card" id="knockoutAnexos">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h3 data-bind="text: Anexos.text" class="card-title"></h3>
                        </div>
                        <div class="card-body row">
                            <div class="col-12 col-md-4">
                                <button class="btn btn-success mb-2" type="button" data-bind="click: AdicionarAnexo.eventClick, attr: { id: AdicionarAnexo.id}">
                                    <span data-bind="text: AdicionarAnexo.text"></span>
                                </button>
                            </div>
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Anexos.idGrid }" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-md-12" id="knockoutOcorrencia">
                    <div class="card">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h3 class="card-title">Detalhes</h3>
                        </div>
                        <div class="row ms-1">
                            <section class="col col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <label><b data-bind="text: Numero.text"></b> <span data-bind="text: Numero.val"></span></label>
                            </section>
                            <section class="col col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <label><b data-bind="text: DataOcorrencia.text"></b> <span data-bind="text: DataOcorrencia.val"></span></label>
                            </section>
                            <section class="col col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <label><b data-bind="text: TipoOcorrencia.text"></b> <span data-bind="text: TipoOcorrencia.val"></span></label>
                            </section>
                            <section class="col col-xs-12 col-sm-12">
                                <label><b data-bind="text: Observacao.text"></b> <span data-bind="text: Observacao.val"></span></label>
                            </section>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-md-12">
                    <div class="card" id="knockoutDocumentos">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <div class="card-header d-flex align-items-center justify-content-between">
                                <h3 class="card-title" data-bind="text: Documentos.text"></h3>
                            </div>
                        </div>
                        <div class="card-body">
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Documentos.idGrid}" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="divModalGerenciarAnexos" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Adicionar Anexo</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutModalAnexos">
                <div class="row">
                    <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}"  />
                        </div>
                    </div>
                    <div class="form-group col-12 col-md-6 mt-1" data-bind="visible: Arquivo.visible">
                        <p class="form-label" data-bind="text: Arquivo.text"></p>
                        <div class="custom-file" data-bind="visible: Arquivo.visible">
                            <input for="file" type="file" accept=".jpg,.jpeg,.png,.pdf" class="custom-file-input"  data-bind="value: Arquivo.val, enable : Arquivo.enable, attr: { id: Arquivo.id }" readonly>
                            <label class="custom-file-label" data-bind="text: Arquivo.val, attr: { for: Arquivo.id }">Selecione uma imagem para anexar</label>
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end mt-3">
                    <input type="button" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick,visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}" class="btnPesquisarFiltroPesquisa btn btn-labeled btn-success" />
                </footer>
            </div>
        </div>
    </div>
</div>




<script src="~/scripts/aceiteDebito"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("Ocorrencias.Ocorrencia").then(loadAceiteDebito);
    });

</script>

