
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Ocorrências</li>
    <li class="breadcrumb-item active">Ocorrência Parâmetro Ocorrência</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Parâmetros da Carga
    </h1>
</div>

<div id="knockoutPesquisaOcorrenciaParametroOcorrencia">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Parâmetros Gerados
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <!-- Campos de filtro -->
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Parâmetro -->

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Parâmetro
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutOcorrenciaParametroOcorrencia">
                <div class="row">
                    <div class="col-12 col-md-3" data-bind="visible: Numero.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Numero.text"></label>
                            <input type="text" class="form-control" data-bind="value: Numero.val, enable: Numero.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Numero.maxlength, id : Numero.id}" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-md-4" data-bind="visible: TipoOcorrencia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoOcorrencia.text, attr: { for: TipoOcorrencia.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: TipoOcorrencia.requiredClass, value: TipoOcorrencia.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoOcorrencia.text, maxlength: TipoOcorrencia.maxlength, id: TipoOcorrencia.id, name: TipoOcorrencia.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoOcorrencia.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: Carga.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Carga.text, maxlength: Carga.maxlength, id: Carga.id, name: Carga.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Carga.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: DataParametro.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataParametro.text"></label>
                            <input type="text" class="form-control" data-bind="value: DataParametro.val, enable: DataParametro.enable, valueUpdate: 'afterkeydown', attr: { maxlength: DataParametro.maxlength, id : DataParametro.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Observacao.text"></label>
                            <textarea type="text" rows="5" class="form-control" data-bind="value: Observacao.val, enable: Observacao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                        </div>
                    </div>
                </div>
            </form>
            <!-- Footer Principal -->
            <footer id="knockoutCRUDOcorrenciaParametroOcorrencia">
                <div class="col-0 d-flex justify-content-end">
                    <button type="button" class="btn btn-labeled btn-success mx-3" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                </div>
            </footer>
        </div>
    </div>
</div>

<div class="panel col-12" id="knockoutParametros" data-bind="visible: Ocorrencia.visible">
    <div class="row">
        <div class="panel-container col col-md-4">
            <div class="panel-hdr">
                <h3>
                    Parâmetros da Ocorrência
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div>
                        <div class="row">
                            <div class="col-12 col-md-12" data-bind="visible: Data.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Data.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Data.val, enable: Data.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Data.maxlength, id : Data.id}" />
                                </div>
                            </div>
                            <div class="col col-md-6" data-bind="visible: HoraInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: HoraInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="value: HoraInicial.val, enable: HoraInicial.enable, valueUpdate: 'afterkeydown', attr: { maxlength: HoraInicial.maxlength, id : HoraInicial.id}" />
                                </div>
                            </div>
                            <div class="col col-md-6" data-bind="visible: HoraFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: HoraFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="value: HoraFinal.val, enable: HoraFinal.enable, valueUpdate: 'afterkeydown', attr: { maxlength: HoraFinal.maxlength, id : HoraFinal.id}" />
                                </div>
                            </div>
                            <div class="col col-md-6" data-bind="visible: KmInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: KmInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="value: KmInicial.val, enable: KmInicial.enable, valueUpdate: 'afterkeydown', attr: { maxlength: KmInicial.maxlength, id : KmInicial.id}" />
                                </div>
                            </div>
                            <div class="col col-md-6" data-bind="visible: KmFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: KmFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="value: KmFinal.val, enable: KmFinal.enable, valueUpdate: 'afterkeydown', attr: { maxlength: KmFinal.maxlength, id : KmFinal.id}" />
                                </div>
                            </div>
                            <footer>
                                <div class="col-0 d-flex justify-content-end">
                                    <button type="button" class="btn btn-labeled btn-secondary mx-3" data-bind="click: ObservacaoCTe.eventClick, visible: ObservacaoCTe.visible, attr: { value: ObservacaoCTe.text, id : ObservacaoCTe.id}">
                                        <span data-bind="text: ObservacaoCTe.text"></span>
                                    </button>
                                    <button type="button" class="btn btn-labeled btn-success" data-bind="click: VisulizarOcorrencias.eventClick, visible: VisulizarOcorrencias.visible, attr: { value: VisulizarOcorrencias.text, id : VisulizarOcorrencias.id}">
                                        <span data-bind="text: VisulizarOcorrencias.text"></span>
                                    </button>
                                </div>
                            </footer>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- Ocorrências Geradas -->
        <div class="panel-container col col-md-8" data-bind="visible: Ocorrencia.visible">
            <div class="panel-hdr">
                <h3>
                    Ocorrência
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div>
                        <div class="row">
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Ocorrencia.idGrid }" cellspacing="0"></table>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalObservacaoCTe" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" id="knockoutObservacaoCTe">
        <div class="modal-content">
            <div class="modal-header">
                <h3>Observação CT-e</h3>
                <button type="button" class="btn-close" data-dismiss="modal" data-bind="click: Fechar.eventClick, attr: { id: Fechar.id}" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Observacao.text"></label>
                            <textarea type="text" rows="5" class="form-control" data-bind="value: Observacao.val, enable: Observacao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal" data-bind="click: Fechar.eventClick, attr: { id: Fechar.id}">Fechar</button>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/ocorrenciaParametroOcorrencia"></script>

