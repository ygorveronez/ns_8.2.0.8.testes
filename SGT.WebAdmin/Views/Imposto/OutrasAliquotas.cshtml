                            <label class="form-label" data-bind="text: TipoImposto.text, attr: { for: TipoImposto.id }"></label>
                            <select class="form-control" data-bind="class: TipoImposto.requiredClass, options: TipoImposto.options, optionsText: 'text', optionsValue: 'value', value: TipoImposto.val, attr: { maxlength: TipoImposto.maxlength, id : TipoImposto.id}, enable: TipoImposto.enable"></select>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12 col-md-2" data-bind="visible: ExibirCBS.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Aliquota.text"></label>
                            <input type="text" class="form-control" data-bind="css: Aliquota.requiredClass, value: Aliquota.val, valueUpdate: 'afterkeydown', attr: { maxlength: Aliquota.maxlength, id : Aliquota.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: ExibirCBS.visible">
