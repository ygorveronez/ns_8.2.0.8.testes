                        </button>
                        <button class="btn btn-default waves-effect waves-themed" type="button" data-bind="click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}">
                            <i class="fal fa-check"></i><span data-bind="text: SelecionarTodos.text"></span>
                        </button>
                    </div>
                </div>
                <div class="panel-content">

                    <table width="100%" class="table table-bordered table-hover" id="grid-documento-destino-empresa" cellspacing="0"></table>

                    <div class="d-flex justify-content-between mt-3">
                        <div class="d-flex">
                        </div>
                        <div class="d-flex justify-content-end">
                            <button class="btn btn-success me-2" type="button" data-bind="click: DownloadLoteXML.eventClick, attr: { id: DownloadLoteXML.id }, visible: DownloadLoteXML.visible">
                                <i data-bind="attr: { class: DownloadLoteXML.icon}" class="fal fa-download"></i><span data-bind="text: DownloadLoteXML.text"></span>
                            </button>
                            <button class="btn btn-success" type="button" data-bind="click: DownloadLoteXMLCTe.eventClick, attr: { id: DownloadLoteXMLCTe.id }, visible: DownloadLoteXMLCTe.visible">
