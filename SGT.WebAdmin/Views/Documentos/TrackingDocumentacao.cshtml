                </div>
                <table width="100%" class="table table-bordered table-hover" data-bind="enable: Registros.enable, attr: { id: Registros.idGrid}" cellspacing="0"></table>
                <footer class="d-flex justify-content-end">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-warning ms-1" data-bind="click: GerarTracking.eventClick, visible: GerarTracking.visible, attr: { id : GerarTracking.id}">
                        <span data-bind="text: GerarTracking.text"></span>
