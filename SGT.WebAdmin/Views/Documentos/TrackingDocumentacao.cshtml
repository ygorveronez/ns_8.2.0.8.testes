                            <select class="form-control" data-bind="class: TipoTrackingDocumentacao.requiredClass, options: TipoTrackingDocumentacao.options, optionsText: 'text', optionsValue: 'value', value: TipoTrackingDocumentacao.val, attr: { maxlength: TipoTrackingDocumentacao.maxlength, id : TipoTrackingDocumentacao.id}"></select>
