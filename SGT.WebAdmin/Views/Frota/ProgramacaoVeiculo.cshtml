            <div class="panel-content">
                <div class="col-12 col-md-12">

                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: ProgramacaoVeiculo.idGrid}" cellspacing="0"></table>

                    <div class="d-flex justify-content-end mt-3 mb-3">
                        <button class="btn btn-success" type="button" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                            <i class="fal fa-check"></i><span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>

                    <div class="tab-content border p-3">
                        <div id="knockoutCarga" class="tab-pane fade active show">
                            <h3>Sumarizadores</h3>
                            <div class="row row-cols-2 row-cols-md-2">
                                <div class="col">
                                    <table class="table table-clean align-self-auto">
                                        <tbody>
                                            <!-- ko foreach: Status -->
                                            <tr>
                                                <td>
                                                    <strong data-bind="text: $data.Descricao"></strong>
                                                </td>
                                                <td>
                                                    <span data-bind="text: $data.Quantidade"></span>
                                                </td>
                                            </tr>
                                            <!-- /ko -->
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
