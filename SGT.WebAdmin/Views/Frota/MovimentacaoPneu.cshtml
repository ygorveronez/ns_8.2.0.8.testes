<style>
.pneu-section {
    padding: 15px;
}
.border-start {
    border-left: 1px solid #dee2e6 !important;
}
@@media (max-width: 767px) {
    .border-start {
        border-left: none !important;
        border-top: 1px solid #dee2e6 !important;
        margin-top: 20px;
        padding-top: 20px;
    }
}
</style>

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Frota</li>
        <li class="breadcrumb-item active">Movimentação de Pneus</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-truck"></i> Movimentação de Pneus
        </h1>
    </div>

    <div class="row" id="container-movimentacao-pneu">
        <div class="col-12 col-md-5">
            <div id="knockoutPesquisaVeiculo" class="panel">
                <div class="panel-hdr" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <h2>Pesquisar Veículo</h2>
                </div>
                <div class="panel-container" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <div class="panel-content">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-8" data-bind="visible: Veiculo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary" type="button" data-bind="attr: { id: Veiculo.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: NumeroFogo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroFogo.text, attr: { for: NumeroFogo.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: NumeroFogo.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroFogo.maxlength, id : NumeroFogo.id}" />
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="d-flex justify-content-end mt-3">
                                        <div data-bind='component: {name: "import-file", params: { ko: Importar }}' class="me-2"></div>
                                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                            <i class="fal fa-search"></i> <span data-bind="text: Pesquisar.text"></span>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

            <div id="knockoutDadosVeiculo" class="panel dados-veiculo-container" style="display: none;">
                <div class="panel-hdr" data-bind="click: ExibirDados.eventClick, id: ExibirDados.id">
                    <h2>Dados do Veículo</h2>
                </div>
                <div class="panel-container" data-bind="id: ExibirDados.idFade, fadeVisible: ExibirDados.visibleFade">
                    <div class="panel-content">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-4" data-bind="visible: Hodometro.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Hodometro.text, attr: { for: Hodometro.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Hodometro.val, enable: Hodometro.enable, attr: { maxlength: Hodometro.maxlength, id : Hodometro.id} , valueUpdate: 'afterkeydown'" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: PlacaVeiculo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: PlacaVeiculo.text, attr: { for: PlacaVeiculo.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: PlacaVeiculo.val, enable: PlacaVeiculo.enable, attr: { maxlength: PlacaVeiculo.maxlength, id : PlacaVeiculo.id} , valueUpdate: 'afterkeydown'" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: FrotaVeiculo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: FrotaVeiculo.text, attr: { for: FrotaVeiculo.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: FrotaVeiculo.val, enable: FrotaVeiculo.enable, attr: { maxlength: FrotaVeiculo.maxlength, id : FrotaVeiculo.id} , valueUpdate: 'afterkeydown'" />
                                    </div>
                                </div>
                                <div class="col-12" data-bind="visible: LiberarVeiculoManutencao.visible">
                                    <div class="d-flex justify-content-end">
                                        <button class="btn btn-success" type="button" data-loading-text="Aguarde..." data-bind="click: LiberarVeiculoManutencao.eventClick, text: LiberarVeiculoManutencao.text, attr: { id : LiberarVeiculoManutencao.id}"></button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

            <div id="knockoutVeiculo" class="panel">
