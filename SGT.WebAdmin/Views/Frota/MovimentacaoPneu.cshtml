        <div class="col-12 col-md-2">
            <div id="knockoutAcaoMovimentacaoPneuVeiculo" class="movimentacao">
                <div id="envio-pneu-estoque" class="movimentar" data-tipo-container-pneu="5">
                    <div class="movimentar-icone"><i class="fal fa-archive"></i></div>
                    <h3 class="movimentar-texto">Estoque</h3>
                </div>
                <div id="envio-pneu-sucata" class="movimentar" data-tipo-container-pneu="6">
                    <span class="movimentar-icone"><i class="fal fa-trash"></i></span>
                    <span class="movimentar-texto">Sucata</span>
                </div>
                <div id="envio-pneu-reforma" class="movimentar" data-tipo-container-pneu="7">
                    <span class="movimentar-icone"><i class="fal fa-cogs"></i></span>
                    <span class="movimentar-texto">Reforma</span>
                </div>
            </div>
        </div>

        <div class="col-12 col-md-5">
            <div id="knockoutPesquisaEstoque" class="panel">
                <div class="panel-hdr" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <h2>Pesquisar Estoque</h2>
                </div>
                <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade" class="panel-container">
                    <div class="panel-content">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-8" data-bind="visible: Almoxarifado.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Almoxarifado.text, attr: { for: Almoxarifado.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="value: Almoxarifado.val, valueUpdate: 'afterkeydown', attr: { maxlength: Almoxarifado.maxlength, id : Almoxarifado.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary" type="button" data-bind="attr: { id: Almoxarifado.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: NumeroFogo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroFogo.text, attr: { for: NumeroFogo.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: NumeroFogo.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroFogo.maxlength, id : NumeroFogo.id}" />
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="d-flex justify-content-end">
                                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                            <i class="fal fa-search"></i> <span data-bind="text: Pesquisar.text"></span>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

