                                        <span class="pneu-numero" data-bind="text: NumeroFogo.val().substring(0, 5)"></span>
                                        <div data-bind="attr: { 'data-posicao-pneu': PosicaoPneu.val }" class="pneu-tooltip">
                                            <div class="pneu-tooltip-conteudo">
                                                <table>
                                                    <tbody>
                                                        @*<tr>
                                                                <td class="pneu-tooltip-conteudo-texto-label">Almoxarifado: </td>
                                                                <td data-bind="text: Almoxarifado.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                            </tr>*@
                                                        <tr>
                                                            <td class="pneu-tooltip-conteudo-texto-label">Número Fogo: </td>
                                                            <td data-bind="text: NumeroFogo.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                        </tr>
                                                        <tr>
                                                            <td class="pneu-tooltip-conteudo-texto-label">Vida: </td>
                                                            <td data-bind="text: Vida.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                        </tr>
                                                        <tr>
                                                            <td class="pneu-tooltip-conteudo-texto-label">Km Rodado: </td>
                                                            <td data-bind="text: KmAtualRodado.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                        </tr>
                                                        <tr>
                                                            <td class="pneu-tooltip-conteudo-texto-label">Marca: </td>
                                                            <td data-bind="text: Marca.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                        </tr>
                                                        <tr>
                                                            <td class="pneu-tooltip-conteudo-texto-label">Modelo: </td>
                                                            <td data-bind="text: Modelo.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                        </tr>
                                                        <tr>
                                                            <td class="pneu-tooltip-conteudo-texto-label">Bd Rodagem: </td>
                                                            <td data-bind="text: BandaRodagem.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                        </tr>
                                                        <tr>
                                                            <td class="pneu-tooltip-conteudo-texto-label">Movimentação: </td>
                                                            <td data-bind="text: DataMovimentacao.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /ko -->
                                    <!-- /ko -->
                                    <!-- /ko -->
                                </div>
                            </div>
                            <!-- /ko -->
                        </div>
                        <!-- ko if: Estepes().length > 0 -->
                        <div class="estepes">
                            <!-- ko foreach: Estepes -->
                            <!-- ko if: CodigoPneu.val() == 0 -->
                            <div class="pneu" data-bind="attr: { id: 'estepe-' + Codigo.val(), 'data-codigo-estepe': Codigo.val, 'data-tipo-container-pneu': 2 }">
                                <img src="/img/Eixos/PneuDesabilitado.png">
                                <div class="pneu-posicao pneu-numero" data-bind="text: Numero.val"></div>
                            </div>
                            <!-- /ko -->
                            <!-- ko if: CodigoPneu.val() > 0 -->
                            <div class="pneu" data-bind="attr: { id: 'estepe-' + Codigo.val(), 'data-codigo-estepe': Codigo.val, 'data-tipo-container-pneu': 2 }">
                                <img src="img/Eixos/Pneu.png">
                                <div class="pneu-posicao" data-bind="text: Numero.val"></div>
                                <span class="pneu-numero" data-bind="text: NumeroFogo.val().substring(0, 5)"></span>
                                <div data-bind="attr: { 'data-posicao-pneu': ($index() + 1) }" class="pneu-tooltip">
                                    <div class="pneu-tooltip-conteudo">
                                        <table>
                                            <tbody>
                                                @*<tr>
                                                        <td class="pneu-tooltip-conteudo-texto-label">Almoxarifado: </td>
                                                        <td data-bind="text: Almoxarifado.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                    </tr>*@
                                                <tr>
                                                    <td class="pneu-tooltip-conteudo-texto-label">Número Fogo: </td>
                                                    <td data-bind="text: NumeroFogo.val" class="pneu-tooltip-conteudo-texto-descricao"></td>
                                                </tr>
                                                <tr>
                                                    <td class="pneu-tooltip-conteudo-texto-label">Vida: </td>
