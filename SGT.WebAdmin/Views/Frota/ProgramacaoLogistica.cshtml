
<style>
    #knockoutProgramacaoLogistica.ocultarPaginacao .dt-toolbar-footer {
        display: none
    }
</style>
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Frota</li>
    <li class="breadcrumb-item active">Programação Logística</li>
</ol>
<div class="subheader"> 
    <h1 class="subheader-title">
        Programação de Motoristas
    </h1>
</div>

<div>
    <div class="panel" id="knockoutPesquisaProgramacaoLogistica">
        <div class="panel-container">

            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Motoristas
                </button>
                <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade" class="mt-3">
                    <form>
                        <div class="row">

                            <div class="col-6 col-md-3" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Motorista.val, enable: Motorista.enable, attr: { maxlength: Motorista.maxlength, id: Motorista.id}, event: { blur: Motorista.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : Motorista.enable, attr: { id: Motorista.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-3" data-bind="visible: SituacaoColaborador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoColaborador.text, attr: { for: SituacaoColaborador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: SituacaoColaborador.val, enable: SituacaoColaborador.enable, attr: { maxlength: SituacaoColaborador.maxlength, id: SituacaoColaborador.id}, event: { blur: SituacaoColaborador.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : SituacaoColaborador.enable, attr: { id: SituacaoColaborador.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-3" data-bind="visible: ProgramacaoAlocacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ProgramacaoAlocacao.text, attr: { for: ProgramacaoAlocacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: ProgramacaoAlocacao.val, enable: ProgramacaoAlocacao.enable, attr: { maxlength: ProgramacaoAlocacao.maxlength, id: ProgramacaoAlocacao.id}, event: { blur: ProgramacaoAlocacao.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : ProgramacaoAlocacao.enable, attr: { id: ProgramacaoAlocacao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-3">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoMotorista.text, attr: { for: TipoMotorista.id }"></label>
                                    <select class="form-control" data-bind="options: TipoMotorista.options, optionsText: 'text', optionsValue: 'value', value: TipoMotorista.val, attr: { id : TipoMotorista.id}, event: {change: TipoMotorista.eventChange}"></select>
                                </div>
                            </div>

                            <div class="col-6 col-md-4" data-bind="visible: TipoVeiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoVeiculo.text, attr: { for: TipoVeiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: TipoVeiculo.val, enable: TipoVeiculo.enable, attr: { maxlength: TipoVeiculo.maxlength, id: TipoVeiculo.id}, event: { blur: TipoVeiculo.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : TipoVeiculo.enable, attr: { id: TipoVeiculo.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-4" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Veiculo.val, enable: Veiculo.enable, attr: { maxlength: Veiculo.maxlength, id: Veiculo.id}, event: { blur: Veiculo.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : Veiculo.enable, attr: { id: Veiculo.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-4" data-bind="visible: PlotagemVeiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PlotagemVeiculo.text, attr: { for: PlotagemVeiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: PlotagemVeiculo.val, enable: PlotagemVeiculo.enable, attr: { maxlength: PlotagemVeiculo.maxlength, id: PlotagemVeiculo.id}, event: { blur: PlotagemVeiculo.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : PlotagemVeiculo.enable, attr: { id: PlotagemVeiculo.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-4" data-bind="visible: TipoCarroceria.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoCarroceria.text, attr: { for: TipoCarroceria.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: TipoCarroceria.val, enable: TipoCarroceria.enable, attr: { maxlength: TipoCarroceria.maxlength, id: TipoCarroceria.id}, event: { blur: TipoCarroceria.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : TipoCarroceria.enable, attr: { id: TipoCarroceria.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-2">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoVeiculo.text, attr: { for: SituacaoVeiculo.id }"></label>
                                    <select class="form-control" data-bind="options: SituacaoVeiculo.options, optionsText: 'text', optionsValue: 'value', value: SituacaoVeiculo.val, attr: { id : SituacaoVeiculo.id}, event: {change: SituacaoVeiculo.eventChange}"></select>
                                </div>
                            </div>

                            <div class="col-6 col-md-2">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EmViagem.text, attr: { for: EmViagem.id }"></label>
                                    <select class="form-control" data-bind="options: EmViagem.options, optionsText: 'text', optionsValue: 'value', value: EmViagem.val, attr: { id : EmViagem.id}, event: {change: EmViagem.eventChange}"></select>
                                </div>
                            </div>

                            <div class="col-6 col-md-2">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoGuarita.text, attr: { for: TipoGuarita.id }"></label>
                                    <select class="form-control" data-bind="options: TipoGuarita.options, optionsText: 'text', optionsValue: 'value', value: TipoGuarita.val, attr: { id : TipoGuarita.id}, event: {change: TipoGuarita.eventChange}"></select>
                                </div>
                            </div>

                            <div class="col-6 col-md-2">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EmManutencao.text, attr: { for: EmManutencao.id }"></label>
                                    <select class="form-control" data-bind="options: EmManutencao.options, optionsText: 'text', optionsValue: 'value', value: EmManutencao.val, attr: { id : EmManutencao.id}, event: {change: EmManutencao.eventChange}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-12 " data-bind="visible: SomenteMotoristaComOciosidade.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: SomenteMotoristaComOciosidade.val, valueUpdate: 'afterkeydown', attr: { name: SomenteMotoristaComOciosidade.id, id : SomenteMotoristaComOciosidade.id}">
                                        <label class="custom-control-label" data-bind="text: SomenteMotoristaComOciosidade.text, attr: { for: SomenteMotoristaComOciosidade.id }"></label>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex justify-content-end no-padding">

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel" id="knockoutProgramacaoLogistica">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-truck"></i>
                    Motoristas
                </h3>
            </div>
            <div class="panel-content">
                <div class="col-12 col-md-12">

                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: ProgramacaoLogistica.idGrid}" cellspacing="0"></table>

                    <div class="tab-content border p-3 mt-2">
                        <div class="row row-cols-2">
                            <div class="col">
                                <h3>Sumarizadores</h3>
                                <div class="row row-cols-2">
                                    <div class="col col-md-12">
                                        <table class="table table-clean table-sm align-self-end">
                                            <tbody>
                                                <!-- ko foreach: Status -->
                                                <tr>
                                                    <td>
                                                        <strong data-bind="text: $data.Descricao"></strong>
                                                    </td>
                                                    <td>
                                                        <span data-bind="text: $data.Quantidade"></span>
                                                    </td>
                                                </tr>
                                                <!-- /ko -->
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="col">
                                <h3>Manutenção por Segmento de Veículo</h3>
                                <div class="row row-cols-2">
                                    <div class="col col-md-12">
                                        <table class="table table-clean table-sm align-self-end">
                                            <tbody>
                                                <!-- ko foreach: StatusManutencao -->
                                                <tr>
                                                    <td>
                                                        <strong data-bind="text: $data.Descricao"></strong>
                                                    </td>
                                                    <td>
                                                        <span data-bind="text: $data.Quantidade"></span>
                                                    </td>
                                                </tr>
                                                <!-- /ko -->
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



<div class="modal fade" id="divModalProgramacaoLogistica" role="dialog" aria-hidden="true">
    <div id="knoutProgramacaoLogistica" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Programação do Motorista</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-6 col-md-12" data-bind="visible: Motorista.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, enable: Motorista.enable, attr: { maxlength: Motorista.maxlength, id: Motorista.id}, event: { blur: Motorista.eventChange }, valueUpdate: 'afterkeydown'" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : Motorista.enable, attr: { id: Motorista.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-6 col-md-12" data-bind="visible: ProgramacaoAlocacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ProgramacaoAlocacao.text, attr: { for: ProgramacaoAlocacao.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: ProgramacaoAlocacao.requiredClass, value: ProgramacaoAlocacao.val, enable: ProgramacaoAlocacao.enable, attr: { maxlength: ProgramacaoAlocacao.maxlength, id: ProgramacaoAlocacao.id}, event: { blur: ProgramacaoAlocacao.eventChange }, valueUpdate: 'afterkeydown'" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : ProgramacaoAlocacao.enable, attr: { id: ProgramacaoAlocacao.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-4 col-md-3">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CategoriaCNH.text, attr: { for: CategoriaCNH.id }"></label>
                            <input type="text" class="form-control" data-bind="enable: Motorista.enable, value: CategoriaCNH.val, attr: { maxlength: CategoriaCNH.maxlength, id : CategoriaCNH.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>

                    <div class="col-4 col-md-3">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: VencimentoCNH.text, attr: { for: VencimentoCNH.id }"></label>
                            <input type="text" class="form-control" data-bind="enable: Motorista.enable, value: VencimentoCNH.val, attr: { maxlength: VencimentoCNH.maxlength, id : VencimentoCNH.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>

                    <div class="col-4 col-md-3">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: VencimentoMoop.text, attr: { for: VencimentoMoop.id }"></label>
                            <input type="text" class="form-control" data-bind="enable: Motorista.enable, value: VencimentoMoop.val, attr: { maxlength: VencimentoMoop.maxlength, id : VencimentoMoop.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>

                    <div class="col-4 col-md-3">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataAdmissao.text, attr: { for: DataAdmissao.id }"></label>
                            <input type="text" class="form-control" data-bind="enable: Motorista.enable, value: DataAdmissao.val, attr: { maxlength: DataAdmissao.maxlength, id : DataAdmissao.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>

                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-success me-2" type="button" data-bind="click: Salvar.eventClick, attr: { id : Salvar.id}">
                       <span data-bind="text: Salvar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

