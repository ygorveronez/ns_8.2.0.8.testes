
<style>
    #knockoutProgramacaoMotorista.ocultarPaginacao .dt-toolbar-footer {
        display: none
    }
</style>
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Frota</li>
    <li class="breadcrumb-item active">Programação Motorista</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Porgramação de Motoristas
    </h1>
</div>

<div>
    <div class="panel" id="knockoutPesquisaProgramacaoMotorista">
        <div class="panel-container">

            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Motoristas
                </button>
                <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade" class="mt-3">
                    <form>
                        <div class="row">

                            <div class="col-6 col-md-3" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Motorista.val, enable: Motorista.enable, attr: { maxlength: Motorista.maxlength, id: Motorista.id}, event: { blur: Motorista.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : Motorista.enable, attr: { id: Motorista.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-3" data-bind="visible: ProgramacaoEspecialidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ProgramacaoEspecialidade.text, attr: { for: ProgramacaoEspecialidade.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: ProgramacaoEspecialidade.val, enable: ProgramacaoEspecialidade.enable, attr: { maxlength: ProgramacaoEspecialidade.maxlength, id: ProgramacaoEspecialidade.id}, event: { blur: ProgramacaoEspecialidade.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : ProgramacaoEspecialidade.enable, attr: { id: ProgramacaoEspecialidade.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-3" data-bind="visible: ProgramacaoSituacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ProgramacaoSituacao.text, attr: { for: ProgramacaoSituacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: ProgramacaoSituacao.val, enable: ProgramacaoSituacao.enable, attr: { maxlength: ProgramacaoSituacao.maxlength, id: ProgramacaoSituacao.id}, event: { blur: ProgramacaoSituacao.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : ProgramacaoSituacao.enable, attr: { id: ProgramacaoSituacao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-3" data-bind="visible: ProgramacaoAlocacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ProgramacaoAlocacao.text, attr: { for: ProgramacaoAlocacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: ProgramacaoAlocacao.val, enable: ProgramacaoAlocacao.enable, attr: { maxlength: ProgramacaoAlocacao.maxlength, id: ProgramacaoAlocacao.id}, event: { blur: ProgramacaoAlocacao.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : ProgramacaoAlocacao.enable, attr: { id: ProgramacaoAlocacao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex justify-content-end align-items-center">
                        <div class="col-12 col-md-3" data-bind="visible: HabilitarPainel.visible">
                            <div class="form-group">

                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: HabilitarPainel.val, valueUpdate: 'afterkeydown', attr: { name: HabilitarPainel.id, id : HabilitarPainel.id}, event: {change: HabilitarPainel.eventChange}">
                                    <label class="custom-control-label" data-bind="text: HabilitarPainel.text, attr: { for: HabilitarPainel.id }" style="margin-top: 27px;"></label>
                                </div>

                            </div>
                        </div>

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel" id="knockoutProgramacaoMotorista">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>Motoristas</h3>
            </div>
            <div class="panel-content">
                <div class="col col-xs-12 col-sm-12 col-md-12 col-lg-12">

                    <table width="100%" class="table table-bordered table-hover mb-3" data-bind="attr: { id: ProgramacaoMotorista.idGrid}" cellspacing="0"></table>

                    <div class="tab-content border p-3">
                        <div id="knockoutCarga" class="tab-pane fade active show">
                            <h3>Sumarizadores</h3>
                            <div class="row row-cols-1 row-cols-md-2">
                                <div class="col">
                                    <table class="table table-clean table-sm align-self-end">
                                        <tbody>
                                            <!-- ko foreach: Status -->
                                            <tr>
                                                <td>
                                                    <strong data-bind="text: $data.Descricao"></strong>
                                                </td>
                                                <td>
                                                    <span data-bind="text: $data.Quantidade"></span>
                                                </td>
                                            </tr>
                                            <!-- /ko -->
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



