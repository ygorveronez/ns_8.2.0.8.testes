
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Frota</li>
    <li class="breadcrumb-item active">Dimensão Pneu</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Dimensões do Pneu
    </h1>
</div>

<div id="knockoutPesquisaDimensaoPneu">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Dimensões
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-8" data-bind="visible: Aplicacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Aplicacao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Aplicacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Aplicacao.maxlength, id : Aplicacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                Cadastrar Dimensão do Pneu
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutDimensaoPneu">
                    <div class="col-12 col-md-2" data-bind="visible: Largura.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Largura.text"></label>
                            <input type="text" class="form-control" data-bind="css:Largura.requiredClass, value: Largura.val, attr: { maxlength: Largura.maxlength, id : Largura.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Perfil.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Perfil.text"></label>
                            <input type="text" class="form-control" data-bind="css:Perfil.requiredClass, value: Perfil.val, attr: { maxlength: Perfil.maxlength, id : Perfil.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" style="margin-top:40px;" data-bind="visible: Radial.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: Radial.val, attr: { maxlength: Radial.maxlength, name: Radial.id, id: Radial.id }">
                                <label class="custom-control-label" data-bind="text: Radial.text, attr: { for: Radial.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Aro.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Aro.text"></label>
                            <input type="text" class="form-control" data-bind="css:Aro.requiredClass, value: Aro.val, attr: { maxlength: Aro.maxlength, id : Aro.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Aplicacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Aplicacao.text"></label>
                            <input type="text" class="form-control" data-bind="enable:Aplicacao.enable, css:Aplicacao.requiredClass, value: Aplicacao.val, enable: Aplicacao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Aplicacao.maxlength, id : Aplicacao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end" id="knockoutCRUDDimensaoPneu">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/dimensaoPneu"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadDimensaoPneu();
    });

</script>