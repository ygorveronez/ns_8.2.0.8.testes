</div>

<div class="modal fade" id="divModalJustificativa" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content well-no-padding">
            <div class="modal-header">
                <h4 class="modal-title">Informar Justificativa</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutModalJustificativa">
                <div class="row">
                    <div class="col-12 col-md-12" data-bind="visible: JustificativaDeIndisponibilidadeDeFrota.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: JustificativaDeIndisponibilidadeDeFrota.text, attr: { for: JustificativaDeIndisponibilidadeDeFrota.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: JustificativaDeIndisponibilidadeDeFrota.requiredClass, value: JustificativaDeIndisponibilidadeDeFrota.val, valueUpdate: 'afterkeydown', attr: { maxlength: JustificativaDeIndisponibilidadeDeFrota.maxlength, id : JustificativaDeIndisponibilidadeDeFrota.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: JustificativaDeIndisponibilidadeDeFrota.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end mt-3">
                    <button type="button" data-bind="click: Confirmar.eventClick, attr: { id : Confirmar.id}" class="btn btn-success">
                        <span data-bind="text: Confirmar.text"></span>
                    </button>
                </footer>
            </div>
        </div>
    </div>
</div>


