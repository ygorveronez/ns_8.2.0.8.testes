
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Frota</li>
    <li class="breadcrumb-item active">Contorle Tacógrafo</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Controle de Tacógrafos
    </h1>
</div>

<div id="knockoutPesquisaControleTacografo">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Controle de Tacógrafos
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: Codigo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Codigo.text, attr: { for: Codigo.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Codigo.requiredClass, value: Codigo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Codigo.maxlength, id : Codigo.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataRecebimentoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataRecebimentoInicial.text, attr: { for: DataRecebimentoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataRecebimentoInicial.requiredClass,  value: DataRecebimentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRecebimentoInicial.maxlength, id : DataRecebimentoInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataRecebimentoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataRecebimentoFinal.text, attr: { for: DataRecebimentoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataRecebimentoFinal.requiredClass, value: DataRecebimentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRecebimentoFinal.maxlength, id : DataRecebimentoFinal.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Excesso.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Excesso.text, attr: { for: Excesso.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Excesso.val, valueUpdate: 'afterkeydown', attr: { maxlength: Excesso.maxlength, id : Excesso.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>

                            <div class="col-6 col-md-6" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="class: Veiculo.requiredClass, value: Veiculo.val, enable: Veiculo.enable, attr: { maxlength: Veiculo.maxlength, id: Veiculo.id}, event: { blur: Veiculo.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : Veiculo.enable, attr: { id: Veiculo.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 col-md-6" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="class: Motorista.requiredClass, value: Motorista.val, enable: Motorista.enable, attr: { maxlength: Motorista.maxlength, id: Motorista.id}, event: { blur: Motorista.eventChange }, valueUpdate: 'afterkeydown'" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : Motorista.enable, attr: { id: Motorista.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

