
<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Localidades</li>
        <li class="breadcrumb-item active">País</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            País
        </h1>
    </div>
    <div id="knockoutPesquisaPais">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-content">
                    <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i>
                        Pesquisar País
                    </button>
                    <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-12" data-bind="visible: Descricao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa ms-3" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                        <div class="widget-body">
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-edit"></i>
                   Editar Cadastro País
                </h3>
            </div>
            <div class="panel-content">
                <form id="knockoutCadastroPais">
                    <div class="row">
                        <div class="col-12 col-md-2" data-bind="visible: Codigo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Codigo.text, attr: { for: Codigo.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: Codigo.enable, class: Codigo.requiredClass, value: Codigo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Codigo.maxlength, id : Codigo.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-8" data-bind="visible: Nome.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Nome.text, attr: { for: Nome.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: Nome.enable, class: Nome.requiredClass, value: Nome.val, valueUpdate: 'afterkeydown', attr: { maxlength: Nome.maxlength, id : Nome.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: Sigla.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Sigla.text, attr: { for: Sigla.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: Sigla.enable, class: Sigla.requiredClass, value: Sigla.val, valueUpdate: 'afterkeydown', attr: { maxlength: Sigla.maxlength, id : Sigla.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: Abreviacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Abreviacao.text, attr: { for: Abreviacao.id }"></label>
                                <input type="text" class="form-control" data-bind="class: Abreviacao.requiredClass, value: Abreviacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Abreviacao.maxlength, id : Abreviacao.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: CodigoTelefonico.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoTelefonico.text, attr: { for: CodigoTelefonico.id }"></label>
                                <input type="text" class="form-control" data-bind="class: CodigoTelefonico.requiredClass, value: CodigoTelefonico.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoTelefonico.maxlength, id : CodigoTelefonico.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: LicencaTNTI.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: LicencaTNTI.text, attr: { for: LicencaTNTI.id }"></label>
                                <input type="text" class="form-control" data-bind="class: LicencaTNTI.requiredClass, value: LicencaTNTI.val, valueUpdate: 'afterkeydown', attr: { maxlength: LicencaTNTI.maxlength, id : LicencaTNTI.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: VencimentoLicencaTNTI.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: VencimentoLicencaTNTI.text, attr: { for: VencimentoLicencaTNTI.id }"></label>
                                <input type="text" class="form-control" data-bind="class: VencimentoLicencaTNTI.requiredClass, value: VencimentoLicencaTNTI.val, valueUpdate: 'afterkeydown', attr: { maxlength: VencimentoLicencaTNTI.maxlength, id : VencimentoLicencaTNTI.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: CodigoPais.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoPais.text"></label>
                                <input type="text" class="form-control" data-bind="class: CodigoPais.requiredClass, value: CodigoPais.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoPais.maxlength, id : CodigoPais.id}" />
                            </div>
                        </div>
                    </div>
                    <footer>
                        <div class="row mt-3">
                            <div class="col-6">
                                <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible,  attr: { id : Cancelar.id}">
                                    <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"> </span>
                                </button>
                            </div>
                            <div class="col-6 d-flex justify-content-end">
                                <button type="button" class="btn btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible,  attr: { id : Atualizar.id}">
                                    <i class="fal fa-check"></i><span data-bind="text: Atualizar.text"></span>
                                </button>
                            </div>
                        </div>
                    </footer>
                </form>
            </div>
        </div>
    </div>
</section>


<script src="~/scripts/pais"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadPais();
    });

</script>

