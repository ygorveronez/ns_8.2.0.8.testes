
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pedidos</li>
    <li class="breadcrumb-item active">Tipo Terminal Importação</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Terminais de Importação
    </h1>
</div>

<div id="knockoutPesquisaTipoTerminalImportacao">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Terminais de Importação
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-5" data-bind="visible: Porto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Porto.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Porto.val, valueUpdate: 'afterkeydown', attr: { maxlength: Porto.maxlength, id : Porto.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Terminal de Importação
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutCadastroTipoTerminalImportacao">
                <div class="row">
                    <div class="col-12 col-md-2" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoMercante.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoMercante.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoMercante.requiredClass,value: CodigoMercante.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoMercante.maxlength, id : CodigoMercante.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoDocumento.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoDocumento.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoDocumento.requiredClass,value: CodigoDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoDocumento.maxlength, id : CodigoDocumento.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoTerminal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoTerminal.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoTerminal.requiredClass,value: CodigoTerminal.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoTerminal.maxlength, id : CodigoTerminal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoIntegracao.requiredClass,value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Ativo.text"></label>
                            <select class="form-control" data-bind="css: Ativo.requiredClass,options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: QuantidadeDiasEnvioDocumentacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeDiasEnvioDocumentacao.text"></label>
                            <input type="text" class="form-control" data-bind="css: QuantidadeDiasEnvioDocumentacao.requiredClass,value: QuantidadeDiasEnvioDocumentacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeDiasEnvioDocumentacao.maxlength, id : QuantidadeDiasEnvioDocumentacao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Porto.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Porto.text, attr: { for: Porto.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Porto.requiredClass, value: Porto.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Porto.text, maxlength: Porto.maxlength, id: Porto.id, name: Porto.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Porto.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Terminal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Terminal.text, attr: { for: Terminal.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Terminal.requiredClass, value: Terminal.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Terminal.text, maxlength: Terminal.maxlength, id: Terminal.id, name: Terminal.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Terminal.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Empresa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Empresa.text, maxlength: Empresa.maxlength, id: Empresa.id, name: Empresa.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Empresa.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoObservacaoContribuinte.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoObservacaoContribuinte.text"></label>
                            <input type="text" class="form-control" data-bind="value: CodigoObservacaoContribuinte.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoObservacaoContribuinte.maxlength, id : CodigoObservacaoContribuinte.id}" />
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/tipoTerminalImportacao"></script>
<script type="text/javascript">


    pageSetUp();

    $(document).ready(function () {
        loadTipoTerminalImportacao();
    });

</script>

