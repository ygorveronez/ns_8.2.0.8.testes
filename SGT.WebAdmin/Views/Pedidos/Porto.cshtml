
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pedidos</li>
    <li class="breadcrumb-item active">Porto</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Porto
    </h1>
</div>

<div id="knockoutPesquisaPorto">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Porto
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-10" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Porto
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutCadastroPorto">
                <div class="row">
                    <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: CodigoIATA.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoIATA.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoIATA.requiredClass, value: CodigoIATA.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIATA.maxlength, id : CodigoIATA.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoMercante.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoMercante.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoMercante.requiredClass, value: CodigoMercante.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoMercante.maxlength, id : CodigoMercante.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Ativo.text"></label>
                            <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoDocumento.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoDocumento.text"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoDocumento.requiredClass, value: CodigoDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoDocumento.maxlength, id : CodigoDocumento.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Localidade.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Localidade.text, attr: { for: Localidade.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Localidade.requiredClass, value: Localidade.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Localidade.text, maxlength: Localidade.maxlength, id: Localidade.id, name: Localidade.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Localidade.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Empresa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Empresa.text, maxlength: Empresa.maxlength, id: Empresa.id, name: Empresa.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Empresa.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: FormaEmissaoSVM.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: FormaEmissaoSVM.text"></label>
                            <select class="form-control" data-bind="options: FormaEmissaoSVM.options, optionsText: 'text', optionsValue: 'value', value: FormaEmissaoSVM.val, attr: { id : FormaEmissaoSVM.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: QuantidadeHorasFaturamentoAutomatico.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeHorasFaturamentoAutomatico.text"></label>
                            <input type="text" class="form-control" data-bind="value: QuantidadeHorasFaturamentoAutomatico.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeHorasFaturamentoAutomatico.maxlength, id : QuantidadeHorasFaturamentoAutomatico.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: DiasAntesDoPodParaEnvioDaDocumentacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DiasAntesDoPodParaEnvioDaDocumentacao.text"></label>
                            <div class="input-group mb-3">
                                <div class="input-group-text">
                                    <input class="form-check-input mt-0" type="checkbox" data-bind="checked: AtivarNulo.val, attr: {  name: AtivarNulo.id, id: AtivarNulo.id }">
                                </div>
                                <input type="text" class="form-control" data-bind="value: DiasAntesDoPodParaEnvioDaDocumentacao.val, enable: DiasAntesDoPodParaEnvioDaDocumentacao.enable, attr: { maxlength: DiasAntesDoPodParaEnvioDaDocumentacao.maxlength, id : DiasAntesDoPodParaEnvioDaDocumentacao.id} " />
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: RKST.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: RKST.text"></label>
                            <input type="text" class="form-control" data-bind="value: RKST.val, valueUpdate: 'afterkeydown', attr: { maxlength: RKST.maxlength, id : RKST.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: AtivarDespachanteComoConsignatario.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: AtivarDespachanteComoConsignatario.val, attr: { maxlength: AtivarDespachanteComoConsignatario.maxlength, name: AtivarDespachanteComoConsignatario.id, id: AtivarDespachanteComoConsignatario.id }">
                                <label class="custom-control-label" data-bind="text: AtivarDespachanteComoConsignatario.text, attr: { for: AtivarDespachanteComoConsignatario.id }"></label>
                            </div>
                        </div>
                        <div class="col-12 col-md-12" data-bind="visible: DividirCargasAcordoComQuantidadeContainerRecebidoPortoDestino.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: DividirCargasAcordoComQuantidadeContainerRecebidoPortoDestino.val, attr: { maxlength: DividirCargasAcordoComQuantidadeContainerRecebidoPortoDestino.maxlength, name: DividirCargasAcordoComQuantidadeContainerRecebidoPortoDestino.id, id: DividirCargasAcordoComQuantidadeContainerRecebidoPortoDestino.id }">
                                    <label class="custom-control-label" data-bind="text: DividirCargasAcordoComQuantidadeContainerRecebidoPortoDestino.text, attr: { for: DividirCargasAcordoComQuantidadeContainerRecebidoPortoDestino.id }"></label>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-12" data-bind="visible: DividirCargasAcordoComQuantidadeContainerRecebidoPortoOrigem.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: DividirCargasAcordoComQuantidadeContainerRecebidoPortoOrigem.val, attr: { maxlength: DividirCargasAcordoComQuantidadeContainerRecebidoPortoOrigem.maxlength, name: DividirCargasAcordoComQuantidadeContainerRecebidoPortoOrigem.id, id: DividirCargasAcordoComQuantidadeContainerRecebidoPortoOrigem.id }">
                                    <label class="custom-control-label" data-bind="text: DividirCargasAcordoComQuantidadeContainerRecebidoPortoOrigem.text, attr: { for: DividirCargasAcordoComQuantidadeContainerRecebidoPortoOrigem.id }"></label>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-12" data-bind="visible: CriarSequenciaCargasMesmoComPedidoExistente.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: CriarSequenciaCargasMesmoComPedidoExistente.val, attr: { maxlength: CriarSequenciaCargasMesmoComPedidoExistente.maxlength, name: CriarSequenciaCargasMesmoComPedidoExistente.id, id: CriarSequenciaCargasMesmoComPedidoExistente.id }">
                                    <label class="custom-control-label" data-bind="text: CriarSequenciaCargasMesmoComPedidoExistente.text, attr: { for: CriarSequenciaCargasMesmoComPedidoExistente.id }"></label>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-12" data-bind="visible: GerarUmaCargaSVMPorCargaMTLQuandoPortoDestino.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: GerarUmaCargaSVMPorCargaMTLQuandoPortoDestino.val, attr: { maxlength: GerarUmaCargaSVMPorCargaMTLQuandoPortoDestino.maxlength, name: GerarUmaCargaSVMPorCargaMTLQuandoPortoDestino.id, id: GerarUmaCargaSVMPorCargaMTLQuandoPortoDestino.id }">
                                    <label class="custom-control-label" data-bind="text: GerarUmaCargaSVMPorCargaMTLQuandoPortoDestino.text, attr: { for: GerarUmaCargaSVMPorCargaMTLQuandoPortoDestino.id }"></label>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-12" data-bind="visible: GerarUmaCargaSVMPorCargaMTLQuandoPortoOrigem.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: GerarUmaCargaSVMPorCargaMTLQuandoPortoOrigem.val, attr: { maxlength: GerarUmaCargaSVMPorCargaMTLQuandoPortoOrigem.maxlength, name: GerarUmaCargaSVMPorCargaMTLQuandoPortoOrigem.id, id: GerarUmaCargaSVMPorCargaMTLQuandoPortoOrigem.id }">
                                    <label class="custom-control-label" data-bind="text: GerarUmaCargaSVMPorCargaMTLQuandoPortoOrigem.text, attr: { for: GerarUmaCargaSVMPorCargaMTLQuandoPortoOrigem.id }"></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

