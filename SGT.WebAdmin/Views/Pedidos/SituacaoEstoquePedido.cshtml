


<style>
    .dropdown-colorselector > .dropdown-menu {
        min-width: 120px;
        max-width: 120px;
    }

    .btn-colorselector {
        width: 30px !important;
        height: 30px !important;
        border-radius: 5px !important;
        border: 1px solid #BDBDBD !important;
    }

    .grid-color {
        width: 12px;
        height: 12px;
        border-radius: 5px;
        border: 1px solid #BDBDBD;
        display: inline-block;
    }
</style>

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pedidos</li>
    <li class="breadcrumb-item active">Situação Estoque do Pedido</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Situação Estoque do Pedido
    </h1>
</div>

<div id="knockoutPesquisaSituacaoEstoquePedido">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Situação Estoque do Pedido
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: BloqueiaPedido.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: BloqueiaPedido.text, attr: { for: BloqueiaPedido.id }"></label>
                                    <select class="form-control" data-bind="class: BloqueiaPedido.requiredClass, options: BloqueiaPedido.options, optionsText: 'text', optionsValue: 'value', value: BloqueiaPedido.val, attr: { maxlength: BloqueiaPedido.maxlength, id : BloqueiaPedido.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Situação Estoque do Pedido
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutSituacaoEstoquePedido">
                    <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: CodigoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoIntegracao.text, attr: { for: CodigoIntegracao.id }"></label>
                            <input type="text" class="form-control" data-bind="class: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                        </div>
                    </div>
                    <div id="lista-cor" data-bind="visible: Cor.visible" class="col-12 col-md-2 " style="margin-top:14px">
                        <label class="label"><b data-bind="text: Cor.text"></b></label>
                        <select width="80" data-bind="foreach: Cor.options, optionsText: 'text', optionsValue: 'value', attr: { id : Cor.id}">
                            <option data-bind="attr: {'data-color': value, value: value}"></option>
                        </select>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: BloqueiaPedido.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: BloqueiaPedido.val, attr: { maxlength: BloqueiaPedido.maxlength, name: BloqueiaPedido.id, id: BloqueiaPedido.id }">
                                <label class="custom-control-label" data-bind="text: BloqueiaPedido.text, attr: { for: BloqueiaPedido.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                            <textarea class="form-control" rows="4" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id }"></textarea>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDSituacaoEstoquePedido">
                    <div class="row">
                        <div class="d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-default ms-2" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-danger ms-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success ms-2" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success ms-2" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>

    </div>
</div>

<script src="~/scripts/situacaoEstoquePedido"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadSituacaoEstoquePedido();
    });

</script>
