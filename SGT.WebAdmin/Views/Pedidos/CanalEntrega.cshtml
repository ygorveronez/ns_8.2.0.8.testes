
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">@Localization.Resources.Pedidos.CanalEntrega.Pedidos</li>
    <li class="breadcrumb-item active">@Localization.Resources.Pedidos.CanalEntrega.CanalEntregas</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        @Localization.Resources.Pedidos.CanalEntrega.CanalEntregas
    </h1>
</div>

<div id="knockoutPesquisaCanalEntrega">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    @Localization.Resources.Pedidos.CanalEntrega.PesquisarCanaisEntrega
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4 col-lg-4" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Filial.text, maxlength: Filial.maxlength, id: Filial.id, name: Filial.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Filial.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text, attr: { for: CodigoIntegracao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex justify-content-end mb-3">
                        
                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>

                </div>

            </div>
        </div>
    </div>

</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                @Localization.Resources.Pedidos.CanalEntrega.CadastrarCanalEntrega
            </h3>
        </div>
        <div class="panel-content">
            <form novalidate id="knockoutCanalEntrega">
                <div class="row">
                    <div class="col-12 col-md-3" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, enable: Descricao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Filial.text, maxlength: Filial.maxlength, id: Filial.id, name: Filial.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Filial.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoIntegracao.text, attr:{ for: CodigoIntegracao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoIntegracao.requiredClass,value: CodigoIntegracao.val, enable: CodigoIntegracao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: NivelPrioridade.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NivelPrioridade.text, attr: { for: NivelPrioridade.id }"></label>
                            <input type="text" class="form-control" data-bind="css: NivelPrioridade.requiredClass,value: NivelPrioridade.val, enable: NivelPrioridade.enable, valueUpdate: 'afterkeydown', attr: { maxlength: NivelPrioridade.maxlength, id : NivelPrioridade.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                            <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: Principal.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: Principal.val, attr: { maxlength: Principal.maxlength, name: Principal.id, id: Principal.id }">
                                <label class="custom-control-label" data-bind="text: Principal.text, attr: { for: Principal.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: GerarCargaAutomaticamente.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: GerarCargaAutomaticamente.val, attr: { maxlength: GerarCargaAutomaticamente.maxlength, name: GerarCargaAutomaticamente.id, id: GerarCargaAutomaticamente.id }">
                                <label class="custom-control-label" data-bind="text: GerarCargaAutomaticamente.text, attr: { for: GerarCargaAutomaticamente.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: NaoUtilizarCapacidadeVeiculoMontagemCarga.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: NaoUtilizarCapacidadeVeiculoMontagemCarga.val, attr: { maxlength: NaoUtilizarCapacidadeVeiculoMontagemCarga.maxlength, name: NaoUtilizarCapacidadeVeiculoMontagemCarga.id, id: NaoUtilizarCapacidadeVeiculoMontagemCarga.id }">
                                <label class="custom-control-label" data-bind="text: NaoUtilizarCapacidadeVeiculoMontagemCarga.text, attr: { for: NaoUtilizarCapacidadeVeiculoMontagemCarga.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: LiberarPedidoSemNFeAutomaticamente.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: LiberarPedidoSemNFeAutomaticamente.val, attr: { maxlength: LiberarPedidoSemNFeAutomaticamente.maxlength, name: LiberarPedidoSemNFeAutomaticamente.id, id: LiberarPedidoSemNFeAutomaticamente.id }">
                                <label class="custom-control-label" data-bind="text: LiberarPedidoSemNFeAutomaticamente.text, attr: { for: LiberarPedidoSemNFeAutomaticamente.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Circuito.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: Circuito.val, attr: { maxlength: Circuito.maxlength, name: Circuito.id, id: Circuito.id }">
                                <label class="custom-control-label" data-bind="text: Circuito.text, attr: { for: Circuito.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-7" data-bind="visible: CanalEntregaPrincipal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CanalEntregaPrincipal.text, attr: { for: CanalEntregaPrincipal.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="enable: CanalEntregaPrincipal.enable, css: CanalEntregaPrincipal.requiredClass, value: CanalEntregaPrincipal.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': CanalEntregaPrincipal.text, maxlength: CanalEntregaPrincipal.maxlength, id: CanalEntregaPrincipal.id, name: CanalEntregaPrincipal.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CanalEntregaPrincipal.idBtnSearch}, enable: CanalEntregaPrincipal.enable()">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: QuantidadePedidosPermitidosNoCanal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadePedidosPermitidosNoCanal.text, attr: { for: QuantidadePedidosPermitidosNoCanal.id }"></label>
                            <input type="text" class="form-control" data-bind="class: QuantidadePedidosPermitidosNoCanal.requiredClass, value: QuantidadePedidosPermitidosNoCanal.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadePedidosPermitidosNoCanal.maxlength, id : QuantidadePedidosPermitidosNoCanal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                            <textarea class="form-control" rows="4" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id }"></textarea>
                        </div>
                    </div>
                </div>

                <footer>
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>


<script src="~/scripts/canalEntrega"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("Pedidos.CanalEntrega").then(loadCanalEntrega);
    });

</script>

