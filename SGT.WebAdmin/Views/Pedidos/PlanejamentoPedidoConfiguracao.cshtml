
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pedidos</li>
    <li class="breadcrumb-item active">Configurações Planejamento Pedido</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Configurações Planejamento Pedido
    </h1>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Configurações
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutPlanejamentoPedidoConfiguracao">
                    <div class="col-12 col-md-12" data-bind="visible: Email.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Email.text,"></label>
                            <textarea type="text" rows="5" class="form-control" data-bind="css: Email.requiredClass, value: Email.val, valueUpdate: 'afterkeydown', attr: { maxlength: Email.maxlength, id : Email.id}"></textarea>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDPlanejamentoPedidoConfiguracao">
                    <div class="row">
                        <div class="col-12 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/PlanejamentoPedidoConfiguracao"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadPlanejamentoPedidoConfiguracao();
    });

</script>