
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pedidos</li>
    <li class="breadcrumb-item active">Navios</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Navios
    </h1>
</div>

<div id="knockoutPesquisaNavio">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Navios
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="panel mt-3" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs active show">
                <li class="nav-item active" id="liTabNavio">
                    <a href="#tabNavio" class="nav-link active" data-bs-toggle="tab">
                        <i class="fal fa-edit"></i> <span class="hidden-mobile hidden-tablet">Cadastrar Navios</span>
                    </a>
                </li>
                <li class="nav-item" id="liTabNavioOperador">
                    <a href="#tabNavioOperador" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-edit"></i> <span class="hidden-mobile hidden-tablet">Cadastro do Operador</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3">

                <!-- Cadastrar Navio-->

                <div class="tab-pane fade active show" id="tabNavio" role="tabpanel">
                    <form>
                        <div class="row" id="knockoutCadastroNavio">
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Irin.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Irin.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Irin.requiredClass, value: Irin.val, valueUpdate: 'afterkeydown', attr: { maxlength: Irin.maxlength, id : Irin.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoEmbarcacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoEmbarcacao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoEmbarcacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoEmbarcacao.maxlength, id : CodigoEmbarcacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: TipoEmbarcacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoEmbarcacao.text"></label>
                                    <select class="form-control" data-bind="valueAllowUnset: true, options: TipoEmbarcacao.options, optionsText: 'text', optionsValue: 'value', value: TipoEmbarcacao.val, attr: { id : TipoEmbarcacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CodigoDocumento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoDocumento.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoDocumento.maxlength, id : CodigoDocumento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CodigoIMO.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIMO.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoIMO.requiredClass, value: CodigoIMO.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIMO.maxlength, id : CodigoIMO.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CapacidadePlug.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CapacidadePlug.text, attr: { for: CapacidadePlug.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CapacidadePlug.requiredClass, value: CapacidadePlug.val, valueUpdate: 'afterkeydown', attr: { maxlength: CapacidadePlug.maxlength, id : CapacidadePlug.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CapacidadeTeus.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CapacidadeTeus.text, attr: { for: CapacidadeTeus.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CapacidadeTeus.requiredClass, value: CapacidadeTeus.val, valueUpdate: 'afterkeydown', attr: { maxlength: CapacidadeTeus.maxlength, id : CapacidadeTeus.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CapacidadeTons.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CapacidadeTons.text, attr: { for: CapacidadeTons.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CapacidadeTons.requiredClass, value: CapacidadeTons.val, valueUpdate: 'afterkeydown', attr: { maxlength: CapacidadeTons.maxlength, id : CapacidadeTons.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NavioID.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NavioID.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NavioID.requiredClass, value: NavioID.val, valueUpdate: 'afterkeydown', attr: { maxlength: NavioID.maxlength, id : NavioID.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoOperador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoOperador.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoOperador.requiredClass, value: CodigoOperador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoOperador.maxlength, id : CodigoOperador.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoNavio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoNavio.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoNavio.requiredClass, value: CodigoNavio.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoNavio.maxlength, id : CodigoNavio.id}" />
                                </div>
                            </div>
                        </div>
                     </form>
                </div>

                <!-- Navio Operador -->

                <div class="tab-pane fade" id="tabNavioOperador" role="tabpanel" data-bind="visible: Status.visible">
                    <form novalidate id="knockoutNavioOperador">
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Operadores.id}" cellspacing="0"></table>
                    </form>
                </div>
                <br />
                <footer id="knockoutCRUDNavio" class="d-flex justify-content-end ms-1">
                    <button type="button" class="btn btn-labeled btn-default " data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-danger ms-1 " data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </div>
        </div>
    </div>
</div>





