                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3">

                <!-- Cadastro do Tipo de Container -->

                <div class="tab-pane fade active show" id="tabTipoContainer" role="tabpanel">
                    <form novalidate id="knockoutCadastroTipoContainer">
                        <div class="row">
