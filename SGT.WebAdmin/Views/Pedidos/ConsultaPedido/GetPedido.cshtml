

<!DOCTYPE html>
<html lang="pt-br" id="extr-page">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>@Html.Raw(ViewBag.Titulo)</title>

	<meta name="description" content="">
	<meta name="author" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

	<link rel="stylesheet" href="~/css/bootstrap" />
	<link rel="stylesheet" href="~/css/fontAwesome" />
	<link rel="stylesheet" href="~/css/plugins" />
	<link rel="stylesheet" href="~/css/fullCalendar" />
	<link rel="stylesheet" href="~/css/production" />
	<link rel="stylesheet" href="~/css/skins" />
	<link rel="stylesheet" href="~/css/bootstrap-datetimepicker" />
	<link rel="stylesheet" href="~/css/bootstrapSelect" />
	<link rel="stylesheet" href="~/css/bootstrapToggle" />
	<link rel="stylesheet" href="~/css/dataTablesCheckboxes" />
	<link rel="stylesheet" href="~/css/bootstrap-colorselector" />
	<link rel="stylesheet" href="~/css/googleMaps" />
	<link rel="stylesheet" href="~/css/yourStyle" />
	<link rel="stylesheet" href="~/css/retiradaProduto" />

	<script src="~/scripts/jquery"></script>
	<script src="~/scripts/knockout"></script>
	<script src="~/scripts/blockUI"></script>
	<script src="~/scripts/enumeradores"></script>
	<script src="~/scripts/consultas"></script>
	<script src="~/scripts/fileDownload"></script>
	<script src="~/scripts/maskedInput"></script>
	<script src="~/scripts/maskMoney"></script>
	<script src="~/scripts/promise"></script>
	<script src="~/scripts/globalize"></script>
	<script src="~/scripts/localization"></script>
	<script src="~/scripts/global"></script>
	<script src="~/scripts/twbsPagination"></script>
	<script src="~/scripts/smartNotification"></script>
	<script src="~/scripts/dataTables"></script>
	<script src="~/scripts/moment"></script>
	<script src="~/scripts/datetimepicker"></script>
	<script src="~/scripts/bootstrap"></script>
	<script src="~/scripts/bootstrapSelect"></script>
	<script src="~/scripts/signalR"></script>
	<script src="~/scripts/signalRGlobal"></script>
	<script src="~/scripts/consultaPedido"></script>

	<style type="text/css">

		.smart-form header {
			border-bottom: none !important;
			padding: 0px !important;
			font-size: 14px;
		}

		#content {
			margin-top: 60px;
			min-height: 950px
		}

		header.header {
			padding: 30px 0;
			-webkit-box-shadow: 0 3px 15px 3px #f1f1f1;
			box-shadow: 0 3px 15px 3px #f1f1f1;
			border-bottom: 1px solid #d9dde9;
			background-color: #fefeff;
			position: fixed;
			top: 0;
			left: 0;
			width: 100%;
			z-index: 810;
		}

			header.header .header-logo {
				max-height: 70px;
			}

				header.header .header-logo img {
					height: 100%;
				}

			header.header .search-form form {
				display: block;
				text-align: center;
				line-height: 50px;
			}

			header.header .search-form .search-input {
				margin-left: 10px;
				margin-right: 10px;
			}

			header.header .header-actions {
				text-align: right;
				border-left: 1px solid #d9dde9;
				line-height: 50px;
				font-size: 25px;
			}

				header.header .header-actions .dropdown-toggle::after {
					display: none;
				}

				header.header .header-actions.open .dropdown-menu {
					display: block;
					right: calc(-25%);
					left: auto;
				}

				header.header .header-actions i {
					margin: auto 5px;
					color: #475999;
				}

				header.header .header-actions a:hover {
					text-decoration: none;
				}


		.well {
			border: 2px solid #eff1f6;
			border-radius: 10px;
			-webkit-box-shadow: 0 0 25px 10px #eaecf7;
			box-shadow: 0 0 25px 10px #eaecf7;
			margin-bottom: 20px;
		}

		.well-dark {
			background-color: #f1f2f9;
		}

		.well .well-title .well-title-icon {
			float: left;
		}

			.well .well-title .well-title-icon::after {
				content: '';
				display: block;
				clear: both;
			}

		.etapa {
			display: inline-block;
			width: 13.5%;
			height: 250px;
			align-items: center;
		}

			.etapa .descricaoEtapa {
				width: 100%;
				height: 40%;
				position: relative;
			}

				.etapa .descricaoEtapa div {
					width: 80%;
					height: 100%;
					position: relative;
					margin: 0 10%;
				}

				.etapa .descricaoEtapa span {
					position: absolute;
					bottom: 10px;
				}

			.etapa .iconeEtapa {
				height: 40%;
			}

			.etapa .line {
				position: relative;
				bottom: 45px;
				left: 50%;
				border: 2px solid #eef617;
				width: 100%;
			}

			.etapa .dataHoraEtapa {
				height: 20%;
			}

			.etapa div span {
				font-size: 16px;
				text-align: center;
				width: 100%;
			}

			.etapa .dataHoraEtapa span {
				display: block;
			}

		.icone {
			background: #FFFFFF;
			width: 80px;
			height: 80px;
			margin: 0 auto;
			border: 2px solid #eef617;
			border-radius: 100%;
			position: relative;
			z-index: 999;
		}

			.icone img {
				width: 50px;
				height: 50px;
				position: absolute;
				top: 50%;
				left: 50%;
				margin: -25px 0 0 -25px;
			}

		.iconeEtapa .icone .iconeCheck {
			position: relative;
			font-size: 28px;
			left: 0px;
			top: 55px;
			z-index: 1000;
			color: #85cc00;
		}

		.botaoRemessa {
			margin-top: 20px;
			padding: 20px;
			border: 1px solid #efefef;
			background-color: #efefef;
		}

			.botaoRemessa i {
				display: block;
			}

		.divRemessas {
			position: relative;
			width: 60%;
			left: 20%;
			background-color: #efefef;
		}

		.remessa {
			padding: 30px 0;
		}

			.remessa h2 {
				text-align: center;
			}

		button.botaoDetalheRemessa {
			margin: 20px auto 0px auto;
			padding: 10px 20px;
			border: 1px solid #d9d9d9;
			background-color: #d9d9d9;
			display: flex;
			flex-direction: column;
			align-items: center;
		}

			button.botaoDetalheRemessa i {
				display: block;
			}

		.containerDetalheRemessa {
			width: 100%;
			background-color: #d9d9d9;
			padding-bottom: 20px;
		}

		.cabecalhoDetalheCarga {
			width: 60%;
			margin: 0 auto;
			padding-top: 20px;
		}

		section .tableDetalheRemessa {
			width: 80%;
			margin: 0 auto;
		}

		.tableDetalheRemessa .table thead th {
			background-color: #838383 !important;
			color: #ffffff !important;
			border-color: #626262;
		}

		.tableDetalheRemessa .dt-toolbar-footer {
			background: #d9d9d9;
		}

		.tableDetalheRemessa .paginate_button a {
			background-color: #c7c7c7;
		}

		.botaoRetorno {
			margin: 20px;
			text-align: center;
		}

			.botaoRetorno button {
				padding: 10px 50px;
			}
	</style>

	<script src="~/ViewsScripts/Configuracao/Sistema/ConfiguracaoTMS.js"></script>
	<link rel="shortcut icon" href="@Html.Raw(ViewBag.Favicon)" type="image/x-icon">
	<link rel="icon" href="@Html.Raw(ViewBag.Favicon)" type="image/x-icon">

	<script src="@Html.Raw(ViewBag.HTTPConnection)://maps.googleapis.com/maps/api/js?key=@Html.Raw(ViewBag.APIKeyGoogle)&libraries=places,geometry,drawing"></script>

	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar-style" content="black">
</head>
<body>
	<header class="header" style="padding:5px">
		<div class="container" style="margin-left:10px !important;">
			<div class="row">
				<div class="col-lg-3 header-logo">
					<img src="~/img/logo-cliente.png" class="logo" style="height:50px" />
				</div>
			</div>
		</div>
	</header>
	<main role="main" style="padding:0px !important">
		<div id="content">
			<div class="row">
				<div class="col-12 col col-md-12">
					<div class="smart-form">
						<div>
							<div id="knockoutConsultaPedido">
								<div class="smart-form" id="divConsultarPedido" style="display:none">
									<section>
										<div class="well">
											<header>Monitorar Pedido</header>
											<fieldset>
												<div class="row">
													<div class="col-12 col-md-6" data-bind="visible: NumeroPedido.visible">
														<div class="form-group">
															<label class="form-label" data-bind="text: NumeroPedido.text, attr: { for: NumeroPedido.id }"></label>
															<input type="text" class="form-control" data-bind="class: NumeroPedido.requiredClass, value: NumeroPedido.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroPedido.maxlength, id : NumeroPedido.id}" />
														</div>
													</div>

													<div data-bind="visible: BuscarDadosPedido.visible" class="col-12 col col-md-6">
														<div class="input-group-btn">
															<button data-bind="click: BuscarDadosPedido.eventClick, attr: { id: BuscarDadosPedido.idBtnSearch}" class="btn btn-primary  " style="margin-top:18px" type="button">
																<i class="fal fa-search"></i> Consultar
															</button>

														</div>
													</div>
												</div>
											</fieldset>
										</div>
									</section>
								</div>
								<div id="divloginInvalido" style="display:none ">
									<div class="col col-sm-12">
										<div class="well">
											<header>Acesso Inválido</header>
											<div style="padding-top:20px">
												<div class="row">
													<div class="col col-12">
														<div class="alert alert-warning">
															<span id="spanMsg"></span>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
							<div id="divDadosPedido" style="display:none "></div>

						</div>
					</div>
				</div>
			</div>
		</div>
	</main>

	<script type="text/javascript">

		var _CONFIGURACAO_TMS = @Html.Raw(ViewBag.ConfiguracaoPadrao);
		var _FormularioSomenteLeitura = false;

		$(document).ready(function () {

			var codUserSap = "@ViewBag.codUserSap";
			var numOv = "@ViewBag.numOv";

			loadGetPedidoSap(codUserSap, numOv);

		});

	</script>
</body>
</html>


