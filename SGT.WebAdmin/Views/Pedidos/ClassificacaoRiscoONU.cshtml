
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pedidos</li>
    <li class="breadcrumb-item active">Classificação Risco ONU</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Classificação de Risco ONU
    </h1>
</div>

<div id="knockoutPesquisaClassificacaoRiscoONU">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Classificação de Risco ONU
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: NumeroONU.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroONU.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroONU.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroONU.maxlength, id : NumeroONU.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind=" visible : Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Classificação de Risco ONU
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutCadastroClassificacaoRiscoONU">
                <div class="row">
                    <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: NumeroONU.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroONU.text, attr: { for: NumeroONU.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="class: Descricao.requiredClass, value: NumeroONU.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': NumeroONU.text, maxlength: NumeroONU.maxlength, id: NumeroONU.id, name: NumeroONU.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: NumeroONU.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind=" visible : Ativo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Ativo.text"></label>
                            <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: ClasseRisco.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ClasseRisco.text"></label>
