
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pedidos</li>
    <li class="breadcrumb-item active">Container</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Containers
    </h1>
</div>

<div id="knockoutPesquisaContainer">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Containers
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Numero.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Numero.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Numero.val, valueUpdate: 'afterkeydown', attr: { maxlength: Numero.maxlength, id : Numero.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Container
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroContainer">
                    <div class="col-12 col-md-7" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text"></label>
                            <input type="text" class="form-control" data-bind="value: Descricao.val, class: Descricao.requiredClass ,valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}, enable: Descricao.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: CodigoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                            <input type="text" class="form-control" data-bind="value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}, enable: CodigoIntegracao.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}, enable: Status.enable"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Numero.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Numero.text"></label>
                            <input type="text" class="form-control" data-bind="value: Numero.val, class: Numero.requiredClass, valueUpdate: 'afterkeydown', attr: { maxlength: Numero.maxlength, id : Numero.id}, enable: Numero.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: PesoLiquido.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PesoLiquido.text"></label>
                            <input type="text" class="form-control" data-bind="value: PesoLiquido.val, valueUpdate: 'afterkeydown', attr: { maxlength: PesoLiquido.maxlength, id : PesoLiquido.id}, enable: PesoLiquido.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Tara.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Tara.text"></label>
                            <input type="text" class="form-control" data-bind="value: Tara.val ,  class: Numero.requiredClass,valueUpdate: 'afterkeydown', attr: { maxlength: Tara.maxlength, id : Tara.id}, enable: Tara.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Valor.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Valor.text"></label>
                            <input type="text" class="form-control" data-bind="value: Valor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Valor.maxlength, id : Valor.id}, enable: Valor.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: TipoContainer.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoContainer.text, attr: { for: TipoContainer.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: TipoContainer.requiredClass, value: TipoContainer.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoContainer.text, maxlength: TipoContainer.maxlength, id: TipoContainer.id, name: TipoContainer.id }, enable: TipoContainer.enable" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoContainer.idBtnSearch}, enable: TipoContainer.enable">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: MetrosCubicos.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: MetrosCubicos.text"></label>
                            <input type="text" class="form-control" data-bind="value: MetrosCubicos.val, valueUpdate: 'afterkeydown', attr: { maxlength: MetrosCubicos.maxlength, id : MetrosCubicos.id}, enable: MetrosCubicos.enable" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: TipoPropriedade.visible" >
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoPropriedade.text"></label>
                            <select class="form-control" data-bind="valueAllowUnset: true, options: TipoPropriedade.options, optionsText: 'text', optionsValue: 'value', value: TipoPropriedade.val, attr: { id : TipoPropriedade.id}, enable: TipoPropriedade.enable"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: TipoCarregamentoNavio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoCarregamentoNavio.text, attr: { for: TipoCarregamentoNavio.id }"></label>
                            <select class="form-control" data-bind="class: TipoCarregamentoNavio.requiredClass, options: TipoCarregamentoNavio.options, optionsText: 'text', optionsValue: 'value', value: TipoCarregamentoNavio.val, attr: { maxlength: TipoCarregamentoNavio.maxlength, id : TipoCarregamentoNavio.id}, enable: TipoCarregamentoNavio.enable"></select>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDContainer">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger me-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success"  data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>
      
<script src="~/scripts/container"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        _PermissoesPersonalizadas = @Html.Raw(ViewBag.PermissoesPersonalizadas);
        loadContainer();
    });

</script>

