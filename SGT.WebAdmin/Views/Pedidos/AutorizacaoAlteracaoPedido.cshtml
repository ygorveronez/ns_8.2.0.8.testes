                        <div class="row">
                            <div class="col-12 col-md-6">
                                <div class="row">
                                    <label>
                                        <strong data-bind="text: NumeroPedidoEmbarcador.text"></strong>
                                        <span data-bind="text: NumeroPedidoEmbarcador.val"></span>
                                    </label>
                                    <label>
                                        <strong data-bind="text: Filial.text"></strong>
                                        <span data-bind="text: Filial.val"></span>
                                    </label>

                                </div>
                            </div>
                            <div class="col-12 col-md-6">
                                <div class="row">
                                    <label>
                                        <strong data-bind="text: TipoCarga.text"></strong>
                                        <span data-bind="text: TipoCarga.val"></span>
                                    </label>
                                    <label>
                                        <strong data-bind="text: TipoOperacao.text"></strong>
                                        <span data-bind="text: TipoOperacao.val"></span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="knockoutCamposAlterados" class="tab-pane fade">
                        <h3>Detalhes das Alterações</h3>
                        <div class="row">
                            <table width="100%" class="table table-bordered table-hover" id="grid-pedido-campos-alterados"></table>
                        </div>
                    </div>
                    <div id="knockoutCamposAlterados" class="tab-pane fade">
                        <h3>Detalhes das Alterações</h3>
                        <div class="row">
                            <div id="knockoutCamposAlterados" class="tab-pane fade">
                                <h3>Detalhes das Alterações</h3>
                                <div class="row">
                                    <div>
                                        <table width="100%" class="table table-bordered table-hover" id="grid-pedido-campos-alterados"></table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="knockoutAutorizacao" class="tab-pane fade">
                        <h3>Regras</h3>
                        <div class="row">
                            <div class="col-12" data-bind="visible: Motivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                                    <textarea class="form-control" rows="5" type="text" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}"></textarea>
                                </div>
                            </div>
                            <div class="col-12" data-bind="visible: Rejeitar.visible">
                                <div class="d-flex justify-content-end mb-3">
                                    <button class="btn btn-danger ms-1" type="button" data-bind="click: Rejeitar.eventClick, attr: { id : Rejeitar.id}">
                                        <span data-bind="text: Rejeitar.text"></span>
                                    </button>
                                    <button class="btn btn-default ms-1" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                                        <span data-bind="text: Cancelar.text"></span>
                                    </button>
                                </div>
                            </div>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id : Regras.idGrid}"></table>
                        <div class="mt-3" data-bind="visible: AprovarTodas.visible">
                            <div class="d-flex justify-content-end">
                                <button class="btn btn-success" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id: AprovarTodas.id}">
                                    <span data-bind="text: AprovarTodas.text"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div id="knockoutAutorizacaoTransportador" class="tab-pane fade">
                        <h3>Transportadores</h3>
                        <div class="row">
