
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Financeiros</li>
    <li class="breadcrumb-item active">Centro Resultado</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i>Centro de Resultados
    </h1>
</div>

<div id="knockoutPesquisaCentroResultado">
    <div class="jarviswidget" data-widget-colorbutton="false" data-widget-sortable="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-togglebutton="false">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-content">
                    <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i>
                        Pesquisar Centro de Resultados
                    </button>

                    <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-5" data-bind="visible: Plano.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Plano.text, attr: { for: Plano.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Plano.val, valueUpdate: 'afterkeydown', attr: { maxlength: Plano.maxlength, id : Plano.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                        <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="jarviswidget" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel mt-3">
        <div class="panel-container">
            <div class="panel-content">
                <ul class="nav nav-tabs">
                    <li class="nav-item">
                        <a href="#knockoutCadastroCentroResultado" class="nav-link active" data-bs-toggle="tab">
                            <i class="fal fa-edit"></i><span class="hidden-mobile hidden-tablet">Detalhes</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#knockoutVeiculos" class="nav-link" data-bs-toggle="tab">
                            <i class="fal fa-truck"></i><span class="hidden-mobile hidden-tablet">Veículos</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="#knockoutTipoOperacao" class="nav-link" data-bs-toggle="tab">
                            <i class="fal fa-tasks"></i><span class="hidden-mobile hidden-tablet">Tipos de Operação</span>
                        </a>
                    </li>
                </ul>
                <div class="tab-content border border-top-0 p-3">
                    <div class="tab-pane fade active show" id="knockoutCadastroCentroResultado" role="tabpanel">
                        <div class="row">
                            <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Plano.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: Plano.val, attr: { maxlength: Plano.maxlength, id : Plano.id}, valueUpdate: 'afterkeydown', class: Plano.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: Plano.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: CentroResultadoSintetico.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CentroResultadoSintetico.text, attr: { for: CentroResultadoSintetico.id }"></label>
                                    <input type="text" class="form-control" data-bind="enable:CentroResultadoSintetico.enable,value: CentroResultadoSintetico.val, valueUpdate: 'afterkeydown', attr: { maxlength: CentroResultadoSintetico.maxlength, id : CentroResultadoSintetico.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: PlanoContabilidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PlanoContabilidade.text, attr: { for: PlanoContabilidade.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: PlanoContabilidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: PlanoContabilidade.maxlength, id : PlanoContabilidade.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoCompanhia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCompanhia.text, attr: { for: CodigoCompanhia.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoCompanhia.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCompanhia.maxlength, id : CodigoCompanhia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: AnaliticoSintetico.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: AnaliticoSintetico.text, attr: { for: AnaliticoSintetico.id }"></label>
                                    <select class="form-control" data-bind="options: AnaliticoSintetico.options, optionsText: 'text', optionsValue: 'value', value: AnaliticoSintetico.val, attr: { id : AnaliticoSintetico.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="knockoutVeiculos" role="tabpanel">
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: SegmentoVeiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SegmentoVeiculo.text, attr: { for: SegmentoVeiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: SegmentoVeiculo.requiredClass, value: SegmentoVeiculo.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': SegmentoVeiculo.text, maxlength: SegmentoVeiculo.maxlength, id: SegmentoVeiculo.id, name: SegmentoVeiculo.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: SegmentoVeiculo.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <button type="button" class="btn btn-success mb-2" data-loading-text="Aguarde..." data-bind="visible: Veiculo.visible, attr: { id: Veiculo.idBtnSearch}">
                            <i class="fal fa-plus"></i><span data-bind="text: Veiculo.text"></span>
                        </button>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Grid.id}" cellspacing="0"></table>
                    </div>
                    <div class="tab-pane fade" id="knockoutTipoOperacao">
                        <button type="button" class="btn btn-success mb-2" data-loading-text="Aguarde..." data-bind="visible: TipoOperacao.visible, attr: { id: TipoOperacao.idBtnSearch}">
                            <i class="fal fa-plus"></i><span data-bind="text: TipoOperacao.text"></span>
                        </button>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Grid.id}" cellspacing="0"></table>
                    </div>
                    <footer id="knockoutCRUDCentroResultado" class="d-flex justify-content-end mt-2">
                        <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                            <span data-bind="text: Cancelar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                            <span data-bind="text: Excluir.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </footer>
                </div>
            </div>
        </div>
    </div>
</div>



<script src="~/scripts/CentroResultado"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadCentroResultado();
    });

</script>

