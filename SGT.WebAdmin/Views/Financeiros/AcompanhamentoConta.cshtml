
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Financeiros</li>
    <li class="breadcrumb-item active">Controle Arquivos Contas a pagar </li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Controle arquivos contas a pagar
    </h1>
</div>

<div id="knockoutPesquisaAcompanhamentoContasAPagar">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-filter"></i>
                    Pesquisa de Arquivos
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <div class="row">
                        <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicial.text"></label>
                                <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinal.text"></label>
                                <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                            </div>
                        </div>
                        @*Foi colocado pq não foi possivel adicionar as acentuações*@
                        <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="attr: { for: Situacao.id }">Situa&ccedil;&atilde;o</label>
                                <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: TermoQuitacaoFinanceiro.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TermoQuitacaoFinanceiro.text, attr: { for: TermoQuitacaoFinanceiro.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TermoQuitacaoFinanceiro.requiredClass, value: TermoQuitacaoFinanceiro.val, valueUpdate: 'afterkeydown', attr: { maxlength: TermoQuitacaoFinanceiro.maxlength, id : TermoQuitacaoFinanceiro.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TermoQuitacaoFinanceiro.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="d-flex align-items-center justify-content-between mb-3">

                        <button class="btn btn-default waves-effect waves-themed" data-bind="click: Importar.eventClick, attr: { id: Importar.id}" type="button">
                            <i class="fal fa-cloud-download"></i><span data-bind="text: Importar.text"></span>
                        </button>

                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalAdicionarArquivo" tabindex="-1" >
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content" id="knockouModalArquivoConta">
            <div class="modal-header">
                <h4 class="modal-title">Adicionar um arquivo para processar</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body row">
                <div class="col-12 col-md-6" data-bind="visible: TipoDocumento.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: TipoDocumento.text, attr: { for: TipoDocumento.id }"></label>
                        <select class="form-control" data-bind="class: TipoDocumento.requiredClass, options: TipoDocumento.options, optionsText: 'text', optionsValue: 'value', value: TipoDocumento.val, attr: { maxlength: TipoDocumento.maxlength, id : TipoDocumento.id}"></select>
                    </div>
                </div>
                <div class="form-group col-md-6 mt-3 mb-0" data-bind="visible: Documento.visible">
                    <label></label>
                    <div class="custom-file">
                        <input type="file" accept=".csv" name="file" class="custom-file-input" data-bind="value: Documento.val, enable : Documento.enable, attr: { id: Documento.id }" />
                        <label class="custom-file-label" data-bind="text: Documento.text, attr: { for: Documento.id }"></label>
                    </div>
                </div>
                <div class="col-12">
                    <div class="d-flex justify-content-end my-2">
                        <button class="btn btn-primary" type="button" data-bind="click: Importar.eventClick, attr: { id: Importar.id}">
                            <span data-bind="text: Importar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalLogErros" tabindex="-1" >
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content" id="knockouModalLogErros">
            <div class="modal-header">
                <h4 class="modal-title">Logs Error Conta Pagar</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body row">
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Grid.idGrid}" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/acompanhamentoContaAPagar"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadAcompanhamentoContaAPagar();
    });

</script>