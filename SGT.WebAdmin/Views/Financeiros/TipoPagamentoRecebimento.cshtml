                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCentroResultado.text, attr: { for: CodigoCentroResultado.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoCentroResultado.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCentroResultado.maxlength, id : CodigoCentroResultado.id}" />
