
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Financeiros</li>
    <li class="breadcrumb-item active">Movimentação Conta Pagar</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        Movimentação Contas a Pagar
    </h1>
</div>
<div id="knockoutPesquisaMovimentacaoContaPagar">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-filter"></i>
                    Pesquisar Movimentação Conta a Pagar
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: DataDocInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataDocInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataDocInicial.requiredClass, value: DataDocInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataDocInicial.maxlength, id : DataDocInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataDocFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataDocFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataDocFinal.requiredClass, value: DataDocFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataDocFinal.maxlength, id : DataDocFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCompensacaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCompensacaoInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCompensacaoInicial.requiredClass, value: DataCompensacaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCompensacaoInicial.maxlength, id : DataCompensacaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCompensacaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCompensacaoFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCompensacaoFinal.requiredClass, value: DataCompensacaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCompensacaoFinal.maxlength, id : DataCompensacaoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroTermoQuitacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroTermoQuitacao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroTermoQuitacao.requiredClass, value: NumeroTermoQuitacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroTermoQuitacao.maxlength, id : NumeroTermoQuitacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoArquivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoArquivo.text, attr: { for: TipoArquivo.id }"></label>
                                    <select class="form-control" data-bind="class: TipoArquivo.requiredClass, options: TipoArquivo.options, optionsText: 'text', optionsValue: 'value', value: TipoArquivo.val, attr: { maxlength: TipoArquivo.maxlength, id : TipoArquivo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: SituacaoProcessamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoProcessamento.text, attr: { for: SituacaoProcessamento.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoProcessamento.requiredClass, options: SituacaoProcessamento.options, optionsText: 'text', optionsValue: 'value', value: SituacaoProcessamento.val, attr: { maxlength: SituacaoProcessamento.maxlength, id : SituacaoProcessamento.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: SituacaoDocumento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoDocumento.text, attr: { for: SituacaoDocumento.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoDocumento.requiredClass, options: SituacaoDocumento.options, optionsText: 'text', optionsValue: 'value', value: SituacaoDocumento.val, attr: { maxlength: SituacaoDocumento.maxlength, id : SituacaoDocumento.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 row" data-bind="visible: TodasFiliaisTransportador.visible">
                                <div class="form-group col-4">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: TodasFiliaisTransportador.val, attr: { maxlength: TodasFiliaisTransportador.maxlength, name: TodasFiliaisTransportador.id, id: TodasFiliaisTransportador.id }">
                                        <label class="custom-control-label" data-bind="text: TodasFiliaisTransportador.text, attr: { for: TodasFiliaisTransportador.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-between mb-3">
                        <button class="btn btn-danger waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: ReprocessarMovimentosContaPagar.eventClick, attr: { id: ReprocessarMovimentosContaPagar.id}" type="button">
                            <i class="fal fa-sync"></i><span data-bind="text: ReprocessarMovimentosContaPagar.text"></span>
                        </button>
                        <div class="gap-2 d-flex">
                            <button class="btn btn-default ms-2" type="button" data-bind="visible: SelecionarTodos.visible, click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}">
                                <span data-bind="text: SelecionarTodos.text"></span>
                            </button>
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>

                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    <div class="col-12 my-2 mx-0 d-flex justify-content-end">
                        <button class="btn btn-success ms-2" type="button" data-bind="visible: Adicionar.visible, click: Adicionar.eventClick, attr: { id: Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalVincularDocumento" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md modal-dialog-centered" role="document">
        <div class="modal-content" id="knockoutVincularDocumento">
            <div class="modal-header">
                <h4 class="modal-title">Adicionar CT-e</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12" data-bind="visible: CTe.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CTe.text, attr: { for: CTe.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: CTe.requiredClass, value: CTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: CTe.maxlength, id : CTe.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CTe.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" data-bind="click: Adicionar.eventClick,visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                    <span data-bind="text: Adicionar.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalReprocessarVinculo" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
        <div class="modal-content" id="knockoutReprocessarVinculo">
            <div class="modal-header">
                <h4 class="modal-title">Reprocessar Vinculo</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-md-4" data-bind="visible: DataInicial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataInicial.text"></label>
                            <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: DataFinal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataFinal.text"></label>
                            <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-bind="click: Cancelar.eventClick,visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                    <span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-warning" data-bind="click: Adicionar.eventClick,visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                    <span data-bind="text: Adicionar.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/movimentacaoContaPagar"></script>
<script src="~/scripts/cte"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadMovimentacaoContaPagar();
    });

</script>

