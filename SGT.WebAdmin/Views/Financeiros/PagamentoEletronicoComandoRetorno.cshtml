
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Financeiro</li>
    <li class="breadcrumb-item"> Comandos de Retorno de Pagamento</li>

</ol>
<div class="subheader">
    <h1>
       Comandos de Retorno de Pagamento
    </h1>
</div>
<div id="knockoutPesquisaPagamentoEletronicoComandoRetorno">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Comando de Retorno de Pagamento
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Comando.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Comando.text, attr: { for: Comando.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Comando.val, valueUpdate: 'afterkeydown', attr: { maxlength: Comando.maxlength, id : Comando.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-8" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-10" data-bind="visible: BoletoConfiguracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: BoletoConfiguracao.text, attr: { for: BoletoConfiguracao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: BoletoConfiguracao.requiredClass,value: BoletoConfiguracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: BoletoConfiguracao.maxlength, id : BoletoConfiguracao.id} " />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: BoletoConfiguracao.idBtnSearch}">
                                                <i class="fal fa-search"></i> &nbsp; Buscar &nbsp;
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 d-flex justify-content-end mb-3">
                                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id }" type="button">
                                    <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text">Pesquisar</span>
                                </button>
                            </div>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Comando de Retorno de Pagamento
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroPagamentoEletronicoComandoRetorno">
                    <div class="col-12 col-md-4" data-bind="visible: Comando.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Comando.text, attr: { for: Comando.id }"></label>
                            <input type="text" class="form-control" data-bind="value: Comando.val, valueUpdate: 'afterkeydown', attr: { maxlength: Comando.maxlength, id : Comando.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-8" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-10" data-bind="visible: BoletoConfiguracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: BoletoConfiguracao.text, attr: { for: BoletoConfiguracao.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: BoletoConfiguracao.requiredClass,value: BoletoConfiguracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: BoletoConfiguracao.maxlength, id : BoletoConfiguracao.id} " />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: BoletoConfiguracao.idBtnSearch}">
                                        <i class="fal fa-search"></i> &nbsp; Buscar &nbsp;
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                            <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: ComandoDeLiquidacao.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: ComandoDeLiquidacao.val, attr: { maxlength: ComandoDeLiquidacao.maxlength, name: ComandoDeLiquidacao.id, id: ComandoDeLiquidacao.id }">
                                <label class="custom-control-label" data-bind="text: ComandoDeLiquidacao.text, attr: { for: ComandoDeLiquidacao.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: ComandoDeEstorno.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: ComandoDeEstorno.val, attr: { maxlength: ComandoDeEstorno.maxlength, name: ComandoDeEstorno.id, id: ComandoDeEstorno.id }">
                                <label class="custom-control-label" data-bind="text: ComandoDeEstorno.text, attr: { for: ComandoDeEstorno.id }"></label>
                            </div>
                        </div>
                    </div>

                    <footer>
                        <div class="row">
                            <div class="col-6">
                                <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                    <i class="fal fa-ban"></i>
                                    <span data-bind="text: Cancelar.text"></span>
                                </button>
                            </div>
                            <div class="col-6 d-flex justify-content-end">
                                <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                    <i class="fal fa-trash"></i>
                                    <span data-bind="text: Excluir.text"></span>
                                </button>
                                <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                    <i class="fal fa-check"></i>
                                    <span data-bind="text: Atualizar.text"></span>
                                </button>
                                <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                    <span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                        </div>
                    </footer>
                </div>
            </form>
        </div>
    </div>
</div>
<script src="~/scripts/pagamentoEletronicoComandoRetorno"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadPagamentoEletronicoComandoRetorno();
    });

</script>

