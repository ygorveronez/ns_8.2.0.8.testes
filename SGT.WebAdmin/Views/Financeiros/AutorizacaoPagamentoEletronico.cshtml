                    </li>
                </ul>
                <div class="tab-content border border-top-0 p-3">
                    <div id="knockoutPagamentoEletronico" class="tab-pane fade active show">
                        <h3>Detalhes do Pagamento Eletrônico</h3>
                        <div class="row row-cols-1 row-cols-md-2">
                            <div class="col">
                                <table class="table table-clean table-sm align-self-end">
                                    <tbody>
                                        <tr data-bind="visible: Numero.visible">
                                            <td>
                                                <strong data-bind="text: Numero.text"></strong>
                                                <span data-bind="text: Numero.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: Data.visible">
                                            <td>
                                                <strong data-bind="text: Data.text"></strong>
                                                <span data-bind="text: Data.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: SituacaoDescricao.visible">
                                            <td>
                                                <strong data-bind="text: SituacaoDescricao.text"></strong>
                                                <span data-bind="text: SituacaoDescricao.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: Operador.visible">
                                            <td>
                                                <strong data-bind="text: Operador.text"></strong>
                                                <span data-bind="text: Operador.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: BoletoConfiguracao.visible">
                                            <td>
                                                <strong data-bind="text: BoletoConfiguracao.text"></strong>
                                                <span data-bind="text: BoletoConfiguracao.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: DataGeracao.visible">
                                            <td>
                                                <strong data-bind="text: DataGeracao.text"></strong>
                                                <span data-bind="text: DataGeracao.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: DataGeracao.visible">
                                            <td>
                                                <strong data-bind="text: DataGeracao.text"></strong>
                                                <span data-bind="text: Modalidade.val"></span>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="col">
                                <table class="table table-clean table-sm align-self-end">
                                    <tbody>
                                        <tr data-bind="visible: TipoConta.visible">
                                            <td>
                                                <strong data-bind="text: TipoConta.text"></strong>
                                                <span data-bind="text: TipoConta.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: Finalidade.visible">
                                            <td>
                                                <strong data-bind="text: Finalidade.text"></strong>
                                                <span data-bind="text: Finalidade.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: ValorTotal.visible">
                                            <td>
                                                <strong data-bind="text: ValorTotal.text"></strong>
                                                <span data-bind="text: ValorTotal.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: QtdTitulos.visible">
                                            <td>
                                                <strong data-bind="text: QtdTitulos.text"></strong>
                                                <span data-bind="text: QtdTitulos.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: Empresa.visible">
                                            <td>
                                                <strong data-bind="text: Empresa.text"></strong>
                                                <span data-bind="text: Empresa.val"></span>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div id="knockoutAutorizacao" class="tab-pane fade">
                        <h3>Regras</h3>
                        <div class="row">
                            <div class="col-12" data-bind="visible: Motivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                                    <textarea class="form-control" rows="5" type="text" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}"></textarea>
                                </div>
                            </div>
                            <div class="col-12" data-bind="visible: Rejeitar.visible">
                                <div class="d-flex justify-content-end mb-3">
                                    <button class="btn btn-danger ms-1" type="button" data-bind="click: Rejeitar.eventClick, attr: { id : Rejeitar.id}">
                                        <span data-bind="text: Rejeitar.text"></span>
                                    </button>
                                    <button class="btn btn-default ms-1" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                                        <span data-bind="text: Cancelar.text"></span>
                                    </button>
                                </div>
                            </div>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id : Regras.idGrid}"></table>
                        <div class="mt-3" data-bind="visible: AprovarTodas.visible">
                            <div class="d-flex justify-content-end">
                                <button class="btn btn-success" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id: AprovarTodas.id}">
                                    <span data-bind="text: AprovarTodas.text"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div id="knockoutTitulos" class="tab-pane fade">
                        <h3>Títulos</h3>
                        <table width="100%" class="table table-bordered table-hover my-3" data-bind="attr: { id : Titulos.idGrid}"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalRejeitarPagamentoEletronico" role="dialog" aria-hidden="true">
    <div id="knockoutRejeicaoPagamentoEletronico" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Rejeitar</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group" data-bind="visible: Motivo.visible">
                    <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                    <textarea class="form-control" rows="5" type="text" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}"></textarea>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default ms-1" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button class="btn btn-danger ms-1" type="button" data-bind="click: Rejeitar.eventClick, attr: { id : Rejeitar.id}">
                        <span data-bind="text: Rejeitar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="divModalDelegarSelecionados" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutDelegarSelecionados" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Delegar Infrações</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group" data-bind="visible: UsuarioDelegado.visible">
                    <label class="form-label" data-bind="text: UsuarioDelegado.text, attr: { for: UsuarioDelegado.id }"></label>
                    <div class="input-group">
                        <input type="text" class="form-control" data-bind="value: UsuarioDelegado.val, valueUpdate: 'afterkeydown', attr: { maxlength: UsuarioDelegado.maxlength, id : UsuarioDelegado.id}" />
                        <div class="input-group-append">
                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: UsuarioDelegado.idBtnSearch}">
                                <i class="fal fa-search"></i>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default ms-1" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button class="btn btn-success ms-1" type="button" data-bind="click: Delegar.eventClick, attr: { id : Delegar.id}">
                        <span data-bind="text: Delegar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

