
<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Financeiros</li>
        <li class="breadcrumb-item active">Boleto Remessa</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Remessa dos Boletos
        </h1>
    </div>
    <div id="knockoutPesquisaBoletoRemessa">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-content">
                    <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i>
                        Pesquisar Remessas dos Boletos
                    </button>
                    <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-3" data-bind="visible: NumeroSequencial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroSequencial.text, attr: { for: NumeroSequencial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroSequencial.requiredClass, value: NumeroSequencial.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroSequencial.maxlength, id : NumeroSequencial.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: DownloadRealizado.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DownloadRealizado.text, attr: { for: DownloadRealizado.id }"></label>
                                        <select class="form-control" data-bind="class: DownloadRealizado.requiredClass, options: DownloadRealizado.options, optionsText: 'text', optionsValue: 'value', value: DownloadRealizado.val, attr: { maxlength: DownloadRealizado.maxlength, id : DownloadRealizado.id}"></select>
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: BoletoConfiguracao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: BoletoConfiguracao.text, attr: { for: BoletoConfiguracao.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: BoletoConfiguracao.requiredClass, value: BoletoConfiguracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: BoletoConfiguracao.maxlength, id : BoletoConfiguracao.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: BoletoConfiguracao.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Remessa de Boletos
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroBoletoRemessa">
                    <div class="col-12 col-md-3" data-bind="visible: NumeroSequencial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroSequencial.text, attr: { for: NumeroSequencial.id }"></label>
                            <input type="text" class="form-control" data-bind="class: NumeroSequencial.requiredClass, value: NumeroSequencial.val,enable: NumeroSequencial.enable, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroSequencial.maxlength, id : NumeroSequencial.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataGeracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataGeracao.text, attr: { for: DataGeracao.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataGeracao.requiredClass, value: NumeroSequencial.val,enable: DataGeracao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: DataGeracao.maxlength, id : DataGeracao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: BoletoConfiguracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: BoletoConfiguracao.text, attr: { for: BoletoConfiguracao.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: BoletoConfiguracao.requiredClass, value: BoletoConfiguracao.val, valueUpdate: 'afterkeydown', enable: BoletoConfiguracao.enable, attr: { maxlength: BoletoConfiguracao.maxlength, id : BoletoConfiguracao.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="enable: BoletoConfiguracao.enable, attr: { id: BoletoConfiguracao.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: CaminhoArquivo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CaminhoArquivo.text, attr: { for: CaminhoArquivo.id }"></label>
                            <input type="text" class="form-control" data-bind="class: CaminhoArquivo.requiredClass, value: CaminhoArquivo.val,enable: CaminhoArquivo.enable, valueUpdate: 'afterkeydown', attr: { maxlength: CaminhoArquivo.maxlength, id : CaminhoArquivo.id}" />
                        </div>
                    </div>
                    <div class="custom-control custom-switch ms-3 mb-2">
                        <input type="checkbox" class="custom-control-input" data-bind="enable: RemessaDeCancelamento.enable, checked: RemessaDeCancelamento.val, attr: { name: RemessaDeCancelamento.id, id: RemessaDeCancelamento.id }">
                        <label class="custom-control-label" data-bind="text: RemessaDeCancelamento.text, attr: { for: RemessaDeCancelamento.id }"></label>
                    </div>

                    <footer>
                        <div class="row">
                            <div class="col-6 mt-2">
                                <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                    <i class="fal fa-ban"></i>
                                    <span data-bind="text: Cancelar.text"></span>
                                </button>
                            </div>
                            <div class="col-6 d-flex justify-content-end mt-2">
                                <button type="button" class="btn btn-labeled btn-danger me-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                    <i class="fal fa-trash"></i>
                                    <span data-bind="text: Excluir.text"></span>
                                </button>
                                <button type="button" class="btn btn-labeled btn-success me-2" data-bind="click: LimparBoletos.eventClick, visible: LimparBoletos.visible, attr: { id : LimparBoletos.id}">
                                    <i class="fal fa-check"></i>
                                    <span data-bind="text: LimparBoletos.text"></span>
                                </button>
                                <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Download.eventClick, visible: Download.visible, attr: { id : Download.id}">
                                    <span data-bind="text: Download.text"></span>
                                </button>
                            </div>
                        </div>
                    </footer>
                </div>
            </form>
        </div>
    </div>
</div>






<script src="~/scripts/boletoRemessa"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadBoletoRemessa();
    });

</script>

