
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">@Localization.Resources.Pessoas.SetorFuncionario.Pessoas</li>
    <li class="breadcrumb-item active">@Localization.Resources.Pessoas.SetorFuncionario.SetorFuncionarioDescricao</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i> @Localization.Resources.Pessoas.SetorFuncionario.SetorDeFuncionarioDescricao
    </h1>
</div>

<div id="knockoutPesquisaSetorFuncionario">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    @Localization.Resources.Pessoas.SetorFuncionario.PesquisarSetorFuncionario
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                @Localization.Resources.Pessoas.SetorFuncionario.CadastrarSetorFuncionario
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutSetorFuncionario">
                    <div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible : TipoCargoFuncionario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoCargoFuncionario.text"></label>
                            <select class="form-control" data-bind="enable: TipoCargoFuncionario.enable, options: TipoCargoFuncionario.options, optionsText: 'text', optionsValue: 'value', selectedOptions: TipoCargoFuncionario.val, attr: { id : TipoCargoFuncionario.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: TipoGrot.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoGrot.text, attr: { for: TipoGrot.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: TipoGrot.requiredClass, value: TipoGrot.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoGrot.maxlength, id : TipoGrot.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoGrot.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: PermitirAssumirChamadosDoMesmoSetor.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: PermitirAssumirChamadosDoMesmoSetor.val, attr: { maxlength: PermitirAssumirChamadosDoMesmoSetor.maxlength, name: PermitirAssumirChamadosDoMesmoSetor.id, id: PermitirAssumirChamadosDoMesmoSetor.id }">
                                <label class="custom-control-label" data-bind="text: PermitirAssumirChamadosDoMesmoSetor.text, attr: { for: PermitirAssumirChamadosDoMesmoSetor.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: PermitirCancelarAtendimento.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: PermitirCancelarAtendimento.val, attr: { maxlength: PermitirCancelarAtendimento.maxlength, name: PermitirCancelarAtendimento.id, id: PermitirCancelarAtendimento.id }">
                                <label class="custom-control-label" data-bind="text: PermitirCancelarAtendimento.text, attr: { for: PermitirCancelarAtendimento.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: NotificarCenarioPosEntregaImprocedenteGestaoDevolucao.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: NotificarCenarioPosEntregaImprocedenteGestaoDevolucao.val, attr: { maxlength: NotificarCenarioPosEntregaImprocedenteGestaoDevolucao.maxlength, name: NotificarCenarioPosEntregaImprocedenteGestaoDevolucao.id, id: NotificarCenarioPosEntregaImprocedenteGestaoDevolucao.id }">
                                <label class="custom-control-label" data-bind="text: NotificarCenarioPosEntregaImprocedenteGestaoDevolucao.text, attr: { for: NotificarCenarioPosEntregaImprocedenteGestaoDevolucao.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: TipoSetorFuncionario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoSetorFuncionario.text, attr: { for: TipoSetorFuncionario.id }"></label>
                            <select class="form-control" data-bind="class: TipoSetorFuncionario.requiredClass, options: TipoSetorFuncionario.options, optionsText: 'text', optionsValue: 'value', value: TipoSetorFuncionario.val, attr: { maxlength: TipoSetorFuncionario.maxlength, id : TipoSetorFuncionario.id}"></select>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDSetorFuncionario">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/setorFuncionario"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources(["Pessoas.SetorFuncionario", "Pessoas.Usuario"]).then(loadSetorFuncionario);
    });

</script>