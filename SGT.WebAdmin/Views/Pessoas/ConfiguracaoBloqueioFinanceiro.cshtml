
<section>
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Pessoas</li>
        <li class="breadcrumb-item active">Configuração de bloqueio financeiro</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Configuração de bloqueio automático do financeiro de Clientes
        </h1>

    </div>

    <div class="panel">
        <div class="panel-container">
            <div id="knockoutPesquisaConfiguracaoBloqueioFinanceiro" class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i> Pesquisar Configuração de bloqueio
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6 col-lg-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6 col-lg-6" data-bind="visible: GrupoPessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoPessoa.text, attr: { for: GrupoPessoa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: GrupoPessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoPessoa.maxlength, id : GrupoPessoa.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoPessoa.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex justify-content-end mb-3">
                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>

                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Formulário de Cadastro
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutConfigConfiguracaoBloqueioFinanceiro">
                    <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: HabilitarRegra.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: HabilitarRegra.text, attr: { for: HabilitarRegra.id }"></label>
                            <select class="form-control" data-bind="options: HabilitarRegra.options, optionsText: 'text', optionsValue: 'value', value: HabilitarRegra.val, attr: { id : HabilitarRegra.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: QuantidadeDiasAtrasoPagamento.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeDiasAtrasoPagamento.text, attr: { for: QuantidadeDiasAtrasoPagamento.id }"></label>
                            <input type="text" class="form-control" data-bind="css: QuantidadeDiasAtrasoPagamento.requiredClass,value: QuantidadeDiasAtrasoPagamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeDiasAtrasoPagamento.maxlength, id : QuantidadeDiasAtrasoPagamento.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: QuantidadeDiasNovoBloqueio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeDiasNovoBloqueio.text, attr: { for: QuantidadeDiasNovoBloqueio.id }"></label>
                            <input type="text" class="form-control" data-bind="css: QuantidadeDiasNovoBloqueio.requiredClass,value: QuantidadeDiasNovoBloqueio.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeDiasNovoBloqueio.maxlength, id : QuantidadeDiasNovoBloqueio.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: GrupoPessoa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: GrupoPessoa.text, attr: { for: GrupoPessoa.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="value: GrupoPessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoPessoa.maxlength, id : GrupoPessoa.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoPessoa.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <footer id="knockoutCRUDConfiguracaoBloqueioFinanceiro" class="d-flex justify-content-end">
                <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                    <span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                    <span data-bind="text: Excluir.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                    <span data-bind="text: Atualizar.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                    <span data-bind="text: Adicionar.text"></span>
                </button>
            </footer>
        </div>
    </div>
</div>








<script src="~/scripts/configuracaoBloqueioFinanceiro"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadConfiguracaoBloqueioFinanceiro();
    });

</script>

