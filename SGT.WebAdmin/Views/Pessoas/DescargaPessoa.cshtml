
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Pessoas</li>
    <li class="breadcrumb-item active">Descarga Pessoa</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Descarga Pessoa
    </h1>
</div>

<div id="knockoutPesquisaDescargaPessoa">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Descarga
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Pessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Pessoa.requiredClass, value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Pessoa.text, maxlength: Pessoa.maxlength, id: Pessoa.id, name: Pessoa.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Pessoa.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: PessoaOrigem.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PessoaOrigem.text, attr: { for: PessoaOrigem.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: PessoaOrigem.requiredClass, value: PessoaOrigem.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': PessoaOrigem.text, maxlength: PessoaOrigem.maxlength, id: PessoaOrigem.id, name: PessoaOrigem.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: PessoaOrigem.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Descarga Pessoa
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroDescargaPessoa">
                    <div class="col-12 col-md-6" data-bind="visible: Pessoa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Pessoa.requiredClass, value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Pessoa.text, maxlength: Pessoa.maxlength, id: Pessoa.id, name: Pessoa.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Pessoa.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: PessoaOrigem.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PessoaOrigem.text, attr: { for: PessoaOrigem.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: PessoaOrigem.requiredClass, value: PessoaOrigem.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': PessoaOrigem.text, maxlength: PessoaOrigem.maxlength, id: PessoaOrigem.id, name: PessoaOrigem.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: PessoaOrigem.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-1" data-bind="visible : HoraInicio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: HoraInicio.text"></label>
                            <input type="text" class="form-control"  data-bind="value: HoraInicio.val, attr: { maxlength: HoraInicio.maxlength, id : HoraInicio.id} "  />
                        </div>
                    </div>
                    <div class="col-12 col-md-1" data-bind="visible : HoraFim.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: HoraFim.text"></label>
                            <input type="text" class="form-control" data-bind="value: HoraFim.val, attr: { maxlength: HoraFim.maxlength, id : HoraFim.id} " />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible : ValorPallet.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ValorPallet.text"></label>
                            <input type="text" class="form-control" data-bind="value: ValorPallet.val, attr: { maxlength: ValorPallet.maxlength, id : ValorPallet.id} "  />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible : ValorPorVolume.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ValorPorVolume.text"></label>
                            <input type="text" class="form-control" data-bind="value: ValorPorVolume.val, attr: { maxlength: ValorPorVolume.maxlength, id : ValorPorVolume.id} " />
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: DeixarReboqueParaDescarga.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: DeixarReboqueParaDescarga.val, valueUpdate: 'afterkeydown', attr: { name: DeixarReboqueParaDescarga.id, id : DeixarReboqueParaDescarga.id}">
                                <label class="custom-control-label" data-bind="text: DeixarReboqueParaDescarga.text, attr: { for: DeixarReboqueParaDescarga.id }"></label>
                            </div>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDCadastroDescargaPessoa">
                    <div class="row">
                        <div class="col-6">
                            <span data-bind='component: {name: "import-file", params: { ko: Importar }}'></span>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick,visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick,visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick,visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick,visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/descargaPessoa"></script>

<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadDescarga();
    });

</script>

