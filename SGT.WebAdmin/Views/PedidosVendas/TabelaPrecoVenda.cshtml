
<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Pedidos Vendas </li>
        <li class="breadcrumb-item active"> Tabela de Preço de Venda</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Tabela de Preço de Venda
        </h1>

    </div>
    <div id="knockoutPesquisaTabelaPrecoVenda">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-content">
                    <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i>
                        Pesquisar Tabelas de Preços
                    </button>
                    <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-5" data-bind="visible: GrupoProduto.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: GrupoProduto.text, attr: { for: GrupoProduto.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="value: GrupoProduto.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoProduto.maxlength, id : GrupoProduto.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoProduto.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                        <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                    </div>
                                </div>

                            </div>
                        </form>
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-edit"></i>
                    Cadastrar Tabela de Preço
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div class="row" id="knockoutCadastroTabelaPrecoVenda">
                        <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: TipoPessoa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoPessoa.text, attr: { for: TipoPessoa.id }"></label>
                                <select class="form-control" data-bind="options: TipoPessoa.options, optionsText: 'text', optionsValue: 'value', value: TipoPessoa.val, attr: { id : TipoPessoa.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-10" data-bind="visible: Pessoa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Pessoa.requiredClass,value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pessoa.maxlength, id : Pessoa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Pessoa.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-10" data-bind="visible: GrupoPessoa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: GrupoPessoa.text, attr: { for: GrupoPessoa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: GrupoPessoa.requiredClass,value: GrupoPessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoPessoa.maxlength, id : GrupoPessoa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoPessoa.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: DataInicioVigencia.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicioVigencia.text, attr: { for: DataInicioVigencia.id }"></label>
                                <input type="text" class="form-control" data-bind="css: DataInicioVigencia.requiredClass,value: DataInicioVigencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicioVigencia.maxlength, id : DataInicioVigencia.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: DataFimVigencia.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFimVigencia.text, attr: { for: DataFimVigencia.id }"></label>
                                <input type="text" class="form-control" data-bind="css: DataFimVigencia.requiredClass,value: DataFimVigencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFimVigencia.maxlength, id : DataFimVigencia.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: Valor.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Valor.text, attr: { for: Valor.id }"></label>
                                <input type="text" class="form-control" data-bind="css: Valor.requiredClass,value: Valor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Valor.maxlength, id : Valor.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-6" data-bind="visible: GrupoProduto.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: GrupoProduto.text, attr: { for: GrupoProduto.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind=" css: GrupoProduto.requiredClass,value: GrupoProduto.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoProduto.maxlength, id : GrupoProduto.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoProduto.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <footer class="d-flex justify-content-end" id="knockoutCRUDTabelaPrecoVenda">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </div>
        </div>
    </div>
</section>












<script src="~/scripts/tabelaPrecoVenda"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadTabelaPrecoVenda();
    });

</script>
