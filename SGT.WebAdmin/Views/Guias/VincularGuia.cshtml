
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Guias</li>
    <li class="breadcrumb-item active">Vincular Guia</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Vincular Guias e Comprovantes
    </h1>
</div>

<div id="knockoutPesquisaGuia">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Imagens
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: NumeroDocumento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroDocumento.text, attr: { for: NumeroDocumento.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroDocumento.requiredClass, value: NumeroDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroDocumento.maxlength, id : NumeroDocumento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', selectedOptions: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-sm-2" data-bind="visible: DataEntrega.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEntrega.text, attr: { for: DataEntrega.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEntrega.requiredClass, value: DataEntrega.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEntrega.maxlength, id : DataEntrega.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" style="margin-top:32px;">
                                <div class="custom-file-btn" data-bind="visible: Upload.visible">
                                    <label class="btn btn-primary" data-bind="attr: { for: Upload.idFile }">
                                        <i class="fal fa-upload"></i> Upload
                                    </label>
                                    <input type="file" accept="text/plain" data-bind="event: {change: Upload.eventChange}, attr: { id : Upload.idFile, accept: Upload.accept, multiple: true }">
                                </div>
                            </div>
                        </div>
                    </form>
                    <footer>
                        <div class="row">
                            @*<div class="col-12 col-md-3" style="margin-top:32px;">
                                    <div data-bind='component: {name: "import-file", params: { ko: Upload }}, event : {change : Upload.eventChange}'></div>
                                </div>*@
                            <div class="col-12 d-flex justify-content-end">
                                <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                    <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                                </button>
                            </div>
                        </div>
                    </footer>

                </div>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Imagens Não Reconhecidas
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div class="row">
                        <table width="100%" class="table table-bordered table-hover table-condensed table-striped" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<div id="divModalVincularGuia" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutGuia" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Vincular Guias e Comprovantes</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <section class="col col-md-12" data-bind="visible: Imagem.visible">
                    <img src="" data-bind="attr: { id: Imagem.id}" class="img-fluid mx-auto d-block img-guia" />
                </section>
                <section class="col col-xs-12" data-bind="visible: SemImagem.val">
                    <div class="sem-imagem-guia"></div>
                </section>
                <section class="col col-xs-12 col-sm-12" data-bind="visible: PDFViewer.visible">
                    <embed data-bind="attr: { id: PDFViewer.id}" src="" style="width: 100%; height: 565px;" width="500" height="565">
                </section>
                <div class="col-12 col-md-12" data-bind="visible: Guia.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: Guia.text, attr: { for: Guia.id }"></label>
                        <div class="input-group">
                            <input type="text" class="form-control" data-bind="css: Guia.requiredClass, value: Guia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Guia.maxlength, id : Guia.id}" />
                            <div class="input-group-append">
                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Guia.idBtnSearch}" type="button" aria-label="Buscar">
                                    <i class="fal fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <section data-bind="visible: !Guia.visible()" class="col col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <b data-bind="text: Guia.text"></b> <span data-bind="text: Guia.val"></span>
                </section>
                <div class="d-flex justify-content-between">
                    <div class="col-12 col-md-6">
                        <div class="col-12 col-md-3" data-bind="visible: TipoAnexo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoAnexo.text, attr: { for: TipoAnexo.id }"></label>
                                <select class="form-control" data-bind="class: TipoAnexo.requiredClass, options: TipoAnexo.options, optionsText: 'text', optionsValue: 'value', value: TipoAnexo.val, attr: { maxlength: TipoAnexo.maxlength, id : TipoAnexo.id}"></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6 d-flex justify-content-end" data-bind="visible: Confirmar.visible">
                        <button class="btn btn-danger  me-2" type="button" style="max-height: 45px; margin-top: 32px" data-bind="click: Descartar.eventClick, visible: Confirmar.visible, attr: { value: Descartar.text, id : Descartar.id}">
                            <span data-bind="text: Descartar.text"></span>
                        </button>
                        <button class="btn btn-success" type="button" style="max-height: 45px; margin-top: 32px" data-bind="click: Confirmar.eventClick, visible: Confirmar.visible, attr: { value: Confirmar.text, id : Confirmar.id}">
                            <span data-bind="text: Confirmar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



<script src="~/scripts/vincularGuia"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadVincularGuia();
    });

</script>
