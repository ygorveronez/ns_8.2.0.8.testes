                                        <label class="form-label" data-bind="text: DataImportacao.text, attr: { for: DataImportacao.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataImportacao.requiredClass, value: DataImportacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataImportacao.maxlength, id : DataImportacao.id}" />
                                    </div>
