

<style>
    #knockoutContratoPrestacaoServicoFilial,
    #knockoutContratoPrestacaoServicoTransportador {
        padding: 15px 14px 20px;
    }

        #knockoutContratoPrestacaoServicoFilial .dt-toolbar,
        #knockoutContratoPrestacaoServicoTransportador .dt-toolbar {
            display: none;
        }

    #knockoutEtapaContratoPrestacaoServico .col {
        float: left;
    }

    table.dataTable.margin-top-0 {
        margin-top: 0 !important;
    }

    .tab-content + .tab-content {
        margin-top: 0;
    }

    @@media (max-width: 767px) {
        .tab-content + .tab-content {
            margin-top: 25px;
        }
    }

    .well + .well {
        margin-top: 25px;
    }
</style>

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Fretes</li>
    <li class="breadcrumb-item active">Contrato Prestação Serviço</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Contrato de Prestação de Serviço
    </h1>
</div>

<div id="knockoutPesquisaContratoPrestacaoServico">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Contratos de Prestação de Serviço
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text"></label>
                                    <select class="form-control" data-bind="enable: Situacao.enable, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Lançamento de Contratos de Prestação de Serviços
            </h3>
        </div>
        <div class="panel-content">
            <div id="knockoutEtapaContratoPrestacaoServico">
                <ul class="nav nav-steps">
                    <li data-target="#step1" data-bind="visible : EtapaDados.visible">
                        <button class="active" data-bs-target="#dadosContratoPrestacaoServico" data-bs-toggle="tab" data-bind="attr: {id : EtapaDados.idTab }, click : EtapaDados.eventClick, visible : EtapaDados.visible">
                            <span class="step" data-bind="attr: {'data-content' : EtapaDados.tooltip, 'data-original-title' : EtapaDados.tooltipTitle }">
                                <i class="fal fa-database"></i>
                            </span>
                            <span class="title" data-bind="text: EtapaDados.text"></span>
                        </button>
                    </li>
                    <li data-target="#step2" data-bind="visible : EtapaAprovacao.visible">
                        <button data-bs-target="#aprovacaoContratoPrestacaoServico" data-bs-toggle="tab" data-bind="attr: {id : EtapaAprovacao.idTab }, click : EtapaAprovacao.eventClick">
                            <span class="step" data-bind="attr: {'data-content' : EtapaAprovacao.tooltip, 'data-original-title' : EtapaAprovacao.tooltipTitle }">
                                <i class="fal fa-check-circle"></i>
                            </span>
                            <span class="title" data-bind="text: EtapaAprovacao.text"></span>
                        </button>
                    </li>
                </ul>
            </div>
            <div class="tab-content">
                <!-- Dados -->
                <div class="tab-pane fade active show" id="dadosContratoPrestacaoServico">
                    <ul class="nav nav-tabs">
                        <li class="nav-item active">
                            <a href="#tabContratoPrestacaoServicoDados" class="nav-link active" data-bs-toggle="tab">
                                <span class="hidden-mobile hidden-tablet">Dados</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#tabContratoPrestacaoServicoTransportador" class="nav-link" data-bs-toggle="tab">
                                <span class="hidden-mobile hidden-tablet">Transportadores</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#tabContratoPrestacaoServicoFilial" class="nav-link" data-bs-toggle="tab">
                                <span class="hidden-mobile hidden-tablet">Filiais</span>
                            </a>
                        </li>
                    </ul>

                    <div class="tab-content">

                        <!-- Dados -->
                        <div class="tab-pane fade active show" id="tabContratoPrestacaoServicoDados">
                            <div id="knockoutContratoPrestacaoServico">
                                <div class="row mt-3">
                                    <div class="col-12 col-md-12" data-bind="visible: Descricao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Descricao.text"></label>
                                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass, value: Descricao.val, enable: Descricao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataInicial.text"></label>
                                            <input type="text" class="form-control" data-bind="css: DataInicial.requiredClass, value: DataInicial.val, enable: DataInicial.enable, attr: { maxlength: DataInicial.maxlength, id : DataInicial.id} " />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataFinal.text"></label>
                                            <input type="text" class="form-control" data-bind="css: DataFinal.requiredClass, value: DataFinal.val, enable: DataFinal.enable, attr: { maxlength: DataFinal.maxlength, id : DataFinal.id} " />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-3" data-bind="visible: ValorTeto.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: ValorTeto.text"></label>
                                            <input type="text" class="form-control" data-bind="css: ValorTeto.requiredClass, value: ValorTeto.val, enable: ValorTeto.enable, valueUpdate: 'afterkeydown', attr: { maxlength: ValorTeto.maxlength, id : ValorTeto.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                            <select class="form-control" data-bind="enable: Status.enable, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Transportadores -->
                        <div class="tab-pane fade" id="tabContratoPrestacaoServicoTransportador">
                            <div class="row" id="knockoutContratoPrestacaoServicoTransportador">
                                <div class="col-6">
                                    <button class="btn btn-primary mb-3" data-bind="visible: ListaTransportador.visible, attr: { id: ListaTransportador.idBtnSearch}" type="button">
                                        <i class="fal fa-plus"></i>
                                        <span data-bind="text: ListaTransportador.text"></span>
                                    </button>
                                </div>
                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: ListaTransportador.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>

                        <!-- Filiais -->
                        <div class="tab-pane fade" id="tabContratoPrestacaoServicoFilial">
                            <div class="row" id="knockoutContratoPrestacaoServicoFilial">
                                <div class="col-6">
                                    <button class="btn btn-primary mb-3" data-bind="visible: ListaFilial.visible, attr: { id: ListaFilial.idBtnSearch}" type="button">
                                        <i class="fal fa-plus"></i>
                                        <span data-bind="text: ListaFilial.text"></span>
                                    </button>
                                </div>
                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: ListaFilial.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>


                    </div>
                </div>

                <!-- Aprovação -->
                <div class="tab-pane fade" id="aprovacaoContratoPrestacaoServico">
                    <div id="knockoutAprovacaoContratoPrestacaoServico">
                        <div class="row">
                            <div class="col-12 col-md-6">
                                <div class="card" data-bind="visible: PossuiRegras.val">
                                    <div class="card-header">
                                        <div class="card-title">Detalhes</div>
                                    </div>
                                    <div class="card-body">
                                        <div class="row">
                                            <section data-bind="visible: AprovacoesNecessarias.visible" class="col-12 mb-3">
                                                <label><b data-bind="text: AprovacoesNecessarias.text"></b> <span data-bind="text: AprovacoesNecessarias.val"></span></label>
                                            </section>
                                            <section data-bind="visible: Aprovacoes.visible" class="col-12 mb-3">
                                                <label><b data-bind="text: Aprovacoes.text"></b> <span data-bind="text: Aprovacoes.val"></span></label>
                                            </section>
                                            <section data-bind="visible: Reprovacoes.visible" class="col-12 mb-3">
                                                <label><b data-bind="text: Reprovacoes.text"></b> <span data-bind="text: Reprovacoes.val"></span></label>
                                            </section>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6">
                                <div class="card" data-bind="visible: PossuiRegras.val">
                                    <div class="card-header">
                                        <div class="card-title">Aprovadores</div>
                                    </div>
                                    <div class="card-body">
                                        <div class="row">
                                            <table width="100%" class="table table-bordered table-hover margin-top-0" data-bind="attr: { id: Regras.idGrid}" cellspacing="0"></table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div data-bind="visible: !PossuiRegras.val()">
                            <div class="alert alert-warning" role="alert">
                                Não foi encontrada nenhuma regra para aprovação.<br />
                                O contrato de prestação de servico ficará pendente até existir alguma regra de aprovação. Clique em reprocessar regras para verificar.
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <footer id="knockoutCRUDCadastroContratoPrestacaoServico">
                <div class="row mt-3">
                    <div class="col-12 d-flex justify-content-end">
                        <button type="button" class="btn btn-labeled btn-default mx-3" data-bind="click: Limpar.eventClick, visible: Limpar.visible, attr: { value: Limpar.text, id : Limpar.id}">
                            <span data-bind="text: Limpar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-primary mx-3" data-bind="click: ReprocessarRegras.eventClick, visible: ReprocessarRegras.visible, attr: { value: ReprocessarRegras.text, id : ReprocessarRegras.id}">
                            <span data-bind="text: ReprocessarRegras.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                            <i class="fal fa-check"></i>
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </div>
            </footer>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalDetalhesAutorizacao" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalhes da Autorização</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutDetalheAutorizacao">
                <div class="row">
                    <section data-bind="visible: Regra.visible" class="col-12 col-md-8 mb-3">
                        <label><b data-bind="text: Regra.text"></b> <span data-bind="text: Regra.val"></span></label>
                    </section>
                    <section data-bind="visible: Situacao.visible" class="col-12 col-md-4 mb-3">
                        <label><b data-bind="text: Situacao.text"></b> <span data-bind="text: Situacao.val"></span></label>
                    </section>

                    <section data-bind="visible: Usuario.visible" class="col-12 col-md-8 mb-3">
                        <label><b data-bind="text: Usuario.text"></b> <span data-bind="text: Usuario.val"></span></label>
                    </section>
                    <section data-bind="visible: Data.visible && Data.val() != ''" class="col-12 col-md-4 mb-3">
                        <label><b data-bind="text: Data.text"></b> <span data-bind="text: Data.val"></span></label>
                    </section>
                    <section data-bind="visible: Motivo.visible && Motivo.val() != ''" class="col-12 col-md-12 mb-3">
                        <label><b data-bind="text: Motivo.text"></b> <span data-bind="text: Motivo.val"></span></label>
                    </section>
                </div>
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/contratoPrestacaoServico"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadContratoPrestacaoServico();
    });

</script>

