
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Taxa Descarga</li>
    <li class="breadcrumb-item active">Regras Autorização de Taxa Descarga</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Regras Autorização Taxa Descarga
    </h1>
</div>

<div class="panel">
    <div class="panel-container">
        <div id="knockoutPesquisaRegrasAutorizacao" class="panel-content">
            <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                <i class="fal fa-search"></i> Pesquisar Regras de Taxa Descarga
            </button>
            <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                <form>
                    <div class="row">
                        <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Descricao.text"></label>
                                <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicio.text"></label>
                                <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinal.text"></label>
                                <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                            </div>
                        </div>
                    </div>
                </form>
                <div class="d-flex justify-content-end mb-3">
                    <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                        <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                    </button>
                </div>

                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>

<div class="panel" id="knockoutCadastroRegrasAutorizacao">
    <div class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs">
                <li class="nav-item">
                    <a href="#tabRegra" class="nav-link active" data-bs-toggle="tab"><span class="hidden-mobile hidden-tablet">Regra</span></a>
                </li>
                <!-- ko foreach: { data: Alcadas.val, as: 'Aba' }  -->
                <li class="nav-item">
                    <a data-bind="attr: { href: '#tabRegras' + Aba.prop }, click: function(){ $root.Alcadas.changeAba(Aba.prop) }" class="nav-link" data-bs-toggle="tab"><span data-bind="text: Aba.descricao" class="hidden-mobile hidden-tablet"></span></a>
                </li>
                <!-- /ko -->
            </ul>
            <div class="tab-content border border-top-0 p-3">
                <div id="tabRegra" class="tab-pane fade active show">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: PrioridadeAprovacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PrioridadeAprovacao.text, attr: { for: PrioridadeAprovacao.id }"></label>
                                    <select class="form-control" data-bind="class: PrioridadeAprovacao.requiredClass, options: PrioridadeAprovacao.options, optionsText: 'text', optionsValue: 'value', value: PrioridadeAprovacao.val, attr: { maxlength: PrioridadeAprovacao.maxlength, id : PrioridadeAprovacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Vigencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Vigencia.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Vigencia.requiredClass, value: Vigencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Vigencia.maxlength, id : Vigencia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroAprovadores.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroAprovadores.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroAprovadores.requiredClass, value: NumeroAprovadores.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroAprovadores.maxlength, id : NumeroAprovadores.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="class: Ativo.requiredClass, options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { maxlength: Ativo.maxlength, id : Ativo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Observacoes.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Observacoes.text, attr: { for: Observacoes.id }"></label>
                                    <textarea class="form-control" rows="4" data-bind="value: Observacoes.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacoes.maxlength, id : Observacoes.id }"></textarea>
                                </div>
                            </div>

                            <div class="col-6">
                                <div class="card" data-bind="visible: Aprovadores.visible">
                                    <div class="card-header d-flex align-items-center justify-content-between">
                                        <h3 class="card-title">Aprovador</h3>
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: AdicionarAprovador.idBtnSearch, text: AdicionarAprovador.text }">
                                            <i class="fal fa-plus"></i><span data-bind="text: AdicionarAprovador.text"></span>
                                        </button>
                                    </div>
                                    <div class="card-body">
                                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Aprovadores.idGrid}" cellspacing="0"></table>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                </div>

                <!-- ko foreach: { data: Alcadas.val, as: 'Aba' }  -->
                <div data-bind="attr: { id: 'tabRegras' + Aba.prop }" class="tab-pane fade">
                    <div>
                        <div class="custom-control custom-switch mb-3">
                            <input type="checkbox" class="custom-control-input" data-bind="checked: $root['UsarRegraPor' + Aba.prop].val, attr: { name: $root['UsarRegraPor' + Aba.prop].id, id: $root['UsarRegraPor' + Aba.prop].id }">
                            <label class="custom-control-label" data-bind="text: $root['UsarRegraPor' + Aba.prop].text, attr: { for: $root['UsarRegraPor' + Aba.prop].id }"></label>
                        </div>
                        <hr />
                        <div class="row exchange" data-bind="id: $root['UsarRegraPor' + Aba.prop].idFade, visible: $root['UsarRegraPor' + Aba.prop].val">
                            <div class="col-12 col-md-6">
                                <form>
                                    <div class="row">
                                        <div class="col-12 col-md-6">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: $root['Condicao' + Aba.prop].text"></label>
                                                <select class="form-control" data-bind="options: $root['Condicao' + Aba.prop].options, optionsText: 'text', optionsValue: 'value', value: $root['Condicao' + Aba.prop].val, attr: { id : $root['Condicao' + Aba.prop].id}"></select>
                                            </div>
                                        </div>
                                        <div class="col-12 col-md-6">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: $root['Juncao' + Aba.prop].text"></label>
                                                <select class="form-control" data-bind="options: $root['Juncao' + Aba.prop].options, optionsText: 'text', optionsValue: 'value', value: $root['Juncao' + Aba.prop].val, attr: { id : $root['Juncao' + Aba.prop].id}"></select>
                                            </div>
                                        </div>

                                        <!-- ko if: Aba.tipoPropriedadeAlcada === EnumTipoPropriedadeAlcada.Entidade -->
                                        <div class="col-12 ">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: $root[Aba.prop].text"></label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control" data-bind="class: $root[Aba.prop].requiredClass, value: $root[Aba.prop].val, valueUpdate: 'afterkeydown', attr: { maxlength: $root[Aba.prop].maxlength, id : $root[Aba.prop].id}" />
                                                    <div class="input-group-append">
                                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: $root[Aba.prop].idBtnSearch}">
                                                            <i class="fal fa-search"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- /ko -->
                                        <!-- ko if: Aba.tipoPropriedadeAlcada !== EnumTipoPropriedadeAlcada.Entidade -->
                                        <div class="col-12 col-md-12" data-bind="visible: $root[Aba.prop].visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: $root[Aba.prop].text"></label>
                                                <input type="text" class="form-control" data-bind="class: $root[Aba.prop].requiredClass, value: $root[Aba.prop].val, valueUpdate: 'afterkeydown', attr: { maxlength: $root[Aba.prop].maxlength, id : $root[Aba.prop].id}" />
                                            </div>
                                        </div>
                                        <!-- /ko -->
                                    </div>
                                </form>
                                <div class="d-flex justify-content-between">
                                    <div class="d-flex">
                                        <button class="btn btn-default me-2" type="button" data-bind="click: function(data, evt){ $root.Cancelar.eventClick(Aba.prop, data, evt) }, visible: $root.Cancelar.visible, attr: { value: $root.Cancelar.text, id : $root.Cancelar.id}">
                                            <i class="fal fa-ban"></i><span data-bind="text: $root.Cancelar.text"></span>
                                        </button>
                                        <button class="btn btn-danger" type="button" data-bind="click: function(data, evt){ $root.Excluir.eventClick(Aba.prop, data, evt) }, visible: $root.Excluir.visible, attr: { value: $root.Excluir.text, id : $root.Excluir.id}">
                                            <i class="fal fa-times"></i><span data-bind="text: $root.Excluir.text"></span>
                                        </button>
                                    </div>
                                    <div class="d-flex">
                                        <button class="btn btn-success" type="button" data-bind="click: function(data, evt){ $root.Atualizar.eventClick(Aba.prop, data, evt) }, visible: $root.Atualizar.visible, attr: { value: $root.Atualizar.text, id : $root.Atualizar.id}">
                                            <i class="fal fa-check"></i><span data-bind="text: $root.Atualizar.text"></span>
                                        </button>
                                        <button class="btn btn-success" type="button" data-loading-text="Aguarde..." data-bind="click: function(data, evt){ $root.Adicionar.eventClick(Aba.prop, data, evt) }, visible: $root.Adicionar.visible, attr: { value: $root.Adicionar.text, id : $root.Adicionar.id}">
                                            <i class="fal fa-check"></i><span data-bind="text: $root.Adicionar.text"></span>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6">
                                <h3 class="form-label" data-bind="text: Aba.descricao"></h3>
                                <div data-bind="attr: { id: $root['Alcadas' + Aba.prop].idGrid}"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /ko -->

                <div id="knockoutCRUDRegras" class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default ms-1" type="button" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button class="btn btn-danger ms-1" type="button" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button class="btn btn-success ms-1" type="button" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button class="btn btn-success ms-1" type="button" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/regraAutorizacaoTaxaDescarga"></script>

<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadRegrasAutorizacao();
    });</script>
