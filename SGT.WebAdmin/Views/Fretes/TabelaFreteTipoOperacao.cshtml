
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Fretes</li>
    <li class="breadcrumb-item active">Tabela Frete Tipo Operação</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Configuração de Frete por Tipo de Operação
    </h1>
</div>

<div id="knockoutPesquisaTabelaFreteTipoOperacao">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Configurações
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: TipoOperacaoEmissao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacaoEmissao.text"></label>
                                    <select class="form-control" data-bind="options: TipoOperacaoEmissao.options, optionsText: 'text', optionsValue: 'value', value: TipoOperacaoEmissao.val, attr: { id : TipoOperacaoEmissao.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Configuração de Frete por Tipo de Operação
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroTabelaFreteTipoOperacao">
                    <div class="col-12 col-md-6" data-bind="visible: TipoOperacaoEmissao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoOperacaoEmissao.text, attr: { for: TipoOperacaoEmissao.id }"></label>
                            <select class="form-control" data-bind="options: TipoOperacaoEmissao.options, optionsText: 'text', optionsValue: 'value', value: TipoOperacaoEmissao.val, attr: { id : TipoOperacaoEmissao.id}, event: {change: TipoOperacaoEmissao.eventChange}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-12" data-bind="visible: TabelaFrete.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TabelaFrete.text, attr: { for: TabelaFrete.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: TabelaFrete.requiredClass, value: TabelaFrete.val, valueUpdate: 'afterkeydown', attr: { maxlength: TabelaFrete.maxlength, id : TabelaFrete.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TabelaFrete.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: PagarPorTonelada.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: PagarPorTonelada.val, attr: { maxlength: PagarPorTonelada.maxlength, name: PagarPorTonelada.id, id: PagarPorTonelada.id }">
                                <label class="custom-control-label" data-bind="text: PagarPorTonelada.text, attr: { for: PagarPorTonelada.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: TabelaFreteRedespacho.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TabelaFreteRedespacho.text, attr: { for: TabelaFreteRedespacho.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: TabelaFreteRedespacho.requiredClass, value: TabelaFreteRedespacho.val, valueUpdate: 'afterkeydown', attr: { maxlength: TabelaFreteRedespacho.maxlength, id : TabelaFreteRedespacho.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TabelaFreteRedespacho.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12" data-bind="visible: PagarPorToneladaRedespacho.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: PagarPorToneladaRedespacho.val, attr: { maxlength: PagarPorToneladaRedespacho.maxlength, name: PagarPorToneladaRedespacho.id, id: PagarPorToneladaRedespacho.id }">
                                <label class="custom-control-label" data-bind="text: PagarPorToneladaRedespacho.text, attr: { for: PagarPorToneladaRedespacho.id }"></label>
                            </div>
                        </div>
                    </div>
                    <footer>
                        <div class="row">
                            <div class="col-6">
                                <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                                    <i class="fal fa-ban"></i>
                                    <span data-bind="text: Cancelar.text"></span>
                                </button>
                            </div>
                            <div class="col-6 d-flex justify-content-end">
                                <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                                    <i class="fal fa-trash"></i>
                                    <span data-bind="text: Excluir.text"></span>
                                </button>
                                <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                    <i class="fal fa-check"></i>
                                    <span data-bind="text: Atualizar.text"></span>
                                </button>
                                <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                    <span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                        </div>
                    </footer>
                </div>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/tabelaFreteTipoOperacao"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadTabelaFreteTipoOperacao();
    });

</script>

