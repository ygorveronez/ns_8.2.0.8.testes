</div>

<div class="modal fade" id="divModalLicitacaoParticipacao" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Participar da Licitação</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="card" id="knockoutResumoLicitacaoParticipacao">
                    <div class="card-body">
                        <div class="row">
                            <section class="col col-md-6 mb-3">
                                <label><b data-bind="text: Numero.text"></b> <span data-bind="text: Numero.val"></span></label>
                            </section>
                            <section class="col col-md-6 mb-3">
                                <label><b data-bind="text: Descricao.text"></b> <span data-bind="text: Descricao.val"></span></label>
                            </section>
                            <section class="col col-md-6 mb-3">
                                <label><b data-bind="text: Validade.text"></b> <span data-bind="text: Validade.val"></span></label>
                            </section>
                            <section data-bind="visible: Situacao.visible" class="col col-md-6 mb-3">
                                <label><b data-bind="text: Situacao.text"></b> <span data-bind="text: Situacao.val"></span></label>
                            </section>
                        </div>
                    </div>
                </div>

                <div id="knockoutEtapaLicitacaoParticipacao">
                    <ul class="nav nav-steps">
                        <li data-target="#step1" data-bind="visible : EtapaInscricao.visible">
                            <button class="active" data-bs-target="#inscricao" data-bs-toggle="tab" data-bind="attr: {id : EtapaInscricao.idTab }, click : EtapaInscricao.eventClick, visible : EtapaInscricao.visible">
                                <span class="step" data-bind="attr: {'data-content' : EtapaInscricao.tooltip, 'data-original-title' : EtapaInscricao.tooltipTitle }">
                                    <i class="fal fa-file-alt"></i>
                                </span>
                                <span class="title" data-bind="text: EtapaInscricao.text"></span>
                            </button>
                        </li>
                        <li data-target="#step2" data-bind="visible : EtapaOferta.visible">
                            <button data-bs-target="#oferta" data-bs-toggle="tab" data-bind="attr: {id : EtapaOferta.idTab }, click : EtapaOferta.eventClick">
                                <span class="step" data-bind="attr: {'data-content' : EtapaOferta.tooltip, 'data-original-title' : EtapaOferta.tooltipTitle }">
                                    <i class="fal fa-usd-circle"></i>
                                </span>
                                <span class="title" data-bind="text: EtapaOferta.text"></span>
                            </button>
                        </li>
                        <li data-target="#step3" data-bind="visible : EtapaRetornoOferta.visible">
                            <button data-bs-target="#retornoOferta" data-bs-toggle="tab" data-bind="attr: {id : EtapaRetornoOferta.idTab }, click : EtapaRetornoOferta.eventClick">
                                <span class="step" data-bind="attr: {'data-content' : EtapaRetornoOferta.tooltip, 'data-original-title' : EtapaRetornoOferta.tooltipTitle }">
                                    <i class="fal fa-file-check"></i>
                                </span>
                                <span class="title" data-bind="text: EtapaRetornoOferta.text"></span>
                            </button>
                        </li>
                    </ul>
                </div>

                <div class="tab-content">
                    <!-- Inscrição -->
                    <div id="inscricao" class="tab-pane fade show active">
                        <div class="panel mt-3" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
                            <div class="panel-container">
                                <div class="panel-content">
                                    <ul class="nav nav-tabs">
                                        <li class="nav-item active">
                                            <a href="#knockoutLicitacaoParticipacaoInscricao" id="abaLicitacaoParticipacaoInscricao" class="nav-link active" data-bs-toggle="tab">
                                                <span class="hidden-mobile hidden-tablet">Inscrição</span>
                                            </a>
                                        </li>
                                        <li class="nav-item" id="tabLicitacaoParticipacaoInscricaoAnexo">
                                            <a href="#knockoutLicitacaoParticipacaoAnexo" class="nav-link" data-bs-toggle="tab">
                                                <span class="hidden-mobile hidden-tablet">Anexos</span>
                                            </a>
                                        </li>
                                    </ul>
                                    <div class="tab-content border border-top-0 p-3">
                                        <!-- Inscrição -->
                                        <div id="knockoutLicitacaoParticipacaoInscricao" class="tab-pane fade show active">
