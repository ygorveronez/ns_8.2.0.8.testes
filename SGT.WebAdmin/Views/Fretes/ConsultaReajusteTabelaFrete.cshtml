
<section>
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Fretes</li>
        <li class="breadcrumb-item active">Consulta Reajuste Tabela Frete </li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-file-alt"></i>  Consulta de Reajustes de Tabela de Frete
        </h1>
        <div class="subheader-help">
            <button type="button" onclick="iniciarAjuda(960)" data-action="toggle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Abrir a ajuda da página" aria-label="Abrir a ajuda da página"><i class="fal fa-question-circle"></i></button>
        </div>
    </div>

    <div id="knockoutPesquisaReajustes">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-hdr">
                    <h3><i class="fal fa-search"></i> Pesquisar Reajustes</h3>
                </div>
                <div class="panel-content">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Tabela.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Tabela.text, attr: { for: Tabela.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Tabela.requiredClass, value: Tabela.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Tabela.text, maxlength: Tabela.maxlength, id: Tabela.id, name: Tabela.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Tabela.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Transportador.text, maxlength: Transportador.maxlength, id: Transportador.id, name: Transportador.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Transportador.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoOperacao.text, maxlength: TipoOperacao.maxlength, id: TipoOperacao.id, name: TipoOperacao.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoOperacao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: EtapaAjuste.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EtapaAjuste.text, attr: { for: EtapaAjuste.id }"></label>
                                    <select class="form-control" data-bind="options: EtapaAjuste.options, optionsText: 'text', optionsValue: 'value', value: EtapaAjuste.val, attr: { id : EtapaAjuste.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: EtapaAjuste.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EtapaAutorizacao.text, attr: { for: EtapaAutorizacao.id }"></label>
                                    <select class="form-control" data-bind="options: EtapaAutorizacao.options, optionsText: 'text', optionsValue: 'value', value: EtapaAutorizacao.val, attr: { id : EtapaAutorizacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Usuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Usuario.text, maxlength: Usuario.maxlength, id: Usuario.id, name: Usuario.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Usuario.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: DataInicio.requiredClass,value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: DataFim.requiredClass,value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                </div>
                            </div>
                        </div>

                    </form>
                    <div class="d-flex justify-content-end">
                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel">
            <div class="panel-container">
                <div class="panel-hdr">
                    <h3>Reajustes</h3>
                </div>
                <div class="panel-content">
                    <div class="d-flex justify-content-end mb-2">
                        <button class="btn btn-primary" type="button" data-bind="click: SelecionarTodos.eventClick, text: SelecionarTodos.text, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible"></button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>

                    <div class="d-flex justify-content-between mt-3">
                        <div class="d-flex">

                        </div>
                        <div class="d-flex justify-content-end">
                            <div class="m-1">
                                <button class="btn btn-success" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id: AprovarTodas.id }, visible: AprovarTodas.visible">
                                    <i class="fal fa-check"></i><span data-bind="text: AprovarTodas.text"></span>
                                </button>
                            </div>
                            <div class="m-1">
                                <button class="btn btn-default" type="button" data-bind="click: ExportarDetalhes.eventClick, attr: { id: ExportarDetalhes.id }, visible: ExportarDetalhes.visible">
                                    <span data-bind="text: ExportarDetalhes.text"></span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>




<div id="divModalAjusteTabela" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Informações do Ajuste</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <ul class="nav nav-tabs">
                    <li class="nav-item">
                        <a href="#knockoutAjusteTabela" class="nav-link active" data-bs-toggle="tab">
                            <i class="fal fa-info-circle"></i><span class="hidden-mobile hidden-tablet"> Ajuste Tabela</span>
                        </a>
                    </li>
                    <li id="liVigenciaAnexo" class="nav-item">
                        <a href="#knockoutVigenciaAnexo" class="nav-link" data-bs-toggle="tab">
                            <i class="fal fa-info-file"></i><span class="hidden-mobile hidden-tablet"> Anexos</span>
                        </a>
                    </li>
                    <li style="display:none" id="liAlteracoes" class="nav-item">
                        <a href="#knockoutAlteracoes" class="nav-link" data-bs-toggle="tab">
                            <i class="fal fa-list"></i><span class="hidden-mobile hidden-tablet"> Alterações</span>
                        </a>
                    </li>
                    <li id="liAutorizacao" class="nav-item">
                        <a href="#knockoutAutorizacao" class="nav-link" data-bs-toggle="tab">
                            <i class="fal fa-check"></i><span class="hidden-mobile hidden-tablet"> Autorização</span>
                        </a>
                    </li>
                </ul>
                <div class="tab-content border border-top-0 p-3">
                    <div id="knockoutAjusteTabela" class="tab-pane fade active show">
                        <h3>Detalhes do Ajuste</h3>
                        <div class="row row-cols-1 row-cols-md-2">
                            <div class="col">
                                <table class="table table-clean table-sm align-self-end">
                                    <tbody>
                                        <tr data-bind="visible: DescricaoTabelaFrete.visible">
                                            <td>
                                                <strong data-bind="text: DescricaoTabelaFrete.text"></strong>
                                            </td>
                                            <td>
                                                <span data-bind="text: DescricaoTabelaFrete.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: DataCriacao.visible">
                                            <td>
                                                <strong data-bind="text: DataCriacao.text"></strong>
                                            </td>
                                            <td>
                                                <span data-bind="text: DataCriacao.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: DataAjuste.visible">
                                            <td>
                                                <strong data-bind="text: DataAjuste.text"></strong>
                                            </td>
                                            <td>
                                                <span data-bind="text: DataAjuste.val"></span>
                                            </td>
                                        </tr>
                                        <tr data-bind="visible: Situacao.visible">
                                            <td>
                                                <strong data-bind="text: Situacao.text"></strong>
                                            </td>
                                            <td>
                                                <span data-bind="text: Situacao.val"></span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong data-bind="text: Etapa.text"></strong>
                                            </td>
                                            <td>
                                                <span data-bind="text: Etapa.val"></span>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>

                    <div id="knockoutVigenciaAnexo" class="tab-pane fade">
                        <h3>Anexos </h3>
                        <div class="row">
                            <div class="col-12">
                                <b data-bind="text: Motivo.text"></b> <span data-bind="text: Motivo.val"></span>
                            </div>
                            <div class="col-12">
                                <b data-bind="text: Observacao.text"></b> <span data-bind="text: Observacao.val"></span>
                            </div>
                        </div>

                        <table width="100%" class="table table-bordered table-hover dataTable no-footer" cellspacing="0" id="grid-vigencia-anexo"></table>
                    </div>

                    <div id="knockoutAlteracoes" class="tab-pane fade">
                        <h3>Alterações</h3>
                        <div class="row">
                            <div data-bind="visible: Preview.visible">
                                <table width="100%" class="table table-bordered table-hover dataTable no-footer" data-bind="attr: { id: Preview.idGrid}" cellspacing="0"></table>
                            </div>
                            <div class="d-flex justify-content-end mt-3">
                                <button class="btn btn-primary me-2" type="button" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id : GerarRelatorioPDF.id}">
                                    <span data-bind="text: GerarRelatorioPDF.text"></span>
                                </button>
                                <button class="btn btn-default me-2" type="button" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id : GerarRelatorioExcel.id}">
                                    <span data-bind="text: GerarRelatorioExcel.text"></span>
                                </button>
                                <button class="btn btn-primary " type="button" data-bind="click: Preview.eventClick, attr: { id : Preview.id}">
                                    <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                                </button>
                            </div>
                        </div>
                        <div class="hide">
                            <div id="divConfigRelatorio">
                            </div>
                        </div>


                    </div>


                    <div id="knockoutAutorizacao" class="tab-pane fade">
                        <h3>Regras</h3>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: MotivoRejeicao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: MotivoRejeicao.text, attr: { for: MotivoRejeicao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: MotivoRejeicao.requiredClass, value: MotivoRejeicao.val, valueUpdate: 'afterkeydown', attr: { maxlength: MotivoRejeicao.maxlength, id : MotivoRejeicao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: MotivoRejeicao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: Motivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                                    <textarea class="form-control" rows="4" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id }"></textarea>
                                </div>
                            </div>
                            <div class="d-flex justify-content-end mt-3" data-bind="visible: Rejeitar.visible">
                                <button class="btn btn-default me-2" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                                    <i class="fal fa-times"></i><span data-bind="text: Cancelar.text"></span>
                                </button>
                                <button class="btn btn-danger" type="button" data-bind="click: Rejeitar.eventClick, attr: { id : Rejeitar.id}">
                                    <i class="fal fa-ban"></i><span data-bind="text: Rejeitar.text"></span>
                                </button>
                            </div>
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id : Regras.idGrid}"></table>
                            <footer data-bind="visible: AprovarTodas.visible">
                                <button class="btn btn-success me-2 justify-content-end" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id : AprovarTodas.id}">
                                    <i class="fal fa-check"></i><span data-bind="text: AprovarTodas.text"></span>
                                </button>
                            </footer>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="divModalExportarDetalhes" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" id="knockoutTabelaFreteAlteracoesSelecionadas">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Valores alterados da Tabela de Frete</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table width="100%" class="table table-bordered table-hover" id="grid-valores-selecionados-alterados-tabelas" cellspacing="0"></table>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-success me-1" type="button" data-bind="click: GerarPlanilhaExcel.eventClick, attr: { id: GerarPlanilhaExcel.id}">
                        <i class="fal fa-download"></i><span data-bind="text: GerarPlanilhaExcel.text"></span>
                    </button>
                    <button class="btn btn-primary me-1" type="button" data-bind="click: Preview.eventClick, attr: { id: Preview.id}">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>



