

<style>
    #grid-fechamento-pontuacao-transportador-detalhes_wrapper .dt-toolbar {
        display: none;
    }

    #knockoutEtapaFechamentoPontuacao .col {
        float: left;
    }

    table.dataTable.margin-top-0 {
        margin-top: 0 !important;
    }

    .padding-top-30 {
        padding-top: 30px !important;
    }
</style>

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Fretes</li>
    <li class="breadcrumb-item active">Fechamento Pontuação</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Fechamento de Pontuação
    </h1>
</div>

<div id="knockoutPesquisaFechamentoPontuacao">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Fechamentos de Pontuação
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: Numero.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Numero.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Numero.val, attr: { maxlength: Numero.maxlength, id: Numero.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Mes.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Mes.text"></label>
                                    <select class="form-control" data-bind="options: Mes.options, optionsText: 'text', optionsValue: 'value', value: Mes.val, attr: { id : Mes.id}, event: {change: Mes.eventChange}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Ano.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ano.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Ano.val, attr: { maxlength: Ano.maxlength, id: Ano.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}, event: {change: Situacao.eventChange}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Fechamento de Pontuação
            </h3>
        </div>
        <div class="panel-content">
            <div id="knockoutFechamentoPontuacaoEtapa">
                <ul class="nav nav-steps">
                    <li data-target="#step1" data-bind="visible : EtapaDadosFechamentoPontuacao.visible">
                        <button class="active" data-bs-target="#dadosFechamentoPontuacao" data-bs-toggle="tab" data-bind="attr: {id : EtapaDadosFechamentoPontuacao.idTab }, click : EtapaDadosFechamentoPontuacao.eventClick, visible : EtapaDadosFechamentoPontuacao.visible">
                            <span class="step" data-bind="attr: {'data-content' : EtapaDadosFechamentoPontuacao.tooltip, 'data-original-title' : EtapaDadosFechamentoPontuacao.tooltipTitle }">
                                <i class="fal fa-file-alt"></i>
                            </span>
                            <span class="title" data-bind="text: EtapaDadosFechamentoPontuacao.text"></span>
                        </button>
                    </li>
                    <li data-target="#step2" data-bind="visible : EtapaPontuacaoTransportadores.visible">
                        <button data-bs-target="#pontuacaoTransportadores" data-bs-toggle="tab" data-bind="attr: {id : EtapaPontuacaoTransportadores.idTab }, click : EtapaPontuacaoTransportadores.eventClick">
                            <span class="step" data-bind="attr: {'data-content' : EtapaPontuacaoTransportadores.tooltip, 'data-original-title' : EtapaPontuacaoTransportadores.tooltipTitle }">
                                <i class="fal fa-chart-line"></i>
                            </span>
                            <span class="title" data-bind="text: EtapaPontuacaoTransportadores.text"></span>
                        </button>
                    </li>
                </ul>
            </div>

            <div class="tab-content" id="conteudoTransacao">
                <!-- Dados do Fechamento -->
                <div id="dadosFechamentoPontuacao" class="tab-pane fade show active">

                    <div class="col-12 col-md-12">
                        <div class="card">
                            <div class="card-header d-flex align-items-center justify-content-between">
                                <h3 class="card-title">Dados do Fechamento de Pontuação</h3>

                            </div>
                            <div class="card-body">
                                <div class="row" id="knockoutFechamentoPontuacao">
                                    <div class="col-12 col-md-3" data-bind="visible: Numero.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Numero.text"></label>
                                            <input type="text" class="form-control" data-bind="enable: Numero.enable, value: Numero.val, attr: { maxlength: Numero.maxlength, id: Numero.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-3" data-bind="visible: Mes.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Mes.text, attr: { for: Mes.id }"></label>
                                            <select class="form-control" data-bind="enable: Mes.enable, options: Mes.options, optionsText: 'text', optionsValue: 'value', value: Mes.val, attr: { id : Mes.id}, event: {change: Mes.eventChange}"></select>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-3" data-bind="visible: Ano.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Ano.text"></label>
                                            <input type="text" class="form-control" data-bind="enable: Ano.enable, value: Ano.val, attr: { maxlength: Ano.maxlength, id: Ano.id}" />
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <!-- Pontuação dos Transportadores -->
                <div id="pontuacaoTransportadores" class="tab-pane fade">
                    
                    <div class="col-12 col-md-12">
                        <div class="card">
                            <div class="card-header d-flex align-items-center justify-content-between">
                                <h3 class="card-title">Pontuação dos Transportadores</h3>
                            </div>
                            <div class="card-body">
                                <div class="row" id="knockoutFechamentoPontuacaoTransportador">
                                    <div class="col col-md-12" data-bind="visible: AguardandoFinalizacao.val">
                                        <div class="alert alert-info margin-bottom-0">
                                            <i class="fal fa-spinner fa-lg fa-spin fa-pulse"> </i>
                                            <span> Aguarde, realizando o fechamento de pontuação dos transportadores</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <div id="knockoutCRUDCadastroFechamentoPontuacao">
                <div class="row mt-2">
                    <div class="col-6">
                        <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                            <i class="fal fa-ban"></i>
                            <span data-bind="text: Cancelar.text"></span>
                        </button>
                    </div>
                    <div class="col-6 d-flex justify-content-end">
                        <button type="button" class="btn btn-labeled btn-default me-2" data-bind="click: Limpar.eventClick, visible: Limpar.visible, attr: { value: Limpar.text, id : Limpar.id}">
                            <span data-bind="text: Limpar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </div>
            </div> 
        </div>
    </div>
</div>



<div class="modal fade" id="divModalDetalhesPontuacao" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" id="knockoutRejeicaoRequisicaoMercadoria" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalhes da Pontuação</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutFechamentoPontuacaoTransportadorDetalhes">
                <div class="row">
                    <section class="col col-md-12 mb-3">
                        <label><b data-bind="text: Pontuacao.text"></b></label>
                        <label><span data-bind="text: Pontuacao.val"></span></label>
                    </section>
                </div>
                <table width="100%" class="table table-bordered table-hover dataTable no-footer" id="grid-fechamento-pontuacao-transportador-detalhes" cellspacing="0"></table>
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/fechamentoPontuacao"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadFechamentoPontuacao();
    });

</script>

