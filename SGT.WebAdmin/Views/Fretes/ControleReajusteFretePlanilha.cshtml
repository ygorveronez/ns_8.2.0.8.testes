
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Fretes</li>
    <li class="breadcrumb-item active">Controle Reajuste Frete Planilha</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Controle de Reajuste de Frete por Planilha
    </h1>
</div>

<div id="knockoutPesquisaControleReajusteFretePlanilha">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Controle de Reajuste de Frete por Planilha
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: Numero.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Numero.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Numero.val, attr: { maxlength: Numero.maxlength, id: Numero.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-8" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: SituacaoControleReajusteFretePlanilha.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoControleReajusteFretePlanilha.text, attr: { for: SituacaoControleReajusteFretePlanilha.id }"></label>
                                    <select class="form-control" data-bind="options: SituacaoControleReajusteFretePlanilha.options, optionsText: 'text', optionsValue: 'value', value: SituacaoControleReajusteFretePlanilha.val, attr: { id : SituacaoControleReajusteFretePlanilha.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Empresa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Empresa.maxlength, id : Empresa.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Empresa.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="wid-id-4" class="panel" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Controle de Reajuste de Frete por Planilha
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutEtapaControleReajusteFretePlanilha">
                    <ul class="nav nav-steps">
                        <li data-target="#step1" data-bind="visible : Etapa1.visible">
                            <button class="active" data-bs-target="#knockoutControleReajusteFretePlanilha" data-bs-toggle="tab" data-bind="attr: {id : Etapa1.idTab }, click : Etapa1.eventClick, visible : Etapa1.visible">
                                <span class="step" data-bind="attr: {'data-content' : Etapa1.tooltip, 'data-original-title' : Etapa1.tooltipTitle }">
                                    <i class="fal fa-file-alt"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa1.text"></span>
                            </button>
                        </li>
                        <li data-target="#step2" data-bind="visible : Etapa2.visible">
                            <button data-bs-target="#knockoutAprovacao" data-bs-toggle="tab" data-bind="attr: {id : Etapa2.idTab }, click : Etapa2.eventClick">
                                <span class="step" data-bind="attr: {'data-content' : Etapa2.tooltip, 'data-original-title' : Etapa2.tooltipTitle }">
                                    <i class="fal fa-hourglass-half"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa2.text"></span>
                            </button>
                        </li>
                        <li data-target="#step2" data-bind="visible : Etapa3.visible">
                            <button data-bs-target="#knockoutFinalizacao" data-bs-toggle="tab" data-bind="attr: {id : Etapa3.idTab }, click : Etapa3.eventClick">
                                <span class="step" data-bind="attr: {'data-content' : Etapa3.tooltip, 'data-original-title' : Etapa3.tooltipTitle }">
                                    <i class="fal fa-hourglass-end"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa3.text"></span>
                            </button>
                        </li>
                    </ul>
                </div>
                <div class="tab-content">
                    <!-- Planilhas Reajuste  -->
                    <div id="knockoutControleReajusteFretePlanilha" class="tab-pane fade show active">
                        <div class="card mb-3">
                            <div class="card-header">
                                <h3 class="card-title">Controle de Reajuste de Frete por Planilha</h3>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12 col-md-2" data-bind="visible: Numero.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Numero.text"></label>
                                            <input type="text" class="form-control" data-bind="value: Numero.val, enable: Numero.enable, attr: { maxlength: Numero.maxlength, id : Numero.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-4" data-bind="visible: Filial.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}, enable: Filial.enable" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6" data-bind="visible: TipoOperacao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}, enable: TipoOperacao.enable" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6" data-bind="visible: Empresa.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Empresa.maxlength, id : Empresa.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Empresa.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div data-bind="visible: Planilha.visible" class="col col-md-6">
                                        <div class="row">
                                            <div class="form-group col-12 col-md-11">
                                                <p class="form-label" data-bind="text: Planilha.text"></p>
                                                <div class="custom-file" data-bind="visible: Planilha.visible">
                                                    <input type="file" class="custom-file-input" data-bind="value: Planilha.val, enable: Planilha.enable, attr: { id: Planilha.id }" readonly>
                                                    <label class="custom-file-label" data-bind="text: Planilha.val, attr: { for: Planilha.id }">Selecione um arquivo para anexar</label>
                                                </div>
                                            </div>
                                            <div class="input-group-btn col-12 col-md-1 mt-5">
                                                <button class="btn btn-sm btn-default" data-bind="click: DownloadPlanilha.eventClick, attr: { id: DownloadPlanilha.id}">
                                                    <i class="fal fa-download"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                                        <textarea class="form-control" rows="5" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id }, enable: Observacao.enable"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Aprovação -->
                    <div id="knockoutAprovacao" class="tab-pane fade">
                        <div class="row" data-bind="visible: DetalheAprovadores.visible">
                            <div class="col-12 col-md-6">
                                <div class="card mb-3" data-bind="visible: PossuiRegras.val">
                                    <div class="card-header">
                                        <h3 class="card-title">Detalhes</h3>
                                    </div>
                                    <div class="card-body">
                                        <div class="row">
                                            <section data-bind="visible: Solicitante.visible" class="col col-md-12 mb-3">
                                                <label><b data-bind="text: Solicitante.text"></b> <span data-bind="text: Solicitante.val"></span></label>
                                            </section>
                                            <section data-bind="visible: DataSolicitacao.visible" class="col col-md-12 mb-3">
                                                <label><b data-bind="text: DataSolicitacao.text"></b> <span data-bind="text: DataSolicitacao.val"></span></label>
                                            </section>
                                            <section data-bind="visible: AprovacoesNecessarias.visible" class="col col-md-12 mb-3">
                                                <label><b data-bind="text: AprovacoesNecessarias.text"></b> <span data-bind="text: AprovacoesNecessarias.val"></span></label>
                                            </section>
                                            <section data-bind="visible: Aprovacoes.visible" class="col col-md-12 mb-3">
                                                <label><b data-bind="text: Aprovacoes.text"></b> <span data-bind="text: Aprovacoes.val"></span></label>
                                            </section>
                                            <section data-bind="visible: Reprovacoes.visible" class="col col-md-12 mb-3">
                                                <label><b data-bind="text: Reprovacoes.text"></b> <span data-bind="text: Reprovacoes.val"></span></label>
                                            </section>
                                            <section data-bind="visible: Situacao.visible" class="col col-md-12 mb-3">
                                                <label><b data-bind="text: Situacao.text"></b> <span data-bind="text: Situacao.val"></span></label>
                                            </section>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6">
                                <div class="card" data-bind="visible: PossuiRegras.val">
                                    <div class="card-body">
                                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Regras.idGrid}" cellspacing="0"></table>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: !PossuiRegras.val()">
                                <div class="alert alert-warning">
                                    <span>
                                        Não foi encontrada nenhuma regra para aprovar o reajuste.<br />
                                        A solicitação ficará pendente até existir alguma regra de aprovação. Clique em reprocessar regras para verificar.
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Finalização -->
                    <div class="tab-pane" id="knockoutFinalizacao">
                        <div class="card mb-3">
                            <div class="card-header">
                                <h3 class="card-title">Autorização do Reajuste</h3>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-6">
                                        <button type="button" class="btn btn-labeled btn-success" data-bind="click: ConfirmarFinalizacao.eventClick, visible: ConfirmarFinalizacao.visible, text: ConfirmarFinalizacao.text, attr: { id: ConfirmarFinalizacao.idBtnSearch}">
                                            <span data-bind="text: ConfirmarFinalizacao.text"></span>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>

            <footer id="knockoutCRUD">
                <div class="row">
                    <div class="col-6">
                        <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                            <i class="fal fa-ban"></i>
                            <span data-bind="text: Cancelar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-default" data-bind="click: Limpar.eventClick, visible: Limpar.visible, attr: { value: Limpar.text, id : Limpar.id}">
                            <span data-bind="text: Limpar.text"></span>
                        </button>
                    </div>
                    <div class="col-6 d-flex justify-content-end">
                        <button type="button" class="btn btn-labeled btn-warning mx-2" data-bind="click: Reprocessar.eventClick, visible: Reprocessar.visible, attr: { value: Reprocessar.text, id : Reprocessar.id}">
                            <span data-bind="text: Reprocessar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </div>
            </footer>
        </div>


    </div>
</div>

<!------------------------------->

<div id="divModalDetalhesAutorizacao" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalhes da Autorização</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="widget-body no-padding">
                    <div class="padding-10 no-padding-bottom" id="knockoutDetalheAutorizacao">
                        <fieldset class="smart-form">
                            <section data-bind="visible: Regra.visible" class="col col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                <label><b data-bind="text: Regra.text"></b> <span data-bind="text: Regra.val"></span></label>
                            </section>
                            <section data-bind="visible: Situacao.visible" class="col col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <label><b data-bind="text: Situacao.text"></b> <span data-bind="text: Situacao.val"></span></label>
                            </section>

                            <section data-bind="visible: Usuario.visible" class="col col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                <label><b data-bind="text: Usuario.text"></b> <span data-bind="text: Usuario.val"></span></label>
                            </section>
                            <section data-bind="visible: Data.visible && Data.val() != ''" class="col col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <label><b data-bind="text: Data.text"></b> <span data-bind="text: Data.val"></span></label>
                            </section>
                            <section data-bind="visible: Motivo.visible && Motivo.val() != ''" class="col col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <label><b data-bind="text: Motivo.text"></b> <span data-bind="text: Motivo.val"></span></label>
                            </section>
                        </fieldset>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/controleReajusteFretePlanilha"></script>
