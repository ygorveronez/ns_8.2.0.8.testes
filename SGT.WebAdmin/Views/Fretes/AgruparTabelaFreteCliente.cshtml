
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Fretes</li>
    <li class="breadcrumb-item active">Agrupar Tabela Frete Cliente</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class="fal fa-file-alt"></i>
        Agrupar Tabela de Fretes
    </h1>
</div>

<div id="knockoutAgruparTabelaFreteCliente">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Agrupar Tabela de Fretes
                </h3>
            </div>
            <div class="panel-content">
                <div class="mt-3">
                    <form>
                        <div class="row">

                            <div class="col-12 col-md-4" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text, attr: { for: CodigoIntegracao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-8">
                                <div>
                                    <button data-bind="click: Buscar.eventClick, attr: { id: Buscar.id}, visible: Buscar.visible" class="btn btn-primary botaoDentroSmartAdmimForm" type="button" style="margin-top: 27px;">
                                        <span data-bind="text: Buscar.text"></span>
                                    </button>

                                </div>
                            </div>

                            <div class="d-flex justify-content-end mb-2">
                                <button data-bind="click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible" class="btn btn-default botaoDentroSmartAdmimForm" style="float:right" type="button">
                                    <span data-bind="text: SelecionarTodos.text"></span>
                                </button>
                            </div>

                            <div class="col-12 col-md-12 mb-2">
                                <table width="100%" class="table table-bordered table-hover dataTable no-footer" data-bind="attr: { id: TabelaFrete.idGrid}" cellspacing="0"></table>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex justify-content-end">
                        <input type="button" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}" class="btn btn-labeled btn-success" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/agruparTabelaFreteCliente"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadAgruparTabelaFreteCliente();
    });

</script>

