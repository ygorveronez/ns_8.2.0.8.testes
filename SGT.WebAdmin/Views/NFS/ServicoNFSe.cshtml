
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">NFS</li>
    <li class="breadcrumb-item active"> Serviços da NFS-e</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Serviços da NFS-e
    </h1>
</div>

<div id="knockoutPesquisaServicoNFSe">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Serviços da NFS-e
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-12" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-8" data-bind="visible: Localidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Localidade.text, attr: { for: Localidade.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Localidade.requiredClass, value: Localidade.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Localidade.text, maxlength: Localidade.maxlength, id: Localidade.id, name: Localidade.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Localidade.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Serviços da NFS-e
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroServicoNFSe">
                    <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-6" data-bind="visible: Localidade.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Localidade.text, attr: { for: Localidade.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Localidade.requiredClass, value: Localidade.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Localidade.text, maxlength: Localidade.maxlength, id: Localidade.id, name: Localidade.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Localidade.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Numero.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Numero.text, attr: { for: Numero.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Numero.requiredClass, value: Numero.val, valueUpdate: 'afterkeydown', attr: { maxlength: Numero.maxlength, id : Numero.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: CNAE.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CNAE.text, attr: { for: CNAE.id }"></label>
                            <input type="text" class="form-control" data-bind="css: CNAE.requiredClass,value: CNAE.val, valueUpdate: 'afterkeydown', attr: { maxlength: CNAE.maxlength, id : CNAE.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: NBS.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NBS.text, attr: { for: NBS.id }"></label>
                            <input type="text" class="form-control" data-bind="css: NBS.requiredClass,value: NBS.val, valueUpdate: 'afterkeydown', attr: { maxlength: NBS.maxlength, id : NBS.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: CodigoTributacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoTributacao.text, attr: { for: CodigoTributacao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: CodigoTributacao.requiredClass,value: CodigoTributacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoTributacao.maxlength, id : CodigoTributacao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                        </div>
                    </div>
                    <div class="d-flex justify-content-between mt-2">
                        <div class="d-flex">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, text: Cancelar.text, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="d-flex justify-content-end">
                            <button class="btn btn-danger me-2" type="button" data-bind="visible: Excluir.visible, click: Excluir.eventClick, attr: { id: Excluir.id}">
                                <i class="fal fa-trash"></i><span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, text: Atualizar.text, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i><span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                                <i class="fal fa-spinner"></i><span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<script src="~/scripts/servicoNFSe"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadServicoNFSe();
    });

</script>

