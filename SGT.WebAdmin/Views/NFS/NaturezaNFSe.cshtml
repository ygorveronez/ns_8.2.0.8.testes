
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Notas Fiscais</li>
    <li class="breadcrumb-item active"> Natureza  NFSe</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i>  Naturezas da NFS-e
    </h1>
</div>

<div id="knockoutPesquisaNaturezaNFSe">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Naturezas da NFS-e
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-12" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, attr: { maxlength: Descricao.maxlength, id : Descricao.id} , valueUpdate: 'afterkeydown'" />
                                </div>
                            </div>

                            <div class="col-12 col-md-8">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Localidade.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: Localidade.val, attr: { maxlength: Localidade.maxlength, id : Localidade.id} , valueUpdate: 'afterkeydown',  class: Localidade.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Localidade.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible : Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>



<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Natureza da NFS-e
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutCadastroNaturezaNFSe">
                <div class="row" >
                    <div class="col-12 col-md-12" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="value: Descricao.val,class: Descricao.requiredClass, attr: { maxlength: Descricao.maxlength, id : Descricao.id} , valueUpdate: 'afterkeydown'" />
                        </div>
                    </div>

                    <div class="col-12 col-md-6">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Localidade.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: Localidade.val, attr: { maxlength: Localidade.maxlength, id : Localidade.id} , valueUpdate: 'afterkeydown', class: Localidade.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Localidade.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible : Numero.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Numero.text"></label>
                            <input type="text" class="form-control" data-bind="value: Numero.val,class: Numero.requiredClass, attr: { maxlength: Numero.maxlength, id : Numero.id}, enable : Numero.enable " />
                        </div>
                    </div>


                    <div class="col-12 col-md-3"  data-bind="visible : Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-6" data-bind="visible: IntegracaoMigrateNFSeNatureza.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: IntegracaoMigrateNFSeNatureza.text, attr: { for: IntegracaoMigrateNFSeNatureza.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: IntegracaoMigrateNFSeNatureza.requiredClass, value: IntegracaoMigrateNFSeNatureza.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': IntegracaoMigrateNFSeNatureza.text, maxlength: IntegracaoMigrateNFSeNatureza.maxlength, id: IntegracaoMigrateNFSeNatureza.id, name: IntegracaoMigrateNFSeNatureza.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: IntegracaoMigrateNFSeNatureza.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <footer>
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger me-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>




<script src="~/scripts/naturezaNFSe"></script>
<script type="text/javascript">
    pageSetUp();

    _CONFIGURACAO_TMS = $.extend({}, _CONFIGURACAO_TMS, @Html.Raw(ViewBag.ConfiguracaoNFS));

    $(document).ready(function () {
        loadNaturezaNFSe();
    });

</script>

