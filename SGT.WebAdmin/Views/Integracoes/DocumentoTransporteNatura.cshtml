<div id="knockoutImportacaoEDI" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Importação de NOTFIS</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-md-12" data-bind="visible: Empresa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Empresa.maxlength, id : Empresa.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Empresa.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th style="width: 40%">
                                        <strong>Arquivo</strong>
                                    </th>
                                    <th style="width: 20%">
                                        <strong>Tamanho</strong>
                                    </th>
                                    <th style="width: 40%">
                                        <strong>Situação</strong>
                                    </th>
                                </tr>
                            </thead>

                            <tbody data-bind="foreach: Arquivos">
                                <tr data-bind="css: Class">
                                    <td data-bind="text: Descricao"></td>
                                    <td data-bind="text: Tamanho"></td>
                                    <!-- ko if: Enviando -->
                                    <td><i class="fal fa-spinner"></i></td>
                                    <!-- /ko -->
                                    <!-- ko ifnot: Enviando -->
                                    <td data-bind="text: Mensagem, attr: {title: Mensagem}" ></td>
                                    <!-- /ko -->
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer" data-bind="visible: Fechar.visible() || Importar.visible()">
                <div class="d-flex justify-content-end">
                    <button class="btn btn-default" type="button" data-bind="click: Fechar.eventClick, attr: { id : Fechar.id}">
                        <i data-bind="css: Fechar.icon"></i><span data-bind="text: Fechar.text"></span>
                    </button>
                    <button class="btn btn-primary ms-3" type="button" data-bind="click: Importar.eventClick, attr: { id : Importar.id}">
                        <i data-bind="css: Importar.icon"></i><span data-bind="text: Importar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>


