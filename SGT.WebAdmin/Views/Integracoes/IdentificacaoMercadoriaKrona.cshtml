
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Integrações</li>
    <li class="breadcrumb-item active">Identificação Mercadoria Krona</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Identificações de Mercadorias da Krona
    </h1>
</div>

<div id="knockoutPesquisaIdentificacaoMercadoriaKrona">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Identificações de Mercadorias da Krona
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: Identificador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Identificador.text, attr: { for: Identificador.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Identificador.requiredClass, value: Identificador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Identificador.maxlength, id : Identificador.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: IdentificadorDescricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: IdentificadorDescricao.text, attr: { for: IdentificadorDescricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: IdentificadorDescricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: IdentificadorDescricao.maxlength, id : IdentificadorDescricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Identificação de Mercadoria da Krona
            </h3>
        </div>
        <div class="panel-content">
            <form >
                <div class="row" id="knockoutIdentificacaoMercadoriaKrona" >
                    <div class="col-12 col-md-3" data-bind="visible: Identificador.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Identificador.text, attr: { for: Identificador.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Identificador.requiredClass, value: Identificador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Identificador.maxlength, id : Identificador.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-6" data-bind="visible: IdentificadorDescricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: IdentificadorDescricao.text, attr: { for: IdentificadorDescricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: IdentificadorDescricao.requiredClass, value: IdentificadorDescricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: IdentificadorDescricao.maxlength, id : IdentificadorDescricao.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDIdentificacaoMercadoriaKrona">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>


<script src="~/scripts/identificacaoMercadoriaKrona"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadIdentificacaoMercadoriaKrona();
    });

</script>