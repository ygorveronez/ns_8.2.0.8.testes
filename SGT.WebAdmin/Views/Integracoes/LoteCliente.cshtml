
<section id="widget-grid">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Integrações</li>
        <li class="breadcrumb-item active">Lote Cliente</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-file-alt"></i> Lote de Clientes
        </h1>
    </div>
    <div id="knockoutPesquisaLoteCliente" class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <div data-widget-colorbutton="false" data-widget-sortable="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-togglebutton="false">
                    <button class="btn btn-primary" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i> Pesquisar Lote de Clientes
                    </button>

                    <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade" class="mt-3">
                        <form>
                            <div class="row">

                                <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-3" data-bind="visible: DataFim.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-3" data-bind="visible: Numero.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Numero.text, attr: { for: Numero.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: Numero.requiredClass, value: Numero.val, valueUpdate: 'afterkeydown', attr: { maxlength: Numero.maxlength, id : Numero.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                        <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                    </div>
                                </div>

                            </div>
                        </form>
                        <footer class="d-flex justify-content-end mb-3">
                            <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </footer>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="wid-id-4" class="panel" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-edit"></i>
                    Lançamento do Lote de Clientes
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutEtapaLoteCliente">
                        <ul class="nav nav-steps">
                            <li data-target="#step1" data-bind="visible : Etapa1.visible">
                                <button class="active" data-bs-target="#knockoutSelecaoClientes" data-bs-toggle="tab" data-bind="attr: {id : Etapa1.idTab }, click : Etapa1.eventClick, visible : Etapa1.visible">
                                    <span class="step" data-bind="attr: {'data-content' : Etapa1.tooltip, 'data-original-title' : Etapa1.tooltipTitle }">
                                        <i class="fal fa-user"></i>
                                    </span>
                                    <span class="title" data-bind="text: Etapa1.text"></span>
                                </button>
                            </li>
                            <li data-target="#step2" data-bind="visible : Etapa2.visible">
                                <button data-bs-target="#knockoutIntegracao" data-bs-toggle="tab" data-bind="attr: {id : Etapa2.idTab }, click : Etapa2.eventClick">
                                    <span class="step" data-bind="attr: {'data-content' : Etapa2.tooltip, 'data-original-title' : Etapa2.tooltipTitle }">
                                        <i class="fal fa-link"></i>
                                    </span>
                                    <span class="title" data-bind="text: Etapa2.text"></span>
                                </button>
                            </li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <!-- Etapa Selecao Documentos -->
                        <div id="knockoutSelecaoClientes" class="tab-pane fade show active">
                            <div class="card mb-3">
                                <div class="card-header">
                                    <h3 class="card-title">Seleção de Clientes</h3>
                                </div>
                                <div class="card-body">
                                    <!-- Filtro dos Documento -->
                                    <div class="row" data-bind="visible: Filtro.visible">

                                        <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, enable: DataInicio.enable, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                            </div>
                                        </div>

                                        <div class="col-12 col-md-3" data-bind="visible: DataFim.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, enable: DataFim.enable, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                            </div>
                                        </div>

                                        <div class="col-12 col-md-3" data-bind="visible: RazaoSocial.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: RazaoSocial.text, attr: { for: RazaoSocial.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: RazaoSocial.requiredClass, value: RazaoSocial.val, valueUpdate: 'afterkeydown', attr: { maxlength: RazaoSocial.maxlength, id : RazaoSocial.id}, enable: RazaoSocial.enable" />
                                            </div>
                                        </div>

                                        <div class="col-12 col-md-3" data-bind="visible: CNPJ.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: CNPJ.text, attr: { for: CNPJ.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: CNPJ.requiredClass, value: CNPJ.val, valueUpdate: 'afterkeydown', attr: { maxlength: CNPJ.maxlength, id : CNPJ.id}, enable: CNPJ.enable" />
                                            </div>
                                        </div>

                                        <div class="col-12 col-md-3" data-bind="visible: InscricaoEstadual.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: InscricaoEstadual.text, attr: { for: InscricaoEstadual.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: InscricaoEstadual.requiredClass, value: InscricaoEstadual.val, valueUpdate: 'afterkeydown', attr: { maxlength: InscricaoEstadual.maxlength, id : InscricaoEstadual.id}, enable: InscricaoEstadual.enable" />
                                            </div>
                                        </div>

                                        <div class="col-12 col-md-3" data-bind="visible: Endereco.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: Endereco.text, attr: { for: Endereco.id }"></label>
                                                <input type="text" class="form-control" data-bind="class: Endereco.requiredClass, value: Endereco.val, valueUpdate: 'afterkeydown', attr: { maxlength: Endereco.maxlength, id : Endereco.id}, enable: Endereco.enable" />
                                            </div>
                                        </div>

                                        <div class="col-12 col-md-3" data-bind="visible: Localidade.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: Localidade.text, attr: { for: Localidade.id }"></label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control" data-bind="css: Localidade.requiredClass, value: Localidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Localidade.maxlength, id : Localidade.id}" />
                                                    <div class="input-group-append">
                                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Localidade.idBtnSearch}" type="button" aria-label="Buscar">
                                                            <i class="fal fa-search"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="d-flex justify-content-end">
                                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                                        </button>
                                    </div>
                                </div>
                            </div>

                            <div class="mb-1" data-bind="visible: SelecionarTodos.visible">
                                <div class="d-flex justify-content-end mb-2">
                                    <button class="btn btn-primary botaoDentroSmartAdmimForm" type="button" data-bind="click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}">
                                        <i class="fal fa-check"></i><span data-bind="text: SelecionarTodos.text"></span>
                                    </button>
                                </div>
                            </div>
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>

                            <div class="d-flex justify-content-end mt-3">
                                <button class="btn btn-success" type="button" data-bind="click: Criar.eventClick, visible: Criar.visible, text: Criar.text, attr: { id : Criar.id}"></button>
                            </div>
                        </div>
                        <!-- Etapa Integracao -->
                        <div id="knockoutIntegracao" class="tab-pane fade">
                            <div class="card">
                                <div class="card-header">
                                    <h3 class="card-title">Integração dos Clientes</h3>

                                </div>
                                <div class="card-body">
                                    <div id="tabIntegracoesConteudo"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div id="knockoutCRUD" class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default" type="button" data-bind="click: Limpar.eventClick, visible: Limpar.visible, text: Limpar.text, attr: { id : Limpar.id}"></button>
                </div>
            </div>
        </div>
    </div>
</section>


<script src="~/scripts/loteCliente"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLoteCliente();
    });

</script>

