
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Integrações</li>
    <li class="breadcrumb-item active"> Indicador Integração NFe</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Indicadores de Integração de NF-e   
    </h1>
</div>

<div id="knockoutPesquisaIndicadorIntegracaoNFe">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Indicadores de Integração de NF-e
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataLimite.text, attr: { for: DataLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataLimite.requiredClass, value: DataLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataLimite.maxlength, id : DataLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                   
                </div>
            </div>
        </div>
    </div>
</div>



<div class="row d-none" id="indicador-integracao-nfe-container">
    <div class="col-12 col-md-6"  id="knockoutIntegracaoAutomatica">
        <div class="card" id="wid-id-1">
            <div class="card-header d-flex align-items-center justify-content-between">
                <h3 class="card-title">Integração Automática</h3>
            </div>
            <div class="card-body">
                <div data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">

                    <div data-bind="visible: (Mensagem.val() == '')">
                        <div>
                            <span class="grafico-informacao-label">Total:</span>
                            <span class="grafico-informacao-valor" data-bind="text: TotalRegistros.val"></span>
                        </div>
                        <div id="grafico-integracao-automatica-nfe"></div>
                        <div class="grafico-exportar-excel mt-2">
                            <a href="javascript:void(0);" class="btn btn-labeled btn-success" id="exportar-grafico-integracao-automatica-nfe"><i class="fa fa-file-excel-o"></i> Exportar Excel</a>
                        </div>
                    </div>

                    <div data-bind="visible: (Mensagem.val() != '')">
                        <div class="grafico-sem-registro">
                            <span data-bind="text: Mensagem.val"></span>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>


    <div class="col-12 col-md-6" id="knockoutIntegracaoPorEmail">
        <div class="card" id="wid-id-1">
            <div class="card-header d-flex align-items-center justify-content-between">
                <h3 class="card-title">Integração por E-mail</h3>
            </div>
            <div class="card-body">
                <div data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">

                    <div data-bind="visible: (Mensagem.val() == '')">
                        <div>
                            <span class="grafico-informacao-label">Total:</span>
                            <span class="grafico-informacao-valor" data-bind="text: TotalRegistros.val"></span>
                        </div>
                        <div id="grafico-integracao-email-nfe"></div>
                        <div class="grafico-exportar-excel mt-2">
                            <a href="javascript:void(0);" class="btn btn-labeled btn-success" id="exportar-grafico-integracao-email-nfe"><i class="fa fa-file-excel-o"></i> Exportar Excel</a>
                        </div>
                    </div>
                    <div data-bind="visible: (Mensagem.val() != '')">
                        <div class="grafico-sem-registro">
                            <span data-bind="text: Mensagem.val"></span>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>
    <div class="col-12 col-md-12 mt-2" id="knockoutTabelaIntegracaoRejeitada">
        <div class="card" id="wid-id-2">
            <div class="card-header d-flex align-items-center justify-content-between">
                <h3 class="card-title">Últimas Rejeitadas</h3>
            </div>
            <div class="card-body">
                <div data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">

                    <table width="100%" class="table table-bordered table-hover table-condensed table-striped tableCursorMove" id="grid-integracao-rejeitada" cellspacing="0"></table>
                </div>

            </div>

        </div>
    </div>


</div>

<script src="~/scripts/canvg"></script>
<script src="~/scripts/d3js"></script>
<script src="~/scripts/indicadorIntegracaoNFe"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadIndicadorIntegracaoNFe();
    });

</script>

