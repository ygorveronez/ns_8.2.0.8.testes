
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Produtos</li>
    <li class="breadcrumb-item active">Marca de Produtos</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Indicadores de Integração de CT-e
    </h1>
</div>

<div id="knockoutPesquisaIndicadorIntegracaoCTe">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Indicadores de Integração de CT-e
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: DataEmissaoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoInicio.text, attr: { for: DataEmissaoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoInicio.requiredClass, value: DataEmissaoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoInicio.maxlength, id : DataEmissaoInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataEmissaoLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoLimite.text, attr: { for: DataEmissaoLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoLimite.requiredClass, value: DataEmissaoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoLimite.maxlength, id : DataEmissaoLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataIntegracaoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataIntegracaoInicio.text, attr: { for: DataIntegracaoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataIntegracaoInicio.requiredClass, value: DataIntegracaoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataIntegracaoInicio.maxlength, id : DataIntegracaoInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataIntegracaoLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataIntegracaoLimite.text, attr: { for: DataIntegracaoLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataIntegracaoLimite.requiredClass, value: DataIntegracaoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataIntegracaoLimite.maxlength, id : DataIntegracaoLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>



<div class="row d-none" id="indicador-integracao-cte-container">
    <div class="col-12 col-md-6" id="knockoutIntegracaoAutomaticaCTe">
        <div class="card" id="wid-id-1">
            <div class="card-header d-flex align-items-center justify-content-between">
                <h3 class="card-title">Integração Automática</h3>
            </div>
            <div class="card-body">
                <div data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">

                    <div data-bind="visible: (Mensagem.val() == '')">
                        <div>
                            <span class="grafico-informacao-label">Total Emitidos:</span>
                            <span class="grafico-informacao-valor" data-bind="text: TotalEmitidos.val"></span>
                        </div>
                        <div id="grafico-integracao-automatica-cte"></div>
                        <div class="grafico-exportar-excel">
                            <a href="javascript:void(0);" class="btn btn-labeled btn-success" id="exportar-grafico-integracao-automatica-cte"><i class="fa fa-file-excel-o"></i> Exportar Excel</a>
                        </div>
                    </div>
                    <div data-bind="visible: (Mensagem.val() != '')">
                        <div class="grafico-sem-registro">
                            <span data-bind="text: Mensagem.val"></span>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>


   
    <div class="col-12 col-md-6" id="knockoutCTeAguardandoIntegracao">
        <div class="card" id="wid-id-2">
            <div class="card-header d-flex align-items-center justify-content-between">
                <h3 class="card-title">CT-e Aguardando Integração</h3>
            </div>
            <div class="card-body">
                <div data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">

                    <table width="100%" class="table table-bordered table-hover table-condensed table-striped tableCursorMove" id="grid-cte-aguardando-integracao" cellspacing="0"></table>
                </div>

            </div>

        </div>
    </div>


</div>


<script src="~/scripts/canvg"></script>
<script src="~/scripts/d3js"></script>
<script src="~/scripts/indicadorIntegracaoCTe"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadIndicadorIntegracaoCTe();
    });

</script>

