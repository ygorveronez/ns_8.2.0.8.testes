

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">@Localization.Resources.Integracoes.Integradora.Integracoes</li>
    <li class="breadcrumb-item active">@Localization.Resources.Integracoes.Integradora.Integradora_</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        @Localization.Resources.Integracoes.Integradora.Integradoras
    </h1>
</div>
<div id="knockoutPesquisaIntegradora">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    @Localization.Resources.Integracoes.Integradora.PesquisarIntegradoras
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <div class="widget-body">
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel mt-3" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs">
                <li class="nav-item active">
                    <a href="#knockoutCadastroIntegradora" class="nav-link active" data-bs-toggle="tab">
                        <i class="fal fa-edit"></i> <span class="hidden-mobile hidden-tablet">@Localization.Resources.Integracoes.Integradora.Integradora_</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#knockoutClientes" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-users"></i><span class="hidden-mobile hidden-tablet">@Localization.Resources.Gerais.Geral.Cliente</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#knockoutPermissoesWebService" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-users"></i><span class="hidden-mobile hidden-tablet">@Localization.Resources.Integracoes.Integradora.PermisoesWebService</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#tabIntegracao" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-link"></i> <span class="hidden-mobile hidden-tablet">@Localization.Resources.Integracoes.Integradora.Integracoes</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3">
                <div class="tab-pane fade active show" id="knockoutCadastroIntegradora" role="tabpanel">
                    <form novalidate>
                        <div class="row">
                            <div class="col-12 col-md-8" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="css:Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoAutenticacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoAutenticacao.text, attr: { for: TipoAutenticacao.id }"></label>
                                    <select class="form-control" data-bind="class: TipoAutenticacao.requiredClass, options: TipoAutenticacao.options, optionsText: 'text', optionsValue: 'value', value: TipoAutenticacao.val, attr: { maxlength: TipoAutenticacao.maxlength, id : TipoAutenticacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Token.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Token.text, attr: { for: Token.id }"></label>
                                    <input type="text" class="form-control" data-bind="enable:Token.enable,value: Token.val, valueUpdate: 'afterkeydown', attr: { maxlength: Token.maxlength, id : Token.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Usuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Usuario.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Usuario.maxlength, id : Usuario.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Senha.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Senha.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Senha.requiredClass, value: Senha.val, valueUpdate: 'afterkeydown', attr: { maxlength: Senha.maxlength, id : Senha.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TempoExpiracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TempoExpiracao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: TempoExpiracao.requiredClass, value: TempoExpiracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TempoExpiracao.maxlength, id : TempoExpiracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Empresa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Empresa.text, maxlength: Empresa.maxlength, id: Empresa.id, name: Empresa.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Empresa.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: GrupoPessoas.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoPessoas.text, attr: { for: GrupoPessoas.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: GrupoPessoas.requiredClass, value: GrupoPessoas.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': GrupoPessoas.text, maxlength: GrupoPessoas.maxlength, id: GrupoPessoas.id, name: GrupoPessoas.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoPessoas.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Cliente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Cliente.text, attr: { for: Cliente.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Cliente.requiredClass, value: Cliente.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Cliente.text, maxlength: Cliente.maxlength, id: Cliente.id, name: Cliente.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Cliente.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-12 mt-2" data-bind="visible: TodosWebServicesLiberados.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="enable: TodosWebServicesLiberados.enable, checked: TodosWebServicesLiberados.val, attr: { maxlength: TodosWebServicesLiberados.maxlength, name: TodosWebServicesLiberados.id, id: TodosWebServicesLiberados.id }">
                                        <label class="custom-control-label" data-bind="text: TodosWebServicesLiberados.text, attr: { for: TodosWebServicesLiberados.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>

                <div class="tab-pane fade" id="knockoutClientes" role="tabpanel">
                    <div>
                        <button type="button" class="btn btn-primary mb-2" data-bind="attr: { id: Cliente.idBtnSearch}">
                            <i class="fal fa-plus"></i><span data-bind="text: Cliente.text"></span>
                        </button>

                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Grid.id }" cellspacing="0"></table>
                    </div>
                </div>

                <div class="tab-pane fade" id="knockoutPermissoesWebService" role="tabpanel">
                    <form novalidate>
                        <div class="panel-header">
                            <h3>@Localization.Resources.Integracoes.Integradora.PermisoesWebService</h3> <!-- {Localize::Integracoes.ControleIntegracao.PermisoesWebService}-->
                        </div>

                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: GridPermissoesWebService.idGrid}" cellspacing="0"></table>

                        <div class="row mt-2">

                            <div class="col-12 col-md-2" data-bind="visible: NomeMetodo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NomeMetodo.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NomeMetodo.requiredClass, value: NomeMetodo.val, valueUpdate: 'afterkeydown', attr: { maxlength: NomeMetodo.maxlength, id : NomeMetodo.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: RequisicoesMinuto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: RequisicoesMinuto.text"></label>
                                    <input type="text" class="form-control" data-bind="class: RequisicoesMinuto.requiredClass, value: RequisicoesMinuto.val, valueUpdate: 'afterkeydown', attr: { maxlength: RequisicoesMinuto.maxlength, id : RequisicoesMinuto.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: QtdRequisicoes.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: QtdRequisicoes.text"></label>
                                    <input type="text" class="form-control" data-bind="class: QtdRequisicoes.requiredClass, value: QtdRequisicoes.val, valueUpdate: 'afterkeydown', attr: { maxlength: QtdRequisicoes.maxlength, id : QtdRequisicoes.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: UltimoReset.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: UltimoReset.text"></label>
                                    <input type="text" class="form-control" data-bind="class: UltimoReset.requiredClass, value: UltimoReset.val, valueUpdate: 'afterkeydown', attr: { maxlength: UltimoReset.maxlength, id : UltimoReset.id}" />
                                </div>
                            </div>
                        </div>

                        <footer class="mt-2">
                            <div class="row">
                                <div class="col-12 d-flex justify-content-start">
                                    <button type="button" class="btn btn-labeled btn-default" data-bind="click: CancelarPermissoesWebService.eventClick, visible: CancelarPermissoesWebService.visible, attr: { value: CancelarPermissoesWebService.text, id : CancelarPermissoesWebService.id}">
                                        <i class="fal fa-ban"></i>
                                        <span data-bind="text: CancelarPermissoesWebService.text"></span>
                                    </button>
                                    <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: ExcluirPermissoesWebService.eventClick, visible: ExcluirPermissoesWebService.visible, attr: { value: ExcluirPermissoesWebService.text, id : ExcluirPermissoesWebService.id}">
                                        <i class="fal fa-trash"></i>
                                        <span data-bind="text: ExcluirPermissoesWebService.text"></span>
                                    </button>
                                    <button type="button" class="btn btn-labeled btn-success" data-bind="click: AtualizarPermissoesWebService.eventClick, visible: AtualizarPermissoesWebService.visible, attr: { value: AtualizarPermissoesWebService.text, id : AtualizarPermissoesWebService.id}">
                                        <i class="fal fa-check"></i>
                                        <span data-bind="text: AtualizarPermissoesWebService.text"></span>
                                    </button>
                                    <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: AdicionarPermissoesWebService.eventClick, visible: AdicionarPermissoesWebService.visible, attr: { value: AdicionarPermissoesWebService.text, id : AdicionarPermissoesWebService.id}">
                                        <span data-bind="text: AdicionarPermissoesWebService.text"></span>
                                    </button>
                                </div>
                            </div>
                        </footer>
                    </form>
                </div>

                <div class="tab-pane fade" id="tabIntegracao" role="tabpanel">
                    <form novalidate>
                        <div class="panel-heading">
                            <h3>@Localization.Resources.Integracoes.Integradora.Integracoes</h3>
                        </div>
                        <div id="knockoutConfiguracaoIntegracao">
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: ListaSistemaIntegracao.idGrid}" cellspacing="0"></table>
                        </div>
                        <div class="row" id="knockoutCadastroSistemaIntegracao">
                            <div class="col-12 col-md-12" data-bind="visible: SistemaIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SistemaIntegracao.text, attr: { for: SistemaIntegracao.id }"></label>
                                    <select class="form-control" data-bind="class: SistemaIntegracao.requiredClass, options: SistemaIntegracao.options, optionsText: 'text', optionsValue: 'value', value: SistemaIntegracao.val, attr: { maxlength: SistemaIntegracao.maxlength, id : SistemaIntegracao.id}"></select>
                                </div>
                            </div>

                            <div class="col-6 mb-2">
                                <button class="btn btn-success" type="button" data-bind="click: Adicionar.eventClick, attr: { id: Adicionar.id}">
                                    <i class="fal fa-plus"></i><span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <footer id="knockoutCRUDCadastroIntegradora" class="d-flex justify-content-end mt-3">
                <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, text: Cancelar.text, attr: { id : Cancelar.id}">
                </button>
                <button type="button" class="btn btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, text: Excluir.text, attr: { id : Excluir.id}">
                </button>
                <button type="button" class="btn btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, text: Atualizar.text, attr: { id : Atualizar.id}">
                </button>
                <button type="button" class="btn btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                </button>
            </footer>
        </div>
    </div>
</div>


<script src="~/scripts/integradora"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("Integracoes.Integradora").then(loadIntegradora);
    });

</script>

