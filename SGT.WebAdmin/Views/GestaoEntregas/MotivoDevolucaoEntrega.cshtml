
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Gestão Entregas</li>
    <li class="breadcrumb-item active">Motivo Devolução Entrega</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Motivos Devolução Entrega
    </h1>
</div>

<div id="knockoutPesquisaMotivoDevolucaoEntrega">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Motivos
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">

                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Motivo Devolução Entrega
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div>
                    <div class="row" id="knockoutMotivoDevolucaoEntrega">

                        <div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { class: Descricao.requiredClass, maxlength: Descricao.maxlength, id : Descricao.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: MotivoChamado.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: MotivoChamado.text, attr: { for: MotivoChamado.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: MotivoChamado.requiredClass, value: MotivoChamado.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': MotivoChamado.text, maxlength: MotivoChamado.maxlength, id: MotivoChamado.id, name: MotivoChamado.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: MotivoChamado.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: ChecklistSuperApp.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: ChecklistSuperApp.text, attr: { for: ChecklistSuperApp.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: ChecklistSuperApp.requiredClass, value: ChecklistSuperApp.val, valueUpdate: 'afterkeydown', attr: { maxlength: ChecklistSuperApp.maxlength, id : ChecklistSuperApp.id}, enable: ChecklistSuperApp.enable" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ChecklistSuperApp.idBtnSearch}, enable: ChecklistSuperApp.enable" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: TipoOcorrencia.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoOcorrencia.text, attr: { for: TipoOcorrencia.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TipoOcorrencia.requiredClass, value: TipoOcorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOcorrencia.maxlength, id : TipoOcorrencia.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOcorrencia.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: EntregaParcialSuperAppId.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EntregaParcialSuperAppId.text, attr: { for: EntregaParcialSuperAppId.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: EntregaParcialSuperAppId.enable, value: EntregaParcialSuperAppId.val, valueUpdate: 'afterkeydown', attr: { class: EntregaParcialSuperAppId.requiredClass, maxlength: EntregaParcialSuperAppId.maxlength, id : EntregaParcialSuperAppId.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: NaoEntregaSuperAppId.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: NaoEntregaSuperAppId.text, attr: { for: NaoEntregaSuperAppId.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: NaoEntregaSuperAppId.enable, value: NaoEntregaSuperAppId.val, valueUpdate: 'afterkeydown', attr: { class: NaoEntregaSuperAppId.requiredClass, maxlength: NaoEntregaSuperAppId.maxlength, id : NaoEntregaSuperAppId.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-12" data-bind="visible: ObrigarFoto.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: ObrigarFoto.val, attr: { maxlength: ObrigarFoto.maxlength, name: ObrigarFoto.id, id: ObrigarFoto.id }">
                                    <label class="custom-control-label" data-bind="text: ObrigarFoto.text, attr: { for: ObrigarFoto.id }"></label>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Observacao.text"></label>
                                <textarea type="text" rows="5" class="form-control" data-bind="enable: Observacao.enable, value: Observacao.val, attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                            </div>
                        </div>


                        <footer>
                            <div class="row">
                                <div class="col-6">
                                    <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                        <i class="fal fa-ban"></i>
                                        <span data-bind="text: Cancelar.text"></span>
                                    </button>
                                </div>
                                <div class="col-6 d-flex justify-content-end">
                                    <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                        <i class="fal fa-trash"></i>
                                        <span data-bind="text: Excluir.text"></span>
                                    </button>
                                    <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                        <i class="fal fa-check"></i>
                                        <span data-bind="text: Atualizar.text"></span>
                                    </button>
                                    <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                        <span data-bind="text: Adicionar.text"></span>
                                    </button>
                                </div>
                            </div>
                        </footer>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>


<script src="~/scripts/motivoDevolucaoEntrega"></script>

<script type="text/javascript">

    pageSetUp();
    const _LiberarCamposV3Trizy = @Html.Raw(ViewBag.LiberarCamposV3Trizy);
    $(document).ready(function () {
        loadMotivoDevolucaoEntrega();
    });

</script>


