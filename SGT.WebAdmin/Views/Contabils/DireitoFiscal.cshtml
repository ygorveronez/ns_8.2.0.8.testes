
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Contábil</li>
    <li class="breadcrumb-item">Direito Fiscal</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        Direito Fiscal
    </h1>
</div>

<div id="knockoutPesquisaDireitoFiscal">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-filter"></i>
                    <span>Pesquisar Direitos Fiscais</span>
                </button>

                <div class="mt-2" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">

                            <div class="col-12 col-md-4" data-bind="visible: IVA.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: IVA.text, attr: { for: IVA.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: IVA.requiredClass, value: IVA.val, valueUpdate: 'afterkeydown', attr: { maxlength: IVA.maxlength, id : IVA.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: IVA.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-2">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Direito Fiscal
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutDireitoFiscal">

                    <div class="col-12 col-md-4" data-bind="visible: IVA.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: IVA.text, attr: { for: IVA.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: IVA.requiredClass, value: IVA.val, valueUpdate: 'afterkeydown', attr: { maxlength: IVA.maxlength, id : IVA.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: IVA.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text"></label>
                            <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-4" data-bind="visible: Situacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                            <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: ICMS_ISS.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ICMS_ISS.text"></label>
                            <input type="text" class="form-control" data-bind="class: ICMS_ISS.requiredClass, value: ICMS_ISS.val, valueUpdate: 'afterkeydown', attr: { maxlength: ICMS_ISS.maxlength, id : ICMS_ISS.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: IPI.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: IPI.text"></label>
                            <input type="text" class="form-control" data-bind="class: IPI.requiredClass, value: IPI.val, valueUpdate: 'afterkeydown', attr: { maxlength: IPI.maxlength, id : IPI.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: PIS.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PIS.text"></label>
                            <input type="text" class="form-control" data-bind="class: PIS.requiredClass, value: PIS.val, valueUpdate: 'afterkeydown', attr: { maxlength: PIS.maxlength, id : PIS.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: COFINS.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: COFINS.text"></label>
                            <input type="text" class="form-control" data-bind="class: COFINS.requiredClass, value: COFINS.val, valueUpdate: 'afterkeydown', attr: { maxlength: COFINS.maxlength, id : COFINS.id}" />
                        </div>
                    </div>

                </div>
            </form>
            <footer id="knockoutCRUD_DireitoFiscal">
                <div class="row">
                    <div class="col-6">
                        <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                            <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
                        </button>
                    </div>
                    <div class="col-6 d-flex justify-content-end">
                        <button class="btn btn-danger me-2" type="button" data-bind="visible: Excluir.visible, click: Excluir.eventClick, attr: { id: Excluir.id}">
                            <i class="fal fa-trash"></i><span data-bind="text: Excluir.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                            <i class="fal fa-check"></i><span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="visible: Adicionar.visible, click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </div>
            </footer>
        </div>
    </div>
</div>

<script src="~/scripts/direitoFiscal"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadDireitoFiscal();
    });

</script>
