
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Contabils</li>
    <li class="breadcrumb-item active">Calculo ISS</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class="subheader-icon fal fa-copyright"></i> Calculo ISS
    </h1>
</div>
<div id="knockoutPesquisaCalculoISS">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Tabela de Calculo de Imposto ISS
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: CodigoServico.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoServico.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoServico.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoServico.maxlength, id : CodigoServico.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-9" data-bind="visible: Localidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Localidade.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: Localidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Localidade.maxlength, id : Localidade.id},  class: Localidade.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: Localidade.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Marca de Produto
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroCalculoISS">
                    <div class="col-12 col-md-6" data-bind="visible: Localidade.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Localidade.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: Localidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Localidade.maxlength, id : Localidade.id}, class: Localidade.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: Localidade.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CodigoServico.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoServico.text"></label>
                            <input type="text" class="form-control" data-bind="value: CodigoServico.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoServico.maxlength, id : CodigoServico.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Aliquota.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Aliquota.text"></label>
                            <input type="text" class="form-control" data-bind="value: Aliquota.val, valueUpdate: 'afterkeydown', attr: { maxlength: Aliquota.maxlength, id : Aliquota.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: PercentualRetencao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PercentualRetencao.text"></label>
                            <input type="text" class="form-control" data-bind="value: PercentualRetencao.val, valueUpdate: 'afterkeydown', attr: { maxlength: PercentualRetencao.maxlength, id : PercentualRetencao.id}" />
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end" id="knockoutCRUDCalculoISS">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1"  data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/calculoISS"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadCalculoISS();
    });

</script>

