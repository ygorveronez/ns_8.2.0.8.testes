                    <div>
                        <b data-bind="text: Tolerancia.text" style="font-size: 1.3rem;"></b> <span style="font-size: 1.3rem;" data-bind="text: Tolerancia.val"></span>
                    </div>
                    <div>
                        <b data-bind="text: TotalValorHistorico.text" style="font-size: 1.3rem;"></b> <span style="font-size: 1.3rem;" data-bind="text: TotalValorHistorico.val"></span>
