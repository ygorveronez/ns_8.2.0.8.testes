
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Produtos</li>
    <li class="breadcrumb-item active">Grupo Produto TMS</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i> Grupos de Produtos
    </h1>
</div>

<div id="knockoutPesquisaGrupoProdutoTMS">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Grupos de Produtos
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible : Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel mt-3">
    <div class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs">
                <li class="nav-item">
                    <a href="#tabGrupoProduto" class="nav-link active" data-bs-toggle="tab">
                        <i class="fal fa-cubes"></i><span class="hidden-mobile hidden-tablet">Grupo de Produto</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#tabDepreciacao" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-money-bill"></i><span class="hidden-mobile hidden-tablet">Depreciação</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3">
                <div class="tab-pane fade active show" id="tabGrupoProduto" role="tabpanel">
                    <form novalidate id="knockoutCadastroGrupoProdutoTMS">
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible : Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: GrupoProdutoTMSPai.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoProdutoTMSPai.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: GrupoProdutoTMSPai.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoProdutoTMSPai.maxlength, id : GrupoProdutoTMSPai.id}, class: GrupoProdutoTMSPai.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoProdutoTMSPai.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </form>
                </div>
                <div class="tab-pane fade" id="tabDepreciacao" role="tabpanel">
                    <form novalidate id="knockoutDepreciacaoGrupoProdutoTMS">
                        <div class="row">

                            <div class="col-12 col-md-12" data-bind="visible: GerarDepreciacao.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: GerarDepreciacao.val, attr: { maxlength: GerarDepreciacao.maxlength, name: GerarDepreciacao.id, id: GerarDepreciacao.id }">
                                        <label class="custom-control-label" data-bind="text: GerarDepreciacao.text, attr: { for: GerarDepreciacao.id }"></label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: PercentualDepreciacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PercentualDepreciacao.text"></label>
                                    <input type="text" class="form-control" data-bind="css: PercentualDepreciacao.requiredClass,value: PercentualDepreciacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: PercentualDepreciacao.maxlength, id : PercentualDepreciacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoMovimentoBaixa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoMovimentoBaixa.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: TipoMovimentoBaixa.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoMovimentoBaixa.maxlength, id : TipoMovimentoBaixa.id}, class: TipoMovimentoBaixa.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: TipoMovimentoBaixa.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoMovimentoDepreciacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoMovimentoDepreciacao.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: TipoMovimentoDepreciacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoMovimentoDepreciacao.maxlength, id : TipoMovimentoDepreciacao.id},class: TipoMovimentoDepreciacao.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: TipoMovimentoDepreciacao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoMovimentoDepreciacaoAcumulada.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoMovimentoDepreciacaoAcumulada.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: TipoMovimentoDepreciacaoAcumulada.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoMovimentoDepreciacaoAcumulada.maxlength, id : TipoMovimentoDepreciacaoAcumulada.id},class: TipoMovimentoDepreciacaoAcumulada.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: TipoMovimentoDepreciacaoAcumulada.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                </div>
            </div>
            <footer id="knockoutCRUDGrupoProdutoTMS">
                <div class="row my-3">
                    <div class="col-6">
                        <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                            <i class="fal fa-ban"></i>
                            <span data-bind="text: Cancelar.text"></span>
                        </button>
                    </div>
                    <div class="col-6 d-flex justify-content-end">
                        <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                            <i class="fal fa-trash"></i>
                            <span data-bind="text: Excluir.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                            <i class="fal fa-check"></i>
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </div>
            </footer>
        </div>
    </div>
</div>
<script src="~/scripts/grupoProdutoTMS"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadGrupoProdutoTMS();
    });

</script>