
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Terceiros</li>
    <li class="breadcrumb-item active">ContratoFrete</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Contrato de Frete
    </h1>
    <div class="subheader-help">
        <button type="button" onclick="iniciarAjuda(387)" data-action="toggle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Abrir a ajuda da página" aria-label="Abrir a ajuda da página"><i class="fal fa-question-circle"></i></button>
    </div>
</div>
<div>
    <div class="panel">
        <div class="panel-container" id="knockoutPesquisaContratoFrete">
            <div class="panel-content">
                <div data-widget-colorbutton="false" data-widget-sortable="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-togglebutton="false">
                    <button class="btn btn-primary" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i> Pesquisar Contrato de Frete
                    </button>

                    <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade" class="mt-3">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-2" data-bind="visible: NumeroContrato.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroContrato.text, attr: { for: NumeroContrato.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroContrato.requiredClass, value: NumeroContrato.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroContrato.maxlength, id : NumeroContrato.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: Carga.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: SituacaoContratoFrete.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: SituacaoContratoFrete.text, attr: { for: SituacaoContratoFrete.id }"></label>
                                        <select class="form-control" data-bind="options: SituacaoContratoFrete.options, optionsText: 'text', optionsValue: 'value', selectedOptions: SituacaoContratoFrete.val, attr: { id : SituacaoContratoFrete.id}" multiple="multiple" data-actions-box="false" data-selected-text-format="count > 3" title="Todos"></select>
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: TransportadorTerceiro.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: TransportadorTerceiro.text, attr: { for: TransportadorTerceiro.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: TransportadorTerceiro.requiredClass, value: TransportadorTerceiro.val, valueUpdate: 'afterkeydown', attr: { maxlength: TransportadorTerceiro.maxlength, id : TransportadorTerceiro.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TransportadorTerceiro.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: NumeroCIOT.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroCIOT.text, attr: { for: NumeroCIOT.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroCIOT.requiredClass, value: NumeroCIOT.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCIOT.maxlength, id : NumeroCIOT.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: DataInicialContratoFrete.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataInicialContratoFrete.text, attr: { for: DataInicialContratoFrete.id }"></label>
                                        <input type="text" class="form-control" data-bind="enable: DataInicialContratoFrete.enable,class: DataInicialContratoFrete.requiredClass, value: DataInicialContratoFrete.val, valueUpdate: 'afterkeydown', attr: { maxlenglabel: DataInicialContratoFrete.maxlenglabel, id : DataInicialContratoFrete.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: DataFinalContratoFrete.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataFinalContratoFrete.text, attr: { for: DataFinalContratoFrete.id }"></label>
                                        <input type="text" class="form-control" data-bind="enable: DataFinalContratoFrete.enable,class: DataFinalContratoFrete.requiredClass, value: DataFinalContratoFrete.val, valueUpdate: 'afterkeydown', attr: { maxlenglabel: DataFinalContratoFrete.maxlenglabel, id : DataFinalContratoFrete.id}" />
                                    </div>
                                </div>
                            <div class="d-flex justify-content-end mb-3">
                                <button class="btn btn-primary ms-2" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                    <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                                </button>
                            </div>
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="wid-id-4" class="panel" style="display:none">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Formulário de Cadastro
            </h3>
        </div>
        <div class="panel-content">
            <div id="knockoutEtapaContratoFrete">
                <ul class="nav nav-steps">
                    <li data-target="#step1" data-bind="visible : Etapa1.visible">
                        <button class="active" data-bs-target="#tabContratoFreteContainer" data-bs-toggle="tab" data-bind="attr: {id : Etapa1.idTab }, click : Etapa1.eventClick, visible : Etapa1.visible">
                            <span class="step" data-bind="attr: {'data-content' : Etapa1.tooltip, 'data-original-title' : Etapa1.tooltipTitle }">
                                <i class="fal fa-file-alt"></i>
                            </span>
                            <span class="title" data-bind="text: Etapa1.text"></span>
                        </button>
                    </li>
                    <li data-target="#step2" data-bind="visible : Etapa2.visible">
                        <button data-bs-target="#knockoutAprovacao" data-bs-toggle="tab" data-bind="attr: {id : Etapa2.idTab }, click : Etapa2.eventClick">
                            <span class="step" data-bind="attr: {'data-content' : Etapa2.tooltip, 'data-original-title' : Etapa2.tooltipTitle }">
                                <i class="fal fa-link"></i>
                            </span>
                            <span class="title" data-bind="text: Etapa2.text"></span>
                        </button>
                    </li>
                </ul>
            </div>
            <div class="tab-content" id="myTabContent">
                <!-- Etapa 1 -->
                <div id="tabContratoFreteContainer" class="tab-pane fade show active">
                    <ul class="nav nav-tabs">
                        <li>
                            <a href="#tabContratoFrete" class="nav-link active show" data-bs-toggle="tab">
                                <span>Contrato de Frete</span><i class="fal fa-pencil-square-o"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#tabCarga" class="nav-link" data-bs-toggle="tab">
                                <span>Carga</span>
                            </a>
                        </li>
                        <li id="liTabAcrescimoDesconto" style="display:none;">
                            <a href="#tabAcrescimoDesconto" class="nav-link" data-bs-toggle="tab">
                                <span>Acréscimo/Desconto</span>
                            </a>
                        </li>
                    </ul>
                    <div class="tab-content border border-top-0 p-2" id="myTabContentCadastro">
                        <div class="tab-pane fade active show" id="tabContratoFrete" role="tabpanel">
                            <div id="knockoutContratoFrete"></div>
                        </div>
                        <div class="tab-pane fade" id="tabCarga" role="tabpanel">
                            <div id="knoutCarga">
                                <div id="fdsCarga"></div>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="tabAcrescimoDesconto" role="tabpanel">
                            <div id="knoutAcrescimoDesconto">
                                <table width="100%" class="table table-bordered table-hover mt-2" data-bind="attr: { id: GridAcrescimoDesconto.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Etapa Aprovacao -->
                <div id="knockoutAprovacao" class="tab-pane fade">
                    <div class="row">
                        <div data-bind="visible: DetalheAprovadores.visible">
                            <!-- Detalhes -->
                            <div class="row">
                                <div class="col-12 col-md-6" data-bind="visible: PossuiRegras.val">
                                    <div class="card">
                                        <div class="card-header d-flex align-items-center justify-content-between">
                                            <h3 class="card-title">Detalhes</h3>
                                        </div>
                                        <div class="card-body">
                                            <div class="row col-12 col-md-6">
                                                <div class="row row-cols-1">
                                                    <div class="col">

                                                        <table class="table table-clean table-sm align-self-end">
                                                            <tbody>
                                                                <tr>
                                                                    <td>
                                                                        <strong data-bind="text: Solicitante.text"></strong>
                                                                        <span data-bind="text: Solicitante.val"></span>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td>
                                                                        <strong data-bind="text: DataSolicitacao.text"></strong>
                                                                        <span data-bind="text: DataSolicitacao.val"></span>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td>
                                                                        <strong data-bind="text: AprovacoesNecessarias.text"></strong>
                                                                        <span data-bind="text: AprovacoesNecessarias.val"></span>
                                                                    </td>
                                                                </tr>

                                                                <tr>
                                                                    <td>
                                                                        <strong data-bind="text: Aprovacoes.text"></strong>
                                                                        <span data-bind="text: Aprovacoes.val"></span>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td>
                                                                        <strong data-bind="text: Reprovacoes.text"></strong>
                                                                        <span data-bind="text: Reprovacoes.val"></span>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td>
                                                                        <strong data-bind="text: Situacao.text"></strong>
                                                                        <span data-bind="text: Situacao.val"></span>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!--/ Detalhes -->
                                <!-- Aprovadores -->
                                <div class="col col-sm-6" data-bind="visible: PossuiRegras.val">
                                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Regras.idGrid}" cellspacing="0"></table>
                                </div>
                                <!--/ Aprovadores -->
                                <!-- MensagemPadrao -->
                                <div class="col col-sm-12" data-bind="visible: !PossuiRegras.val()">
                                    <div class="alert alert-warning">
                                        Não foi encontrada nenhuma regra para aprovar o Contrato.<br />
                                        A solicitação ficará pendente até existir alguma regra de aprovação. Clique em reprocessar regras para verificar.
                                    </div>
                                </div>
                                <!--/ MensagemPadrao -->
                            </div>
                        </div>
                    </div>
                </div>

                <div class="d-flex justify-content-end mt-2" id="knockoutCadastroContratoFrete">
                    <button type="button" class="btn btn-success mx-1 mx-sm-2" data-bind="click: ReabrirContrato.eventClick, visible: ReabrirContrato.visible, text: ReabrirContrato.text, attr: { id : ReabrirContrato.id}">
                        <i class="fal fa-trash"></i>
                    </button>
                    <button type="button" class="btn btn-success ms-2" data-bind="click: Reprocessar.eventClick, visible: Reprocessar.visible, text: Reprocessar.text, attr: { id : Reprocessar.id}">
                        <i class="fal fa-check"></i>
                    </button>
                    <button type="button" class="btn btn-success ms-2" data-loading-text="Aguarde..." data-bind="click: FinalizarContrato.eventClick, visible: FinalizarContrato.visible, text: FinalizarContrato.text, attr: { id : FinalizarContrato.id}">
                        <i class="fal fa-spinner"></i>
                    </button>
                    <button type="button" class="btn btn-success ms-2" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, text: Atualizar.text, attr: { id : Atualizar.id}">
                        <i class="fal fa-check"></i>
                    </button>

                </div>
            </div>
        </div>

    </div>
</div>


<div id="ModaisCarga">
</div>





<div class="modal fade" id="divModalBloqueioContratoFrete" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Bloquear o Contrato de Frete</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group" data-bind="visible: Justificativa.visible">
                    <label class="form-label" data-bind="text: Justificativa.text, attr: { for: Justificativa.id }"></label>
                    <textarea class="form-control" rows="5" type="text" data-bind="value: Justificativa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Justificativa.maxlength, id : Justificativa.id}"></textarea>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default me-2" type="button" data-bind="click: Bloquear.eventClick, visible: Bloquear.visible, enable: Bloquear.enable, attr: { id: Bloquear.id}">
                        <i class="fal fa-ban"></i><span data-bind="text: Bloquear.text"></span>
                    </button>
                    <button class="btn btn-danger" type="button" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, enable: Cancelar.enable, attr: { id: Cancelar.id}">
                        <i class="fal fa-ban"></i><span data-bind="text: Cancelar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>



