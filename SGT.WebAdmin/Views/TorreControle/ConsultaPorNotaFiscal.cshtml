@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Torre Controle</li>
        <li class="breadcrumb-item active">Consulta Por Nota Fiscal</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Torre de Controle
        </h1>
    </div>
    <div class="row" id="divConteudoRelatorio">
        <div id="wid-id-4" class="panel" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
            <div class="panel-container">
                <div class="panel-hdr">
                    <h3>
                        <i class="fal fa-search"></i>
                         Consulta Por Nota Fiscal

                    </h3>
                </div>
                <div class="col-12 col-md-12">
                    <div class="card" id="knockoutPesquisaConsultaPorNotaFiscal">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h3 class="card-title">Filtros de Pesquisa</h3>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-12 col-md-3" data-bind="visible: DataCarregamentoInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataCarregamentoInicial.text, attr: { for: DataCarregamentoInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataCarregamentoInicial.requiredClass, value: DataCarregamentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCarregamentoInicial.maxlength, id : DataCarregamentoInicial.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: DataCarregamentoFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataCarregamentoFinal.text, attr: { for: DataCarregamentoFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataCarregamentoFinal.requiredClass, value: DataCarregamentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCarregamentoFinal.maxlength, id : DataCarregamentoFinal.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: DataPrevisaoEntregaInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataPrevisaoEntregaInicial.text, attr: { for: DataPrevisaoEntregaInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataPrevisaoEntregaInicial.requiredClass, value: DataPrevisaoEntregaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataPrevisaoEntregaInicial.maxlength, id : DataPrevisaoEntregaInicial.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: DataPrevisaoEntregaFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataPrevisaoEntregaFinal.text, attr: { for: DataPrevisaoEntregaFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataPrevisaoEntregaFinal.requiredClass, value: DataPrevisaoEntregaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataPrevisaoEntregaFinal.maxlength, id : DataPrevisaoEntregaFinal.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: DataAgendamentoInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataAgendamentoInicial.text, attr: { for: DataAgendamentoInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataAgendamentoInicial.requiredClass, value: DataAgendamentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAgendamentoInicial.maxlength, id : DataAgendamentoInicial.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: DataAgendamentoFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataAgendamentoFinal.text, attr: { for: DataAgendamentoFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataAgendamentoFinal.requiredClass, value: DataAgendamentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAgendamentoFinal.maxlength, id : DataAgendamentoFinal.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: NumeroCarga.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroCarga.text, attr: { for: NumeroCarga.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroCarga.requiredClass, value: NumeroCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCarga.maxlength, id : NumeroCarga.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: NumeroNota.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroNota.text, attr: { for: NumeroNota.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroNota.requiredClass, value: NumeroNota.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroNota.maxlength, id : NumeroNota.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-2" data-bind="visible: SituacaoAgendamento.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: SituacaoAgendamento.text, attr: { for: SituacaoAgendamento.id }"></label>
                                        <select class="form-control" data-bind="class: SituacaoAgendamento.requiredClass, options: SituacaoAgendamento.options, optionsText: 'text', optionsValue: 'value', value: SituacaoAgendamento.val, attr: { maxlength: SituacaoAgendamento.maxlength, id : SituacaoAgendamento.id}"></select>
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: Cliente.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Cliente.text, attr: { for: Cliente.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Cliente.requiredClass, value: Cliente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Cliente.maxlength, id : Cliente.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Cliente.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: TipoOperacao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <footer class="d-flex justify-content-end mb-3">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id }" type="button">
                                        <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text">Pesquisar</span>
                                    </button>
                                </footer>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="divConfigRelatorio">
                </div>
                <div class="mt-3 mb-3">
                    <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="grid-consulta-por-nota-fiscal" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</section>








<script src="~/scripts/consultaPorNotaFiscal"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadConsultaPorNotaFiscal();
    });

</script>
