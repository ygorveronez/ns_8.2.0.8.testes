<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">@Localization.Resources.AbastecimentoInterno.LiberacaoAbastecimentoAutomatizado.AbastecimentoInterno</li>
    <li class="breadcrumb-item active">@Localization.Resources.AbastecimentoInterno.LiberacaoAbastecimentoAutomatizado.DescricaoLiberacaoAbastecimentoAutomatizado</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        @Localization.Resources.AbastecimentoInterno.LiberacaoAbastecimentoAutomatizado.DescricaoLiberacaoAbastecimentoAutomatizado
    </h1>
</div>

<div id="knockoutPesquisaLiberacaoAbastecimentoAutomatizado">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    @Localization.Resources.AbastecimentoInterno.LiberacaoAbastecimentoAutomatizado.PesquisarLiberacaoAbastecimentoAutomatizado
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: BombaAbastecimento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: BombaAbastecimento.text, attr: { for: BombaAbastecimento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="enable: BombaAbastecimento.enable, css: BombaAbastecimento.requiredClass, value: BombaAbastecimento.val, valueUpdate: 'afterkeydown', attr: { maxlength: BombaAbastecimento.maxlength, id : BombaAbastecimento.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="enable: BombaAbastecimento.enable, attr: { id: BombaAbastecimento.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                @Localization.Resources.AbastecimentoInterno.LiberacaoAbastecimentoAutomatizado.CadastrarLiberacaoAbastecimentoAutomatizado
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div>
                    <div class="row" id="knockoutLiberacaoAbastecimentoAutomatizado">
                        <div class="col-12 col-md-2" data-bind="visible: DataAbastecimento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataAbastecimento.text"></label>
                                <input type="text" class="form-control" data-bind="value: DataAbastecimento.val, enable: DataAbastecimento.enable, valueUpdate: 'afterkeydown', attr: { maxlength: DataAbastecimento.maxlength, id : DataAbastecimento.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: Veiculo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="enable: Veiculo.enable, css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="enable: Veiculo.enable, attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="enable: Motorista.enable, css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="enable: Motorista.enable, attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: BombaAbastecimento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: BombaAbastecimento.text, attr: { for: BombaAbastecimento.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="enable: BombaAbastecimento.enable, css: BombaAbastecimento.requiredClass, value: BombaAbastecimento.val, valueUpdate: 'afterkeydown', attr: { maxlength: BombaAbastecimento.maxlength, id : BombaAbastecimento.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="enable: BombaAbastecimento.enable, attr: { id: BombaAbastecimento.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: UltimaQuilometragem.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: UltimaQuilometragem.text, attr: { for: UltimaQuilometragem.id }"></label>
                                <input type="text" class="form-control" data-bind=" enable: UltimaQuilometragem.enable,class: UltimaQuilometragem.requiredClass, value: UltimaQuilometragem.val, valueUpdate: 'afterkeydown', attr: { maxlength: UltimaQuilometragem.maxlength, id : UltimaQuilometragem.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: QuilometragemAtual.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: QuilometragemAtual.text, attr: { for: QuilometragemAtual.id }"></label>
                                <input type="text" class="form-control" data-bind=" enable: QuilometragemAtual.enable,class: QuilometragemAtual.requiredClass, value: QuilometragemAtual.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuilometragemAtual.maxlength, id : QuilometragemAtual.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: QuilometrosRodados.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: QuilometrosRodados.text, attr: { for: QuilometrosRodados.id }"></label>
                                <input type="text" class="form-control" data-bind=" enable: QuilometrosRodados.enable,class: QuilometrosRodados.requiredClass, value: QuilometrosRodados.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuilometrosRodados.maxlength, id : QuilometrosRodados.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: QuantidadeAbastecida.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: QuantidadeAbastecida.text, attr: { for: QuantidadeAbastecida.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: QuantidadeAbastecida.enable, class: QuantidadeAbastecida.requiredClass, value: QuantidadeAbastecida.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeAbastecida.maxlength, id : QuantidadeAbastecida.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CodigoAutorizacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoAutorizacao.text, attr: { for: CodigoAutorizacao.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: CodigoAutorizacao.enable, class: CodigoAutorizacao.requiredClass, value: CodigoAutorizacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoAutorizacao.maxlength, id : CodigoAutorizacao.id}" />
                            </div>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDLiberacaoAbastecimentoAutomatizado">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: NovoRegistro.eventClick, visible: NovoRegistro.visible, attr: { id : NovoRegistro.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: NovoRegistro.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                    <b class="font-md"><i class="fa fa-spinner fa-fw fa-spin fa-pulse" data-bind=" visible: ProcessandoAbastecimento.visible"></i>&nbsp;&nbsp;<span data-bind="text: ProcessandoAbastecimento.text, visible: ProcessandoAbastecimento.visible"></span></b>
                    <div id="timer" style="font-size:20px" data-bind=" visible: TimerProcessandoAbastecimento.visible">2:00</div>
                </footer>
            </form>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalHistoricoIntegracao" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Histórico de Integração</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table width="100%" class="table table-bordered table-hover" cellspacing="0" id="tblHistoricoIntegracao"></table>
            </div>
        </div>
    </div>
</div>
<script src="/scripts/liberacaoAbastecimentoAutomatizado"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("AbastecimentoInterno.LiberacaoAbastecimentoAutomatizado").then(LoadLiberacaoAbastecimentoAutomatizado);

    });

</script>

