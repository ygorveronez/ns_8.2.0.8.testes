                                            <label class="form-label" data-bind="text: ValorLiberado.text, attr: { for: ValorLiberado.id }"></label>
                                            <input type="text" class="form-control" data-bind="enable : ValorLiberado.enable, class: ValorLiberado.requiredClass, value: ValorLiberado.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorLiberado.maxlength, id : ValorLiberado.id}" />
