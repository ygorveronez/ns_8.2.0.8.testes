                            <div class="card-body">
                                <div id="fdsCarga"></div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="tab-pane fade" id="knockoutCTeComplementar" role="tabpanel">
                    <div class="col-12 col-md-12">
                        <div class="card">
                            <div class="card-header d-flex align-items-center justify-content-between">
