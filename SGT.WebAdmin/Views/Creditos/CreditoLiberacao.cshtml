                                            <label class="form-label" data-bind="text: ContaContabil.text, attr: { for: ContaContabil.id }"></label>
                                            <input type="text" class="form-control" data-bind="enable : ContaContabil.enable, class: ContaContabil.requiredClass, value: ContaContabil.val, valueUpdate: 'afterkeydown', attr: { maxlength: ContaContabil.maxlength, id : ContaContabil.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-success" type="button" data-bind="enable: DownloadArquivo.enable, click: DownloadArquivo.eventClick, attr: { id: DownloadArquivo.id}">
                                            <i class="fal fa-download"></i><span data-bind="text: DownloadArquivo.text"></span>
                                        </button>
                                    </div>
                                    <div class="col-12 col-md-12 mt-2" data-bind="visible: RetornoSolicitacao.visible">
