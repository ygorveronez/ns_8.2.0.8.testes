                                <button class="btn btn-primary" type="button" data-bind="visible: CTesComplementares.visible, click: BuscarNovamenteCTesComplementares.eventClick, attr: { id: BuscarNovamenteCTesComplementares.id}">
                                    <i class="fal fa-repeat"></i><span data-bind="text: BuscarNovamenteCTesComplementares.text"></span>
                                </button>
                            </div>
                            <div class="card-body">
                                <div data-bind="visible: CTesComplementares.visible">
                                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: CTesComplementares.idGrid}" cellspacing="0"></table>
                                </div>
                                <div data-bind="visible: !CTesComplementares.visible()">
                                    <label><b>Não foram emitidos CT-e(s) complementar(es) para a ocorrência.</b></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <footer id="knockoutCRUDCreditoLiberado">
                <div class="row">
                    <div class="col-6">
                    </div>
                    <div class="col-6 d-flex justify-content-end mt-3 mb-3">
                        <button class="btn btn-success me-2" type="button" data-bind="click: ResponderSolicitacao.eventClick, attr: { id: ResponderSolicitacao.id}">
                            <span data-bind="text: ResponderSolicitacao.text"></span>
                        </button>
                        <button class="btn btn-danger" type="button" data-bind="click: RejeitarSolicitacao.eventClick, attr: { id: RejeitarSolicitacao.id}">
                           <span data-bind="text: RejeitarSolicitacao.text"></span>
