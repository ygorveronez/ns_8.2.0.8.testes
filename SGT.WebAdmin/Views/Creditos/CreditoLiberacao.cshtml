<div id="knockoutPesquisaCreditoLiberacao">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Solicitações
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: NumeroCargaOcorrencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCargaOcorrencia.text, attr: { for: NumeroCargaOcorrencia.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroCargaOcorrencia.requiredClass, value: NumeroCargaOcorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCargaOcorrencia.maxlength, id : NumeroCargaOcorrencia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: SituacaoSolicitacaoCredito.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoSolicitacaoCredito.text, attr: { for: SituacaoSolicitacaoCredito.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoSolicitacaoCredito.requiredClass, options: SituacaoSolicitacaoCredito.options, optionsText: 'text', optionsValue: 'value', value: SituacaoSolicitacaoCredito.val, attr: { maxlength: SituacaoSolicitacaoCredito.maxlength, id : SituacaoSolicitacaoCredito.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: CodigoCargaEmbarcador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text, attr: { for: CodigoCargaEmbarcador.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoCargaEmbarcador.requiredClass, value: CodigoCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Solicitante.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Solicitante.text, attr: { for: Solicitante.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Solicitante.requiredClass, value: Solicitante.val, valueUpdate: 'afterkeydown', attr: { maxlength: Solicitante.maxlength, id : Solicitante.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Solicitante.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Recebedor.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Recebedor.text, attr: { for: Recebedor.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Recebedor.requiredClass, value: Recebedor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Recebedor.maxlength, id : Recebedor.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Recebedor.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Ocorrencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ocorrencia.text, attr: { for: Ocorrencia.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Ocorrencia.requiredClass, value: Ocorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Ocorrencia.maxlength, id : Ocorrencia.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Ocorrencia.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

