<div id="wid-id-4" class="panel" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Formulário de Cadastro
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutEtapasSeparacaoPedido">
                    <ul class="nav nav-steps">
                        <li data-target="#step1" data-bind="visible : Etapa1.visible">
                            <button class="active" data-bs-target="#tabSeparacao" data-bs-toggle="tab" data-bind="attr: {id : Etapa1.idTab }, click : Etapa1.eventClick, visible : Etapa1.visible">
                                <span class="step" data-bind="attr: {'data-content' : Etapa1.tooltip, 'data-original-title' : Etapa1.tooltipTitle }">
                                    <i class="fal fa-file-alt"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa1.text"></span>
                            </button>
                        </li>
                        <li data-target="#step2" data-bind="visible : Etapa2.visible">
                            <button data-bs-target="#tabInformacao" data-bs-toggle="tab" data-bind="attr: {id : Etapa2.idTab }, click : Etapa2.eventClick">
                                <span class="step" data-bind="attr: {'data-content' : Etapa2.tooltip, 'data-original-title' : Etapa2.tooltipTitle }">
                                    <i class="fal fa-info-square"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa2.text"></span>
                            </button>
                        </li>
                        <li data-target="#step3" data-bind="visible : Etapa3.visible">
                            <button data-bs-target="#tabIntegracao" data-bs-toggle="tab" data-bind="attr: {id : Etapa3.idTab }, click : Etapa3.eventClick">
                                <span class="step" data-bind="attr: {'data-content' : Etapa3.tooltip, 'data-original-title' : Etapa3.tooltipTitle }">
                                    <i class="fal fa-link"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa3.text"></span>
                            </button>
                        </li>
                    </ul>
                </div>
                <div id="myTabContent" class="tab-content">
                    <!-- Etapa Selecao Documentos -->
                    <div id="tabSeparacao" class="tab-pane fade show active">
                        <div class="card mb-3">
                            <div id="knoutAreaPedido" class="tab-pane in active">
                                <fieldset style="padding:5px 0px 5px 5px">
                                    <div class="widget-body" style="height:450px; overflow-y:scroll" data-bind="event: { scroll: Total.eventChange }">
                                        <div data-bind="attr: { id : Pedido.id }" class="row">

                                        </div>
                                        <div class="col col-xs-12 col-sm-12 col-md-12 col-lg-12 margin-bottom-5 margin-top-5" data-bind="visible: CarregandoPedidos.val">
                                            <p class="text-align-center">
                                                <span class="txt-color-blue"><i class="fa fa-spinner fa-lg fa-spin"></i> Carregando...</span>
                                            </p>
                                        </div>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                        <div id="knoutSeparacaoPedido"></div>
                        <footer id="knoutCRUDSeparacaoPedido">
                            <div class="row">
                                <div class="col-6">

                                    <button type="button" class="btn btn-labeled btn-default " data-bind="click: Limpar.eventClick, visible: Limpar.visible, attr: { value: Limpar.text, id : Limpar.id}">

                                        <span data-bind="text: Limpar.text"></span>
                                    </button>
                                </div>
                                <div class="col-6 d-flex justify-content-end">
                                    <button type="button" class="btn btn-labeled btn-danger me-2" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">

                                        <span data-bind="text: Cancelar.text"></span>
                                    </button>
