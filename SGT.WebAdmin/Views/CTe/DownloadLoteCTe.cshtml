
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">CTE</li>
    <li class="breadcrumb-item active">Download Lote CTE</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Consulta de Documentos
    </h1>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Pesquisar Lotes CT-es
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutConsultaLoteCTe">
                    <div class="col-12 col-md-4" data-bind="visible: DataImportacaoInicial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataImportacaoInicial.text, attr: { for: DataImportacaoInicial.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataImportacaoInicial.requiredClass, value: DataImportacaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataImportacaoInicial.maxlength, id : DataImportacaoInicial.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: DataImportacaoFinal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataImportacaoFinal.text, attr: { for: DataImportacaoFinal.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataImportacaoFinal.requiredClass, value: DataImportacaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataImportacaoFinal.maxlength, id : DataImportacaoFinal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: Situacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                            <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                        </div>
                    </div>
                    <footer>
                        <div class="row">
                            <div class="col-6 mt-2 mb-3">
                                <div data-bind='component: {name: "import-file", params: { ko: Importar }}'></div>
                            </div>
                            <div class="col-6 d-flex justify-content-end mt-2 mb-3">
                                <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                    <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                                </button>
                            </div>
                        </div>
                    </footer>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </form>
        </div>
    </div>
</div>

<div id="knockoutVisualizarCTes" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Visualizar CTes</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body row" data-bind="visible: CTes.visible">
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: CTes.idGrid}" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/downloadLoteCTe"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        LoadDownloadLoteCTe();
    });
</script>

