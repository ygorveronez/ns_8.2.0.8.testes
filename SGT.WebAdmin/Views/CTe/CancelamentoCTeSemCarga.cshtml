

<section>
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">CTe</li>
        <li class="breadcrumb-item active">CTe Cancelamento</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Cancelamento CT-e Sem Carga
        </h1>
        <div class="subheader-help">
            <button type="button" onclick="iniciarAjuda(960)" data-action="toggle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Abrir a ajuda da página" aria-label="Abrir a ajuda da página"><i class="fal fa-question-circle"></i></button>
        </div>
    </div>
    <div id="knockoutCTeCancelamento">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-content">
                    <h2>Pesquisar CT-es</h2>
                </div>
                <div class="panel-content">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: DataEmissaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoInicial.text, attr: { for: DataEmissaoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataEmissaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoInicial.maxlength, id : DataEmissaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataEmissaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoFinal.text, attr: { for: DataEmissaoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataEmissaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoFinal.maxlength, id : DataEmissaoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroInicial.text, attr: { for: NumeroInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroInicial.maxlength, id : NumeroInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroFinal.text, attr: { for: NumeroFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroFinal.maxlength, id : NumeroFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex justify-content-end ">
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="row">
    <div class="panel-content">
        <div class="panel-container">
            <div class="panel-content">
                <h2>CT-es sem Carga</h2>
            </div>
            <div class="panel-content">
                <form>
                    <div class="panel">
                        <div id="knockoutEtapaCancelamentoCTeSemCarga">
                            <ul class="nav nav-steps">
                                <li data-target="#step1" data-bind="visible : Etapa1.visible">
                                    <button class="active" data-bs-target="#knockoutResumoCancelamentoSemCarga" data-bs-toggle="tab" data-bind="attr: {id : Etapa1.idTab, idTab : Etapa1.idTab }, click : Etapa1.eventClick, visible : Etapa1.visible">
                                        <span class="step" data-bind="attr: {'data-content' : Etapa1.tooltip, 'data-original-title' : Etapa1.tooltipTitle }">
                                            <i class="fal fa-file-alt"></i>
                                        </span>
                                        <span class="title" data-bind="text: Etapa1.text"></span>
                                    </button>
                                </li>
                                <li data-target="#step2" data-bind="visible : Etapa2.visible">
                                    <button data-bs-target="#knockoutCTeSemCargaCancelamento" data-bs-toggle="tab" data-bind="attr: {id : Etapa2.idTab, idTab : Etapa2.idTab }, click : Etapa2.eventClick, visible : Etapa2.visible">
                                        <span class="step" data-bind="attr: {'data-content' : Etapa2.tooltip, 'data-original-title' : Etapa2.tooltipTitle }">
                                            <i class="fal fa-file-alt"></i>
                                        </span>
                                        <span class="title" data-bind="text: Etapa2.text"></span>
                                    </button>
                                </li>
                               @*  <li data-target="#step3" data-bind="visible : Etapa3.visible">
                                    <button data-bs-target="#divIntegracoes" data-bs-toggle="tab" data-bind="attr: {id : Etapa3.idTab, idTab : Etapa3.idTab }, click : Etapa3.eventClick">
                                        <span class="step" data-bind="attr: {'data-content' : Etapa3.tooltip, 'data-original-title' : Etapa3.tooltipTitle }">
                                            <i class="fal fa-file"></i>
                                        </span>
                                        <span class="title" data-bind="text: Etapa3.text"></span>
                                    </button>
                                </li> *@
                            </ul>
                        </div>
                        <div class="tab-content">
                            <div id="knockoutCTe" class="tab-pane fade ">
                                <div class="panel mt-3">
                                    <div class="panel-container">
                                        <div class="panel-content">
                                            <ul class="nav nav-tabs">
                                                <li class="nav-item active" id="liCTes">
                                                    <a href="#tabCTes" class="nav-link active" data-bs-toggle="tab">
                                                        <span class="hidden-mobile hidden-tablet">CT-e</span>
                                                    </a>
                                                </li>
                                            </ul>
                                            <div class="tab-content border border-top-0 p-3">
                                                <div class="tab-pane fade active show" id="tabCTes">
                                                    <div class="col-12 col-md-12">
                                                        <div class="card" data-bind="visible: BuscarCTes.visible">
                                                            <div class="card-header d-flex align-items-center justify-content-between">
                                                                <h3 class="card-title">CT-es</h3>

                                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="click: BuscarCTes.eventClick, attr: { id: BuscarCTes.idBtnSearch, text: BuscarCTes.text }">
                                                                    <i class="fal fa-repeat-alt"></i><span data-bind="text: BuscarCTes.text"></span>
                                                                </button>
                                                            </div>
                                                            <div class="card-body">
                                                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: BuscarCTes.idGrid}" cellspacing="0"></table>
                                                            </div>

                                                        </div>
                                                    </div>

                                                    <div class="row" data-bind="visible: !BuscarCTes.visible()">
                                                        <section class="col col-12">
                                                            <label><b>{Localize::Cargas.CancelamentoCarga.NãoExistemCTesParaCancelamento}</b></label>
                                                        </section>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="divIntegracoes" class="tab-pane fade ">
                                <div class="col-12 col-md-12">
                                    <div class="card">
                                        <div class="card-header d-flex align-items-center justify-content-between">
                                            <h3 class="card-title">{Localize::Cargas.CancelamentoCarga.Integracoes}</h3>
                                        </div>
                                        <div class="card-body">
                                            <div id="DivIntegracaoCargaCancelamento">
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel-container">
                            <div class="panel-content">
                                <div id="knockoutCTeSemCargaCancelamento" data-bind="visible: ExibirFiltros.visible">
                                    <div class="d-flex justify-content-end mb-3">
                                        <button class="btn btn-default" type="button" data-bind="visible: SelecionarTodos.visible, click: SelecionarTodos.eventClick, text: SelecionarTodos.text"></button>
                                    </div>
                                    <div id="container-grid-fila-carregamento">
                                        <table width="100%" class="table table-bordered table-condensed table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                                    </div>
                                </div>
                                <div id="knockoutResumoCancelamentoSemCarga" data-bind="visible: ExibirFiltros.visible">
                                    <div class="row row-cols-1 row-cols-md-2" data-bind="visible: NumeroCancelamento.visible">
                                        <div class="col">
                                            <table class="table table-clean table-sm align-self-end" data-bind="visible: NumeroCancelamento.visible">
                                                <tbody>
                                                    <tr>
                                                        <td>
                                                            <strong data-bind="text: NumeroCancelamento.text"></strong>
                                                            <span data-bind="text: NumeroCancelamento.val"></span>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <strong data-bind="text: Situacao.text"></strong>
                                                            <span data-bind="text: Situacao.val"></span>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <strong data-bind="text: DataCancelamento.text"></strong>
                                                            <span data-bind="text: DataCancelamento.val"></span>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="col">
                                            <table class="table table-clean table-sm align-self-end">
                                                <tbody>
                                                <td>
                                                    <strong data-bind="text: MensagemRejeicaoCancelamento.text"></strong>
                                                    <span data-bind="text: MensagemRejeicaoCancelamento.val"></span>
                                                </td>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <footer id="knockoutCRUDCancelamentoCTeSemCarga" class="d-flex justify-content-end mb-3 me-2">
                                    <input type="button" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}" class="btn btn-labeled btn-success" />
                                    <input type="button" data-loading-text="Aguarde..." data-bind="click: GerarNovoCancelamentoSemCarga.eventClick, visible: GerarNovoCancelamentoSemCarga.visible, attr: { value: GerarNovoCancelamentoSemCarga.text, id : GerarNovoCancelamentoSemCarga.id}" class="btn btn-default me-3" />
                                </footer>
                            </div>
                        </div>

                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
