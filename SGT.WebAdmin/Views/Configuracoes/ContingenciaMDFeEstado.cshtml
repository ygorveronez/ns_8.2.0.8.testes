
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Configurações</li>
    <li class="breadcrumb-item active">Contingência MDFe</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Contingências de MDFE por Estado
    </h1>
</div>
<div id="knockoutPesquisaContingenciaMDFeEstado">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Estados
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Nome.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Nome.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Nome.val, valueUpdate: 'afterkeydown', attr: { maxlength: Nome.maxlength, id : Nome.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Sigla.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Sigla.text"></label>
                                    <select class="form-control" data-bind="options: Sigla.options, optionsText: 'text', optionsValue: 'value', value: Sigla.val, attr: { id : Sigla.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Configurar Contingência
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroContingenciaMDFeEstado">
                    <div class="col-12 col-md-2" data-bind="visible: Codigo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Codigo.text"></label>
                            <input type="text" class="form-control" data-bind="value: Codigo.val, enable: Codigo.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Codigo.maxlength, id : Codigo.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-5" data-bind="visible: Nome.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Nome.text"></label>
                            <input type="text" class="form-control" data-bind="value: Nome.val, enable: Nome.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Nome.maxlength, id : Nome.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-5" data-bind="visible: TipoEmissaoMDFe.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoEmissaoMDFe.text"></label>
                            <select class="form-control" data-bind="options: TipoEmissaoMDFe.options, enable: TipoEmissaoMDFe.enable, optionsText: 'text', optionsValue: 'value', value: TipoEmissaoMDFe.val, attr: { id : TipoEmissaoMDFe.id}"></select>
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end" id="knockoutCRUDContingenciaMDFeEstado">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick,visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick,visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>
<script src="~/scripts/contingenciaMDFeEstado"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadContingenciaMDFeEstado();
    });

</script>