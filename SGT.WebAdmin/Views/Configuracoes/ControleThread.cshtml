
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Configurações</li>
    <li class="breadcrumb-item active">Controle Thread</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Controles de Thread
    </h1>
</div>

<div id="knockoutPesquisaControleThread">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Controles de Thread
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastro de Controle de Thread
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutCadastroControleThread">
                <div class="row">
                    <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Ativo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                            <select class="form-control" data-bind="class: Ativo.requiredClass, options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { maxlength: Ativo.maxlength, id : Ativo.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataCadastro.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataCadastro.text, attr: { for: DataCadastro.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataCadastro.requiredClass, value: DataCadastro.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCadastro.maxlength, id : DataCadastro.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataFim.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Tempo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Tempo.text, attr: { for: Tempo.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Tempo.requiredClass, value: Tempo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Tempo.maxlength, id : Tempo.id}" />
                        </div>
                    </div>
                </div>
            </form>
            <div class=" d-flex justify-content-end" id="knockoutCRUDControleThread">
                <button type="button" class="btn btn-labeled btn-default ms-2" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                    <i class="fal fa-ban"></i>
                    <span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-success ms-2" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                    <i class="fal fa-check"></i>
                    <span data-bind="text: Atualizar.text"></span>
                </button>

            </div>
        </div>
    </div>
</div>




<script src="~/scripts/controleThread"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadControleThread();
    });

</script>
