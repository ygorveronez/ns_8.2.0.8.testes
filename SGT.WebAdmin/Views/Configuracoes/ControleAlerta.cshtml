
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Configurações</li>
    <li class="breadcrumb-item active">Controle Alerta</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Controle de Alerta
    </h1>
</div>

<div id="knockoutPesquisaControleAlerta">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Controle de Alerta
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: Funcionario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Funcionario.requiredClass, value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Funcionario.text, maxlength: Funcionario.maxlength, id: Funcionario.id, name: Funcionario.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Funcionario.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: FormaAlerta.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: FormaAlerta.text"></label>
                                    <select class="form-control" data-bind="options: FormaAlerta.options, optionsText: 'text', optionsValue: 'value', selectedOptions: FormaAlerta.val, attr: { id : FormaAlerta.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TelaAlerta.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TelaAlerta.text"></label>
                                    <select class="form-control" data-bind="options: TelaAlerta.options, optionsText: 'text', optionsValue: 'value', selectedOptions: TelaAlerta.val, attr: { id : TelaAlerta.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Controle de Alerta
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroControleAlerta">
                    <div class="col-12 col-md-4" data-bind="visible: FormaAlerta.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: FormaAlerta.text"></label>
                            <select class="form-control" data-bind="options: FormaAlerta.options, optionsText: 'text', optionsValue: 'value', selectedOptions: FormaAlerta.val, attr: { id : FormaAlerta.id}" multiple="multiple"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: TelaAlerta.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TelaAlerta.text"></label>
                            <select class="form-control" data-bind="options: TelaAlerta.options, optionsText: 'text', optionsValue: 'value', selectedOptions: TelaAlerta.val, attr: { id : TelaAlerta.id}" multiple="multiple"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: QuantidadeDias.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeDias.text"></label>
                            <input type="text" class="form-control" data-bind="value: QuantidadeDias.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeDias.maxlength, id : QuantidadeDias.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text"></label>
                            <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Funcionario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Funcionario.requiredClass, value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Funcionario.text, maxlength: Funcionario.maxlength, id: Funcionario.id, name: Funcionario.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Funcionario.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: QuantidadeDiasAlertaOsInterna.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeDiasAlertaOsInterna.text"></label>
                            <input type="text" class="form-control" data-bind="value: QuantidadeDiasAlertaOsInterna.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeDiasAlertaOsInterna.maxlength, id : QuantidadeDiasAlertaOsInterna.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: QuantidadeDiasAlertaOsExterna.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeDiasAlertaOsExterna.text"></label>
                            <input type="text" class="form-control" data-bind="value: QuantidadeDiasAlertaOsExterna.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeDiasAlertaOsExterna.maxlength, id : QuantidadeDiasAlertaOsExterna.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Situacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Situacao.text"></label>
                            <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', selectedOptions: Situacao.val, attr: { id : Situacao.id}"></select>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDCadastroControleAlerta">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick,visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick,visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick,visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick,visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/controleAlerta"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadControleAlerta();
    });

</script>

