                                    <div class="card-header d-flex align-items-center justify-content-between">
                                        <h3 class="card-title">{Localize::Veiculos.Veiculo.Rejeicao}</h3>
                                    </div>
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col col-6">
                                                <span data-bind="html: MotivoRejeicao.val"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col col-sm-12 col-md-6 col-lg-6" style="padding-left:0px !important" data-bind="visible: Situacao.visible">
                                <div class="well">
                                    <header>
                                        {Localize::Gerais.Geral.Detalhes}
                                    </header>
                                    <fieldset>
                                        <div class="row">
                                            <section class="col col-12" data-bind="visible: Tipo.visible">
                                                <label><b data-bind="text: Tipo.text"></b> <span data-bind="text: Tipo.val"></span></label>
                                            </section>
                                            <section class="col col-12" data-bind="visible: SituacaoDescricao.visible">
                                                <label><b data-bind="text: SituacaoDescricao.text"></b> <span data-bind="text: SituacaoDescricao.val"></span></label>
                                            </section>
                                            <section class="col col-12" data-bind="visible: Solicitate.visible">
                                                <label><b data-bind="text: Solicitate.text"></b> <span data-bind="text: Solicitate.val"></span></label>
                                            </section>
                                            <section class="col col-12" data-bind="visible: DataCadastro.visible">
                                                <label><b data-bind="text: DataCadastro.text"></b> <span data-bind="text: DataCadastro.val"></span></label>
                                            </section>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                            <section class="col col-sm-12 col-md-12" data-bind="visible: MensagemEtapaSemRegra.visible() && !UsuariosAutorizadores.visible()">
                                <div class="well">
                                    <header data-bind="text: MensagemEtapaSemRegra.title"></header>
                                    <fieldset>
                                        <div class="widget-body" data-bind="html: MensagemEtapaSemRegra.text"></div>
                                    </fieldset>
                                </div>
                            </section>

                            <section class="col col-sm-12 col-md-6 col-lg-6" style="padding-left:0px !important" data-bind="visible: UsuariosAutorizadores.visible">
                                <div class="well">
                                    <header>
                                        {Localize::Veiculos.Veiculo.AutorizadosDoCadastro}
                                    </header>
                                    <fieldset>
                                        <div class="widget-body">
                                            <section class="insideNoPaddignTable">
                                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: UsuariosAutorizadores.idGrid}" cellspacing="0"></table>
                                            </section>
                                        </div>
                                    </fieldset>
                                </div>
                            </section>

                        </div>
                    </div>



                </form>
            </div>
        </div>
    </div>
</div>


