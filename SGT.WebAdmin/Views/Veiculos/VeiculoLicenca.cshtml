
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Veiculos</li>
    <li class="breadcrumb-item active">Veiculo Licença</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        {Localize::Veiculos.VeiculoLicenca.LicencasDosVeiculos}
    </h1>
</div>

<div>
    <div class="panel">
        <div class="panel-container"  id="knockoutPesquisaLicencaVeiculo">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    {Localize::Veiculos.VeiculoLicenca.PesquisarLicencasDosVeiculos}
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form >
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: Placa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Placa.text, attr: { for: Placa.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Placa.requiredClass, value: Placa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Placa.maxlength, id : Placa.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: StatusLicenca.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: StatusLicenca.text, attr: { for: StatusLicenca.id }"></label>
                                    <select class="form-control" data-bind="class: StatusLicenca.requiredClass, options: StatusLicenca.options, optionsText: 'text', optionsValue: 'value', value: StatusLicenca.val, attr: { maxlength: StatusLicenca.maxlength, id : StatusLicenca.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEmissaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoInicial.text, attr: { for: DataEmissaoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoInicial.requiredClass, value: DataEmissaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoInicial.maxlength, id : DataEmissaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEmissaoLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoLimite.text, attr: { for: DataEmissaoLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoLimite.requiredClass, value: DataEmissaoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoLimite.maxlength, id : DataEmissaoLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataVencimentoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoInicial.text, attr: { for: DataVencimentoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoInicial.requiredClass, value: DataVencimentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoInicial.maxlength, id : DataVencimentoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataVencimentoLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoLimite.text, attr: { for: DataVencimentoLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoLimite.requiredClass, value: DataVencimentoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoLimite.maxlength, id : DataVencimentoLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Empresa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Empresa.maxlength, id : Empresa.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Empresa.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div data-bind="id: BuscaAvancada.idFade, fadeVisible: BuscaAvancada.visibleFade" class=" row">
                            <div class="col-12 col-md-2" data-bind="visible: NumeroContainer.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroContainer.text, attr: { for: NumeroContainer.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroContainer.requiredClass, value: NumeroContainer.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroContainer.maxlength, id : NumeroContainer.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: StatusVigencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: StatusVigencia.text, attr: { for: StatusVigencia.id }"></label>
                                    <select class="form-control" data-bind="class: StatusVigencia.requiredClass, options: StatusVigencia.options, optionsText: 'text', optionsValue: 'value', value: StatusVigencia.val, attr: { maxlength: StatusVigencia.maxlength, id : StatusVigencia.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>

                    </div>
                    <table width="100%" class="table table-bordered table-hover" id="grid-pesquisa-veiculo-licencas" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
    <div id="container-licenca-veiculo"></div>
</div>
<script src="~/scripts/veiculoLicenca"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("Veiculos.VeiculoLicenca").then(loadVeiculoLicenca);
    });

</script>
