
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">RH</li>
    <li class="breadcrumb-item active">Folha Lancamento</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Lançamentos da Folha
    </h1>
</div>

<div id="knockoutPesquisaFolhaLancamento">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Lançamentos da Folha
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroEvento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroEvento.text, attr: { for: NumeroEvento.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroEvento.requiredClass, value: NumeroEvento.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroEvento.maxlength, id : NumeroEvento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroContrato.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroContrato.text, attr: { for: NumeroContrato.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroContrato.requiredClass, value: NumeroContrato.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroContrato.maxlength, id : NumeroContrato.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Funcionario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Funcionario.requiredClass, value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Funcionario.maxlength, id : Funcionario.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Funcionario.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: FolhaInformacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: FolhaInformacao.text, attr: { for: FolhaInformacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: FolhaInformacao.requiredClass, value: FolhaInformacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: FolhaInformacao.maxlength, id : FolhaInformacao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: FolhaInformacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Lançamento da Folha
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroFolhaLancamento">
                    <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Funcionario.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Funcionario.requiredClass, value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Funcionario.maxlength, id : Funcionario.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Funcionario.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: FolhaInformacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: FolhaInformacao.text, attr: { for: FolhaInformacao.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: FolhaInformacao.requiredClass, value: FolhaInformacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: FolhaInformacao.maxlength, id : FolhaInformacao.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: FolhaInformacao.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: NumeroEvento.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroEvento.text, attr: { for: NumeroEvento.id }"></label>
                            <input type="text" class="form-control" data-bind="class: NumeroEvento.requiredClass, value: NumeroEvento.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroEvento.maxlength, id : NumeroEvento.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: NumeroContrato.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroContrato.text, attr: { for: NumeroContrato.id }"></label>
                            <input type="text" class="form-control" data-bind="class: NumeroContrato.requiredClass, value: NumeroContrato.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroContrato.maxlength, id : NumeroContrato.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Base.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Base.text, attr: { for: Base.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Base.requiredClass, value: Base.val, valueUpdate: 'afterkeydown', attr: { maxlength: Base.maxlength, id : Base.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Referencia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Referencia.text, attr: { for: Referencia.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Referencia.requiredClass, value: Referencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Referencia.maxlength, id : Referencia.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Valor.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Valor.text, attr: { for: Valor.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Valor.requiredClass, value: Valor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Valor.maxlength, id : Valor.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DataCompetencia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataCompetencia.text, attr: { for: DataCompetencia.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataCompetencia.requiredClass, value: DataCompetencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCompetencia.maxlength, id : DataCompetencia.id}" />
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDFolhaLancamento">

                    <div class="row">

                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success mx-2" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-default mx-2" data-bind="click: Importar.eventClick, visible: Importar.visible, attr: { value: Importar.text, id : Importar.id}">

                                <span data-bind="text: Importar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-primary" data-bind="click: MultiploTitulo.eventClick, visible: MultiploTitulo.visible, attr: { value: MultiploTitulo.text, id : MultiploTitulo.id}">

                                <span data-bind="text: MultiploTitulo.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<div id="divModalImportarFolhaLancamento" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Importar planilha de Folhas</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body" id="knockoutImportarFolhaLancamento">
                <div class="row">
                    <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DataCompetencia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataCompetencia.text, attr: { for: DataCompetencia.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataCompetencia.requiredClass, value: DataCompetencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCompetencia.maxlength, id : DataCompetencia.id}" />
                        </div>
                    </div>
                    <div class="form-group col-12 col-md-6" data-bind="visible: Arquivo.visible">
                        <p class="form-label" data-bind="text: Arquivo.text"></p>
                        <div class="custom-file" data-bind="visible: Arquivo.visible">
                            <input type="file" class="custom-file-input" data-bind="value: Arquivo.val, attr: { id: Arquivo.id }" readonly />
                            <label class="custom-file-label" data-bind="text: Arquivo.val, attr: { for: Arquivo.id }">Arquivo</label>
                        </div>
                    </div>
                </div>

                <div class="d-flex justify-content-end mt-3">

                    <button class="btn btn-success" type="button" data-bind="click: Importar.eventClick, attr: { id : Importar.id}">
                        <i class="fal fa-check"></i><span data-bind="text: Importar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
