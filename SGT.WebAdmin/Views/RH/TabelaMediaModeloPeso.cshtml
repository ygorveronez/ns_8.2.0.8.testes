
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">RH</li>
    <li class="breadcrumb-item active">Tabela Media Modelo Peso</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i ></i> Tabela de Média por Peso e Modelo
    </h1>
</div>

<div id="knockoutPesquisaTabelaMediaModeloPeso">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Tabela de Média por Peso e Modelo
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Modelo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Modelo.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: Modelo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Modelo.maxlength, id : Modelo.id},  class: Modelo.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Modelo.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Tabela de Média por Peso e Modelo
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroTabelaMediaModeloPeso">
                    <div class="row">
                        <div class="col-12 col-md-6" data-bind="visible: Modelo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Modelo.text"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="value: Modelo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Modelo.maxlength, id : Modelo.id},  class: Modelo.requiredClass" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Modelo.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: MediaIdeal.visible">
                            <div class="form-group">
                                <label class="form-label"  data-bind="text: MediaIdeal.text"></label>
                                <input type="text" class="form-control" data-bind="css: MediaIdeal.requiredClass,value: MediaIdeal.val, valueUpdate: 'afterkeydown', attr: { maxlength: MediaIdeal.maxlength, id : MediaIdeal.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: PesoInicial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: PesoInicial.text"></label>
                                <input type="text" class="form-control" data-bind="css: PesoInicial.requiredClass,value: PesoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: PesoInicial.maxlength, id : PesoInicial.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: PesoFinal.visible">
                            <div class="form-group">
                                <label class="form-label"data-bind="text: PesoFinal.text"></label>
                                <input type="text" class="form-control" data-bind="css: PesoFinal.requiredClass,value: PesoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: PesoFinal.maxlength, id : PesoFinal.id}" />
                            </div>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDTabelaMediaModeloPeso">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>
<script src="~/scripts/tabelaMediaModeloPeso"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadTabelaMediaModeloPeso();
    });

</script>

