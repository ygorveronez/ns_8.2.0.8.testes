
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Moedas</li>
    <li class="breadcrumb-item">Cotação</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i> Cotação
    </h1>
</div>

<div id="knockoutPesquisaCotacao">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Cotação moeda
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">

                            <div class="col-12 col-md-2" data-bind=" visible : TipoPessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoPessoa.text"></label>
                                    <select class="form-control" data-bind="options: TipoPessoa.options, optionsText: 'text', optionsValue: 'value', value: TipoPessoa.val, attr: { id : TipoPessoa.id}, event : {change : TipoPessoa.eventChange}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind=" visible : Cliente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Cliente.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: Cliente.val, valueUpdate: 'afterkeydown',attr: { maxlength: Cliente.maxlength, id : Cliente.id},class: Cliente.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Cliente.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind=" visible : GrupoPessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoPessoa.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: GrupoPessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoPessoa.maxlength, id : GrupoPessoa.id}, class: GrupoPessoa.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoPessoa.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataVigencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVigencia.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataVigencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVigencia.maxlength, id : DataVigencia.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind=" visible : MoedaCotacaoBancoCentral.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: MoedaCotacaoBancoCentral.text"></label>
                                    <select class="form-control" data-bind="options: MoedaCotacaoBancoCentral.options, optionsText: 'text', optionsValue: 'value', value: MoedaCotacaoBancoCentral.val, attr: { id : MoedaCotacaoBancoCentral.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind=" visible : CotacaoAtiva.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CotacaoAtiva.text"></label>
                                    <select class="form-control" data-bind="options: CotacaoAtiva.options, optionsText: 'text', optionsValue: 'value', value: CotacaoAtiva.val, attr: { id : CotacaoAtiva.id}"></select>
                                </div>
                            </div>


                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Cotação
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutCadastroCotacao">
                <div class="row">
                    <div class="col-12 col-md-4" data-bind="visible: CotacaoAutomaticaViaWS.visible">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: CotacaoAutomaticaViaWS.val, attr: { maxlength: CotacaoAutomaticaViaWS.maxlength, name: CotacaoAutomaticaViaWS.id, id: CotacaoAutomaticaViaWS.id }">
                                <label class="custom-control-label" data-bind="text: CotacaoAutomaticaViaWS.text, attr: { for: CotacaoAutomaticaViaWS.id }"></label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: UtilizarCotacaoRetroativa.visible && CotacaoAutomaticaViaWS.val">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" data-bind="checked: UtilizarCotacaoRetroativa.val, attr: { maxlength: UtilizarCotacaoRetroativa.maxlength, name: UtilizarCotacaoRetroativa.id, id: UtilizarCotacaoRetroativa.id }">
                                <label class="custom-control-label" data-bind="text: UtilizarCotacaoRetroativa.text, attr: { for: UtilizarCotacaoRetroativa.id }"></label>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="row">
                    <div class="col-12 col-md-4" data-bind=" visible : MoedaCotacaoBancoCentral.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: MoedaCotacaoBancoCentral.text"></label>
                            <select class="form-control" data-bind="options: MoedaCotacaoBancoCentral.options, optionsText: 'text', optionsValue: 'value', value: MoedaCotacaoBancoCentral.val, attr: { id : MoedaCotacaoBancoCentral.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-4" data-bind=" fadeVisible : !CotacaoAutomaticaViaWS.val()">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ValorMoeda.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: ValorMoeda.val, attr: { maxlength: ValorMoeda.maxlength, id : ValorMoeda.id}, class: ValorMoeda.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="click: PesquisarCotacaoBancoCentral.eventClick, visible: PesquisarCotacaoBancoCentral.visible, attr: { value: PesquisarCotacaoBancoCentral.text, id : PesquisarCotacaoBancoCentral.id}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-4" data-bind=" visible : TipoPessoa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoPessoa.text"></label>
                            <select class="form-control" data-bind="options: TipoPessoa.options, optionsText: 'text', optionsValue: 'value', value: TipoPessoa.val, attr: { id : TipoPessoa.id}, event : {change : TipoPessoa.eventChange}"></select>
                        </div>
                    </div>


                    <div class="col-12 col-md-6" data-bind=" visible : Cliente.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Cliente.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: Cliente.val, valueUpdate: 'afterkeydown',attr: { maxlength: Cliente.maxlength, id : Cliente.id},class: Cliente.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Cliente.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-4" data-bind=" visible : GrupoPessoa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: GrupoPessoa.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: GrupoPessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoPessoa.maxlength, id : GrupoPessoa.id}, class: GrupoPessoa.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GrupoPessoa.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-2" data-bind="visible: DataVigenciaInicial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataVigenciaInicial.text, attr: { for: DataVigenciaInicial.id }"></label>
                            <input type="text" class="form-control" data-bind="value: DataVigenciaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVigenciaInicial.maxlength, id : DataVigenciaInicial.id, class: DataVigenciaInicial.requiredClass}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DataVigenciaFinal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataVigenciaFinal.text"></label>
                            <input type="text" class="form-control" data-bind="value: DataVigenciaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVigenciaFinal.maxlength, id : DataVigenciaFinal.id, class: DataVigenciaFinal.requiredClass}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: CotacaoAtiva.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CotacaoAtiva.text"></label>
                            <select class="form-control" data-bind="options: CotacaoAtiva.options, optionsText: 'text', optionsValue: 'value', value: CotacaoAtiva.val, attr: { id : CotacaoAtiva.id}"></select>
                        </div>
                    </div>

                </div>
                <footer>
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger me-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/cotacao"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadCotacao();
    });

</script>

