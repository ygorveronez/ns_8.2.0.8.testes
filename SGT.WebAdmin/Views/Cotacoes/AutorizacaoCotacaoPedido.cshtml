
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Cotações</li>
    <li class="breadcrumb-item active">Autorização Cotação Pedido</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Autorização de Cotação Pedido
    </h1>
</div>

<div id="knockoutPesquisaCotacaoPedido">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-search"></i>
                    Pesquisar Cotações Pedido
                </h3>
            </div>
            <div class="panel-content">
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroPedido.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroPedido.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroPedido.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroPedido.maxlength, id : NumeroPedido.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoCarga.text, attr: { for: TipoCarga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: TipoCarga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': TipoCarga.text, maxlength: TipoCarga.maxlength, id: TipoCarga.id, name: TipoCarga.id }, class: TipoCarga.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: TipoCarga.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Usuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Usuario.text, maxlength: Usuario.maxlength, id: Usuario.id, name: Usuario.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: Usuario.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: GrupoPessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoPessoa.text, attr: { for: GrupoPessoa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: GrupoPessoa.requiredClass, value: GrupoPessoa.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': GrupoPessoa.text, maxlength: GrupoPessoa.maxlength, id: GrupoPessoa.id, name: GrupoPessoa.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: GrupoPessoa.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}, class: TipoOperacao.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: TipoOperacao.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    Cotação Pedido
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: SelecionarTodos.eventClick, attr: { id: SelecionarTodos.id}, visible: SelecionarTodos.visible" type="button">
                            <i></i><span data-bind="text: SelecionarTodos.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </form>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-danger ms-1" type="button" data-bind="click: RejeitarTodas.eventClick, attr: { id: RejeitarTodas.id }, visible: RejeitarTodas.visible">
                        <span data-bind="text: RejeitarTodas.text"></span>
                    </button>
                    <button class="btn btn-primary ms-1" type="button" data-bind="click: DelegarCotacaoPedido.eventClick, attr: { id: DelegarCotacaoPedido.id }, visible: DelegarCotacaoPedido.visible">
                        <span data-bind="text: DelegarCotacaoPedido.text"></span>
                    </button>
                    <button class="btn btn-success ms-1" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id: AprovarTodas.id }, visible: AprovarTodas.visible">
                        <span data-bind="text: AprovarTodas.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="divModalCotacaoPedido" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Informações da Carga</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <ul class="nav nav-tabs">
                    <li class="nav-item">
                        <a href="#knockoutCotacaoPedido" class="nav-link active" data-bs-toggle="tab">
                            <i class="fal fa-info-circle"></i><span class="hidden-mobile hidden-tablet">Cotação Pedido</span>
                        </a>
                    </li>
                    <li id="liAutorizacao" class="nav-item">
                        <a href="#knockoutAutorizacao" class="nav-link" data-bs-toggle="tab">
                            <i class="fal fa-check"></i><span class="hidden-mobile hidden-tablet">Autorização</span>
                        </a>
                    </li>
                    <li id="liDelegar" class="nav-item" style="display:none">
                        <a href="#knockoutDelegar" class="nav-link" data-bs-toggle="tab">
                            <i class="fal fa-info-circle"></i><span class="hidden-mobile hidden-tablet">Delegar</span>
                        </a>
                    </li>
                </ul>
                <div class="tab-content border border-top-0 p-3">
                    <div id="knockoutCotacaoPedido" class="tab-pane fade active show">
                        <h3>Detalhes da Carga</h3>
                        <div class="row">
                            <div class="col col-md-4">
                                <div class="row">
                                    <label>
                                        <b data-bind="text: NumeroPedido.text"></b>
                                        <span data-bind="text: NumeroPedido.val"></span>
                                    </label>
                                    <label>
                                        <b data-bind="text: ValorTotalCotacao.text"></b>
                                        <span data-bind="text: ValorTotalCotacao.val"></span>
                                    </label>
                                    <label>
                                        <b data-bind="text: DataPedido.text"></b>
                                        <span data-bind="text: DataPedido.val"></span>
                                    </label>
                                </div>
                            </div>
                            <div class="col col-md-4">
                                <div class="row">
                                    <label>
                                        <b data-bind="text: NumeroPedido.text"></b>
                                        <span data-bind="text: NumeroPedido.val"></span>
                                    </label>
                                    <label>
                                        <b data-bind="text: ValorTotalCotacao.text"></b>
                                        <span data-bind="text: ValorTotalCotacao.val"></span>
                                    </label>
                                    <label>
                                        <b data-bind="text: DataPedido.text"></b>
                                        <span data-bind="text: DataPedido.val"></span>
                                    </label>
                                </div>
                            </div>
                            <div class="col col-md-4">
                                <div class="row">
                                    <label>
                                        <b data-bind="text: Situacao.text"></b> <span data-bind="text: Situacao.val"></span>
                                    </label>
                                    <label>
                                        <b data-bind="text: TipoCarga.text"></b>
                                        <span data-bind="text: TipoCarga.val"></span>
                                    </label>
                                    <label>
                                        <b data-bind="text: TipoOperacao.text"></b>
                                        <span data-bind="text: TipoOperacao.val"></span>
                                    </label>
                                </div>
                            </div>
                            <div class="col col-md-4">
                                <div class="row">
                                    <label>
                                        <b data-bind="text: Destinatario.text"></b>
                                        <span data-bind="text: Destinatario.val"></span>
                                    </label>
                                    <label>
                                        <b data-bind="text: Solicitante.text"></b>
                                        <span data-bind="text: Solicitante.val"></span>
                                    </label>
                                </div>
                            </div>
                            <div class="col col-md-4">
                                <div class="row">
                                    <label data-bind="visible: Observacao.visible && Observacao.val() != ''">
                                        <b data-bind="text: Observacao.text"></b>
                                        <span data-bind="text: Observacao.val"></span>
                                    </label>
                                    <label data-bind="visible: MotivoCancelamento.visible && MotivoCancelamento.val() != ''">
                                        <b data-bind="text: MotivoCancelamento.text"></b>
                                        <span data-bind="text: MotivoCancelamento.val"></span>

                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="knockoutAutorizacao" class="tab-pane fade">
                        <h3>Regras </h3>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Tomador.visible && Tomador.permiteSelecionarTomador">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Tomador.text"></label>
                                    <select class="form-control" data-bind="options: Tomador.options, optionsText: 'text', optionsValue: 'value', value: Tomador.val, attr: { id : Tomador.id}"></select>
                                </div>
                            </div>
                            <div class="form-group col-12 col-md-6" data-bind="visible: Justificativa.visible">
                                <label class="form-label" data-bind="text: Justificativa.text, attr: { for: Justificativa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="value: Justificativa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Justificativa.maxlength, id : Justificativa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Justificativa.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: Motivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                                    <textarea class="form-control" rows="4" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id }"></textarea>
                                </div>
                            </div>
                        </div>
                        <section data-bind="visible: Rejeitar.visible" class="col col-xs-12 col-sm-12 col-md-12 col-lg-12 text-right" style="margin-bottom: 20px;">
                            <input type="button" data-bind="click: Cancelar.eventClick, attr: { value: Cancelar.text, id : Cancelar.id}" class="btn btn-labeled btn-default botaoDentroSmartAdmimForm">
                            <input type="button" data-bind="click: Rejeitar.eventClick, attr: { value: Rejeitar.text, id : Rejeitar.id}" class="btn btn-labeled btn-danger botaoDentroSmartAdmimForm">
                        </section>

                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id : Regras.idGrid}"></table>
                        <footer data-bind="visible: AprovarTodas.visible">
                            <button class="btn btn-success me-2" type="button" data-bind="click: AprovarTodas.eventClick, attr: { id : AprovarTodas.id}">
                                <i class="fal fa-times"></i><span data-bind="text: AprovarTodas.text"></span>
                            </button>
                        </footer>
                    </div>
                    <div id="knockoutDelegar" class="tab-pane">
                        <h3>Delegar</h3>
                        <div class="col-12 col-md-6" data-bind="visible: UsuarioDelegado.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: UsuarioDelegado.text, attr: { for: UsuarioDelegado.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="value: UsuarioDelegado.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': UsuarioDelegado.text, maxlength: UsuarioDelegado.maxlength, id: UsuarioDelegado.id, name: UsuarioDelegado.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: UsuarioDelegado.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="d-flex justify-content-end mt-3">
                            <button class="btn btn-success" type="button" data-bind="click: Delegar.eventClick, attr: { id: Delegar.id}">
                                <span data-bind="text: Delegar.text"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalRejeitarCotacaoPedido" role="dialog" aria-hidden="true">
    <div id="knockoutRejeicaoCotacaoPedido" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Rejeitar Cotação Pedido</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group" data-bind="visible: Motivo.visible">
                    <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                    <textarea class="form-control" rows="5" type="text" data-bind="value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}"></textarea>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default me-2" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button class="btn btn-danger" type="button" data-bind="click: Rejeitar.eventClick, attr: { id : Rejeitar.id}">
                        <span data-bind="text: Rejeitar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="divModalDelegarCotacaoPedido" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutDelegarCotacaoPedido" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Delegar Cotação Pedido</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group" data-bind="visible: UsuarioDelegado.visible">
                    <label class="form-label" data-bind="text: UsuarioDelegado.text, attr: { for: UsuarioDelegado.id }"></label>
                    <div class="input-group">
                        <input type="text" class="form-control" data-bind="value: UsuarioDelegado.val, valueUpdate: 'afterkeydown', attr: { maxlength: UsuarioDelegado.maxlength, id : UsuarioDelegado.id}" />
                        <div class="input-group-append">
                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: UsuarioDelegado.idBtnSearch}">
                                <i class="fal fa-search"></i>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-default me-2" type="button" data-bind="click: Cancelar.eventClick, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button class="btn btn-success" type="button" data-bind="click: Delegar.eventClick, attr: { id : Delegar.id}">
                        <span data-bind="text: Delegar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="divModalDetalhesRegraCotacaoPedido" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutDetalhesRegraCotacaoPedido" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalhes da Regra</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row row-cols-1 row-cols-md-2">
                    <div class="col">
                        <table class="table table-clean table-sm align-self-end">
                            <tbody>
                                <tr>
                                    <td>
                                        <strong data-bind="text: MotivoRejeicao.text"></strong>
                                        <span data-bind="text: MotivoRejeicao.val"></span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <strong data-bind="text: Observacao.text"></strong>
                                        <span data-bind="text: Observacao.val"></span>
                                    </td>
                                </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



