
<section id="widget-grid">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Seguros</li>
        <li class="breadcrumb-item active">Fechamento Averbacao</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Fechamento Averbações
        </h1>
        <div class="subheader-help">
            <button type="button" onclick="iniciarAjuda(1177)" data-action="toggle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Abrir a ajuda da página" aria-label="Abrir a ajuda da página"><i class="fal fa-question-circle"></i></button>
        </div>
    </div>


    <div id="knockoutPesquisaFechamentoAverbacao" class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <div data-widget-colorbutton="false" data-widget-sortable="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-togglebutton="false">
                    <button class="btn btn-primary" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i> Pesquisar Fechamentos
                    </button>

                    <div data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade" class="mt-3">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-3" data-bind="visible: NumeroInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroInicial.text, attr: { for: NumeroInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroInicial.requiredClass, value: NumeroInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroInicial.maxlength, id : NumeroInicial.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: NumeroFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroFinal.text, attr: { for: NumeroFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroFinal.requiredClass, value: NumeroFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroFinal.maxlength, id : NumeroFinal.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: DataFim.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>

<div id="wid-id-4" class="panel" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Fechamento Averbações
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutEtapaFechamentoAverbacao">
                    <ul class="nav nav-steps">
                        <li data-target="#step1" data-bind="visible : Etapa1.visible">
                            <button class="active" data-bs-target="#knockoutDadosFechamento" data-bs-toggle="tab" data-bind="attr: {id : Etapa1.idTab }, click : Etapa1.eventClick, visible : Etapa1.visible">
                                <span class="step" data-bind="attr: {'data-content' : Etapa1.tooltip, 'data-original-title' : Etapa1.tooltipTitle }">
                                    <i class="fal fa-file-alt"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa1.text"></span>
                            </button>
                        </li>
                        <li data-target="#step2" data-bind="visible : Etapa2.visible">
                            <button data-bs-target="#knockoutAverbacoes" data-bs-toggle="tab" data-bind="attr: {id : Etapa2.idTab }, click : Etapa2.eventClick">
                                <span class="step" data-bind="attr: {'data-content' : Etapa2.tooltip, 'data-original-title' : Etapa2.tooltipTitle }">
                                    <i class="fal fa-dollar-sign"></i>
                                </span>
                                <span class="title" data-bind="text: Etapa2.text"></span>
                            </button>
                        </li>
                    </ul>
                </div>
                <div class="tab-content" id="myTabContent">
                    <!-- Etapa Dados Fechamento -->
                    <div id="knockoutDadosFechamento" class="tab-pane fade show active">
                        <div class="card mb-3">
                            <div class="card-header">
                                <h3 class="card-title">Dados do Fechamento</h3>
                                <button class="card-help" onclick="iniciarAjuda(1178)" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Entenda a página" aria-label="Entenda a página">
                                    <i class="fal fa-question-circle"></i>
                                </button>
                            </div>
                            <div class="card-body">
                                <!-- Filtro dos Documento -->
                                <div class="row">
                                    <div class="col-12 col-md-4" data-bind="visible: Numero.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Numero.text, attr: { for: Numero.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: Numero.requiredClass, value: Numero.val, enable: Numero.enable, valueUpdate: 'afterkeydown', attr: { maxlength: Numero.maxlength, id : Numero.id}" />
                                        </div>
                                    </div>
                                    
                                    <div class="col-12 col-md-4" data-bind="visible: DataInicio.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val,enable: DataInicio.enable, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-4" data-bind="visible: DataFim.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, enable: DataFim.enable, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: Transportador.requiredClass,, enable: Transportador.enable, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}, enable: Transportador.enable" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6" data-bind="visible: TipoOperacao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, enable: TipoOperacao.enable, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}, enable: TipoOperacao.enable" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--/ Filtro das Averbacoes -->
                            </div>
                        </div>

                    </div>
                    <!--/ Etapa Dados Fechamento -->
                    <!-- Etapa Averbacoes -->
                    <div class="tab-pane fade padding-10 no-padding-bottom" id="knockoutAverbacoes">
                        <div class="card mb-3">
                            <div class="card-header">
                                <h3 class="card-title"> Dados da NFS </h3>
                                <button class="card-help" onclick="iniciarAjuda(1179)" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Entenda a página" aria-label="Entenda a página">
                                    <i class="fal fa-question-circle"></i>
                                </button>
                            </div>
                            <div class="card-body">
                                <div class=" row">
                                    <div class="col-12 col-md-3" data-bind="visible: Adicional.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Adicional.text, attr: { for: Adicional.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: Adicional.requiredClass, value: Adicional.val, valueUpdate: 'afterkeydown', attr: { maxlength: Adicional.maxlength, id : Adicional.id}" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-3" data-bind="visible: IOF.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: IOF.text, attr: { for: IOF.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: IOF.requiredClass, value: IOF.val, valueUpdate: 'afterkeydown', attr: { maxlength: IOF.maxlength, id : IOF.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3 mb-2" style="margin-top:27px; ">
                                        <button class="btn btn-success waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: AtualizarValores.eventClick, attr: { id: AtualizarValores.id}" type="button">
                                            <span data-bind="text: AtualizarValores.text"></span>
                                        </button>
                                    </div>
                                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Averbacoes.idGrid}" cellspacing="0"></table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUD">
                    <div class="row">
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button type="button" class="btn btn-labeled btn-danger ms-2" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-primary mx-3 ms-2" data-bind="click: GerarFechamento.eventClick, visible: GerarFechamento.visible, attr: { id : GerarFechamento.id}">

                                <span data-bind="text: GerarFechamento.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success ms-2" data-bind="click: Finalizar.eventClick, visible: Finalizar.visible, attr: { id : Finalizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Finalizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-default ms-2" data-loading-text="Aguarde..." data-bind="click: Limpar.eventClick, visible: Limpar.visible, attr: { id : Limpar.id}">
                                <span data-bind="text: Limpar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>

    </div>
</div>



<script src="~/scripts/fechamentoAverbacao"></script>

