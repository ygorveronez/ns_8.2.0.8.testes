
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Notas Fiscais</li>
    <li class="breadcrumb-item active">Observação Fiscal</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class=""></i> Observação Fiscal
    </h1>
</div>

<div id="knockoutPesquisaObservacaoFiscal">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Observação Fiscal
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-12" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Observação Fiscal
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutCadastroObservacaoFiscal">
                <div class="row">
                    <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Observacao.text"></label>
                            <textarea type="text" rows="3" class="form-control" data-bind="css: Observacao.requiredClass, value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                        </div>
                    </div>
                    <div class="col-12 col-md-8" data-bind="visible : NCMProduto.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NCMProduto.text"></label>
                            <input type="text" class="form-control" data-bind="value: NCMProduto.val, attr: { maxlength: NCMProduto.maxlength, id : NCMProduto.id} " />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible : CSTICMS.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CSTICMS.text"></label>
                            <select class="form-control" data-bind="options: CSTICMS.options, optionsText: 'text', optionsValue: 'value', value: CSTICMS.val, attr: { id : CSTICMS.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Estado.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Estado.text, attr: { for: Estado.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Estado.requiredClass, value: Estado.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Estado.text, maxlength: Estado.maxlength, id: Estado.id, name: Estado.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Estado.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Atividade.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Atividade.text, attr: { for: Atividade.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Atividade.requiredClass, value: Atividade.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Atividade.text, maxlength: Atividade.maxlength, id: Atividade.id, name: Atividade.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Atividade.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: NaturezaDaOperacao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NaturezaDaOperacao.text, attr: { for: NaturezaDaOperacao.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: NaturezaDaOperacao.requiredClass, value: NaturezaDaOperacao.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': NaturezaDaOperacao.text, maxlength: NaturezaDaOperacao.maxlength, id: NaturezaDaOperacao.id, name: NaturezaDaOperacao.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: NaturezaDaOperacao.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: CFOPNotaFiscal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CFOPNotaFiscal.text, attr: { for: CFOPNotaFiscal.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: CFOPNotaFiscal.requiredClass, value: CFOPNotaFiscal.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': CFOPNotaFiscal.text, maxlength: CFOPNotaFiscal.maxlength, id: CFOPNotaFiscal.id, name: CFOPNotaFiscal.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CFOPNotaFiscal.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Empresa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Empresa.requiredClass, value: Empresa.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Empresa.text, maxlength: Empresa.maxlength, id: Empresa.id, name: Empresa.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Empresa.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <footer>
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger me-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/observacaoFiscal"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadObservacaoFiscal();
    });

</script>

