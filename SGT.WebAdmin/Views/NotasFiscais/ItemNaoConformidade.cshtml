                            <div class="col-12 col-md-auto mb-2">
                                <button type="button" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.idBtnSearch}" class="btn btn-primary">
                                    <i class="fal fa-plus"></i><span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                            <div class="insideNoPaddignTable">
                                <table class="table table-bordered table-hover" data-bind="attr: {id: CFOP.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>
                    </div>

                    @*Tab Tipo Operação*@
                    <div class="tab-pane fade" id="tabTipoOperacao">
                        <div class="panel-heading">
                            <h4>Tipo Operação</h4>
                        </div>
                        <div class="row" id="knockoutTipoOperacaoItem">
                            <div class="col-12 col-md-auto mb-2">
                                <button type="button" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.idBtnSearch}" class="btn btn-primary">
                                    <i class="fal fa-plus"></i><span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                            <div class="insideNoPaddignTable">
                                <table class="table table-bordered table-hover" data-bind="attr: {id: TipoOperacao.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>
                    </div>

                    @*Tab Filial*@
                    <div class="tab-pane fade" id="tabFilial">
                        <div class="panel-heading">
                            <h4>Filial</h4>
                        </div>
                        <div class="row" id="knockoutFilialItem">
                            <div class="col-12 col-md-auto mb-2">
                                <button type="button" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.idBtnSearch}" class="btn btn-primary">
                                    <i class="fal fa-plus"></i><span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                            <div class="insideNoPaddignTable">
                                <table class="table table-bordered table-hover" data-bind="attr: {id: Filial.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>
                    </div>

                    @*Tab Fornecedores*@
                    <div class="tab-pane fade" id="tabFornecedor">
                        <div class="panel-heading">
                            <h4>Fornecedor</h4>
                        </div>
                        <div class="row" id="knockoutFornecedorItem">
                            <div class="col-12 col-md-auto mb-2">
                                <button type="button" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.idBtnSearch}" class="btn btn-primary">
                                    <i class="fal fa-plus"></i><span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                            <div class="insideNoPaddignTable">
                                <table class="table table-bordered table-hover" data-bind="attr: {id: Fornecedor.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="col mt-2" id="knockoutCRUDItemNaoConformidade">
                    <footer class="d-flex justify-content-end">
                        <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                            <span data-bind="text: Cancelar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                            <span data-bind="text: Excluir.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </footer>
                </div>

            </div>

        </div>
    </div>
</div>



