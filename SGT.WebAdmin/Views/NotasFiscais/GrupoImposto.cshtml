
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Notas fiscais</li>
    <li class="breadcrumb-item active">Grupo imposto</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Grupo de Imposto
    </h1>
</div>

<div id="knockoutPesquisaGrupoImposto">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Grupo de Imposto
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="Descricao: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="class: Ativo.requiredClass, options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { maxlength: Ativo.maxlength, id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel" id="knockoutCadastroGrupoImposto">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Grupo de Imposto
            </h3>
        </div>
        <div class="panel-content">
            <form>

                <div class="row">
                    <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="css: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: NCM.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NCM.text, attr: { for: NCM.id }"></label>
                            <input type="text" class="form-control" data-bind="class: NCM.requiredClass, value: NCM.val, valueUpdate: 'afterkeydown', attr: { maxlength: NCM.maxlength, id : NCM.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Ativo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                            <select class="form-control" data-bind="class: Ativo.requiredClass, options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { maxlength: Ativo.maxlength, id : Ativo.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-3">
                        <button class="btn btn-default" type="button" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                    </div>

                    <div class="row mt-2" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">


                        <div class="col-12 col-md-6" data-bind="visible: CopiarTabelaImposto.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CopiarTabelaImposto.text, attr: { for: CopiarTabelaImposto.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: CopiarTabelaImposto.requiredClass, value: CopiarTabelaImposto.val, valueUpdate: 'afterkeydown', attr: { maxlength: CopiarTabelaImposto.maxlength, id : CopiarTabelaImposto.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CopiarTabelaImposto.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" >
                            <button class="btn btn-primary" style="margin-top:31px;" type="button" data-bind="click: LancarNovosRegistros.eventClick, attr: { id: LancarNovosRegistros.id}">
                                <span data-bind="text: LancarNovosRegistros.text"></span>
                            </button>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="col-12 col-md-2" data-bind="visible: EstadoOrigem.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EstadoOrigem.text, attr: { for: EstadoOrigem.id }"></label>
                                <select class="form-control" data-bind="class: EstadoOrigem.requiredClass, options: EstadoOrigem.options, optionsText: 'text', optionsValue: 'value', value: EstadoOrigem.val, attr: { maxlength: EstadoOrigem.maxlength, id : EstadoOrigem.id}, event : {change : EstadoOrigem.eventChange}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: EstadoDestino.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EstadoDestino.text, attr: { for: EstadoDestino.id }"></label>
                                <select class="form-control" data-bind="class: EstadoDestino.requiredClass, options: EstadoDestino.options, optionsText: 'text', optionsValue: 'value', value: EstadoDestino.val, attr: { maxlength: EstadoDestino.maxlength, id : EstadoDestino.id}, event : {change : EstadoDestino.eventChange}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: TipoCampo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoCampo.text, attr: { for: TipoCampo.id }"></label>
                                <select class="form-control" data-bind="class: TipoCampo.requiredClass, options: TipoCampo.options, optionsText: 'text', optionsValue: 'value', value: TipoCampo.val, attr: { maxlength: TipoCampo.maxlength, id : TipoCampo.id}, event : {change : TipoCampo.eventChange}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: ValorCampoCFOP.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: ValorCampoCFOP.text, attr: { for: ValorCampoCFOP.id }"></label>
                                <input type="text" class="form-control" data-bind="class: ValorCampoCFOP.requiredClass, value: ValorCampoCFOP.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorCampoCFOP.maxlength, id : ValorCampoCFOP.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: ValorCampoDecimal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: ValorCampoDecimal.text, attr: { for: ValorCampoDecimal.id }"></label>
                                <input type="text" class="form-control" data-bind="class: ValorCampoDecimal.requiredClass, value: ValorCampoDecimal.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorCampoDecimal.maxlength, id : ValorCampoDecimal.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: ValorCampoCSTICMS.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: ValorCampoCSTICMS.text, attr: { for: ValorCampoCSTICMS.id }"></label>
                                <input type="text" class="form-control" data-bind="class: ValorCampoCSTICMS.requiredClass, value: ValorCampoCSTICMS.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorCampoCSTICMS.maxlength, id : ValorCampoCSTICMS.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: ValorCampoCST.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: ValorCampoCST.text, attr: { for: ValorCampoCST.id }"></label>
                                <input type="text" class="form-control" data-bind="class: ValorCampoCST.requiredClass, value: ValorCampoCST.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorCampoCST.maxlength, id : ValorCampoCST.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: ValorCampoString.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: ValorCampoString.text, attr: { for: ValorCampoString.id }"></label>
                                <input type="text" class="form-control" data-bind="class: ValorCampoString.requiredClass, value: ValorCampoString.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorCampoString.maxlength, id : ValorCampoString.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3 " style="margin-top:31px;">
                            <button class="btn btn-warning" type="button" data-bind="click: AlterarValorCampo.eventClick, attr: { id: AlterarValorCampo.id}">
                                <span data-bind="text: AlterarValorCampo.text"></span>
                            </button>
                        </div>
                        <footer class="d-flex justify-content-end mt-1">
                            <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick,visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </footer>
                    </div>
                </div>
                <div class="col-12 col-md-12" style="display: none;" id="divTabelaImposto">
                    <div class="card">
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h3 class="card-title"> Tabela de Impostos</h3>
                        </div>
                        <div class="card-body">
                            @*<table class="table table-condensed table-bordered table-hover" data-bind="attr: {id: ItensImposto.idGrid}" cellspacing="0"></table>*@
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: ItensImposto.idGrid}" cellspacing="0"></table>
                        </div>
                    </div>
                </div>
            </form>
            <footer class="d-flex justify-content-end mt-2">
                <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                    <span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick,visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                    <span data-bind="text: Excluir.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                    <span data-bind="text: Atualizar.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                    <span data-bind="text: Adicionar.text"></span>
                </button>
            </footer>
        </div>
    </div>
</div>

<script src="~/scripts/grupoImposto"></script>
<script type="text/javascript">
    pageSetUp();

    $(document).ready(function () {
        loadGrupoImposto();
    });

</script>

