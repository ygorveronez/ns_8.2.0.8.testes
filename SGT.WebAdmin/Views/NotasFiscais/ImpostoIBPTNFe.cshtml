
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Notas fiscis</li>
    <li class="breadcrumb-item active">Imposto IBPT NFe</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Imposto IBPT NFe
    </h1>
</div>

<div id="knockoutPesquisaImpostoIBPTNFe">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Imposto IBPT NFe
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: NCM.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NCM.text, attr: { for: NCM.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NCM.requiredClass, value: NCM.val, valueUpdate: 'afterkeydown', attr: { maxlength: NCM.maxlength, id : NCM.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-10" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel" id="knockoutCadastroImpostoIBPTNFe">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Imposto IBPT NFe
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row">
                    <div class="col-12 col-md-3" data-bind="visible: NCM.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NCM.text, attr: { for: NCM.id }"></label>
                            <input type="text" class="form-control" data-bind="class: NCM.requiredClass, value: NCM.val, valueUpdate: 'afterkeydown', attr: { maxlength: NCM.maxlength, id : NCM.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Extensao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Extensao.text, attr: { for: Extensao.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Extensao.requiredClass, value: Extensao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Extensao.maxlength, id : Extensao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Tipo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Tipo.text, attr: { for: Tipo.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Tipo.requiredClass, value: Tipo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Tipo.maxlength, id : Tipo.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: NacionalFederal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NacionalFederal.text, attr: { for: NacionalFederal.id }"></label>
                            <input type="text" class="form-control" data-bind="class: NacionalFederal.requiredClass, value: NacionalFederal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NacionalFederal.maxlength, id : NacionalFederal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: ImportadosFederal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ImportadosFederal.text, attr: { for: ImportadosFederal.id }"></label>
                            <input type="text" class="form-control" data-bind="class: ImportadosFederal.requiredClass, value: ImportadosFederal.val, valueUpdate: 'afterkeydown', attr: { maxlength: ImportadosFederal.maxlength, id : ImportadosFederal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Estadual.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Estadual.text, attr: { for: Estadual.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Estadual.requiredClass, value: Estadual.val, valueUpdate: 'afterkeydown', attr: { maxlength: Estadual.maxlength, id : Estadual.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Municipal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Municipal.text, attr: { for: Municipal.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Municipal.requiredClass, value: Municipal.val, valueUpdate: 'afterkeydown', attr: { maxlength: Municipal.maxlength, id : Municipal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: VigenciaInicio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: VigenciaInicio.text, attr: { for: VigenciaInicio.id }"></label>
                            <input type="text" class="form-control" data-bind="class: VigenciaInicio.requiredClass, value: VigenciaInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: VigenciaInicio.maxlength, id : VigenciaInicio.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: VigenciaFim.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: VigenciaFim.text, attr: { for: VigenciaFim.id }"></label>
                            <input type="text" class="form-control" data-bind="class: VigenciaFim.requiredClass, value: VigenciaFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: VigenciaFim.maxlength, id : VigenciaFim.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Versao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Versao.text, attr: { for: Versao.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Versao.requiredClass, value: Versao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Versao.maxlength, id : Versao.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Fonte.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Fonte.text, attr: { for: Fonte.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Fonte.requiredClass, value: Fonte.val, valueUpdate: 'afterkeydown', attr: { maxlength: Fonte.maxlength, id : Fonte.id}" />
                        </div>
                    </div>
                    <div class="form-group col-12 col-md-3" data-bind="visible: Arquivo.visible">
                        <p class="form-label" data-bind="text: Arquivo.text"></p>
                        <div class="custom-file" data-bind="visible: Arquivo.visible">
                            <input type="file" class="custom-file-input" data-bind="value: Arquivo.val, attr: { id: Arquivo.id }" readonly>
                            <label class="custom-file-label" data-bind="text: Arquivo.val, attr: { for: Arquivo.id }">Selecione o arquivo CSV</label>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" style="margin-top:27px;">
                        <button class="btn btn-warning" type="button" data-bind="click: ImportarCSV.eventClick, attr: { id: ImportarCSV.id}">
                            <span data-bind="text: ImportarCSV.text"></span>
                        </button>
                    </div>
                </div>

                <footer class="d-flex justify-content-end">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/impostoIBPTNFe"></script>
