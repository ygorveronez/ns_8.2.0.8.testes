                <div class="badge legend" data-bind="css: LegendaPorSituacao.cssClass" style="border-radius: 0;">
                    <b data-bind="text: LegendaPorSituacao.text"></b>
                </div>
                <!-- /ko -->
            </div>
        </div>

        <div class="panel">
            <div class="panel-container" id="container-janela-descarregamento">
                <div class="panel-hdr">
                    <h4>
                        <i class="fal fa-table"></i>
                        Janelas de Descarregamentos
                    </h4>
                </div>
                <div class="panel-content">
                    <form>
                        <div id="knockoutCapacidadeJanelaDescarregamento" class="capacidade-descarregamento-container" data-bind="visible: ExibirCapacidadeDescarregamento.val">
                            <div id="carousel-capacidade-descarregamento" class="carousel carousel-dark slide capacidade-descarregamento-periodo-container" data-ride="carousel">
                                <div class="carousel-inner">
                                    <!-- ko foreach: ListaCapacidadeDescarregamentoPeriodo -->
                                    <div class="carousel-item" data-bind="css: PeriodoAtivo.val() ? 'active' : ''">
                                        <h3 class="capacidade-descarregamento-titulo" data-bind="text: Periodo.val"></h3>
                                        <h4 class="capacidade-descarregamento-subtitulo" data-bind="text: PeriodoDescricao.val, visible: PeriodoDescricao.val() != ''"></h4>
                                        <div class="row capacidade-descarregamento-dados">
                                            <div class="col-12 col-md-6 col-xl-3">
                                                <span class="capacidade-descarregamento-label font-cyan">Capacidade:</span>
                                                <span class="capacidade-descarregamento-valor font-ambar" data-bind="text: CapacidadeDescarregamento.val"></span>
                                            </div>
                                            <div class="col-12 col-md-6 col-xl-3">
                                                <span class="capacidade-descarregamento-label font-cyan">Capacidade Adicional:</span>
                                                <span class="capacidade-descarregamento-valor font-ambar" data-bind="text: CapacidadeAdicional.val"></span>
                                            </div>
                                            <div class="col-12 col-md-6 col-xl-3">
                                                <span class="capacidade-descarregamento-label font-cyan">Capacidade Utilizada:</span>
                                                <span class="capacidade-descarregamento-valor font-ambar" data-bind="text: CapacidadeUtilizada.val"></span>
                                            </div>
                                            <div class="col-12 col-md-6 col-xl-3">
                                                <span class="capacidade-descarregamento-label font-cyan">Capacidade Disponível:</span>
                                                <span class="capacidade-descarregamento-valor font-ambar" data-bind="text: CapacidadeDisponivel.val"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /ko -->
                                </div>
                                <div data-bind="css: ListaCapacidadeDescarregamentoPeriodo().length > 1 ? '' : 'd-none'">
                                    <button class="carousel-control-prev" type="button" data-bs-target="#carousel-capacidade-descarregamento" data-bs-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="visually-hidden">Previous</span>
                                    </button>
                                    <button class="carousel-control-next" type="button" data-bs-target="#carousel-capacidade-descarregamento" data-bs-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="visually-hidden">Next</span>
                                    </button>
                                </div>
                            </div>

                        </div>
                        <div id="divListaDescarregamento"></div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

