<div id="divModalHorarioDescarregamento" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutHorarioDescarregamento" class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Horário para Descarregamento</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="col-12 col-md-12" data-bind="visible: NovoHorario.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: NovoHorario.text, attr: { for: NovoHorario.id }"></label>
                        <input type="text" class="form-control" data-bind="class: NovoHorario.requiredClass, value: NovoHorario.val, valueUpdate: 'afterkeydown', attr: { maxlength: NovoHorario.maxlength, id : NovoHorario.id}" />
                    </div>
                </div>
                <div class="col-12" data-bind="visible: SolicitouMotivo.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: MotivoReagendamento.text, attr: { for: MotivoReagendamento.id }"></label>
                        <textarea class="form-control" rows="5" type="text" data-bind="class: MotivoReagendamento.requiredClass, value: MotivoReagendamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: MotivoReagendamento.maxlength, id : MotivoReagendamento.id}"></textarea>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-success" type="button" data-bind="click: AlocarHorario.eventClick, visible: AlocarHorario.visible, attr: { value: AlocarHorario.text, id: AlocarHorario.id}">
                        <i class="fal fa-check"></i><span data-bind="text: AlocarHorario.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

