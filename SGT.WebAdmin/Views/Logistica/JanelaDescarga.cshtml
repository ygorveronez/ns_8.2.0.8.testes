    <div id="container-descarregamento" style="display: none;">
        <div class="co-l2" id="knockoutLegendaJanelaDescarregamento">
            <div class="legends mt-4 mb-4" data-bind="visible: ExibirLegendas.val">
                <span><b>Legendas:</b> </span>
                <div data-bind="visible: AguardandoConfirmacaoAgendamento.visible" class="badge legend orange">
                    <b data-bind="text: AguardandoConfirmacaoAgendamento.text"></b>
                </div>
                <div data-bind="visible: AguardandoDescarregamento.visible" class="badge legend blue">
                    <b data-bind="text: AguardandoDescarregamento.text"></b>
                </div>
                <div data-bind="visible: DescarregamentoFinalizado.visible" class="badge legend green">
                    <b data-bind="text: DescarregamentoFinalizado.text"></b>
                </div>
                <div data-bind="visible: NaoComparecimento.visible" class="badge legend pink">
                    <b data-bind="text: NaoComparecimento.text"></b>
                </div>
                <div data-bind="visible: Excedente.visible" class="badge legend gray">
                    <b data-bind="text: Excedente.text"></b>
                </div>
                <div data-bind="visible: DevolvidaParcialmente.visible" class="badge legend cammo-green">
                    <b data-bind="text: DevolvidaParcialmente.text"></b>
                </div>
                <div data-bind="visible: CargaDevolvida.visible" class="badge legend pastel-pink">
                    <b data-bind="text: CargaDevolvida.text"></b>
                </div>
                <div data-bind="visible: EntregueParcialmente.visible" class="badge legend darkgray">
                    <b data-bind="text: EntregueParcialmente.text"></b>
                </div>
                <div data-bind="visible: AguardandoGeracaoSenha.visible" class="badge legend sea-green">
                    <b data-bind="text: AguardandoGeracaoSenha.text"></b>
                </div>
                <div data-bind="visible: ChegadaConfirmada.visible" class="badge legend lightgreen">
                    <b data-bind="text: ChegadaConfirmada.text"></b>
                </div>
                <div data-bind="visible: SaidaVeiculoConfirmada.visible" class="badge legend aqua">
                    <b data-bind="text: SaidaVeiculoConfirmada.text"></b>
                </div>
                <div data-bind="visible: Desagendado.visible" class="legenda-desagendado ms-1">
                    <b data-bind="text: Desagendado.text"></b>
                </div>
                <div data-bind="visible: Cancelado.visible" class="badge legend red">
                    <b data-bind="text: Cancelado.text"></b>
                </div>
                <div data-bind="visible: ValidacaoFiscal.visible, style: { 'background-color': '#e6c972' }" class="badge legend">
                    <b data-bind="text: ValidacaoFiscal.text"></b>
                </div>
                <div data-bind="visible: Nucleo.visible, style: { 'background-color': '#d3c0f9' }" class="badge legend">
                    <b data-bind="text: Nucleo.text"></b>
                </div>
                <!-- ko foreach: { data: ListaLegendaDinamica, as: 'LegendaDinamica' } -->
                <div class="badge legend" data-bind="css: LegendaDinamica.cssClass" style="border-radius: 0;">
                    <b data-bind="text: LegendaDinamica.text"></b>
                </div>
                <!-- /ko -->
            </div>

            <div class="legends mt-4 mb-4" data-bind="visible: ExibirLegendasPorSituacao.val">
                <span><b>Legendas:</b> </span>
