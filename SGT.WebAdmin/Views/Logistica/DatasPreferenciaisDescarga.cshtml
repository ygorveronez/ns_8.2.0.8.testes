
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item active">Datas Preferenciais Descarga</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Datas Preferenciais para Descarga
    </h1>
</div>

<div id="knockoutPesquisaDatasPreferenciaisDescarga">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Datas Preferenciais Descarga
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">

                            <div class="col-12 col-md-4" data-bind="visible: CentroDescarregamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CentroDescarregamento.text, attr: { for: CentroDescarregamento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: CentroDescarregamento.requiredClass, value: CentroDescarregamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: CentroDescarregamento.maxlength, id : CentroDescarregamento.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CentroDescarregamento.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DiaPreferencial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DiaPreferencial.text, attr: { for: DiaPreferencial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DiaPreferencial.requiredClass, value: DiaPreferencial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DiaPreferencial.maxlength, id : DiaPreferencial.id}" />
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3" data-bind="fadeVisible: Pesquisar.visibleFade">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel" id="wid-id-4">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Formulário de Cadastro
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutDatasPreferenciaisDescarga">
                <div class="row">

                    <div class="col-12 col-md-2" data-bind="visible: DiasAnterioresBloqueados.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DiasAnterioresBloqueados.text, attr: { for: DiasAnterioresBloqueados.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DiasAnterioresBloqueados.requiredClass, value: DiasAnterioresBloqueados.val, valueUpdate: 'afterkeydown', attr: { maxlength: DiasAnterioresBloqueados.maxlength, id : DiasAnterioresBloqueados.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-2" data-bind="visible: DataPreferencial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataPreferencial.text, attr: { for: DataPreferencial.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataPreferencial.requiredClass, value: DataPreferencial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataPreferencial.maxlength, id : DataPreferencial.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-4" data-bind="visible: CentroDescarregamento.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CentroDescarregamento.text, attr: { for: CentroDescarregamento.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: CentroDescarregamento.requiredClass, value: CentroDescarregamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: CentroDescarregamento.maxlength, id : CentroDescarregamento.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CentroDescarregamento.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 d-flex mb-2">
                        <button data-bind="attr: { id: AdicionarDataPreferencial.idBtnSearch}, click: AdicionarDataPreferencial.eventClick" class="btn btn-primary botaoAdiconarMultiplosRegistros" type="button">
                            <i class="fal fa-plus"></i> <span data-bind="text: AdicionarDataPreferencial.text"></span>
                        </button>
                    </div>
                    <div class="insideNoPaddignTable">
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Grid.id}" cellspacing="0"></table>
                    </div>

                </div>
                <footer>
                    <div class="d-flex justify-content-end mt-3">
                        <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Limpar.eventClick, visible: Limpar.visible, attr: { id : Limpar.id}">
                            <span data-bind="text: Limpar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                            <span data-bind="text: Excluir.text"></span>
                        </button>
                        <span class="ms-1" data-bind='component: {name: "import-file", params: { ko: Importar }}'></span>
                        <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

<div id="modalAdicionarDatasPreferenciasDescarga" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutAdicionarDataPreferencialDescarga" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Adicionar Fornecedor e Categoria</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="col-12 col-md-12" data-bind="visible: TipoFornecedor.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: TipoFornecedor.text, attr: { for: TipoFornecedor.id }"></label>
                        <select class="form-control" data-bind="class: TipoFornecedor.requiredClass, options: TipoFornecedor.options, optionsText: 'text', optionsValue: 'value', value: TipoFornecedor.val, attr: { maxlength: TipoFornecedor.maxlength, id : TipoFornecedor.id}"></select>
                    </div>
                </div>

                <div class="col-12 col-md-12" data-bind="visible: GrupoFornecedor.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: GrupoFornecedor.text, attr: { for: GrupoFornecedor.id }"></label>
                        <div class="input-group">
                            <input type="text" class="form-control" data-bind="css: GrupoFornecedor.requiredClass, value: GrupoFornecedor.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoFornecedor.maxlength, id : GrupoFornecedor.id}" />
                            <div class="input-group-append">
                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: GrupoFornecedor.idBtnSearch}" type="button" aria-label="Buscar">
                                    <i class="fal fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-md-12" data-bind="visible: Fornecedor.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: Fornecedor.text, attr: { for: Fornecedor.id }"></label>
                        <div class="input-group">
                            <input type="text" class="form-control" data-bind="css: Fornecedor.requiredClass, value: Fornecedor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Fornecedor.maxlength, id : Fornecedor.id}" />
                            <div class="input-group-append">
                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Fornecedor.idBtnSearch}" type="button" aria-label="Buscar">
                                    <i class="fal fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-md-12" data-bind="visible: Categoria.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: Categoria.text, attr: { for: Categoria.id }"></label>
                        <div class="input-group">
                            <input type="text" class="form-control" data-bind="css: Categoria.requiredClass, value: Categoria.val, valueUpdate: 'afterkeydown', attr: { maxlength: Categoria.maxlength, id : Categoria.id}" />
                            <div class="input-group-append">
                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Categoria.idBtnSearch}" type="button" aria-label="Buscar">
                                    <i class="fal fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-3">
                    <button class="btn btn-success" type="button" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                        <i class="fal fa-check"></i><span data-bind="text: Adicionar.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/datasPreferenciaisDescarga"></script>

<script type="text/javascript">

    pageSetUp();

    $("body").addClass("minified");

    $(document).ready(function () {
        loadDatasPreferenciaisDescarga();
    });

</script>

