
<style>
    .widget-body-margin {
        margin: -13px -13px 0px;
    }
</style>
<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Logística</li>
        <li class="breadcrumb-item active">Fila Carregamento Notificação</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Notificações
        </h1>
    </div>

    <div id="knockoutPesquisaNotificacao">
        <div class="panel">
            <div class="panel-container">
                <div class="panel-content">
                    <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                        <i class="fal fa-search"></i>
                        Pesquisar Notificações
                    </button>

                    <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                        <form>
                            <div class="row">
                                <div class="col-12 col-md-2" data-bind="visible: DataInicio.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DataLimite.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataLimite.text, attr: { for: DataLimite.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: DataLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataLimite.maxlength, id : DataLimite.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: Tipo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Tipo.text, attr: { for: Tipo.id }"></label>
                                        <select class="form-control" data-bind="class: Tipo.requiredClass, options: Tipo.options, optionsText: 'text', optionsValue: 'value', value: Tipo.val, attr: { maxlength: Tipo.maxlength, id : Tipo.id}"></select>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6" data-bind="visible: Assunto.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Assunto.text, attr: { for: Assunto.id }"></label>
                                        <input type="text" class="form-control" data-bind="value: Assunto.val, valueUpdate: 'afterkeydown', attr: { maxlength: Assunto.maxlength, id : Assunto.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-6 col-lg-6" data-bind="visible: CentroCarregamento.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CentroCarregamento.text, attr: { for: CentroCarregamento.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="value: CentroCarregamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: CentroCarregamento.maxlength, id : CentroCarregamento.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CentroCarregamento.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6 col-lg-6" data-bind="visible: Motorista.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Motorista.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </form>
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel mt-3" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
        <div class="panel-container">
            <div class="panel-content">
                <ul class="nav nav-tabs">
                    <li class="nav-item active">
                        <a href="#tabCadastroNotificacao" class="nav-link active" data-bs-toggle="tab">
                            <span class="hidden-mobile hidden-tablet">Notificacao</span>
                        </a>
                    </li>
                    <li class="nav-item" style="display: none" id="aba-motorista">
                        <a href="#tabMotoristas" class="nav-link" data-bs-toggle="tab">
                            <span class="hidden-mobile hidden-tablet">Motoristas</span>
                        </a>
                    </li>
                </ul>
                <div class="tab-content border border-top-0 p-3">
                    <div class="tab-pane fade active show" id="tabCadastroNotificacao" role="tabpanel">
                        <form novalidate id="knockoutCadastroNotificacao">
                            <div class="row">
                                <div class="col-12 col-md-6" data-bind="visible: Codigo.val() == 0 || CentroCarregamento.codEntity() > 0">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CentroCarregamento.text, attr: { for: CentroCarregamento.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: CentroCarregamento.requiredClass, value: CentroCarregamento.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': CentroCarregamento.text, maxlength: CentroCarregamento.maxlength, id: CentroCarregamento.id, name: CentroCarregamento.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CentroCarregamento.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6" data-bind="visible: Codigo.val() == 0">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Transportador.text, maxlength: Transportador.maxlength, id: Transportador.id, name: Transportador.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Transportador.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6" data-bind="visible: Codigo.val() == 0">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: ModeloVeicularCarga.text, attr: { for: ModeloVeicularCarga.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: ModeloVeicularCarga.requiredClass, value: ModeloVeicularCarga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': ModeloVeicularCarga.text, maxlength: ModeloVeicularCarga.maxlength, id: ModeloVeicularCarga.id, name: ModeloVeicularCarga.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: ModeloVeicularCarga.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6" data-bind="visible: Codigo.val() == 0">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                        <div class="input-group">
                                            <input type="text" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Motorista.text, maxlength: Motorista.maxlength, id: Motorista.id, name: Motorista.id }" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Motorista.idBtnSearch}">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-12" data-bind="visible: Assunto.visible, css: { 'col-md-6': Codigo.val() > 0 && CentroCarregamento.codEntity() > 0 }">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Assunto.text, attr: { for: Assunto.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: Mensagem.requiredClass, value: Assunto.val, valueUpdate: 'afterkeydown', attr: { maxlength: Assunto.maxlength, id : Assunto.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-12" data-bind="visible: Mensagem.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Mensagem.text, attr: { for: Mensagem.id }"></label>
                                        <textarea class="form-control" rows="5" type="text" data-bind="class: Mensagem.requiredClass, value: Mensagem.val, valueUpdate: 'afterkeydown', attr: { maxlength: Mensagem.maxlength, id : Mensagem.id}"></textarea>
                                    </div>
                                </div>

                            </div>
                        </form>
                    </div>

                    <div class="tab-pane" id="tabMotoristas">
                        <form>
                            <div class="row">
                                <div class="widget-body">
                                    <table id="grid-notificacao-motorista" width="100%" class="table table-bordered table-hover" cellspacing="0"></table>
                                </div>
                            </div>

                        </form>
                    </div>
                    <footer id="knockoutCRUDNotificacao">
                        <div class="row">
                            <div class="col-6">
                                <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, text: Cancelar.text, attr: { id : Cancelar.id}">
                                    <i class="fal fa-ban"></i>
                                </button>
                            </div>
                            <div class="col-6 d-flex justify-content-end">
                                <button type="button" class="btn btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                                    <i class="fal fa-spinner"></i>
                                </button>
                            </div>
                        </div>
                    </footer>
                </div>

            </div>
        </div>
    </div>
</section>

    <script src="~/scripts/filaCarregamentoNotificacao"></script>
    <script type="text/javascript">
        pageSetUp();

        $(document).ready(function () {
            loadFilaCarregamentoNotificacao();
        });

    </script>

