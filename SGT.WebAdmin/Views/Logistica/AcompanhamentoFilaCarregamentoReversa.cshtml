                                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CentroCarregamento.idBtnSearch}" type="button" aria-label="Buscar">
                                                            <i class="fal fa-search"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12 col-md-4" data-bind="visible: Proximidade.visible">
                                            <div class="form-group">
                                                <label class="form-label" data-bind="text: Proximidade.text, attr: { for: Proximidade.id }"></label>
                                                <select class="form-control" data-bind="class: Proximidade.requiredClass, options: Proximidade.options, optionsText: 'text', optionsValue: 'value', value: Proximidade.val, attr: { maxlength: Proximidade.maxlength, id : Proximidade.id}"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class=" d-flex  justify-content-md-end mb-2">

                                        <button class="btn btn-primary btnPesquisarFiltroPesquisa me-2" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="d-none fila-carregamento-container" id="fila-carregamento-reversa-container">
