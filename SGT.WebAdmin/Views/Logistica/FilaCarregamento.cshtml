    <div class="d-none" id="fila-carregamento-container">
        <div class="panel" id="fila-carregamento-container-grafico">
            <div class="panel-container" id="knockoutFilaCarregamentoResumo">
                <div class="panel-content">
                    <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirDados.eventClick, id: ExibirDados.id">
                        <i class="fal fa-table"></i> <span>Resumo da Fila de Carregamento</span>
                    </button>
                    <div data-bind="id: ExibirDados.idFade, fadeVisible: ExibirDados.visibleFade" class="mt-2">
                        <form>
                            <div class="row">
                                <div class="col-2"></div>
                                <div class="col-12 col-md-8">
                                    <div id="fila-carregamento-resumo-container"></div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel" id="knockoutFilaCarregamentoResumoPorSituacao" data-bind="visible: ExibirResumo.val">
            <div class="panel-container">
                <div class="panel-content">
                    <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirDados.eventClick, id: ExibirDados.id">
                        <i class="fal fa-table"></i> <span>Resumo</span>
                    </button>
                    <div data-bind="id: ExibirDados.idFade, fadeVisible: ExibirDados.visibleFade" class="mt-2">
                        <table width="100%" class="table table-bordered table-condensed no-footer dataTable JColResizer" cellspacing="0">
                            <thead>
                                <tr role="row" class="font-ambar">
                                    <th></th>
                                    <th class="text-align-center" data-bind="text: Vazio.text, visible: Vazio.visible, attr: { title: Vazio.text }"></th>
                                    <th class="text-align-center" data-bind="text: AguardandoDesatrelar.text, visible: AguardandoDesatrelar.visible() && UtilizarFilaCarregamentoReversa.val(), attr: { title: AguardandoDesatrelar.text }"></th>
                                    <th class="text-align-center" data-bind="text: EmChecklist.text, visible: EmChecklist.visible, attr: { title: EmChecklist.text }"></th>
                                    <th class="text-align-center" data-bind="text: EmReversa.text, visible: EmReversa.visible, attr: { title: EmReversa.text }"></th>
                                    <th class="text-align-center" data-bind="text: AguardandoAceitePreCarga.text, visible: AguardandoAceitePreCarga.visible(), attr: { title: AguardandoAceitePreCarga.text }"></th>
                                    <th class="text-align-center" data-bind="text: AguardandoAceite.text, visible: AguardandoAceite.visible() && !UtilizarFilaCarregamentoReversa.val(), attr: { title: AguardandoAceite.text }"></th>
                                    <th class="text-align-center" data-bind="text: AguardandoConfirmacao.text, visible: AguardandoConfirmacao.visible() && !UtilizarFilaCarregamentoReversa.val(), attr: { title: AguardandoConfirmacao.text }"></th>
                                    <th class="text-align-center" data-bind="text: AguardandoConjuntos.text, visible: AguardandoConjuntos.visible, attr: { title: AguardandoConjuntos.text }"></th>
                                    <th class="text-align-center" data-bind="text: AguardandoCarga.text, visible: AguardandoCarga.visible() && (UtilizarFilaCarregamentoReversa.val() || UtilizarProgramacaoCarga.val()), attr: { title: AguardandoCarga.text }"></th>
                                    <th class="text-align-center" data-bind="text: CargaCancelada.text, visible: CargaCancelada.visible, attr: { title: CargaCancelada.text }"></th>
                                    <th class="text-align-center" data-bind="text: CargaRecusada.text, visible: CargaRecusada.visible() && !UtilizarFilaCarregamentoReversa.val(), attr: { title: CargaRecusada.text }"></th>
                                    <th class="text-align-center" data-bind="text: PerdeuSenha.text, visible: PerdeuSenha.visible() && !UtilizarFilaCarregamentoReversa.val(), attr: { title: PerdeuSenha.text }"></th>
                                    <th class="text-align-center" data-bind="text: EmRemocao.text, visible: EmRemocao.visible() && !UtilizarFilaCarregamentoReversa.val(), attr: { title: EmRemocao.text }"></th>
                                    <th class="text-align-center" data-bind="text: EmViagem.text, visible: EmViagem.visible() && !UtilizarFilaCarregamentoReversa.val(), attr: { title: EmViagem.text }"></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr role="row" class="text-align-center">
                                    <td class="fw-bold font-cyan">Total</td>
                                    <td data-bind="text: Vazio.val, visible: Vazio.visible"></td>
                                    <td data-bind="text: AguardandoDesatrelar.val, visible: AguardandoDesatrelar.visible() && UtilizarFilaCarregamentoReversa.val()"></td>
                                    <td data-bind="text: EmChecklist.val, visible: EmChecklist.visible"></td>
                                    <td data-bind="text: EmReversa.val, visible: EmReversa.visible"></td>
                                    <td data-bind="text: AguardandoAceitePreCarga.val, visible: AguardandoAceitePreCarga.visible()"></td>
                                    <td data-bind="text: AguardandoAceite.val, visible: AguardandoAceite.visible() && !UtilizarFilaCarregamentoReversa.val()"></td>
                                    <td data-bind="text: AguardandoConfirmacao.val, visible: AguardandoConfirmacao.visible() && !UtilizarFilaCarregamentoReversa.val()"></td>
                                    <td data-bind="text: AguardandoConjuntos.val, visible: AguardandoConjuntos.visible"></td>
                                    <td data-bind="text: AguardandoCarga.val, visible: AguardandoCarga.visible() && (UtilizarFilaCarregamentoReversa.val() || UtilizarProgramacaoCarga.val())"></td>
                                    <td data-bind="text: CargaCancelada.val, visible: CargaCancelada.visible"></td>
                                    <td data-bind="text: CargaRecusada.val, visible: CargaRecusada.visible() && !UtilizarFilaCarregamentoReversa.val()"></td>
                                    <td data-bind="text: PerdeuSenha.val, visible: PerdeuSenha.visible() && !UtilizarFilaCarregamentoReversa.val()"></td>
                                    <td data-bind="text: EmRemocao.val, visible: EmRemocao.visible() && !UtilizarFilaCarregamentoReversa.val()"></td>
                                    <td data-bind="text: EmViagem.val, visible: EmViagem.visible() && !UtilizarFilaCarregamentoReversa.val()"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <div class="row mb-4">
            <div class="legends align-middle" id="knockoutFilaCarregamentoLegenda">
                <span><b>Legendas:</b> </span>
                <div class="badge legend legenda-cor-verde" data-bind="visible: Vazio.visible()">
