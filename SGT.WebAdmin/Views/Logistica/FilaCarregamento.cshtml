                <div class="badge legend legenda-cor-ambar" data-bind="visible: AguardandoCarga.visible() && (UtilizarFilaCarregamentoReversa.val() || UtilizarProgramacaoCarga.val())">
                    <span data-bind="text: AguardandoCarga.text"></span>
                    <span class="fw-bold" data-bind="text: AguardandoCarga.totalItens"></span>
                </div>
                <div class="badge legend legenda-cor-vermelha" data-bind="visible: CargaCancelada.visible()">
                    <span data-bind="text: CargaCancelada.text"></span>
                    <span class="fw-bold" data-bind="text: CargaCancelada.totalItens"></span>
                </div>
                <div class="badge legend legenda-cor-purpura" data-bind="visible: CargaRecusada.visible() && !UtilizarFilaCarregamentoReversa.val()">
                    <span data-bind="text: CargaRecusada.text"></span>
                    <span class="fw-bold" data-bind="text: CargaRecusada.totalItens"></span>
                </div>
