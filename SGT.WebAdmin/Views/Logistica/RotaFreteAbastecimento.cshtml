                                                <div class="form-group">
                                                    <label class="form-label" data-bind="text: ValorUnitario.text, attr: { for: ValorUnitario.id }"></label>
                                                    <input type="text" class="form-control" data-bind="enable: ValorUnitario.enable, class: ValorUnitario.requiredClass, value: ValorUnitario.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorUnitario.maxlength, id : ValorUnitario.id}, event: {blur : calculaValorUnitarioAbascimento}" />
                                                </div>
                                            </div>

                                            <div class="col-12 col-md-3" data-bind="visible: ValorTotal.visible">
