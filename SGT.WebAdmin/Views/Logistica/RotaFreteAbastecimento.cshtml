                                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="enable: Posto.enable, attr: { id: Posto.idBtnSearch}" type="button" aria-label="Buscar">
                                                                <i class="fal fa-search"></i>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-12 col-md-3" data-bind="visible: Produto.visible">
                                                <div class="form-group">
                                                    <label class="form-label" data-bind="text: Produto.text, attr: { for: Produto.id }"></label>
                                                    <div class="input-group">
                                                        <input type="text" class="form-control" data-bind="enable: Produto.enable, css: Produto.requiredClass, value: Produto.val, valueUpdate: 'afterkeydown', attr: { maxlength: Produto.maxlength, id : Produto.id}" />
