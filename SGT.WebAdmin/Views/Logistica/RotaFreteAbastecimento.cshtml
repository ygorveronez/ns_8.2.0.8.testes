                                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="enable: Produto.enable, attr: { id: Produto.idBtnSearch}" type="button" aria-label="Buscar">
                                                                <i class="fal fa-search"></i>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-3" data-bind="visible: Litros.visible">
                                                <div class="form-group">
                                                    <label class="form-label" data-bind="text: Litros.text, attr: { for: Litros.id }"></label>
                                                    <input type="text" class="form-control" data-bind="enable: Litros.enable, class: Litros.requiredClass, value: Litros.val, valueUpdate: 'afterkeydown', attr: { maxlength: Litros.maxlength, id : Litros.id}, event: {blur : calculaLitrosAbascimento}" />
                                                </div>
                                            </div>

