                                                    <label class="form-label" data-bind="text: Posto.text, attr: { for: Posto.id }"></label>
                                                    <div class="input-group">
                                                        <input type="text" class="form-control" data-bind="enable: Posto.enable, css: Posto.requiredClass, value: Posto.val, valueUpdate: 'afterkeydown', attr: { maxlength: Posto.maxlength, id : Posto.id}" />
