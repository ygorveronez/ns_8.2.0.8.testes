
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item active">Guarita TMS</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Guarita
    </h1>
</div>

<div>
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content" id="knockoutPesquisaGuaritaTMS">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Registros de Veículos
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="enable: Veiculo.enable, css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4 col-lg-4" data-bind="visible: Reboque.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Reboque.text, attr: { for: Reboque.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="enable: Reboque.enable, class: Reboque.requiredClass, value: Reboque.val, valueUpdate: 'afterkeydown', attr: { maxlength: Reboque.maxlength, id : Reboque.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Reboque.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4 col-lg-4" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="enable: Carga.enable, class: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Carga.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4 col-lg-4" data-bind="visible: OrdemServico.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: OrdemServico.text, attr: { for: OrdemServico.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="enable: OrdemServico.enable, class: OrdemServico.requiredClass, value: OrdemServico.val, valueUpdate: 'afterkeydown', attr: { maxlength: OrdemServico.maxlength, id : OrdemServico.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: OrdemServico.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4 col-lg-4" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="enable: Motorista.enable, class: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Motorista.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: KMAtual.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: KMAtual.text, attr: { for: KMAtual.id }"></label>
                                    <input type="text" class="form-control" data-bind="enable: KMAtual.enable, class: KMAtual.requiredClass, value: KMAtual.val, valueUpdate: 'afterkeydown', attr: { maxlength: KMAtual.maxlength, id : KMAtual.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="enable: DataInicial.enable, class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="enable: DataFinal.enable, class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: TipoEntradaSaida.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoEntradaSaida.text, attr: { for: TipoEntradaSaida.id }"></label>
                                    <select class="form-control" data-bind="class: TipoEntradaSaida.requiredClass, options: TipoEntradaSaida.options, optionsText: 'text', optionsValue: 'value', value: TipoEntradaSaida.val, attr: { maxlength: TipoEntradaSaida.maxlength, id : TipoEntradaSaida.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: PlacaVeiculoTerceiro.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PlacaVeiculoTerceiro.text"></label>
                                    <input type="text" class="form-control" data-bind="class: PlacaVeiculoTerceiro.requiredClass, value: PlacaVeiculoTerceiro.val, valueUpdate: 'afterkeydown', attr: { maxlength: PlacaVeiculoTerceiro.maxlength, id : PlacaVeiculoTerceiro.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
    <div id="conteudoGuarita">

    </div>
</div>


<script src="~/scripts/guaritaTMS"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadGuaritaTMS();
    });

</script>

