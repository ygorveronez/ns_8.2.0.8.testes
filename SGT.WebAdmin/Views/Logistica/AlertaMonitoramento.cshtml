
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item active">Alerta Monitoramento</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Alerta Monitoramento
    </h1>
</div>
<div id="divConteudoRelatorio" class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastro
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutAlertaMonitoramento">
                    <div class="row">

                        <div class="col-12 col-md-3" data-bind="visible: DataAlerta.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataAlerta.text, attr: { for: DataAlerta.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataAlerta.requiredClass, value: DataAlerta.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAlerta.maxlength, id : DataAlerta.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-3" data-bind="visible: Evento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Evento.text, attr: { for: Evento.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Evento.requiredClass, value: Evento.val, valueUpdate: 'afterkeydown', attr: { maxlength: Evento.maxlength, id : Evento.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Evento.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-3" data-bind="visible: Carga.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-3" data-bind="visible: CargaEntrega.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CargaEntrega.text, attr: { for: CargaEntrega.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: CargaEntrega.requiredClass, value: CargaEntrega.val, valueUpdate: 'afterkeydown', attr: { maxlength: CargaEntrega.maxlength, id : CargaEntrega.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CargaEntrega.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-3" data-bind="visible: Veiculo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
            </form>
            <footer id="knockoutCRUDAlertaMonitoramento">
                <div class="row">
                    <div class="col-6">
                        <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, text: Cancelar.text, attr: { id : Cancelar.id}">
                            <i class="fal fa-ban"></i>
                        </button>
                    </div>
                    <div class="col-6 d-flex justify-content-end">

                        <button type="button" class="btn btn-success ms-3" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                            <i class="fal fa-spinner"></i>
                        </button>
                    </div>
                </div>
            </footer>
        </div>
    </div>
</div>


<script src="~/scripts/alertaMonitoramento"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadAlertaMonitoramento();
    });

</script>

