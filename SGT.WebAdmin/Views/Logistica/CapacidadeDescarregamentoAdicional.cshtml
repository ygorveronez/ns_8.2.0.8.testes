
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item active">Capacidade de Descarregamento Adicional</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Capacidade de Descarregamento Adicional
    </h1>
</div>

<div id="knockoutPesquisaCapacidadeDescarregamentoAdicional">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-filter"></i> <span>Pesquisar Capacidade de Descarregamento Adicional</span>
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: CentroDescarregamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CentroDescarregamento.text, attr: { for: CentroDescarregamento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: CentroDescarregamento.requiredClass, value: CentroDescarregamento.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': CentroDescarregamento.text, maxlength: CentroDescarregamento.maxlength, id: CentroDescarregamento.id, name: CentroDescarregamento.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CentroDescarregamento.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataLimite.text, attr: { for: DataLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataLimite.maxlength, id : DataLimite.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Capacidade de Descarregamento Adicional
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div>
                    <div class="row" id="knockoutCapacidadeDescarregamentoAdicional">
                        <div class="col-12 col-md-6" data-bind="visible: CentroDescarregamento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CentroDescarregamento.text, attr: { for: CentroDescarregamento.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: CentroDescarregamento.requiredClass, value: CentroDescarregamento.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': CentroDescarregamento.text, maxlength: CentroDescarregamento.maxlength, id: CentroDescarregamento.id, name: CentroDescarregamento.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: CentroDescarregamento.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Data.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Data.text, attr: { for: Data.id }"></label>
                                <input type="text" class="form-control" data-bind="css: Data.requiredClass,value: Data.val, valueUpdate: 'afterkeydown', attr: { maxlength: Data.maxlength, id : Data.id}">
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: PeriodoDescarregamento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: PeriodoDescarregamento.text, attr: { for: PeriodoDescarregamento.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind=" value: PeriodoDescarregamento.val,css: PeriodoDescarregamento.requiredClass, valueUpdate: 'afterkeydown', attr: { 'aria-label': PeriodoDescarregamento.text, maxlength: PeriodoDescarregamento.maxlength, id: PeriodoDescarregamento.id, name: PeriodoDescarregamento.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: PeriodoDescarregamento.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CapacidadeDescarregamento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CapacidadeDescarregamento.text, attr: { for: CapacidadeDescarregamento.id }"></label>
                                <input type="text" class="form-control" data-bind="css: CapacidadeDescarregamento.requiredClass, value: CapacidadeDescarregamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: CapacidadeDescarregamento.maxlength, id : CapacidadeDescarregamento.id}" />
                            </div>
                        </div>
                        
                        <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Observacao.text"></label>
                                 <textarea type="text" rows="5" class="form-control" data-bind="value: Observacao.val, attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                            </div>
                        </div>

                    </div>
                </div>
                <footer id="knockoutCRUDCapacidadeDescarregamentoAdicional">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>

