                        <span data-bind='component: {name: "import-file", params: { ko: Importar }}, visible: Importar.visible'></span>

                        <button type="button" class="btn btn-danger mx-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, text: Excluir.text, attr: { id : Excluir.id}">
