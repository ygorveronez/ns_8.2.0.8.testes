                        </button>
                        <button type="button" class="btn btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, text: Atualizar.text, attr: { id : Atualizar.id}">
                            <i class="fal fa-check"></i>
                        </button>
                        <button type="button" class="btn btn-success mx-2" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                            <i class="fal fa-spinner"></i>
                        </button>
                    </div>
                </div>
            </footer>
        </div>
    </div>
</section>

