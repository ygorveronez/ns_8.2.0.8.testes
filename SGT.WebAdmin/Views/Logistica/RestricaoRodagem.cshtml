
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item active">Restrição Rodagem</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i>Restrições de Rodagem
    </h1>
</div>

<div id="knockoutPesquisaRestricaoRodagem">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Restrições de Rodagem
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-8">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CentroCarregamento.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: CentroCarregamento.val, enable: CentroCarregamento.enable, attr: { maxlength: CentroCarregamento.maxlength, id: CentroCarregamento.id}, event: { blur: CentroCarregamento.eventChange }, valueUpdate: 'afterkeydown',  class: CentroCarregamento.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : CentroCarregamento.enable, attr: { id: CentroCarregamento.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: DiaSemana.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DiaSemana.text"></label>
                                    <select class="form-control" data-bind="options: DiaSemana.options, optionsText: 'text', optionsValue: 'value', value: DiaSemana.val, attr: { id : DiaSemana.id}"></select>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>

        </div>
    </div>
</div>


<div class="panel mt-3" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs">
                <li class="nav-item active">
                    <a href="#tabRestricaoRodagemDados" class="nav-link active" data-bs-toggle="tab">
                        <span class="hidden-mobile hidden-tablet">Dados</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#tabRestricaoRodagemClienteDestino" class="nav-link" data-bs-toggle="tab">
                        <span class="hidden-mobile hidden-tablet">Destinos</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3" >
                <div class="tab-pane fade active show" id="tabRestricaoRodagemDados" role="tabpanel">
                    <form novalidate id="knockoutRestricaoRodagem">
                        <div class="row">
                            <div class="col-12 col-md-8">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CentroCarregamento.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: CentroCarregamento.val, enable: CentroCarregamento.enable, attr: { maxlength: CentroCarregamento.maxlength, id: CentroCarregamento.id}, event: { blur: CentroCarregamento.eventChange }, valueUpdate: 'afterkeydown', class: CentroCarregamento.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="enable : CentroCarregamento.enable, attr: { id: CentroCarregamento.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: DiaSemana.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DiaSemana.text"></label>
                                    <select class="form-control" data-bind="options: DiaSemana.options, optionsText: 'text', optionsValue: 'value', value: DiaSemana.val, attr: { id : DiaSemana.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: FinalPlaca.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: FinalPlaca.text"></label>
                                    <select class="form-control" data-bind="options: FinalPlaca.options, optionsText: 'text', optionsValue: 'value', value: FinalPlaca.val, attr: { id : FinalPlaca.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: HoraInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="css: HoraInicial.requiredClass,value: HoraInicial.val, attr: { maxlength: HoraInicial.maxlength, id : HoraInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: HoraFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="css: HoraFinal.requiredClass,value: HoraFinal.val, attr: { maxlength: HoraFinal.maxlength, id : HoraFinal.id}" />
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="tab-pane fade" id="tabRestricaoRodagemClienteDestino" role="tabpanel">

                    

                    <form novalidate>
                        <div id="knockoutRestricaoRodagemClienteDestino">
                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ListaClienteDestino.idBtnSearch}">
                                <i></i>
                                Adicionar Cliente
                            </button>
                            <div class="my-3">
                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: ListaClienteDestino.idGrid}" cellspacing="0"></table>
                            </div>
                        </div>
                    </form>
                </div>
                <footer id="knockoutCRUDRestricaoRodagem">
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                                <span data-bind="text: Excluir.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </div>
        </div>
    </div>
</div>
















