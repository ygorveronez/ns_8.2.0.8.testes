                        <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                    </div>
                </div>
                <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                    <div class="form-group">
                        <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                        <textarea class="form-control" rows="4" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id }"></textarea>
                    </div>
                </div>

            </div>
            <div class="modal-footer d-flex justify-content-end" id="knockoutCRUDDocumentoTransporte">
                <button class="btn btn-secondary waves-effect waves-themed" type="button" data-bind="click: Cancelar.eventClick, visible:Cancelar.visible,attr: { id: Cancelar.id}">
                    <span data-bind="text: Cancelar.text"></span>
                </button>
                <div>
                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id: Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="click: Atualizar.eventClick,visible:Atualizar.visible,  attr: { id: Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                </div>
            </div>

        </div>
    </div>
</div>


