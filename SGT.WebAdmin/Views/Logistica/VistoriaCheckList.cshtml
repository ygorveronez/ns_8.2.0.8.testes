
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item active">Vistoria CheckList</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Cadastro De Vistorias Periódicas
    </h1>
</div>

<div id="knockoutPesquisaVistoriaCheckList">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisa Vistorias Periódicas
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">

                            <div class="col-12 col-md-6" data-bind="visible: Checklist.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Checklist.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Checklist.requiredClass, value: Checklist.val, valueUpdate: 'afterkeydown', attr: { maxlength: Checklist.maxlength, id : Checklist.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: ModeloVeicular.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ModeloVeicular.text, attr: { for: ModeloVeicular.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: ModeloVeicular.requiredClass, value: ModeloVeicular.val, valueUpdate: 'afterkeydown', attr: { maxlength: ModeloVeicular.maxlength, id : ModeloVeicular.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ModeloVeicular.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Vistoria Periódica
            </h3>
        </div>
        <div class="panel-content">
            <div class="col-12 col-md-12 row" >
                <div class="col-12 col-md-6 row"  id="knockoutCadastroVistoriaCheckList">
                    
                    <div class="col-12 col-md-12" data-bind="visible: Checklist.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Checklist.text, attr: { for: Checklist.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Checklist.requiredClass, value: Checklist.val, valueUpdate: 'afterkeydown', attr: { maxlength: Checklist.maxlength, id : Checklist.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Checklist.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-8" data-bind="visible: PeriocidadeVencimento.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PeriocidadeVencimento.text"></label>
                            <input type="text" class="form-control" data-bind="class: PeriocidadeVencimento.requiredClass, value: PeriocidadeVencimento.val, valueUpdate: 'afterkeydown', attr: { maxlength: PeriocidadeVencimento.maxlength, id : PeriocidadeVencimento.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: Status.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                            <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-md-6" id="knockoutCadastroVistoriaCheckListModeloVeicular">
                    <button class="btn btn-primary mb-2" type="button" data-bind="click: AdicionarModeloVeicular.eventClick, attr: { id: AdicionarModeloVeicular.idBtnSearch}">
                        <i class="fal fa-plus"></i><span data-bind="text: AdicionarModeloVeicular.text"></span>
                    </button>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: ModeloVeicular.id}" cellspacing="0"></table>
                </div>
            </div>
            <footer id="knockoutCRUDVistoriaCheckList">
                <div class="row">
                    <div class="col-6">
                        <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                            <i class="fal fa-ban"></i>
                            <span data-bind="text: Cancelar.text"></span>
                        </button>
                    </div>
                    <div class="col-6 d-flex justify-content-end">
                        <button type="button" class="btn btn-labeled btn-danger mx-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                            <i class="fal fa-trash"></i>
                            <span data-bind="text: Excluir.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                            <i class="fal fa-check"></i>
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </div>
            </footer>
        </div>
    </div>
</div>



<script src="~/scripts/VistoriaCheckList"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadVistoriaCheckList();
    });

</script>


