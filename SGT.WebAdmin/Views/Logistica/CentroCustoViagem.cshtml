
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logistica</li>
    <li class="breadcrumb-item active">Centro de Custo de Viagem</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i>  {Localize::Logistica.CentroCustoViagem.CentroCustoViagemDescricao}
    </h1>
</div>

<div id="knockoutPesquisaCentroCustoViagem">
    <div class="panel mt-3">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    {Localize::Logistica.CentroCustoViagem.PesquisarCentroCustoViagem}
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>


 <div class="panel">
    <div class="panel-container" id="divCentroCusto">
        <div class="panel-content">
            <ul class="nav nav-tabs" id="myTab">
                <li class="nav-item active">
                    <a href="#tabCentroCusto" class="nav-link active" data-bs-toggle="tab"><i class="fal fa-edit"></i><span>{Localize::Logistica.CentroCustoViagem.CadastrarCentroCustoViagem}</span></a>
                </li>
                <li class="nav-item" id="liOpenTech">
                    <a href="#tabOpenTech" class="nav-link" data-bs-toggle="tab"><i class="fal fa-link"></i><span>OpenTech</span></a>
                </li>
                <li class="nav-item" id="liRepomFrete">
                    <a href="#tabRepomFrete" class="nav-link" data-bs-toggle="tab"><i class="fal fa-link"></i><span>Repom Frete</span></a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3" >
                <div class="tab-pane fade active show"  role="tabpanel" id="tabCentroCusto">
                    <div class="row" id="knockoutCentroCustoViagem">
                        <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Descricao.text"></label>
                                <input type="text" class="form-control" data-bind="css: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CodigoIntegracao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                <input type="text" class="form-control" data-bind="css: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Status.text"></label>
                                <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" role="tabpanel" id="tabOpenTech">
                    <div class="row" id="knockoutOpenTech">
                        <div class="col-12 col-md-3" data-bind="visible: CodigoTransportadorOpenTech.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoTransportadorOpenTech.text"></label>
                                <input type="text" class="form-control" data-bind="css: CodigoTransportadorOpenTech.requiredClass, value: CodigoTransportadorOpenTech.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoTransportadorOpenTech.maxlength, id : CodigoTransportadorOpenTech.id}" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" role="tabpanel" id="tabRepomFrete">
                    <div class="row" id="knockoutRepomFrete">
                        <div class="col-12 col-md-3" data-bind="visible: CodigoFilialRepom.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CodigoFilialRepom.text"></label>
                                <input type="text" class="form-control" data-bind="value: CodigoFilialRepom.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoFilialRepom.maxlength, id : CodigoFilialRepom.id}" />
                            </div>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDCentroCustoViagem">
                    <div class="d-flex align-items-center justify-content-end">
                        <button type="button" class="btn btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                            <i class="fal fa-trash"></i>
                            <span data-bind="text: Excluir.text"></span>
                        </button>
                        <button type="button" class="btn btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                            <i class="fal fa-check"></i>
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button type="button" class="btn btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                    </div>
                </footer>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/centroCustoViagem"></script>
<script type="text/javascript">

    _CONFIGURACAO_TMS = $.extend({}, _CONFIGURACAO_TMS, @Html.Raw(ViewBag.CentroCustoViagem));

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("Logistica.CentroCustoViagem").then(loadCentroCustoViagem);
    });

</script>
