
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item active">Informação Descarga</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Informação de Descarga
    </h1>
</div>

<div id="knockoutPesquisaInformacaoDescarga">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Informação de Descarga
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: Data.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Data.text, attr: { for: Data.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Data.val, valueUpdate: 'afterkeydown', attr: { maxlength: Data.maxlength, id : Data.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NotaFiscal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NotaFiscal.text, attr: { for: NotaFiscal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: NotaFiscal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NotaFiscal.maxlength, id : NotaFiscal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Placa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Placa.text, attr: { for: Placa.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Placa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Placa.maxlength, id : Placa.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Carga.text, maxlength: Carga.maxlength, id: Carga.id, name: Carga.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Carga.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Informação de Descarga
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutInformacaoDescarga">
                <div class="row" >
                    <div class="col-12 col-md-2" data-bind="visible: Data.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Data.text, attr: { for: Data.id }"></label>
                            <input type="text" class="form-control" data-bind="value: Data.val, valueUpdate: 'afterkeydown',class: Data.requiredClass, attr: { maxlength: Data.maxlength, id : Data.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Hora.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Hora.text, attr: { for: Hora.id }"></label>
                            <input type="time" class="form-control" data-bind="value: Hora.val, valueUpdate: 'afterkeydown', class: Hora.requiredClass,attr: { maxlength: Hora.maxlength, id : Hora.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-8" data-bind="visible: Empresa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                            <input type="text" class="form-control" data-bind="value: Empresa.val, class: Empresa.requiredClass, valueUpdate: 'afterkeydown', attr: { maxlength: Empresa.maxlength, id : Empresa.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: NotaFiscal.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NotaFiscal.text, attr: { for: NotaFiscal.id }"></label>
                            <input type="text" class="form-control" data-bind="value: NotaFiscal.val, valueUpdate: 'afterkeydown',class: NotaFiscal.requiredClass, attr: { maxlength: NotaFiscal.maxlength, id : NotaFiscal.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Serie.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Serie.text, attr: { for: Serie.id }"></label>
                            <input type="text" class="form-control" data-bind="value: Serie.val, valueUpdate: 'afterkeydown',class: Serie.requiredClass, attr: { maxlength: Serie.maxlength, id : Serie.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: Placa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Placa.text, attr: { for: Placa.id }"></label>
                            <input type="text" class="form-control" data-bind="value: Placa.val, valueUpdate: 'afterkeydown', class: Placa.requiredClass,attr: { maxlength: Placa.maxlength, id : Placa.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: PesoDescarga.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: PesoDescarga.text, attr: { for: PesoDescarga.id }"></label>
                            <input type="text" class="form-control" data-bind="value: PesoDescarga.val, valueUpdate: 'afterkeydown',  class: PesoDescarga.requiredClass,attr: { maxlength: PesoDescarga.maxlength, id : PesoDescarga.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: Carga.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                            <div class="input-group">
                                <input type="text" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Carga.text, maxlength: Carga.maxlength, id: Carga.id, name: Carga.id }" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Carga.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>
                <footer>
                    <div class="row">
                        <div class="col-12 mb-3">
                            <div data-bind='component: {name: "import-file", params: { ko: Importar }}'></div>
                        </div>
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, text: Cancelar.text, attr: { id : Cancelar.id}">
                                <i class="fal fa-ban"></i>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            <button type="button" class="btn btn-danger mx-1 mx-sm-3" data-bind="click: Excluir.eventClick, visible: Excluir.visible, text: Excluir.text, attr: { id : Excluir.id}">
                                <i class="fal fa-trash"></i>
                            </button>
                            <button type="button" class="btn btn-success" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, text: Atualizar.text, attr: { id : Atualizar.id}">
                                <i class="fal fa-check"></i>
                            </button>
                            <button type="button" class="btn btn-success" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                                <i class="fal fa-spinner"></i>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>


<script src="~/scripts/informacaoDescarga"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadInformacaoDescarga();
    });

</script>