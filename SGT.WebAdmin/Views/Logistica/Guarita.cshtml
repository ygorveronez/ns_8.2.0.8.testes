        <div class="panel">
            <div class="panel-container">
                <div class="panel-hdr">
                    <h3>
                        <i class="fal fa-box"></i>
                        Cargas
                    </h3>
                </div>
                <div class="panel-content">
                    <div class="row">

                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>

                        <div class="legends">
                            <div>
                                <b>Legendas: </b>
                            </div>
                            <div class="badge legend green">
                                <b data-bind="text: EntradaSaidaLiberada.text"></b>
                            </div>
                            <div class="badge legend yellow">
                                <b data-bind="text: AguardandoChegada.text"></b>
                            </div>
                            <div class="badge legend orange">
                                <b data-bind="text: AguardandoEntrada.text"></b>
                            </div>
                            <div class="badge legend red">
                                <b data-bind="text: CarregamentoAtrasado.text"></b>
                            </div>
                            <div class="badge legend gray">
                                <b data-bind="text: CarregamentoCancelado.text"></b>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div id="divModalEnvioDetalhesCargaEmail" class="modal fade" role="dialog" aria-hidden="true">
