                                    <div class="col-12 col-md-6" id="divAbastecimentos">
                                        <div class="card">
                                            <div class="card-header d-flex align-items-center justify-content-between">
                                                <h3 class="card-title">Abastecimentos</h3>
                                            </div>
                                            <div class="card-body row">
                                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Abastecimentos.idGrid}" cellspacing="0"></table>

                                            </div>
                                            <div class="card-footer d-flex align-items-center justify-content-end">
                                                <button class="btn btn-default" type="button" data-bind="click: Abastecimentos.eventClick, attr: { id: Abastecimentos.id}">
                                                    <i class="fal fa-plus"></i><span data-bind="text: Abastecimentos.text"></span>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6 mb-2">
                                        <div class="card">
                                            <div class="card-header d-flex align-items-center justify-content-between">
                                                <h3 class="card-title"> Acréscimo / Desconto</h3>
                                            </div>
                                            <div class="card-body row">
                                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: DescontosAcrescimos.idGrid}" cellspacing="0"></table>
                                                <div class="col-12 col-md-5" data-bind="visible: Justificativa.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: Justificativa.text, attr: { for: Justificativa.id }"></label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control" data-bind="css: Justificativa.requiredClass, value: Justificativa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Justificativa.maxlength, id : Justificativa.id}" />
                                                            <div class="input-group-append">
                                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Justificativa.idBtnSearch}" type="button" aria-label="Buscar">
                                                                    <i class="fal fa-search"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3" data-bind="visible: Valor.visible">
                                                    <div class="form-group">
                                                        <label class="form-label" data-bind="text: Valor.text, attr: { for: Valor.id }"></label>
                                                        <input type="text" class="form-control" data-bind="class: Valor.requiredClass, value: Valor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Valor.maxlength, id : Valor.id}" />
                                                    </div>
                                                </div>

                                                <div class="col-12 col-md-4" style="margin-top:27px">
                                                    <button class="btn btn-default" type="button" data-bind="click: AdicionarDescontoAcrescimo.eventClick,enable: AdicionarDescontoAcrescimo.enable, attr: { id: AdicionarDescontoAcrescimo.id}">
                                                        <i class="fal fa-plus"></i><span data-bind="text: AdicionarDescontoAcrescimo.text"></span>
                                                    </button>
                                                </div>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="col-12 col-md-6 mb-2" id="divResumo">
                                        <div class="card">
                                            <div class="card-header d-flex align-items-center justify-content-between">
                                                <h3 class="card-title">Resumo</h3>
                                            </div>
                                            <div class="row mx-2 my-2">
                                                <div data-bind="visible: TotalPagamento.visible" class="col-md-12">
                                                    <label><b data-bind="text: TotalPagamento.text"></b> <span data-bind="text: TotalPagamento.val"></span></label>
                                                </div>
                                                <div data-bind="visible: TotalAdiantamentos.visible" class="col-md-12">
                                                    <label><b data-bind="text: TotalAdiantamentos.text"></b> <span data-bind="text: TotalAdiantamentos.val"></span></label>
                                                </div>
                                                <div data-bind="visible: TotalAbastecimentos.visible" class="col-md-12">
                                                    <label><b data-bind="text: TotalAbastecimentos.text"></b> <span data-bind="text: TotalAbastecimentos.val"></span></label>
                                                </div>
                                                <div data-bind="visible: ValorAcrescimo.visible" class="col-md-12">
                                                    <label><b data-bind="text: ValorAcrescimo.text"></b> <span data-bind="text: ValorAcrescimo.val"></span></label>
                                                </div>
                                                <div data-bind="visible: ValorDesconto.visible" class="col-md-12">
                                                    <label><b data-bind="text: ValorDesconto.text"></b> <span data-bind="text: ValorDesconto.val"></span></label>
                                                </div>
                                                <div data-bind="visible: Saldo.visible" class="col-md-12">
                                                    <label><b data-bind="text: Saldo.text"></b> <span data-bind="text: Saldo.val"></span></label>
                                                </div>
                                                <div data-bind="visible: ValorSaldo.visible" class="col-md-12">
                                                    <label><b data-bind="text: ValorSaldo.text"></b> <span data-bind="text: ValorSaldo.val"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div id="fdsPercentualDocumentos" style="display:none" class="mt-2">
                                <div class="col-12 col-md-12 row">
                                    <div class="col-12 col-md-6 ">
                                        <span> O processo de cálculo para o pagamento ao agregado está em processo, por favor aguarde.</span>
                                    </div>
                                    <div class="col-12 col-md-6 d-flex justify-content-end">
                                        <span class="txt-color-purple pull-right"> <i class="fal fa-exclamation-triangle"></i> <span data-bind="text: PercentualProcessado.val"></span> Completos</span>
                                    </div>

                                    <div class="progress mt-2">
                                        <div class="progress progress-striped active">
                                            <div class="progress-bar bg-color-darken" role="progressbar" data-bind="attr: {id : PercentualProcessado.id}" style="width: 0%"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Etapa Aprovação -->
                        <div id="knockoutAprovacao" class="tab-pane fade">
                            <div class="card">
                                <div class="card-body row">
                                    <div class="col-12" data-bind="visible: ExibirAprovadores.visible">
                                        <button class="btn btn-default" type="button" data-bind="click: ExibirAprovadores.eventClick, attr: { id: ExibirAprovadores.id}">
                                            <span data-bind="text: ExibirAprovadores.text"></span>
                                        </button>
                                    </div>
                                    <div class="col col-md-12 my-3" data-bind="visible: PossuiRegras.val() && ExibirAprovadores.val()">
                                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Regras.idGrid}" cellspacing="0"></table>
                                    </div>
                                    <div class="col col-md-12 my-3" data-bind="visible: !PossuiRegras.val()">
                                        <div class="alert alert-warning">
                                            Não foi encontrada nenhuma regra para aprovar o Pagamento do Agregado.<br />
                                            O pagamento ficará pendente até existir alguma regra de aprovação. Clique em reprocessar regras para verificar.
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="card" data-bind="visible: ResumoSolicitacao.visible">
                                            <div class="card-header d-flex align-items-center justify-content-between">
                                                <h3 class="card-title">Solicitação</h3>
                                            </div>
                                            <div class="card-body row">
                                                <label data-bind="visible: Situacao.visible, text: Situacao.text"><span data-bind="text: Situacao.val"></span></label>
                                                <label data-bind="visible: Data.visible, text: Data.text"><span data-bind="text: Data.val"></span></label>
                                                <label data-bind="visible: Usuario.visible, text: Usuario.text"><span data-bind="text: Usuario.val"></span></label>
                                                <label data-bind="visible: Cliente.visible, text: Cliente.text"><span data-bind="text: Cliente.val"></span></label>
                                                <label data-bind="visible: Valor.visible, text: Valor.text"><span data-bind="text: Valor.val"></span></label>
                                                <label data-bind="visible: ValorAdiantado.visible, text: ValorAdiantado.text"><span data-bind="text: ValorAdiantado.val"></span></label>
                                                <label data-bind="visible: ValorAcrescimo.visible, text: ValorAcrescimo.text"><span data-bind="text: ValorAcrescimo.val"></span></label>
                                                <label data-bind="visible: ValorDesconto.visible, text: ValorDesconto.text"><span data-bind="text: ValorDesconto.val"></span></label>
                                                <label data-bind="visible: DataPagamento.visible, text: DataPagamento.text"><span data-bind="text: DataPagamento.val"></span></label>
                                                <label data-bind="visible: TotalAbastecimentos.visible, text: TotalAbastecimentos.text"><span data-bind="text: TotalAbastecimentos.val"></span></label>
                                                <label data-bind="visible: TotalAdiantamentos.visible, text: TotalAdiantamentos.text"><span data-bind="text: TotalAdiantamentos.val"></span></label>
                                                <label data-bind="visible: TotalPagamento.visible, text: TotalPagamento.text"><span data-bind="text: TotalPagamento.val"></span></label>
                                                <label data-bind="visible: Saldo.visible, text: Saldo.text"><span data-bind="text: Saldo.val"></span></label>
                                                <label data-bind="visible: TotalSaldo.visible, text: TotalSaldo.text"><span data-bind="text: TotalSaldo.val"></span></label>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="card" data-bind="visible: ResumoRetorno.visible">
                                            <div class="card-header d-flex align-items-center justify-content-between">
                                                <h3 class="card-title">Retorno da Solicitaçao</h3>

                                            </div>
                                            <div class="card-body row">
                                                <label data-bind="visible: DataRetorno.visible, text: DataRetorno.text"><span data-bind="text: DataRetorno.val"></span></label>
                                                <label data-bind="visible: Aprovador.visible, text: Aprovador.text"><span data-bind="text: Aprovador.val"></span></label>
                                                <label data-bind="visible: Situacao.visible, text: Situacao.text"><span data-bind="text: Situacao.val"></span></label>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <footer id="knockoutCRUD" class="mt-2">
                    <div class="row">
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <div class="dropdown pull-right me-3" data-bind="visible: ImprimirReciboPagamento.visible()">
                                <button class="btn btn-success dropdown-toggle" data-bs-toggle="dropdown" aria-haspopup="true" type="button" aria-expanded="false" id="dropdownMenuButton1">
                                    <i class="fal fa-print"></i>Impressões<span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                                    <li>
                                        <a class="dropdown-item" data-bind="visible: ImprimirReciboPagamento.visible, click: ImprimirReciboPagamento.eventClick, attr: { id: ImprimirReciboPagamento.id}">
                                            <i class="fal fa-solid fa-file-code"></i><span class="ms-2" data-bind="text: ImprimirReciboPagamento.text"></span>
                                        </a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" data-bind="visible: ImprimirExtratoPagamento.visible, click: ImprimirExtratoPagamento.eventClick, attr: { id: ImprimirExtratoPagamento.id}">
                                            <i class="fal fa-solid fa-file-pdf"></i><span class="ms-2" data-bind="text: ImprimirExtratoPagamento.text"></span>
                                        </a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" data-bind="visible: ImprimirFaturaPagamento.visible, click: ImprimirFaturaPagamento.eventClick, attr: { id: ImprimirFaturaPagamento.id}">
                                            <i class="fal fa-solid fa-file-pdf"></i><span class="ms-2" data-bind="text: ImprimirFaturaPagamento.text"></span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <button class="btn btn-warning me-2" type="button" data-bind="click: ReprocessarRegras.eventClick, enable: ReprocessarRegras.enable, visible: ReprocessarRegras.visible, attr: { value: ReprocessarRegras.text, id : ReprocessarRegras.id}">
                                <span data-bind="text: ReprocessarRegras.text"></span>
                            </button>
                            <button class="btn btn-default me-2" type="button" data-bind="click: Limpar.eventClick, enable: Limpar.enable, visible: Limpar.visible, attr: { value: Limpar.text, id : Limpar.id}">
                                <span data-bind="text: Limpar.text"></span>
                            </button>
                            <button class="btn btn-danger me-2" type="button" data-bind="click: Cancelar.eventClick, enable: Cancelar.enable, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button class="btn btn-danger me-2" type="button" data-bind="click: Estornar.eventClick, enable: Estornar.enable, visible: Estornar.visible, attr: { value: Estornar.text, id : Estornar.id}">
                                <span data-bind="text: Estornar.text"></span>
                            </button>
                            <button class="btn btn-success me-2" type="button" data-bind="click: Finalizar.eventClick, enable: Finalizar.enable, visible: Finalizar.visible, attr: { value: Finalizar.text, id : Finalizar.id}">
                                <span data-bind="text: Finalizar.text"></span>
                            </button>
                            <button class="btn btn-success me-2" type="button" data-bind="click: Adicionar.eventClick, enable: Adicionar.enable, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                                <span data-bind="text: Adicionar.text"></span>
                            </button>
                            <button class="btn btn-success me-2" type="button" data-bind="click: Atualizar.eventClick, enable: Atualizar.enable, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                                <span data-bind="text: Atualizar.text"></span>
                            </button>

                        </div>
                    </div>
                </footer>
            </div>
        </div>
    </div>
</section>
<div id="divModalDetalhesAutorizacao" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" id="knockoutDetalheAutorizacao">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalhes da Autorização</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body row">
                <div class="col-12 col-md-8" data-bind="visible: Regra.visible">
                    <label data-bind="text: Regra.text"><span data-bind="text: Regra.val"></span></label>
                </div>
                <div class="col-12 col-md-4" data-bind="visible: Situacao.visible">
                    <label data-bind="text: Situacao.text"><span data-bind="text: Situacao.val"></span></label>
                </div>
                <div class="col-12 col-md-8" data-bind="visible: Usuario.visible">
                    <label data-bind="text: Usuario.text"><span data-bind="text: Usuario.val"></span></label>
                </div>
                <div class="col-12 col-md-4" data-bind="visible: Data.visible && Data.val() != ''">
                    <label data-bind="text: Data.text"><span data-bind="text: Data.val"></span></label>
                </div>
                <div class="col-12 col-md-12" data-bind="visible: Motivo.visible && Motivo.val() != ''">
                    <label data-bind="text: Motivo.text"><span data-bind="text: Motivo.val"></span></label>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="divModalOcorrenciaDocumento" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutOcorrenciaDocumento" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Ocorrências do Documento</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Ocorrencias.idGrid}" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/pagamentoAgregado"></script>
<script type="text/javascript">
    pageSetUp();

    _CONFIGURACAO_TMS = $.extend({}, _CONFIGURACAO_TMS, @Html.Raw(ViewBag.ConfiguracoesPagamentoAoAgregado));

    $(document).ready(function () {
        loadPagamentoAgregado();
    });

</script>

