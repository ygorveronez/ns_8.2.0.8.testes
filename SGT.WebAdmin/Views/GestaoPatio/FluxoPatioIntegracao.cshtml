
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Fluxo Patio</li>
    <li class="breadcrumb-item active">Fluxo Patio Integrações</li>
</ol>

<div class="panel" id="knockoutPesquisaFluxoPatioIntegracao">
    <div class="panel-container">
        <div class="panel-content">
            <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                <i class="far fa-filter"></i>
               Filtros
            </button>
            <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                <div class="row">
                    <div class="col-12 col-md-3" data-bind="visible: NumeroCarga.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroCarga.text"></label>
                            <input type="text" class="form-control" data-bind="class: NumeroCarga.requiredClass, value: NumeroCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCarga.maxlength, id : NumeroCarga.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: Integradora.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Integradora.text, attr: { for: Integradora.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Integradora.requiredClass, value: Integradora.val, valueUpdate: 'afterkeydown', attr: { maxlength: Integradora.maxlength, id : Integradora.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Integradora.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: SituacaoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: SituacaoIntegracao.text, attr: { for: SituacaoIntegracao.id }"></label>
                            <select class="form-control" data-bind="class: SituacaoIntegracao.requiredClass, options: SituacaoIntegracao.options, optionsText: 'text', optionsValue: 'value', value: SituacaoIntegracao.val, attr: { maxlength: SituacaoIntegracao.maxlength, id : SituacaoIntegracao.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: EtapaPatio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: EtapaPatio.text, attr: { for: EtapaPatio.id }"></label>
                            <select class="form-control" data-bind="class: EtapaPatio.requiredClass, options: EtapaPatio.options, optionsText: 'text', optionsValue: 'value', value: EtapaPatio.val, attr: { maxlength: EtapaPatio.maxlength, id : EtapaPatio.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: Data.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Data.text"></label>
                            <input type="text" class="form-control" data-bind="class: Data.requiredClass, value: Data.val, valueUpdate: 'afterkeydown', attr: { maxlength: Data.maxlength, id : Data.id}" />
                        </div>
                    </div>
                </div>
                <div class="d-flex align-items-center justify-content-end mb-3">
                    <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                    </button>
                </div>
            </div>
            <div class="mt-2">
                <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="divModalHistoricoIntegracao" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Histórico de Integração</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <table widlabel="100%" class="table table-bordered table-hover" cellspacing="0" id="tblHistoricoIntegracao"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/scripts/fluxoPatioIntegracao"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadFluxoPatioIntegracao();
    });

</script>
