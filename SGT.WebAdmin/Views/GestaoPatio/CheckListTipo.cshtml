
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Gestão Pátio</li>
    <li class="breadcrumb-item active">Check List Tipo</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Tipo de Check List
    </h1>
</div>

<div id="knockoutPesquisaCheckListTipo">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Tipo de Check List
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>

                        </div>
                    </form>

                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <div class="widget-body">
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel mt-3" id="wid-id-4" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
    <div class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs">
                <li class="nav-item active">
                    <a href="#tabChecklist" class="nav-link active" data-bs-toggle="tab">
                        <i class="fal fa-check"></i><span class="hidden-mobile hidden-tablet">Checklist</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#tabPerfilAcesso" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-user"></i><span class="hidden-mobile hidden-tablet">Perfil de Acesso</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#tabClientes" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-user"></i><span class="hidden-mobile hidden-tablet">Clientes</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3">
                <div class="tab-pane fade active show" id="tabChecklist" role="tabpanel">
                    <form novalidate id="knockoutCheckListTipo">
                        <div class="row">

                            <div class="col-12 col-md-9" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12" data-bind="visible: EnviarEmailParaCliente.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: EnviarEmailParaCliente.val, attr: { maxlength: EnviarEmailParaCliente.maxlength, name: EnviarEmailParaCliente.id, id: EnviarEmailParaCliente.id }">
                                        <label class="custom-control-label" data-bind="text: EnviarEmailParaCliente.text, attr: { for: EnviarEmailParaCliente.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12" data-bind="visible: EnviarEmailParaMotorista.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: EnviarEmailParaMotorista.val, attr: { maxlength: EnviarEmailParaMotorista.maxlength, name: EnviarEmailParaMotorista.id, id: EnviarEmailParaMotorista.id }">
                                    <label class="custom-control-label" data-bind="text: EnviarEmailParaMotorista.text, attr: { for: EnviarEmailParaMotorista.id }"></label>
                                </div>
                            </div>
                        </div>
                        <footer class="d-flex justify-content-end">
                            <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, text: Cancelar.text, attr: { id : Cancelar.id}">
                            </button>
                            <button type="button" class="btn btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, text: Excluir.text, attr: { id : Excluir.id}">
                            </button>
                            <button type="button" class="btn btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, text: Atualizar.text, attr: { id : Atualizar.id}">
                            </button>
                            <button type="button" class="btn btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, text: Adicionar.text, attr: { id : Adicionar.id}">
                            </button>
                        </footer>
                    </form>
                </div>
                <div class="tab-pane fade" id="tabPerfilAcesso" role="tabpanel">
                    <div id="knockoutPerfilAcesso">
                        <button type="button" class="btn btn-primary mb-2" data-bind="attr: { id: PerfilAcesso.idBtnSearch}">
                            <i class="fal fa-plus"></i><span data-bind="text: PerfilAcesso.text"></span>
                        </button>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Grid.id}" cellspacing="0"></table>
                    </div>
                </div>
                <div class="tab-pane fade" id="tabClientes" role="tabpanel">
                    <div id="knockoutCliente">
                        <button type="button" class="btn btn-primary mb-2" data-bind="attr: { id: Cliente.idBtnSearch}">
                            <i class="fal fa-plus"></i><span data-bind="text: Cliente.text"></span>
                        </button>
                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Grid.id}" cellspacing="0"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/scripts/checkListTipo"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadCheckListTipo();
    });

</script>
