
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Gestão Pátio</li>
    <li class="breadcrumb-item active">Montagem Carga Pátio</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Montagens de Carga do Pátio
    </h1>
</div>

<div id="knockoutPesquisaMontagemCargaPatio">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Montagens de Carga do Pátio
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel" style="display: none" id="container-montagem-carga-patio">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Montagens de Carga do Pátio
            </h3>
        </div>
        <div class="panel-content" id="knockoutMontagemCargaPatio">
            <form>
                <div class="row">
                    <!-- ko if: (NumeroCarga.val() != '' && NumeroPreCarga.val() != '') -->
                    <div class="col col-xs-6 col-lg-3">
                        <label><b data-bind="text: NumeroPreCarga.text"></b> <span data-bind="text: NumeroPreCarga.val"></span></label>
                    </div>
                    <div class="col col-xs-6 col-lg-2">
                        <label><b data-bind="text: NumeroCarga.text"></b> <span data-bind="text: NumeroCarga.val"></span></label>
                    </div>
                    <div class="col col-xs-6 col-lg-2">
                        <label><b data-bind="text: CargaData.text"></b> <span data-bind="text: CargaData.val"></span></label>
                    </div>
                    <div class="col col-xs-6 col-lg-2">
                        <label><b data-bind="text: CargaHora.text"></b> <span data-bind="text: CargaHora.val"></span></label>
                    </div>
                    <!-- /ko -->
                    <!-- ko if: (NumeroCarga.val() == '' && NumeroPreCarga.val() != '') -->
                    <div class="col col-xs-12 col-sm-4">
                        <label><b data-bind="text: NumeroPreCarga.text"></b> <span data-bind="text: NumeroPreCarga.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-3">
                        <label><b data-bind="text: CargaData.text"></b> <span data-bind="text: CargaData.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-3">
                        <label><b data-bind="text: CargaHora.text"></b> <span data-bind="text: CargaHora.val"></span></label>
                    </div>

                    <!-- /ko -->
                    <!-- ko if: (NumeroCarga.val() != '' && NumeroPreCarga.val() == '') -->

                    <div class="col col-xs-12 col-sm-4">
                        <label><b data-bind="text: NumeroCarga.text"></b> <span data-bind="text: NumeroCarga.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-3">
                        <label><b data-bind="text: CargaData.text"></b> <span data-bind="text: CargaData.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-3">
                        <label><b data-bind="text: CargaHora.text"></b> <span data-bind="text: CargaHora.val"></span></label>
                    </div>
                    <!-- /ko -->
                </div>
                <div class="row">
                    <div class="col col-xs-12 col-sm-6 margin-bottom-10" data-bind="visible: Transportador.visible">
                        <label><b data-bind="text: Transportador.text"></b> <span data-bind="text: Transportador.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-6 margin-bottom-10">
                        <label><b data-bind="text: Veiculo.text"></b> <span data-bind="text: Veiculo.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-6 margin-bottom-10">
                        <label><b data-bind="text: Remetente.text"></b> <span data-bind="text: Remetente.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-6 margin-bottom-10">
                        <label><b data-bind="text: TipoCarga.text"></b> <span data-bind="text: TipoCarga.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-6 margin-bottom-10">
                        <label><b data-bind="text: TipoOperacao.text"></b> <span data-bind="text: TipoOperacao.val"></span></label>
                    </div>
                    <div class="col col-xs-12 col-sm-6 margin-bottom-10">
                        <label><b data-bind="text: Destinatario.text"></b> <span data-bind="text: CodigoIntegracaoDestinatario.val"></span> <span style="display: block" data-bind="text: Destinatario.val"></span></label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-md-6" data-bind="visible: QuantidadeItens.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeItens.text, attr: { for: QuantidadeItens.id }"></label>
                            <input type="text" class="form-control" data-bind="class: QuantidadeItens.requiredClass, value: QuantidadeItens.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeItens.maxlength, id : QuantidadeItens.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: QuantidadeCaixas.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadeCaixas.text, attr: { for: QuantidadeCaixas.id }"></label>
                            <input type="text" class="form-control" data-bind="class: QuantidadeCaixas.requiredClass, value: QuantidadeCaixas.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadeCaixas.maxlength, id : QuantidadeCaixas.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: QuantidadePalletsInteiros.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadePalletsInteiros.text, attr: { for: QuantidadePalletsInteiros.id }"></label>
                            <input type="text" class="form-control" data-bind="class: QuantidadePalletsInteiros.requiredClass, value: QuantidadePalletsInteiros.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadePalletsInteiros.maxlength, id : QuantidadePalletsInteiros.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: QuantidadePalletsFracionados.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: QuantidadePalletsFracionados.text, attr: { for: QuantidadePalletsFracionados.id }"></label>
                            <input type="text" class="form-control" data-bind="class: QuantidadePalletsFracionados.requiredClass, value: QuantidadePalletsFracionados.val, valueUpdate: 'afterkeydown', attr: { maxlength: QuantidadePalletsFracionados.maxlength, id : QuantidadePalletsFracionados.id}" />
                        </div>
                    </div>
                </div>
                <footer data-bind="visible: (AvancarEtapa.visible() || (ObservacaoFluxoPatio.val() != ''))" >
                    <div class="row">
                        <div class="col-6">
                            <button type="button" class="btn btn-labeled btn-default"data-bind="click: ExibirObservacao.eventClick, enable: ExibirObservacao.enable, visible: (ObservacaoFluxoPatio.val() != ''), attr: { value: ExibirObservacao.text, id : ExibirObservacao.id}">
                                <span data-bind="text: ExibirObservacao.text"></span>
                            </button>
                        </div>
                        <div class="col-6 d-flex justify-content-end">
                            
                            <button type="button" class="btn btn-labeled btn-success" data-loading-text="Aguarde..."data-bind="click: AvancarEtapa.eventClick, enable: AvancarEtapa.enable, visible: AvancarEtapa.visible, attr: { value: AvancarEtapa.text, id : AvancarEtapa.id}">
                                <span data-bind="text: AvancarEtapa.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
            </form>
        </div>
    </div>
</div>
<div id="divContainerObservacaoFluxoPatio"></div>

<script src="~/scripts/observacaoFluxoPatio"></script>
<script src="~/scripts/montagemCargaPatio"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadMontagemCargaPatio();
        loadObservacaoFluxoPatio();
    });

</script>