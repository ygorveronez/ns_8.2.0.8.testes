
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Transportadores</li>
    <li class="breadcrumb-item active">Gerenciar Transportadores </li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i> Gerenciar Transportadores
    </h1>
</div>





<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Pesquisar Transportadores
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutPesquisaTransportadores">
                <div class="row">
                    <div class="col-12 col-md-4">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Nome.text"></label>
                            <input type="text" class="form-control" data-bind="value: Nome.val, valueUpdate: 'afterkeydown', attr: { maxlength: Nome.maxlength, id : Nome.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-4" data-bind="visible: NomeFantasia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NomeFantasia.text"></label>
                            <input type="text" class="form-control" data-bind="value: NomeFantasia.val, valueUpdate: 'afterkeydown', attr: { maxlength: NomeFantasia.maxlength, id : NomeFantasia.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CNPJ.text"></label>
                            <input type="text" class="form-control" data-bind="value: CNPJ.val, valueUpdate: 'afterkeydown', attr: { maxlength: CNPJ.maxlength, id : CNPJ.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Placa.text"></label>
                            <input type="text" class="form-control" data-bind="value: Placa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Placa.maxlength, id : Placa.id}" />
                        </div>
                    </div>
                </div>
                <footer>
                    <div class="row">
                        <div class="d-flex align-items-center justify-content-end mb-3">
                            <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                            </button>
                        </div>
                    </div>
                </footer>
                <div class="widget-body">
                    <header>
                        <b style="font-size:15px; margin-left:5px;" id="divTituloCadastro">Transportadores</b>
                    </header>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </form>
        </div>
    </div>
    
</div>






<script src="~/scripts/gerenciartransportadores"></script>
<script type="text/javascript">

    pageSetUp();
    $(document).ready(function () {
        loadTransportador();
    });

</script>