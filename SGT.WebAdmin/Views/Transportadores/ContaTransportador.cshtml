
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Transportadores</li>
    <li class="breadcrumb-item active">Conta Transportador</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i> Contas Transportador
    </h1>
</div>

<div id="knockoutPesquisaContaTransportador">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Contas de Transportadores
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroAgencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroAgencia.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroAgencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroAgencia.maxlength, id : NumeroAgencia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroConta.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroConta.text"></label>
                                    <input type="text" class="form-control" data-bind="value: NumeroConta.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroConta.maxlength, id : NumeroConta.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Banco.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Banco.text"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="value: Banco.val, valueUpdate: 'afterkeydown', attr: { maxlength: Banco.maxlength, id : Banco.id},  class: Banco.requiredClass" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Banco.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Conta do Transportador
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutCadastroContaTransportador">
                    <div class="col-12 col-md-4" data-bind="visible: Banco.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Banco.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: Banco.val, valueUpdate: 'afterkeydown', attr: { maxlength: Banco.maxlength, id : Banco.id},  class: Banco.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Banco.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: NumeroAgencia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroAgencia.text"></label>
                            <input type="text" class="form-control" data-bind="css: NumeroAgencia.requiredClass,value: NumeroAgencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroAgencia.maxlength, id : NumeroAgencia.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: DigitoAgencia.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DigitoAgencia.text"></label>
                            <input type="text" class="form-control" data-bind="css: DigitoAgencia.requiredClass,value: DigitoAgencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DigitoAgencia.maxlength, id : DigitoAgencia.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: NumeroConta.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroConta.text"></label>
                            <input type="text" class="form-control" data-bind="css: NumeroConta.requiredClass,value: NumeroConta.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroConta.maxlength, id : NumeroConta.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: TipoContaBanco.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoContaBanco.text"></label>
                            <select class="form-control" data-bind="options: TipoContaBanco.options, optionsText: 'text', optionsValue: 'value', value: TipoContaBanco.val, attr: { id : TipoContaBanco.id}"></select>
                        </div>
                    </div>
                </div>
                <footer id="knockoutCRUDContaTransportador" class="d-flex justify-content-end">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/contaTransportador"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadContaTransportador();
    });

</script>

