
<section>
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Gestão Transportador </li>
        <li class="breadcrumb-item active">Autorização de token</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i></i> Regras Autorização de Token
        </h1>
    </div>
    <div class="panel">
        <div class="panel-container">
            <div id="knockoutPesquisaRegrasAutorizacaoToken" class="panel-content">
                <button class="btn btn-default waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-filter"></i> Pesquisar Regras de Autorização de Token
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-6 col-lg-3" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3 col-lg-3" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3 col-lg-3" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6 col-lg-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6 col-lg-3" data-bind="visible: Aprovador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Aprovador.text, attr: { for: Aprovador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Aprovador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Aprovador.maxlength, id : Aprovador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Aprovador.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex justify-content-end mb-3">
                        <button class="btn btn-primary" type="button" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
    

    <div class="panel" id="knockoutCadastroRegrasAutorizacaoToken">
        <div class="panel-container">
            <div class="panel-content">
                <ul class="nav nav-tabs" id="myTab">
                    <li class="nav-item active">
                        <a href="#tabRegra" class="nav-link active" data-bs-toggle="tab"><span class="hidden-mobile hidden-tablet">Regra</span></a>
                    </li>
                </ul>
                <div class="tab-content border border-top-0 p-3" id="myTabContent">
                    <div id="tabRegra" class="tab-pane fade active show">
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: PrioridadeAprovacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PrioridadeAprovacao.text, attr: { for: PrioridadeAprovacao.id }"></label>
                                    <select class="form-control" data-bind="options: PrioridadeAprovacao.options, optionsText: 'text', optionsValue: 'value', value: PrioridadeAprovacao.val, attr: { id : PrioridadeAprovacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: EtapaAutorizacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EtapaAutorizacao.text, attr: { for: EtapaAutorizacao.id }"></label>
                                    <select class="form-control" data-bind="options: EtapaAutorizacao.options, optionsText: 'text', optionsValue: 'value', value: EtapaAutorizacao.val, attr: { id : EtapaAutorizacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Vigencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Vigencia.text, attr: { for: Vigencia.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: Vigencia.requiredClass, value: Vigencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Vigencia.maxlength, id : Vigencia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroAprovadores.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroAprovadores.text, attr: { for: NumeroAprovadores.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroAprovadores.requiredClass, value: NumeroAprovadores.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroAprovadores.maxlength, id : NumeroAprovadores.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroReprovadores.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroReprovadores.text, attr: { for: NumeroReprovadores.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: NumeroReprovadores.requiredClass, value: NumeroReprovadores.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroReprovadores.maxlength, id : NumeroReprovadores.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DiasPrazoAprovacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DiasPrazoAprovacao.text, attr: { for: DiasPrazoAprovacao.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: DiasPrazoAprovacao.requiredClass, value: DiasPrazoAprovacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DiasPrazoAprovacao.maxlength, id : DiasPrazoAprovacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: AprovacaoAutomaticaAposDias.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: AprovacaoAutomaticaAposDias.text, attr: { for: AprovacaoAutomaticaAposDias.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: AprovacaoAutomaticaAposDias.requiredClass, value: AprovacaoAutomaticaAposDias.val, valueUpdate: 'afterkeydown', attr: { maxlength: AprovacaoAutomaticaAposDias.maxlength, id : AprovacaoAutomaticaAposDias.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoDiasAprovacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoDiasAprovacao.text, attr: { for: TipoDiasAprovacao.id }"></label>
                                    <select class="form-control" data-bind="options: TipoDiasAprovacao.options, optionsText: 'text', optionsValue: 'value', value: TipoDiasAprovacao.val, attr: { id : TipoDiasAprovacao.id}"></select>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: EnviarLinkParaAprovacaoPorEmail.visible">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" data-bind="checked: EnviarLinkParaAprovacaoPorEmail.val, attr: { maxlength: EnviarLinkParaAprovacaoPorEmail.maxlength, name: EnviarLinkParaAprovacaoPorEmail.id, id: EnviarLinkParaAprovacaoPorEmail.id }">
                                    <label class="custom-control-label" data-bind="text: EnviarLinkParaAprovacaoPorEmail.text, attr: { for: EnviarLinkParaAprovacaoPorEmail.id }"></label>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Observacoes.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Observacoes.text, attr: { for: Observacoes.id }"></label>
                                    <textarea class="form-control" rows="4" data-bind="value: Observacoes.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacoes.maxlength, id : Observacoes.id }"></textarea>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="card" data-bind="visible: GridAprovadores.visible">
                                    <div class="card-header d-flex align-items-center justify-content-between">
                                        <h3 class="card-title">Aprovador</h3>
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: GridAprovadores.idBtnSearch, text: GridAprovadores.text }">
                                            <i class="fal fa-plus"></i><span data-bind="text: GridAprovadores.text"></span>
                                        </button>
                                    </div>
                                    <div class="card-body">
                                        <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: GridAprovadores.idGrid}" cellspacing="0"></table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <footer class="d-flex justify-content-end mt-3">
                        <button class="btn btn-success ms-1" type="button" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { value: Atualizar.text, id : Atualizar.id}">
                            <span data-bind="text: Atualizar.text"></span>
                        </button>
                        <button class="btn btn-success ms-1" type="button" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { value: Adicionar.text, id : Adicionar.id}">
                            <span data-bind="text: Adicionar.text"></span>
                        </button>
                        <button class="btn btn-danger ms-1" type="button" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { value: Excluir.text, id : Excluir.id}">
                            <span data-bind="text: Excluir.text"></span>
                        </button>
                        <button class="btn btn-default ms-1" type="button" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { value: Cancelar.text, id : Cancelar.id}">
                            <span data-bind="text:Cancelar.text"></span>
                        </button>
                    </footer>
                </div>
            </div>
        </div>
    </div>
</section>


<script src="~/scripts/regraAprovacaoToken"></script>

