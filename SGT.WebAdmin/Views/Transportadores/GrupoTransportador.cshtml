
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Transportadores</li>
    <li class="breadcrumb-item active">Grupo Transportador</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i> Grupo Transportador
    </h1>
</div>
<div id="knockoutPesquisaGrupoTransportador">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Grupo Transportador
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-5" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text"></label>
                                    <select class="form-control" data-bind="options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { id : Situacao.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel mt-3">
    <div id="knockoutCadastroRota" class="panel-container">
        <div class="panel-content">
            <ul class="nav nav-tabs">
                <li class="nav-item">
                    <a href="#knockoutGrupoTransportador" class="nav-link active" data-bs-toggle="tab">
                        <i class="fal fa-edit"></i> <span class="hidden-mobile hidden-tablet">Detalhes</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#knockoutGrupoTransportadorEmpresa" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-truck"></i><span class="hidden-mobile hidden-tablet">Transportadores</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#grupoTransportadorIntegracoes" class="nav-link" data-bs-toggle="tab">
                        <i class="fal fa-link"></i><span class="hidden-mobile hidden-tablet">Integrações</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content border border-top-0 p-3">
                <div class="tab-pane fade active show" id="knockoutGrupoTransportador" role="tabpanel">
                    <form novalidate>
                        <div class="row">
                            <div class="col-12 col-md-5" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: Descricao.requiredClass,value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-5" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: ParquearDoumentosAutomaticamente.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: ParquearDoumentosAutomaticamente.val, attr: { maxlength: ParquearDoumentosAutomaticamente.maxlength, name: ParquearDoumentosAutomaticamente.id, id: ParquearDoumentosAutomaticamente.id }">
                                        <label class="custom-control-label" data-bind="text: ParquearDoumentosAutomaticamente.text, attr: { for: ParquearDoumentosAutomaticamente.id }"></label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                                    <textarea class="form-control" rows="4" data-bind="value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id }"></textarea>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="tab-pane fade" id="knockoutGrupoTransportadorEmpresa" role="tabpanel">
                    <form novalidate>
                        <div class="row">
                            <div>
                                <button class="my-3 btn btn-primary waves-effect waves-themed botaoAdiconarMultiplosRegistros" data-bind="attr: { id: Empresa.idBtnSearch}" type="button">
                                    <i class="fal fa-plus"></i><span data-bind="text: Empresa.text"></span>
                                </button>
                            </div>
                            <section class="insideNoPaddignTable">
                                <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: Grid.id}" cellspacing="0"></table>
                            </section>
                        </div>

                    </form>
                </div>

                <div class="tab-pane fade" id="grupoTransportadorIntegracoes" role="tabpanel">
                    <form novalidate>
                        <div id="knockoutConfiguracaoIntegracao">
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: {id: ListaSistemaIntegracao.idGrid}" cellspacing="0"></table>
                        </div>
                        <div class="row" id="knockoutCadastroSistemaIntegracao">
                            <div class="col-12 col-md-12" data-bind="visible: SistemaIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SistemaIntegracao.text, attr: { for: SistemaIntegracao.id }"></label>
                                    <select class="form-control" data-bind="class: SistemaIntegracao.requiredClass, options: SistemaIntegracao.options, optionsText: 'text', optionsValue: 'value', value: SistemaIntegracao.val, attr: { maxlength: SistemaIntegracao.maxlength, id : SistemaIntegracao.id}"></select>
                                </div>
                            </div>
                            <div class="col-6 mb-2">
                                <button class="btn btn-success" type="button" data-bind="click: Adicionar.eventClick, attr: { id: Adicionar.id}">
                                    <i class="fal fa-plus"></i><span data-bind="text: Adicionar.text"></span>
                                </button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <footer id="knockoutCRUDGrupoTransportador" class="d-flex justify-content-end mt-3">
                <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                    <span data-bind="text: Cancelar.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                    <span data-bind="text: Excluir.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                    <span data-bind="text: Atualizar.text"></span>
                </button>
                <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                    <span data-bind="text: Adicionar.text"></span>
                </button>
            </footer>
        </div>
    </div>
</div>



<script src="~/scripts/grupoTransportador"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadGrupoTransportador();
    });

</script>
