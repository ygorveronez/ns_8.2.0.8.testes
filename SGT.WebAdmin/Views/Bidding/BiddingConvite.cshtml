<div class="modal fade" id="divModalCadastroBaseline" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Adicionar Baseline</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="fieldset-padding" id="knockoutCadastroBaseline">
                    <div class="row">
                        <div class="col-12 col-md-6" data-bind="visible: TipoBaseline.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoBaseline.text, attr: { for: TipoBaseline.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TipoBaseline.requiredClass, value: TipoBaseline.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoBaseline.maxlength, id : TipoBaseline.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoBaseline.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-6" data-bind="visible: Valor.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Valor.text"></label>
                                <input type="text" class="form-control" data-bind="class: Valor.requiredClass, value: Valor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Valor.maxlength, id : Valor.id}" />
                            </div>
                        </div>
                    </div>
                </div>
                <footer class="d-flex justify-content-end" id="knockoutCRUDBaseline">
                    <button type="button" class="btn btn-success ms-2" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, enable: Atualizar.enable, attr: { id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-success ms-2" data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, enable: Adicionar.enable, attr: { id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                    <button type="button" class="btn btn-danger ms-2" data-bind="click: Excluir.eventClick, visible: Excluir.visible, enable: Excluir.enable, attr: { id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                </footer>
            </div>
        </div>
    </div>
</div>

<div id="divModalDetalhesAutorizacao" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalhes da Autorização</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="row" id="knockoutDetalheAutorizacao">
                    <div data-bind="visible: Regra.visible" class="col-12 col-md-8 mb-2">
                        <label><b data-bind="text: Regra.text"></b> <span data-bind="text: Regra.val"></span></label>
                    </div>
                    <div data-bind="visible: Situacao.visible" class="col-12 col-md-4 mb-2">
                        <label><b data-bind="text: Situacao.text"></b> <span data-bind="text: Situacao.val"></span></label>
                    </div>

                    <div data-bind="visible: Usuario.visible" class="col-12 col-md-8 mb-2">
                        <label><b data-bind="text: Usuario.text"></b> <span data-bind="text: Usuario.val"></span></label>
                    </div>
                    <div data-bind="visible: Data.visible && Data.val() != ''" class="col-12 col-md-4 mb-2">
                        <label><b data-bind="text: Data.text"></b> <span data-bind="text: Data.val"></span></label>
                    </div>

                    <div data-bind="visible: Motivo.visible && Motivo.val() != ''" class="col-12 col-md-12 mb-2">
                        <label><b data-bind="text: Motivo.text"></b> <span data-bind="text: Motivo.val"></span></label>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

<script src="~/scripts/biddingConvite"></script>
<script type="text/javascript">

    pageSetUp();
    _CONFIGURACAO_TMS = $.extend({}, _CONFIGURACAO_TMS, @Html.Raw(ViewBag.ConfiguracaoBidding));

    $(document).ready(function () {
        LoadBidding();
    });

</script>
