
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">ICMS</li>
    <li class="breadcrumb-item active">Alíquota ICMS</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Alíquota de ICMS
    </h1>
</div>

<div id="knockoutPesquisaAliquotaICMS">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Alíquota de ICMS
                </button>

                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: EstadoEmpresa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EstadoEmpresa.text, attr: { for: EstadoEmpresa.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: EstadoEmpresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoEmpresa.maxlength, id : EstadoEmpresa.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: EstadoOrigem.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EstadoOrigem.text, attr: { for: EstadoOrigem.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: EstadoOrigem.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoOrigem.maxlength, id : EstadoOrigem.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: EstadoDestino.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EstadoDestino.text, attr: { for: EstadoDestino.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: EstadoDestino.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoDestino.maxlength, id : EstadoDestino.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: AtividadeTomador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: AtividadeTomador.text"></label>
                                    <input type="text" class="form-control" data-bind="class: AtividadeTomador.requiredClass, value: AtividadeTomador.val, valueUpdate: 'afterkeydown', attr: { maxlength: AtividadeTomador.maxlength, id : AtividadeTomador.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: CFOP.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CFOP.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CFOP.requiredClass, value: CFOP.val, valueUpdate: 'afterkeydown', attr: { maxlength: CFOP.maxlength, id : CFOP.id}" />
                                </div>
                            </div>
                            <div class="d-flex align-items-center justify-content-end mb-3">
                                <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                                    <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                                </button>
                            </div>
                            <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar Alíquota de ICMS
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div>
                    <div class="row" id="knockoutAliquotaICMS">
                        <div class="col-12 col-md-2" data-bind="visible: EstadoEmpresa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EstadoEmpresa.text, attr: { for: EstadoEmpresa.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: EstadoEmpresa.enable, value: EstadoEmpresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoEmpresa.maxlength, id : EstadoEmpresa.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: EstadoOrigem.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EstadoOrigem.text, attr: { for: EstadoOrigem.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: EstadoOrigem.enable, value: EstadoOrigem.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoOrigem.maxlength, id : EstadoOrigem.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: EstadoDestino.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EstadoDestino.text, attr: { for: EstadoDestino.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: EstadoDestino.enable, value: EstadoDestino.val, valueUpdate: 'afterkeydown', attr: { maxlength: EstadoDestino.maxlength, id : EstadoDestino.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: AtividadeTomador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: AtividadeTomador.text, attr: { for: AtividadeTomador.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: AtividadeTomador.enable, value: AtividadeTomador.val, valueUpdate: 'afterkeydown', attr: { maxlength: AtividadeTomador.maxlength, id : AtividadeTomador.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: AtividadeDestinatario.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: AtividadeDestinatario.text, attr: { for: AtividadeDestinatario.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: AtividadeDestinatario.enable, value: AtividadeDestinatario.val, valueUpdate: 'afterkeydown', attr: { maxlength: AtividadeDestinatario.maxlength, id : AtividadeDestinatario.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CFOP.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CFOP.text, attr: { for: CFOP.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: CFOP.enable, value: CFOP.val, valueUpdate: 'afterkeydown', attr: { maxlength: CFOP.maxlength, id : CFOP.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: Percentual.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Percentual.text, attr: { for: Percentual.id }"></label>
                                <input type="text" class="form-control" data-bind="css:Percentual.requiredClass, enable: Percentual.enable, value: Percentual.val, valueUpdate: 'afterkeydown', attr: { maxlength: Percentual.maxlength, id : Percentual.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: CST.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CST.text, attr: { for: CST.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: CST.enable, value: CST.val, valueUpdate: 'afterkeydown', attr: { maxlength: CST.maxlength, id : CST.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: AliquotaFCP.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: AliquotaFCP.text, attr: { for: AliquotaFCP.id }"></label>
                                <input type="text" class="form-control" data-bind="enable: AliquotaFCP.enable, value: AliquotaFCP.val, valueUpdate: 'afterkeydown', attr: { maxlength: AliquotaFCP.maxlength, id : AliquotaFCP.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Observacao.text"></label>
                                <textarea type="text" rows="5" class="form-control" data-bind="enable: Observacao.enable, value: Observacao.val, attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                            </div>
                        </div>
                        <footer class=" d-flex justify-content-end" id="knockoutCRUDCoeficientePautaFiscal">
                            <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                                <span data-bind="text: Cancelar.text"></span>
                            </button>
                            <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                                <span data-bind="text: Atualizar.text"></span>
                            </button>
                        </footer>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/aliquotaICMS"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadAliquotaICMS();
    });

</script>
