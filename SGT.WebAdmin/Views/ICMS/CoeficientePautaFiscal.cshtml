
<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">ICMS</li>
    <li class="breadcrumb-item active">Coeficiente Pauta Fiscal</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Coeficiente Pauta Fiscal
    </h1>
</div>

<div id="knockoutPesquisaCoeficientePautaFiscal">
    <div class="panel">
        <div class="panel-container">
            <div class="panel-content">
                <button class="btn btn-primary waves-effect waves-themed" data-bind="click: ExibirFiltros.eventClick, id: ExibirFiltros.id">
                    <i class="fal fa-search"></i>
                    Pesquisar Coeficiente Pauta Fiscal
                </button>
                <div class="mt-3" data-bind="id: ExibirFiltros.idFade, fadeVisible: ExibirFiltros.visibleFade">
                    <form>
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Estado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Estado.text, attr: { for: Estado.id }"></label>
                                    <div class="input-group">
                                        <input type="text" data-bind="css: Estado.requiredClass, value: Estado.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Estado.text, maxlength: Estado.maxlength, id: Estado.id, name: Estado.id }" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Estado.idBtnSearch}">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Mes.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Mes.text, attr: { for: Mes.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Mes.val, valueUpdate: 'afterkeydown', attr: { maxlength: Mes.maxlength, id : Mes.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Ano.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ano.text, attr: { for: Ano.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Ano.val, valueUpdate: 'afterkeydown', attr: { maxlength: Ano.maxlength, id : Ano.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="d-flex align-items-center justify-content-end mb-3">
                        <button class="btn btn-primary waves-effect waves-themed btnPesquisarFiltroPesquisa" data-bind="click: Pesquisar.eventClick, attr: { id: Pesquisar.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Pesquisar.text"></span>
                        </button>
                    </div>
                    <table width="100%" class="table table-bordered table-hover" data-bind="attr: { id: Pesquisar.idGrid}" cellspacing="0"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Cadastrar de Coeficiente Pauta Fiscal
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div>
                    <div class="row" id="knockoutCoeficientePautaFiscal">
                        <div class="col-12 col-md-4" data-bind="visible: Estado.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Estado.text, attr: { for: Estado.id }"></label>
                                <div class="input-group">
                                    <input type="text" data-bind="css: Estado.requiredClass, value: Estado.val, valueUpdate: 'afterkeydown', attr: { 'aria-label': Estado.text, maxlength: Estado.maxlength, id: Estado.id, name: Estado.id }" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" type="button" data-bind="attr: { id: Estado.idBtnSearch}">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: Mes.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Mes.text, attr: { for: Mes.id }"></label>
                                <input type="text" class="form-control" data-bind="css: Mes.requiredClass, value: Mes.val, valueUpdate: 'afterkeydown', attr: { maxlength: Mes.maxlength, id : Mes.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: Ano.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Ano.text, attr: { for: Ano.id }"></label>
                                <input type="text" class="form-control" data-bind="css: Ano.requiredClass, value: Ano.val, valueUpdate: 'afterkeydown', attr: { maxlength: Ano.maxlength, id : Ano.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: Valor.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Valor.text, attr: { for: Valor.id }"></label>
                                <input type="text" class="form-control" data-bind="css: Valor.requiredClass, value: Valor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Valor.maxlength, id : Valor.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                <select class="form-control" data-bind="css: Ativo.requiredClass, options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { id : Ativo.id}"></select>
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: PercentualCoeficiente.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: PercentualCoeficiente.text, attr: { for: PercentualCoeficiente.id }"></label>
                                <input type="text" class="form-control" data-bind="css: PercentualCoeficiente.requiredClass,value: PercentualCoeficiente.val, valueUpdate: 'afterkeydown', attr: { maxlength: PercentualCoeficiente.maxlength, id : PercentualCoeficiente.id}">
                            </div>
                        </div>
                        <div class="col-12 col-md-12" data-bind="visible: Observacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Observacao.text"></label>
                                <textarea type="text" rows="5" class="form-control" data-bind="css: Observacao.requiredClass,value: Observacao.val, attr: { maxlength: Observacao.maxlength, id : Observacao.id}"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
                <footer class=" d-flex justify-content-end" id="knockoutCRUDCoeficientePautaFiscal">
                    <button type="button" class="btn btn-labeled btn-default ms-1" data-bind="click: Cancelar.eventClick, visible: Cancelar.visible, attr: { id : Cancelar.id}">
                        <span data-bind="text: Cancelar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-danger ms-1" data-bind="click: Excluir.eventClick, visible: Excluir.visible, attr: { id : Excluir.id}">
                        <span data-bind="text: Excluir.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-bind="click: Atualizar.eventClick, visible: Atualizar.visible, attr: { id : Atualizar.id}">
                        <span data-bind="text: Atualizar.text"></span>
                    </button>
                    <button type="button" class="btn btn-labeled btn-success ms-1" data-loading-text="Aguarde..." data-bind="click: Adicionar.eventClick, visible: Adicionar.visible, attr: { id : Adicionar.id}">
                        <span data-bind="text: Adicionar.text"></span>
                    </button>
                </footer>
            </form>
        </div>
    </div>
</div>

<script src="~/scripts/coeficientePautaFiscal"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadCoeficientePautaFiscal();
    });

</script>