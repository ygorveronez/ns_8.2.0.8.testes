@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Financeiros</li>
        <li class="breadcrumb-item active">Cheque</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Cheques
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaCheque">
                        <div class="row">
                            <div class="col-12 col-md-6" data-bind="visible: Pessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Pessoa.requiredClass, value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pessoa.maxlength, id : Pessoa.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pessoa.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroCheque.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCheque.text, attr: { for: NumeroCheque.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroCheque.requiredClass, value: NumeroCheque.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCheque.maxlength, id : NumeroCheque.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Tipo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Tipo.text, attr: { for: Tipo.id }"></label>
                                    <select class="form-control" data-bind="class: Tipo.requiredClass, options: Tipo.options, optionsText: 'text', optionsValue: 'value', value: Tipo.val, attr: { maxlength: Tipo.maxlength, id : Tipo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataVencimentoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoInicio.text, attr: { for: DataVencimentoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoInicio.requiredClass, value: DataVencimentoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoInicio.maxlength, id : DataVencimentoInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataVencimentoLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoLimite.text, attr: { for: DataVencimentoLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoLimite.requiredClass, value: DataVencimentoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoLimite.maxlength, id : DataVencimentoLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataTransacaoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataTransacaoInicio.text, attr: { for: DataTransacaoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataTransacaoInicio.requiredClass, value: DataTransacaoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataTransacaoInicio.maxlength, id : DataTransacaoInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataTransacaoLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataTransacaoLimite.text, attr: { for: DataTransacaoLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataTransacaoLimite.requiredClass, value: DataTransacaoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataTransacaoLimite.maxlength, id : DataTransacaoLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCompensacaoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCompensacaoInicio.text, attr: { for: DataCompensacaoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCompensacaoInicio.requiredClass, value: DataCompensacaoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCompensacaoInicio.maxlength, id : DataCompensacaoInicio.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: DataCompensacaoLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCompensacaoLimite.text, attr: { for: DataCompensacaoLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCompensacaoLimite.requiredClass, value: DataCompensacaoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCompensacaoLimite.maxlength, id : DataCompensacaoLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: ValorInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ValorInicio.text, attr: { for: ValorInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: ValorInicio.requiredClass, value: ValorInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorInicio.maxlength, id : ValorInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: ValorLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ValorLimite.text, attr: { for: ValorLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: ValorLimite.requiredClass, value: ValorLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorLimite.maxlength, id : ValorLimite.id}" />
                                </div>
                            </div>
                        </div>
                        <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                            <div class="col-12 col-md-6" data-bind="visible: Titulo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Titulo.text, attr: { for: Titulo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Titulo.requiredClass, value: Titulo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Titulo.maxlength, id : Titulo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Titulo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Banco.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Banco.text, attr: { for: Banco.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Banco.requiredClass, value: Banco.val, valueUpdate: 'afterkeydown', attr: { maxlength: Banco.maxlength, id : Banco.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Banco.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaCheque" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaCheque" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>



<script src="~/relatorios/scripts/cheque"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadCheque();
    });

</script>
