@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Financeiros</li>
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item active">Retorno Boleto</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Relatório de Retorno de Boleto
    </h1>
</div>
<div id="divConteudoRelatorio" class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-search"></i>
                Filtros
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutPesquisaRetornoBoleto">
                    <div class="row">
                        
                        <div class="col-12 col-md-2" data-bind="visible: DataInicialImportacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicialImportacao.text, attr: { for: DataInicialImportacao.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataInicialImportacao.requiredClass, value: DataInicialImportacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialImportacao.maxlength, id : DataInicialImportacao.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataFinalImportacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinalImportacao.text, attr: { for: DataFinalImportacao.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataFinalImportacao.requiredClass, value: DataFinalImportacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalImportacao.maxlength, id : DataFinalImportacao.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataInicialOcorrencia.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicialOcorrencia.text, attr: { for: DataInicialOcorrencia.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataInicialOcorrencia.requiredClass, value: DataInicialOcorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialOcorrencia.maxlength, id : DataInicialOcorrencia.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataFinalOcorrencia.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinalOcorrencia.text, attr: { for: DataFinalOcorrencia.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataFinalOcorrencia.requiredClass, value: DataFinalOcorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalOcorrencia.maxlength, id : DataFinalOcorrencia.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: BoletoConfiguracao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: BoletoConfiguracao.text, attr: { for: BoletoConfiguracao.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: BoletoConfiguracao.requiredClass, value: BoletoConfiguracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: BoletoConfiguracao.maxlength, id : BoletoConfiguracao.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: BoletoConfiguracao.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: BoletoComando.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: BoletoComando.text, attr: { for: BoletoComando.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: BoletoComando.requiredClass, value: BoletoComando.val, valueUpdate: 'afterkeydown', attr: { maxlength: BoletoComando.maxlength, id : BoletoComando.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: BoletoComando.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: TipoRelatorio.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>





                    </div>
                </div>
            </form>

            <div class="row mb-3">
                <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <div id="knockoutCRUDFiltrosPesquisaRetornoBoleto" class="col-12 col-md-6 d-flex justify-content-end">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>
                </div>
            </div>

            <div id="divConfigRelatorio"></div>

            <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

            <div id="knockoutCRUDPesquisaRetornoBoleto" class="d-flex justify-content-center justify-content-md-end mt-3">
                <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                    <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                </button>
                <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                    <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/relatorios/scripts/retornoBoleto"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioRetornoBoleto();
    });

</script>

