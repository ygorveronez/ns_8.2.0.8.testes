@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Financeiros</li>
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item active">Movimento Financeiro</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Relatório de Movimentação Financeira
    </h1>
</div>
<div id="divConteudoRelatorio" class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-file-alt"></i>
                Relatório
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutPesquisaMovimentoFinanceiro">
                    <div class="row">

                        <div class="col-12 col-md-2" data-bind="visible: Codigo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Codigo.text, attr: { for: Codigo.id }"></label>
                                <input type="text" class="form-control" data-bind="class: Codigo.requiredClass, value: Codigo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Codigo.maxlength, id : Codigo.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataMovimentoInicial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataMovimentoInicial.text, attr: { for: DataMovimentoInicial.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataMovimentoInicial.requiredClass, value: DataMovimentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataMovimentoInicial.maxlength, id : DataMovimentoInicial.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataMovimentoFinal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataMovimentoFinal.text, attr: { for: DataMovimentoFinal.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataMovimentoFinal.requiredClass, value: DataMovimentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataMovimentoFinal.maxlength, id : DataMovimentoFinal.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataBaseFinanceiro.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataBaseFinanceiro.text, attr: { for: DataBaseFinanceiro.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataBaseFinanceiro.requiredClass, value: DataBaseFinanceiro.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataBaseFinanceiro.maxlength, id : DataBaseFinanceiro.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: ValorMovimento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: ValorMovimento.text, attr: { for: ValorMovimento.id }"></label>
                                <input type="text" class="form-control" data-bind="class: ValorMovimento.requiredClass, value: ValorMovimento.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorMovimento.maxlength, id : ValorMovimento.id}" />
                            </div>
                        </div>
                        
                        <div class="col-12 col-md-2" data-bind="visible: NumeroDocumento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: NumeroDocumento.text, attr: { for: NumeroDocumento.id }"></label>
                                <input type="text" class="form-control" data-bind="class: NumeroDocumento.requiredClass, value: NumeroDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroDocumento.maxlength, id : NumeroDocumento.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: Observacao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Observacao.text, attr: { for: Observacao.id }"></label>
                                <input type="text" class="form-control" data-bind="class: Observacao.requiredClass, value: Observacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Observacao.maxlength, id : Observacao.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: TipoMovimento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoMovimento.text, attr: { for: TipoMovimento.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TipoMovimento.requiredClass, value: TipoMovimento.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoMovimento.maxlength, id : TipoMovimento.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoMovimento.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: TipoDocumento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoDocumento.text, attr: { for: TipoDocumento.id }"></label>
                                <select class="form-control" data-bind="class: TipoDocumento.requiredClass, options: TipoDocumento.options, optionsText: 'text', optionsValue: 'value', value: TipoDocumento.val, attr: { maxlength: TipoDocumento.maxlength, id : TipoDocumento.id}"></select>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: CentroResultado.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CentroResultado.text, attr: { for: CentroResultado.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: CentroResultado.requiredClass, value: CentroResultado.val, valueUpdate: 'afterkeydown', attr: { maxlength: CentroResultado.maxlength, id : CentroResultado.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CentroResultado.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: PlanoDebito.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: PlanoDebito.text, attr: { for: PlanoDebito.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: PlanoDebito.requiredClass, value: PlanoDebito.val, valueUpdate: 'afterkeydown', attr: { maxlength: PlanoDebito.maxlength, id : PlanoDebito.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: PlanoDebito.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: PlanoCredito.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: PlanoCredito.text, attr: { for: PlanoCredito.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: PlanoCredito.requiredClass, value: PlanoCredito.val, valueUpdate: 'afterkeydown', attr: { maxlength: PlanoCredito.maxlength, id : PlanoCredito.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: PlanoCredito.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: Pessoa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Pessoa.requiredClass, value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pessoa.maxlength, id : Pessoa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pessoa.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-6" data-bind="visible: GrupoPessoa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: GrupoPessoa.text, attr: { for: GrupoPessoa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: GrupoPessoa.requiredClass, value: GrupoPessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoPessoa.maxlength, id : GrupoPessoa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: GrupoPessoa.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-3" data-bind="visible: SituacaoMovimento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: SituacaoMovimento.text, attr: { for: SituacaoMovimento.id }"></label>
                                <select class="form-control" data-bind="class: SituacaoMovimento.requiredClass, options: SituacaoMovimento.options, optionsText: 'text', optionsValue: 'value', value: SituacaoMovimento.val, attr: { maxlength: SituacaoMovimento.maxlength, id : SituacaoMovimento.id}"></select>
                            </div>
                        </div>

                    </div>
                </div>
            </form>

            <div class="row mb-3">
                <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <div id="knockoutCRUDFiltrosPesquisaMovimentoFinanceiro" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>

                </div>
            </div>

            <div id="divConfigRelatorio"></div>

            <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

            <div id="knockoutCRUDPesquisaMovimentoFinanceiro" class="d-flex justify-content-center justify-content-md-end mt-3">
                <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                    <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                </button>
                <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                    <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/relatorios/scripts/movimentoFinanceiro"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadMovimentoFinanceiro();
    });

</script>
