@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Financeiros</li>
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item active">Rateio Despesa Veículo</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Relatório de Rateio de Despesa de Veículos
    </h1>
</div>
<div id="divConteudoRelatorio" class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-search"></i>
                Relatório
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutPesquisaRateioDespesaVeiculo">
                    <div class="row">

                        <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: TipoDespesa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoDespesa.text, attr: { for: TipoDespesa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TipoDespesa.requiredClass, value: TipoDespesa.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoDespesa.maxlength, id : TipoDespesa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoDespesa.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: GrupoDespesa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: GrupoDespesa.text, attr: { for: GrupoDespesa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: GrupoDespesa.requiredClass, value: GrupoDespesa.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoDespesa.maxlength, id : GrupoDespesa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: GrupoDespesa.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: Veiculo.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: CentroResultado.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: CentroResultado.text, attr: { for: CentroResultado.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: CentroResultado.requiredClass, value: CentroResultado.val, valueUpdate: 'afterkeydown', attr: { maxlength: CentroResultado.maxlength, id : CentroResultado.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CentroResultado.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: Segmentos.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Segmentos.text, attr: { for: Segmentos.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Segmentos.requiredClass, value: Segmentos.val, valueUpdate: 'afterkeydown', attr: { maxlength: Segmentos.maxlength, id : Segmentos.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Segmentos.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                            <div class="row">
                                <div class="col-12 col-md-4" data-bind="visible: Pessoa.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Pessoa.requiredClass, value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pessoa.maxlength, id : Pessoa.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pessoa.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DataLancamentoInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataLancamentoInicial.text, attr: { for: DataLancamentoInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataLancamentoInicial.requiredClass, value: DataLancamentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataLancamentoInicial.maxlength, id : DataLancamentoInicial.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DataLancamentoFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataLancamentoFinal.text, attr: { for: DataLancamentoFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataLancamentoFinal.requiredClass, value: DataLancamentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataLancamentoFinal.maxlength, id : DataLancamentoFinal.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: OrigemRateio.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: OrigemRateio.text, attr: { for: OrigemRateio.id }"></label>
                                        <select class="form-control" data-bind="class: OrigemRateio.requiredClass, options: OrigemRateio.options, optionsText: 'text', optionsValue: 'value', value: OrigemRateio.val, attr: { maxlength: OrigemRateio.maxlength, id : OrigemRateio.id}"></select>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </form>

            <div class="row mb-3">
                <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <div id="knockoutCRUDFiltrosPesquisaRateioDespesaVeiculo" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                    <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                        <i data-bind="css: BuscaAvancada.icon"></i><span data-bind="text: BuscaAvancada.text"></span>
                    </button>
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>

                </div>
            </div>

            <div id="divConfigRelatorio"></div>

            <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

            <div id="knockoutCRUDPesquisaRateioDespesaVeiculo" class="d-flex justify-content-center justify-content-md-end mt-3">
                <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                    <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                </button>
                <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                    <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/relatorios/scripts/rateioDespesaVeiculo"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadRateioDespesaVeiculo();
    });

</script>
