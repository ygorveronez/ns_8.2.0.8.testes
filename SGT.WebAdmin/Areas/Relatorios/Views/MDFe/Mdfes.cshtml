                                    <label class="form-label" data-bind="text: CpfMotorista.text, attr: { for: CpfMotorista.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: CpfMotorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: CpfMotorista.maxlength, id : CpfMotorista.id}" />
