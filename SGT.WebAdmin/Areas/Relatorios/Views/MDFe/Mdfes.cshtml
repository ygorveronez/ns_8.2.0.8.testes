                                    <label class="form-label" data-bind="text: StatusMdfe.text, attr: { for: StatusMdfe.id }"></label>
                                    <select class="form-control" data-bind="class: StatusMdfe.requiredClass, options: StatusMdfe.options, optionsText: 'text', optionsValue: 'value', selectedOptions: StatusMdfe.val, attr: { maxlength: StatusMdfe.maxlength, id : StatusMdfe.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible : Empresa.visible">
