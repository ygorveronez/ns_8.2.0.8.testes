                                    <label class="form-label" data-bind="text: MDFeVinculadoACarga.text, attr: { for: MDFeVinculadoACarga.id }"></label>
                                    <select class="form-control" data-bind="options: MDFeVinculadoACarga.options, optionsText: 'text', optionsValue: 'value', value: MDFeVinculadoACarga.val, attr: { id : MDFeVinculadoACarga.id}"></select>
