@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Seguros</li>
        <li class="breadcrumb-item active">CTesAverbados</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de CT-es/NF-es Averbados
        </h1>
        <div class="subheader-help">
            <button type="button" onclick="iniciarAjuda(897)" data-action="toggle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Abrir a ajuda da página" aria-label="Abrir a ajuda da página"><i class="fal fa-question-circle"></i></button>
        </div>

    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaCTes">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataInicialEmissao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialEmissao.text, attr: { for: DataInicialEmissao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicialEmissao.requiredClass, value: DataInicialEmissao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialEmissao.maxlength, id : DataInicialEmissao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinalEmissao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalEmissao.text, attr: { for: DataFinalEmissao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalEmissao.requiredClass, value: DataFinalEmissao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalEmissao.maxlength, id : DataFinalEmissao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataServicoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataServicoInicial.text, attr: { for: DataServicoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataServicoInicial.requiredClass, value: DataServicoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataServicoInicial.maxlength, id : DataServicoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataServicoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataServicoFinal.text, attr: { for: DataServicoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataServicoFinal.requiredClass, value: DataServicoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataServicoFinal.maxlength, id : DataServicoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: SituacaoFechamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoFechamento.text, attr: { for: SituacaoFechamento.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoFechamento.requiredClass, options: SituacaoFechamento.options, optionsText: 'text', optionsValue: 'value', value: SituacaoFechamento.val, attr: { maxlength: SituacaoFechamento.maxlength, id : SituacaoFechamento.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Seguradora.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Seguradora.text, attr: { for: Seguradora.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Seguradora.requiredClass, value: Seguradora.val, valueUpdate: 'afterkeydown', attr: { maxlength: Seguradora.maxlength, id : Seguradora.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Seguradora.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: ClienteProvedorOS.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ClienteProvedorOS.text, attr: { for: ClienteProvedorOS.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: ClienteProvedorOS.requiredClass, value: ClienteProvedorOS.val, valueUpdate: 'afterkeydown', attr: { maxlength: ClienteProvedorOS.maxlength, id : ClienteProvedorOS.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ClienteProvedorOS.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: ModeloDocumentoFiscal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ModeloDocumentoFiscal.text, attr: { for: ModeloDocumentoFiscal.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: ModeloDocumentoFiscal.requiredClass, value: ModeloDocumentoFiscal.val, valueUpdate: 'afterkeydown', attr: { maxlength: ModeloDocumentoFiscal.maxlength, id : ModeloDocumentoFiscal.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ModeloDocumentoFiscal.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                            <div class="col-12 col-md-4" data-bind="visible: GrupoTomador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoTomador.text, attr: { for: GrupoTomador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: GrupoTomador.requiredClass, value: GrupoTomador.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoTomador.maxlength, id : GrupoTomador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: GrupoTomador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: TipoPropriedadeVeiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoPropriedadeVeiculo.text, attr: { for: TipoPropriedadeVeiculo.id }"></label>
                                    <select class="form-control" data-bind="class: TipoPropriedadeVeiculo.requiredClass, options: TipoPropriedadeVeiculo.options, optionsText: 'text', optionsValue: 'value', value: TipoPropriedadeVeiculo.val, attr: { maxlength: TipoPropriedadeVeiculo.maxlength, id : TipoPropriedadeVeiculo.id}"></select>
                                </div>
                            </div>
                        </div>

                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaCTe" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaCTes" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="~/relatorios/scripts/ctesAverbados"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioCTesAverbados();
    });

</script>
