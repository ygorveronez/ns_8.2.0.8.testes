@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage


<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Pedidos</li>
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item active">Pedido Devolução</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Devoluções de Pedidos
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel" style="display: none;">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-search"></i>
                    Relatório de Devoluções de Pedidos
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaPedidoDevolucao">
                        <div class="row">

                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Pedido.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pedido.text, attr: { for: Pedido.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Pedido.requiredClass, value: Pedido.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pedido.maxlength, id : Pedido.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pedido.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Cliente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Cliente.text, attr: { for: Cliente.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Cliente.requiredClass, value: Cliente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Cliente.maxlength, id : Cliente.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Cliente.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Motivo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motivo.text, attr: { for: Motivo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Motivo.requiredClass, value: Motivo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motivo.maxlength, id : Motivo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motivo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                                <div class="row">
                                    <div class="col-12 col-md-2" data-bind="visible: NumeroNF.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: NumeroNF.text, attr: { for: NumeroNF.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: NumeroNF.requiredClass, value: NumeroNF.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroNF.maxlength, id : NumeroNF.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: DataEmissaoNFInicial.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataEmissaoNFInicial.text, attr: { for: DataEmissaoNFInicial.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: DataEmissaoNFInicial.requiredClass, value: DataEmissaoNFInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoNFInicial.maxlength, id : DataEmissaoNFInicial.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: DataEmissaoNFFinal.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataEmissaoNFFinal.text, attr: { for: DataEmissaoNFFinal.id }"></label>
                                            <input type="text" class="form-control" data-bind="class: DataEmissaoNFFinal.requiredClass, value: DataEmissaoNFFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoNFFinal.maxlength, id : DataEmissaoNFFinal.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: TipoDevolucao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: TipoDevolucao.text, attr: { for: TipoDevolucao.id }"></label>
                                            <select class="form-control" data-bind="class: TipoDevolucao.requiredClass, options: TipoDevolucao.options, optionsText: 'text', optionsValue: 'value', value: TipoDevolucao.val, attr: { maxlength: TipoDevolucao.maxlength, id : TipoDevolucao.id}"></select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaPedidoDevolucao" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>

                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

                <div id="knockoutCRUDPesquisaPedidoDevolucao" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/pedidoDevolucao"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadPedidoDevolucao();
    });

</script>