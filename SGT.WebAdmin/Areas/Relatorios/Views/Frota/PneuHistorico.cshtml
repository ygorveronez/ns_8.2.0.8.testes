@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Frota</li>
        <li class="breadcrumb-item active">Pneu Histórico</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-truck"></i> Relatório de Veículos
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaPneuHistorico">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataLimite.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataLimite.text, attr: { for: DataLimite.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataLimite.requiredClass, value: DataLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataLimite.maxlength, id : DataLimite.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Vida.text, attr: { for: Vida.id }"></label>
                                    <select class="form-control" data-bind="class: Vida.requiredClass, options: Vida.options, optionsText: 'text', optionsValue: 'value', value: Vida.val, attr: { maxlength: Vida.maxlength, id : Vida.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: BandaRodagem.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: BandaRodagem.text, attr: { for: BandaRodagem.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: BandaRodagem.requiredClass, value: BandaRodagem.val, valueUpdate: 'afterkeydown', attr: { maxlength: BandaRodagem.maxlength, id : BandaRodagem.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: BandaRodagem.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Dimensao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Dimensao.text, attr: { for: Dimensao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Dimensao.requiredClass, value: Dimensao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Dimensao.maxlength, id : Dimensao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Dimensao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Marca.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Marca.text, attr: { for: Marca.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Marca.requiredClass, value: Marca.val, valueUpdate: 'afterkeydown', attr: { maxlength: Marca.maxlength, id : Marca.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Marca.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Modelo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Modelo.text, attr: { for: Modelo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Modelo.requiredClass, value: Modelo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Modelo.maxlength, id : Modelo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Modelo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Pneu.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pneu.text, attr: { for: Pneu.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Pneu.requiredClass, value: Pneu.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pneu.maxlength, id : Pneu.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pneu.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Servico.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Servico.text, attr: { for: Servico.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Servico.requiredClass, value: Servico.val, valueUpdate: 'afterkeydown', attr: { maxlength: Servico.maxlength, id : Servico.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Servico.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: SituacaoPneu.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoPneu.text, attr: { for: SituacaoPneu.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoPneu.requiredClass, options: SituacaoPneu.options, optionsText: 'text', optionsValue: 'value', value: SituacaoPneu.val, attr: { maxlength: SituacaoPneu.maxlength, id : SituacaoPneu.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: DTO.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DTO.text, attr: { for: DTO.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DTO.requiredClass, value: DTO.val, valueUpdate: 'afterkeydown', attr: { maxlength: DTO.maxlength, id : DTO.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: UsuarioOperador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: UsuarioOperador.text, attr: { for: UsuarioOperador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: UsuarioOperador.requiredClass, value: UsuarioOperador.val, valueUpdate: 'afterkeydown', attr: { maxlength: UsuarioOperador.maxlength, id : UsuarioOperador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: UsuarioOperador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: MotivoSucata.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: MotivoSucata.text, attr: { for: MotivoSucata.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: MotivoSucata.requiredClass, value: MotivoSucata.val, valueUpdate: 'afterkeydown', attr: { maxlength: MotivoSucata.maxlength, id : MotivoSucata.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: MotivoSucata.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: TipoAquisicao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoAquisicao.text, attr: { for: TipoAquisicao.id }"></label>
                                    <select class="form-control" data-bind="class: TipoAquisicao.requiredClass, options: TipoAquisicao.options, optionsText: 'text', optionsValue: 'value', selectedOptions: TipoAquisicao.val, attr: { maxlength: TipoAquisicao.maxlength, id : TipoAquisicao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Almoxarifado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Almoxarifado.text, attr: { for: Almoxarifado.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Almoxarifado.requiredClass, value: Almoxarifado.val, valueUpdate: 'afterkeydown', attr: { maxlength: Almoxarifado.maxlength, id : Almoxarifado.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Almoxarifado.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: SomenteSucata.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: SomenteSucata.val, attr: { maxlength: SomenteSucata.maxlength, name: SomenteSucata.id, id: SomenteSucata.id }">
                                        <label class="custom-control-label" data-bind="text: SomenteSucata.text, attr: { for: SomenteSucata.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaPneuHistorico" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaPneuHistorico" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/pneuHistorico"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadPneuHistorico();
    });

</script>
