@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Frota</li>
        <li class="breadcrumb-item active">Sinistro</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Sinistros
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaSinistro">
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroSinistro.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroSinistro.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroSinistro.requiredClass, value: NumeroSinistro.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroSinistro.maxlength, id : NumeroSinistro.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CausadorSinistro.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CausadorSinistro.text, attr: { for: CausadorSinistro.id }"></label>
                                    <select class="form-control" data-bind="class: CausadorSinistro.requiredClass, options: CausadorSinistro.options, optionsText: 'text', optionsValue: 'value', value: CausadorSinistro.val, attr: { maxlength: CausadorSinistro.maxlength, id : CausadorSinistro.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoSinistro.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoSinistro.text, attr: { for: TipoSinistro.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoSinistro.requiredClass, value: TipoSinistro.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoSinistro.maxlength, id : TipoSinistro.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoSinistro.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumeroBoletimOcorrencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroBoletimOcorrencia.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroBoletimOcorrencia.requiredClass, value: NumeroBoletimOcorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroBoletimOcorrencia.maxlength, id : NumeroBoletimOcorrencia.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Cidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Cidade.text, attr: { for: Cidade.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Cidade.requiredClass, value: Cidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Cidade.maxlength, id : Cidade.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Cidade.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: IndicacaoPagador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: IndicacaoPagador.text, attr: { for: IndicacaoPagador.id }"></label>
                                    <select class="form-control" data-bind="class: IndicacaoPagador.requiredClass, options: IndicacaoPagador.options, optionsText: 'text', optionsValue: 'value', value: IndicacaoPagador.val, attr: { maxlength: IndicacaoPagador.maxlength, id : IndicacaoPagador.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: VeiculoReboque.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: VeiculoReboque.text, attr: { for: VeiculoReboque.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: VeiculoReboque.requiredClass, value: VeiculoReboque.val, valueUpdate: 'afterkeydown', attr: { maxlength: VeiculoReboque.maxlength, id : VeiculoReboque.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: VeiculoReboque.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: SituacaoSinistro.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoSinistro.text, attr: { for: SituacaoSinistro.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoSinistro.requiredClass, options: SituacaoSinistro.options, optionsText: 'text', optionsValue: 'value', value: SituacaoSinistro.val, attr: { maxlength: SituacaoSinistro.maxlength, id : SituacaoSinistro.id}"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-2">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-2 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaSinistro" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaSinistro" class="d-flex justify-content-center justify-content-md-end mt-2">
                    <button class="btn btn-success me-2" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/sinistro"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioSinistro();
    });

</script>
