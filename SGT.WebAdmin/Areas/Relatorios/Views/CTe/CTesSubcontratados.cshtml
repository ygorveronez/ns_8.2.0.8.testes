@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item">CTe</li>
    <li class="breadcrumb-item active">CTEs Subcontratados</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class="subheader-icon fal fa-truck"></i> Relatório de CT-es Subcontratados
    </h1>
</div>

<div id="divConteudoRelatorio" class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-file-alt"></i>
                Relatórios
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutPesquisaCTesSubcontratados">
                    <div class="row">
                        <div class="col-12 col-md-3" data-bind="visible: DataInicialEmissao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicialEmissao.text"></label>
                                <input type="text" class="form-control" data-bind="value: DataInicialEmissao.val, valueUpdate: 'afterkeydown', enable: DataInicialEmissao.enable, attr: { maxlength: DataInicialEmissao.maxlength, id : DataInicialEmissao.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: DataFinalEmissao.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinalEmissao.text"></label>
                                <input type="text" class="form-control" data-bind="value: DataFinalEmissao.val, valueUpdate: 'afterkeydown', enable: DataFinalEmissao.enable, attr: { maxlength: DataFinalEmissao.maxlength, id : DataFinalEmissao.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: Carga.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Carga.text"></label>
                                <input type="text" class="form-control" data-bind="value: Carga.val, valueUpdate: 'afterkeydown', enable: Carga.enable, attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: EmpresaCTeOriginal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EmpresaCTeOriginal.text, attr: { for: EmpresaCTeOriginal.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: EmpresaCTeOriginal.requiredClass, value: EmpresaCTeOriginal.val, valueUpdate: 'afterkeydown', attr: { maxlength: EmpresaCTeOriginal.maxlength, id : EmpresaCTeOriginal.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: EmpresaCTeOriginal.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: EmpresaCTeSubcontratado.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: EmpresaCTeSubcontratado.text, attr: { for: EmpresaCTeSubcontratado.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: EmpresaCTeSubcontratado.requiredClass, value: EmpresaCTeSubcontratado.val, valueUpdate: 'afterkeydown', attr: { maxlength: EmpresaCTeSubcontratado.maxlength, id : EmpresaCTeSubcontratado.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: EmpresaCTeSubcontratado.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="row mb-3 mt-3">
                <!-- CRUD Configurações -->
                <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <!-- CRUD Filtro de Pesquisa -->
                <div id="knockoutCRUDFiltrosPesquisaCTesSubcontratados" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>
                </div>
            </div>
            <!-- Configurações do Relatório -->
            <div id="divConfigRelatorio">
            </div>
            <!-- GRID de Resultados -->
            <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
            <!-- Botões Inferiores-->
            <div id="knockoutCRUDPesquisaCTesSubcontratados" class="d-flex justify-content-center justify-content-md-end mt-3">
                <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                    <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                </button>
                <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                    <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/relatorios/scripts/ctesSubcontratados"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioCTesSubcontratados();
    });

</script>
