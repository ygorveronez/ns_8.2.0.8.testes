@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Veiculos</li>
        <li class="breadcrumb-item active">Tacógrafo</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Tacógrafo
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaTacografo">
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicialRepasse.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialRepasse.text, attr: { for: DataInicialRepasse.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicialRepasse.requiredClass, value: DataInicialRepasse.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialRepasse.maxlength, id : DataInicialRepasse.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinalRepasse.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalRepasse.text, attr: { for: DataFinalRepasse.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalRepasse.requiredClass, value: DataFinalRepasse.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalRepasse.maxlength, id : DataFinalRepasse.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicialRetorno.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialRetorno.text, attr: { for: DataInicialRetorno.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicialRetorno.requiredClass, value: DataInicialRetorno.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialRetorno.maxlength, id : DataInicialRetorno.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinalRetorno.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalRetorno.text, attr: { for: DataFinalRetorno.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalRetorno.requiredClass, value: DataFinalRetorno.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalRetorno.maxlength, id : DataFinalRetorno.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: ExcessoVelocidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ExcessoVelocidade.text, attr: { for: ExcessoVelocidade.id }"></label>
                                    <select class="form-control" data-bind="class: ExcessoVelocidade.requiredClass, options: ExcessoVelocidade.options, optionsText: 'text', optionsValue: 'value', value: ExcessoVelocidade.val, attr: { maxlength: ExcessoVelocidade.maxlength, id : ExcessoVelocidade.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaTacografo" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaTacografo" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/tacografo"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioTacografo();
    });

</script>
