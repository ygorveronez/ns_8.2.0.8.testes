@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Veiculos</li>
        <li class="breadcrumb-item active">Equipamento</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Equipamentos
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaEquipamento">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataAquisicaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAquisicaoInicial.text, attr: { for: DataAquisicaoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataAquisicaoInicial.requiredClass, value: DataAquisicaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAquisicaoInicial.maxlength, id : DataAquisicaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataAquisicaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAquisicaoFinal.text, attr: { for: DataAquisicaoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataAquisicaoFinal.requiredClass, value: DataAquisicaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAquisicaoFinal.maxlength, id : DataAquisicaoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: AnoFabricacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: AnoFabricacao.text, attr: { for: AnoFabricacao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: AnoFabricacao.requiredClass, value: AnoFabricacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: AnoFabricacao.maxlength, id : AnoFabricacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Ativo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ativo.text, attr: { for: Ativo.id }"></label>
                                    <select class="form-control" data-bind="class: Ativo.requiredClass, options: Ativo.options, optionsText: 'text', optionsValue: 'value', value: Ativo.val, attr: { maxlength: Ativo.maxlength, id : Ativo.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Modelo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Modelo.text, attr: { for: Modelo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Modelo.requiredClass, value: Modelo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Modelo.maxlength, id : Modelo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Modelo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Marca.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Marca.text, attr: { for: Marca.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Marca.requiredClass, value: Marca.val, valueUpdate: 'afterkeydown', attr: { maxlength: Marca.maxlength, id : Marca.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Marca.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Segmento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Segmento.text, attr: { for: Segmento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Segmento.requiredClass, value: Segmento.val, valueUpdate: 'afterkeydown', attr: { maxlength: Segmento.maxlength, id : Segmento.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Segmento.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: CentroResultado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CentroResultado.text, attr: { for: CentroResultado.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: CentroResultado.requiredClass, value: CentroResultado.val, valueUpdate: 'afterkeydown', attr: { maxlength: CentroResultado.maxlength, id : CentroResultado.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CentroResultado.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Neokohm.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Neokohm.text, attr: { for: Neokohm.id }"></label>
                                    <select class="form-control" data-bind="class: Neokohm.requiredClass, options: Neokohm.options, optionsText: 'text', optionsValue: 'value', value: Neokohm.val, attr: { maxlength: Neokohm.maxlength, id : Neokohm.id}"></select>
                                </div>
                            </div>
                        </div>
                    </div>

                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaEquipamento" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaEquipamento" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="~/relatorios/scripts/equipamento"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadEquipamento();
    });

</script>
