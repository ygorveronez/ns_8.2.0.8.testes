@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Veiculos</li>
        <li class="breadcrumb-item active">Historico Veiculo Vinculo</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Histórico de Vínculos dos Veículos
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaVeiculo">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataHoraVinculoInicialHistoricoVeiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataHoraVinculoInicialHistoricoVeiculo.text, attr: { for: DataHoraVinculoInicialHistoricoVeiculo.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataHoraVinculoInicialHistoricoVeiculo.requiredClass, value: DataHoraVinculoInicialHistoricoVeiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataHoraVinculoInicialHistoricoVeiculo.maxlength, id : DataHoraVinculoInicialHistoricoVeiculo.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataHoraVinculoFinalHistoricoVeiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataHoraVinculoFinalHistoricoVeiculo.text, attr: { for: DataHoraVinculoFinalHistoricoVeiculo.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataHoraVinculoFinalHistoricoVeiculo.requiredClass, value: DataHoraVinculoFinalHistoricoVeiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataHoraVinculoFinalHistoricoVeiculo.maxlength, id : DataHoraVinculoFinalHistoricoVeiculo.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Reboque.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Reboque.text, attr: { for: Reboque.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Reboque.requiredClass, value: Reboque.val, valueUpdate: 'afterkeydown', attr: { maxlength: Reboque.maxlength, id : Reboque.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Reboque.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Equipamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Equipamento.text, attr: { for: Equipamento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Equipamento.requiredClass, value: Equipamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: Equipamento.maxlength, id : Equipamento.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Equipamento.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicialVinculoCentroResultado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialVinculoCentroResultado.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicialVinculoCentroResultado.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialVinculoCentroResultado.maxlength, id : DataInicialVinculoCentroResultado.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinalVinculoCentroResultado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalVinculoCentroResultado.text"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFinalVinculoCentroResultado.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalVinculoCentroResultado.maxlength, id : DataFinalVinculoCentroResultado.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: VisualizarVinculosSubRelatorio.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: VisualizarVinculosSubRelatorio.val, attr: { maxlength: VisualizarVinculosSubRelatorio.maxlength, name: VisualizarVinculosSubRelatorio.id, id: VisualizarVinculosSubRelatorio.id }">
                                        <label class="custom-control-label" data-bind="text: VisualizarVinculosSubRelatorio.text, attr: { for: VisualizarVinculosSubRelatorio.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>  
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaVeiculo" class="col-12 col-md-6 d-flex justify-content-md-end">
                        <div>
                            <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                            </button>
                        </div>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaVeiculo" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/relatorioHistoricoVeiculoVinculo"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadVeiculo();
    });

</script>
