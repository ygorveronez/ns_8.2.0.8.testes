@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Cargas</li>
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item active">Pacotes
        <li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Pacotes
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-search"></i>
                    Relatório
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaPacotes">
                        <div class="row">

                            <div class="col-12 col-md-5" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataRecebimentoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataRecebimentoInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataRecebimentoInicial.requiredClass, value: DataRecebimentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRecebimentoInicial.maxlength, id : DataRecebimentoInicial.id}" />
                                </div>
                            </div>


                            <div class="col-12 col-md-2" data-bind="visible: DataRecebimentoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataRecebimentoFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataRecebimentoFinal.requiredClass, value: DataRecebimentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRecebimentoFinal.maxlength, id : DataRecebimentoFinal.id}" />
                                </div>
                            </div>


                            <div class="col-12 col-md-3" data-bind="visible: LogKey.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: LogKey.text"></label>
                                    <input type="text" class="form-control" data-bind="class: LogKey.requiredClass, value: LogKey.val, valueUpdate: 'afterkeydown', attr: { maxlength: LogKey.maxlength, id : LogKey.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Pedido.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pedido.text, attr: { for: Pedido.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Pedido.requiredClass, value: Pedido.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pedido.maxlength, id : Pedido.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pedido.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Cubagem.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Cubagem.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Cubagem.requiredClass, value: Cubagem.val, valueUpdate: 'afterkeydown', attr: { maxlength: Cubagem.maxlength, id : Cubagem.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Peso.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Peso.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Peso.requiredClass, value: Peso.val, valueUpdate: 'afterkeydown', attr: { maxlength: Peso.maxlength, id : Peso.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Contratante.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Contratante.text, attr: { for: Contratante.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Contratante.requiredClass, value: Contratante.val, valueUpdate: 'afterkeydown', attr: { maxlength: Contratante.maxlength, id : Contratante.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Contratante.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Origem.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Origem.text, attr: { for: Origem.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Origem.requiredClass, value: Origem.val, valueUpdate: 'afterkeydown', attr: { maxlength: Origem.maxlength, id : Origem.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Origem.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Destino.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Destino.text, attr: { for: Destino.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Destino.requiredClass, value: Destino.val, valueUpdate: 'afterkeydown', attr: { maxlength: Destino.maxlength, id : Destino.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Destino.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">

                                <div class="col-12 col-md-4" data-bind="visible: ChaveCTe.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: ChaveCTe.text"></label>
                                        <input type="text" class="form-control" data-bind="class: ChaveCTe.requiredClass, value: ChaveCTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: ChaveCTe.maxlength, id : ChaveCTe.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: NumeroCTe.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroCTe.text"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroCTe.requiredClass, value: NumeroCTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCTe.maxlength, id : NumeroCTe.id}" />
                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaPacotes" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>

                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

                <div id="knockoutCRUDPesquisaPacotes" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/pacotes"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadPacotes();
    });

</script>
