@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatório</li>
        <li class="breadcrumb-item active">Carga Entrega Checklist</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Checklist
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-search"></i>
                    Pesquisar Checklists
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaCargaEntregaChecklist">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataCarregamentoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCarregamentoInicial.text, attr: { for: DataCarregamentoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCarregamentoInicial.requiredClass, value: DataCarregamentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCarregamentoInicial.maxlength, id : DataCarregamentoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataCarregamentoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCarregamentoFinal.text, attr: { for: DataCarregamentoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCarregamentoFinal.requiredClass, value: DataCarregamentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCarregamentoFinal.maxlength, id : DataCarregamentoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoCargaEmbarcador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoCargaEmbarcador.requiredClass, value: CodigoCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: CheckListTipo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CheckListTipo.text, attr: { for: CheckListTipo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: CheckListTipo.requiredClass, value: CheckListTipo.val, valueUpdate: 'afterkeydown', attr: { maxlength: CheckListTipo.maxlength, id : CheckListTipo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CheckListTipo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Motorista.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: RemetentePecuaria.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: RemetentePecuaria.text, attr: { for: RemetentePecuaria.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: RemetentePecuaria.requiredClass, value: RemetentePecuaria.val, valueUpdate: 'afterkeydown', attr: { maxlength: RemetentePecuaria.maxlength, id : RemetentePecuaria.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: RemetentePecuaria.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                            <div class="col-12 col-md-4" data-bind="visible: DataCargaInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCargaInicial.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCargaInicial.requiredClass, value: DataCargaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCargaInicial.maxlength, id : DataCargaInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: DataCargaFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCargaFinal.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCargaFinal.requiredClass, value: DataCargaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCargaFinal.maxlength, id : DataCargaFinal.id}" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="divConfigRelatorio">
                    </div>
                    <footer class="no-padding col-12 d-flex justify-content-end mb-3">
                        <div id="knockoutCRUDConfiguracaoRelatorio" class="col-6 col-sm mb-3 mb-md-0">
                            <button class="btn btn-primary waves-effect waves-themed ms-2" data-bind="click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id }" type="button">
                                <i class="fal fa-cogs"></i><span data-bind="text: ExibirConfiguracoes.text">Configuracoes</span>
                            </button>
                        </div>
                        <div id="knockoutCRUDFiltrosPesquisaCargaEntregaChecklist" class="col-6 col-sm d-flex justify-content-center justify-content-md-end">
                            <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                                <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                            </button>
                            <button class="btn btn-primary waves-effect waves-themed ms-2" data-bind="click: Preview.eventClick, attr: { id: Preview.id }" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Preview.text">Preview</span>
                            </button>
                        </div>
                    </footer>
                    <footer class="no-padding col-12 d-flex justify-content-end mb-3" id="knockoutCRUDPesquisaCargaEntregaChecklist">
                        <button class="btn btn-success waves-effect waves-themed ms-2" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id }" type="button">
                            <i class="fal fa-file-excel-o"></i><span data-bind="text: GerarRelatorioExcel.text">Gerar Planilha Excel</span>
                        </button>
                    </footer>
                </form>
            </div>
        </div>
    </div>
    <div>
        <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
    </div>
</section>








<script src="~/relatorios/scripts/cargaEntregaChecklist"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioCargaEntregaChecklist();
    });

</script>
