@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Cargas</li>
        <li class="breadcrumb-item active">Rota Controle Entrega</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Rota Controle Entrega
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaRotaControleEntrega">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataInicial.val, valueUpdate: 'afterkeydown' , attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFinal.val, valueUpdate: 'afterkeydown' , attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEntregaPedidoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEntregaPedidoInicial.text, attr: { for: DataEntregaPedidoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataEntregaPedidoInicial.val, valueUpdate: 'afterkeydown' , attr: { maxlength: DataEntregaPedidoInicial.maxlength, id : DataEntregaPedidoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEntregaPedidoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEntregaPedidoFinal.text, attr: { for: DataEntregaPedidoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataEntregaPedidoFinal.val, valueUpdate: 'afterkeydown' , attr: { maxlength: DataEntregaPedidoFinal.maxlength, id : DataEntregaPedidoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataPrevisaoEntregaPedidoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataPrevisaoEntregaPedidoInicial.text, attr: { for: DataPrevisaoEntregaPedidoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataPrevisaoEntregaPedidoInicial.val, valueUpdate: 'afterkeydown' , attr: { maxlength: DataPrevisaoEntregaPedidoInicial.maxlength, id : DataPrevisaoEntregaPedidoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataPrevisaoEntregaPedidoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataPrevisaoEntregaPedidoFinal.text, attr: { for: DataPrevisaoEntregaPedidoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataPrevisaoEntregaPedidoFinal.val, valueUpdate: 'afterkeydown' , attr: { maxlength: DataPrevisaoEntregaPedidoFinal.maxlength, id : DataPrevisaoEntregaPedidoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Carga.val, valueUpdate: 'afterkeydown' , attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Pedidos.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pedidos.text, attr: { for: Pedidos.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Pedidos.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pedidos.maxlength, id : Pedidos.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pedidos.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: NumerosNotaFiscal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumerosNotaFiscal.text, attr: { for: NumerosNotaFiscal.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: NumerosNotaFiscal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumerosNotaFiscal.maxlength, id : NumerosNotaFiscal.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: NumerosNotaFiscal.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Veiculos.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculos.text, attr: { for: Veiculos.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Veiculos.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculos.maxlength, id : Veiculos.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculos.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Destinatario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Destinatario.text, attr: { for: Destinatario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Destinatario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Destinatario.maxlength, id : Destinatario.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Destinatario.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Emitente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Emitente.text, attr: { for: Emitente.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Emitente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Emitente.maxlength, id : Emitente.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Emitente.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </form>
                <div class="row mb-2">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-2 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaRotaControleEntrega" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-2 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>


                <div id="knockoutCRUDPesquisaRotaControleEntrega" class="d-flex justify-content-center justify-content-md-end mt-2">
                    <button class="btn btn-success me-2" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>

</section>


<script src="~/relatorios/scripts/rotaControleEntrega"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadRotaControleEntrega();
    });

</script>
