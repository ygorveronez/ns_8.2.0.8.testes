@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">NFe</li>
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item active">Notas Emitidas</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Notas Emitidas
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-search"></i>
                    Pesquisar Notas Eletrônicas
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaNotasEmitidas">
                        <div class="row">

                            <div class="col-12 col-md-2" data-bind="visible: NumeroInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroInicial.text, attr: { for: NumeroInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroInicial.requiredClass, value: NumeroInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroInicial.maxlength, id : NumeroInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: NumeroFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroFinal.text, attr: { for: NumeroFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroFinal.requiredClass, value: NumeroFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroFinal.maxlength, id : NumeroFinal.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Serie.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Serie.text, attr: { for: Serie.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Serie.requiredClass, value: Serie.val, valueUpdate: 'afterkeydown', attr: { maxlength: Serie.maxlength, id : Serie.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinal.requiredClass, value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataProcessamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataProcessamento.text, attr: { for: DataProcessamento.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataProcessamento.requiredClass, value: DataProcessamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataProcessamento.maxlength, id : DataProcessamento.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataSaida.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataSaida.text, attr: { for: DataSaida.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataSaida.requiredClass, value: DataSaida.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataSaida.maxlength, id : DataSaida.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: TipoEmissao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoEmissao.text, attr: { for: TipoEmissao.id }"></label>
                                    <select class="form-control" data-bind="class: TipoEmissao.requiredClass, options: TipoEmissao.options, optionsText: 'text', optionsValue: 'value', value: TipoEmissao.val, attr: { maxlength: TipoEmissao.maxlength, id : TipoEmissao.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: TipoDocumento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoDocumento.text, attr: { for: TipoDocumento.id }"></label>
                                    <select class="form-control" data-bind="class: TipoDocumento.requiredClass, options: TipoDocumento.options, optionsText: 'text', optionsValue: 'value', value: TipoDocumento.val, attr: { maxlength: TipoDocumento.maxlength, id : TipoDocumento.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Chave.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Chave.text, attr: { for: Chave.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Chave.requiredClass, value: Chave.val, valueUpdate: 'afterkeydown', attr: { maxlength: Chave.maxlength, id : Chave.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: NotaImportada.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NotaImportada.text, attr: { for: NotaImportada.id }"></label>
                                    <select class="form-control" data-bind="class: NotaImportada.requiredClass, options: NotaImportada.options, optionsText: 'text', optionsValue: 'value', value: NotaImportada.val, attr: { maxlength: NotaImportada.maxlength, id : NotaImportada.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-5" data-bind="visible: Pessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Pessoa.requiredClass, value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pessoa.maxlength, id : Pessoa.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pessoa.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-5" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">

                                <div class="col-12 col-md-6" data-bind="visible: NaturezaOperacao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NaturezaOperacao.text, attr: { for: NaturezaOperacao.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: NaturezaOperacao.requiredClass, value: NaturezaOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: NaturezaOperacao.maxlength, id : NaturezaOperacao.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: NaturezaOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6" data-bind="visible: Atividade.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Atividade.text, attr: { for: Atividade.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Atividade.requiredClass, value: Atividade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Atividade.maxlength, id : Atividade.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Atividade.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6" data-bind="visible: Usuario.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Usuario.maxlength, id : Usuario.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Usuario.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-6" data-bind="visible: CFOP.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CFOP.text, attr: { for: CFOP.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: CFOP.requiredClass, value: CFOP.val, valueUpdate: 'afterkeydown', attr: { maxlength: CFOP.maxlength, id : CFOP.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CFOP.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-12" data-bind="visible: ExibirItens.visible">
                                    <div class="form-group">
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" data-bind="checked: ExibirItens.val, attr: { maxlength: ExibirItens.maxlength, name: ExibirItens.id, id: ExibirItens.id }">
                                            <label class="custom-control-label" data-bind="text: ExibirItens.text, attr: { for: ExibirItens.id }"></label>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaNotasEmitidas" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>

                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

                <div id="knockoutCRUDPesquisaNotasEmitidas" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: DownloadLoteXML.eventClick, attr: { id: DownloadLoteXML.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: DownloadLoteXML.text"></span>
                    </button>
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/notasEmitidas"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioNotasEmitidas();
    });

</script>