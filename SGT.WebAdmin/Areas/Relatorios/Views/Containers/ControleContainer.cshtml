
@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Containers</li>
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item active">Controle Container
        <li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório Controle de Container
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-search"></i>
                    Relatório
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaControleContainer">
                        <div class="row">

                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataInicialColeta.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialColeta.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicialColeta.requiredClass, value: DataInicialColeta.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialColeta.maxlength, id : DataInicialColeta.id}" />
                                </div>
                            </div>


                            <div class="col-12 col-md-2" data-bind="visible: DataFinalColeta.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalColeta.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalColeta.requiredClass, value: DataFinalColeta.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalColeta.maxlength, id : DataFinalColeta.id}" />
                                </div>
                            </div>


                            <div class="col-12 col-md-4" data-bind="visible: SituacaoContainer.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoContainer.text, attr: { for: SituacaoContainer.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoContainer.requiredClass, options: SituacaoContainer.options, optionsText: 'text', optionsValue: 'value', value: SituacaoContainer.val, attr: { maxlength: SituacaoContainer.maxlength, id : SituacaoContainer.id}"></select>
                                </div>
                            </div>


                            <div data-bind="fadeVisible: Visible.visibleFade">
                                <div class="row">

                                    <div class="col-12 col-md-2" data-bind="visible: DiasPosseInicial.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DiasPosseInicial.text"></label>
                                            <input type="text" class="form-control" data-bind="class: DiasPosseInicial.requiredClass, value: DiasPosseInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DiasPosseInicial.maxlength, id : DiasPosseInicial.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: DiasPosseFinal.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DiasPosseFinal.text"></label>
                                            <input type="text" class="form-control" data-bind="class: DiasPosseFinal.requiredClass, value: DiasPosseFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DiasPosseFinal.maxlength, id : DiasPosseFinal.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: Carga.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Carga.text"></label>
                                            <input type="text" class="form-control" data-bind="class: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: NumeroContainer.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: NumeroContainer.text"></label>
                                            <input type="text" class="form-control" data-bind="class: NumeroContainer.requiredClass, value: NumeroContainer.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroContainer.maxlength, id : NumeroContainer.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: NumeroPedido.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: NumeroPedido.text"></label>
                                            <input type="text" class="form-control" data-bind="class: NumeroPedido.requiredClass, value: NumeroPedido.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroPedido.maxlength, id : NumeroPedido.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: NumeroBooking.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: NumeroBooking.text"></label>
                                            <input type="text" class="form-control" data-bind="class: NumeroBooking.requiredClass, value: NumeroBooking.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroBooking.maxlength, id : NumeroBooking.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: NumeroEXP.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: NumeroEXP.text"></label>
                                            <input type="text" class="form-control" data-bind="class: NumeroEXP.requiredClass, value: NumeroEXP.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroEXP.maxlength, id : NumeroEXP.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: TipoContainer.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: TipoContainer.text, attr: { for: TipoContainer.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: TipoContainer.requiredClass, value: TipoContainer.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoContainer.maxlength, id : TipoContainer.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoContainer.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: LocalEsperaVazio.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: LocalEsperaVazio.text, attr: { for: LocalEsperaVazio.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: LocalEsperaVazio.requiredClass, value: LocalEsperaVazio.val, valueUpdate: 'afterkeydown', attr: { maxlength: LocalEsperaVazio.maxlength, id : LocalEsperaVazio.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: LocalEsperaVazio.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                    <div class="col-12 col-md-2" data-bind="visible: DataMovimentacao.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataMovimentacao.text"></label>
                                            <input type="text" class="form-control" data-bind="class: DataMovimentacao.requiredClass, value: DataMovimentacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataMovimentacao.maxlength, id : DataMovimentacao.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-2" data-bind="visible: DataPorto.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: DataPorto.text"></label>
                                            <input type="text" class="form-control" data-bind="class: DataPorto.requiredClass, value: DataPorto.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataPorto.maxlength, id : DataPorto.id}" />
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-4" data-bind="visible: LocalAtual.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: LocalAtual.text, attr: { for: LocalAtual.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: LocalAtual.requiredClass, value: LocalAtual.val, valueUpdate: 'afterkeydown', attr: { maxlength: LocalAtual.maxlength, id : LocalAtual.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: LocalAtual.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-12 col-md-4" data-bind="visible: LocalColeta.visible">
                                        <div class="form-group">
                                            <label class="form-label" data-bind="text: LocalColeta.text, attr: { for: LocalColeta.id }"></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" data-bind="css: LocalColeta.requiredClass, value: LocalColeta.val, valueUpdate: 'afterkeydown', attr: { maxlength: LocalColeta.maxlength, id : LocalColeta.id}" />
                                                <div class="input-group-append">
                                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: LocalColeta.idBtnSearch}" type="button" aria-label="Buscar">
                                                        <i class="fal fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaControleContainer" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>

                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

                <div id="knockoutCRUDPesquisaControleContainer" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/controleContainer"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadControleContainer();
    });

</script>
