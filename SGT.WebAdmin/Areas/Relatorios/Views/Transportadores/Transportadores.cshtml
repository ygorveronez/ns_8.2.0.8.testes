@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Transportadores</li>
        <li class="breadcrumb-item active">Transportadores</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            {Localize::Transportadores.Transportadores.RelatorioTransportadores}
        </h1>
        <div class="subheader-help">
            <button type="button" onclick="iniciarAjuda(857)" data-action="toggle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Abrir a ajuda da página" aria-label="Abrir a ajuda da página"><i class="fal fa-question-circle"></i></button>
        </div>

    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    {Localize::Transportadores.Transportadores.Relatorios} 
                </h3>   
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaTransportadores">
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Localidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Localidade.text, attr: { for: Localidade.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Localidade.requiredClass, value: Localidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Localidade.maxlength, id : Localidade.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Localidade.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Estado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Estado.text, attr: { for: Estado.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Estado.requiredClass, value: Estado.val, valueUpdate: 'afterkeydown', attr: { maxlength: Estado.maxlength, id : Estado.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Estado.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: PrazoValidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PrazoValidade.text, attr: { for: PrazoValidade.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: PrazoValidade.requiredClass, value: PrazoValidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: PrazoValidade.maxlength, id : PrazoValidade.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: EmiteEmbarcador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EmiteEmbarcador.text, attr: { for: EmiteEmbarcador.id }"></label>
                                    <select class="form-control" data-bind="class: EmiteEmbarcador.requiredClass, options: EmiteEmbarcador.options, optionsText: 'text', optionsValue: 'value', value: EmiteEmbarcador.val, attr: { maxlength: EmiteEmbarcador.maxlength, id : EmiteEmbarcador.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: OptanteSimplesNacional.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: OptanteSimplesNacional.text, attr: { for: OptanteSimplesNacional.id }"></label>
                                    <select class="form-control" data-bind="class: OptanteSimplesNacional.requiredClass, options: OptanteSimplesNacional.options, optionsText: 'text', optionsValue: 'value', value: OptanteSimplesNacional.val, attr: { maxlength: OptanteSimplesNacional.maxlength, id : OptanteSimplesNacional.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: LiberacaoParaPagamentoAutomatico.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: LiberacaoParaPagamentoAutomatico.text, attr: { for: LiberacaoParaPagamentoAutomatico.id }"></label>
                                    <select class="form-control" data-bind="class: LiberacaoParaPagamentoAutomatico.requiredClass, options: LiberacaoParaPagamentoAutomatico.options, optionsText: 'text', optionsValue: 'value', value: LiberacaoParaPagamentoAutomatico.val, attr: { maxlength: LiberacaoParaPagamentoAutomatico.maxlength, id : LiberacaoParaPagamentoAutomatico.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: ConfiguracaoNFSe.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ConfiguracaoNFSe.text, attr: { for: ConfiguracaoNFSe.id }"></label>
                                    <select class="form-control" data-bind="class: ConfiguracaoNFSe.requiredClass, options: ConfiguracaoNFSe.options, optionsText: 'text', optionsValue: 'value', value: ConfiguracaoNFSe.val, attr: { maxlength: ConfiguracaoNFSe.maxlength, id : ConfiguracaoNFSe.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCriacaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCriacaoInicial.text, attr: { for: DataCriacaoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCriacaoInicial.requiredClass, value: DataCriacaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCriacaoInicial.maxlength, id : DataCriacaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCriacaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCriacaoFinal.text, attr: { for: DataCriacaoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCriacaoFinal.requiredClass, value: DataCriacaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCriacaoFinal.maxlength, id : DataCriacaoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataAlteracaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAlteracaoInicial.text, attr: { for: DataAlteracaoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataAlteracaoInicial.requiredClass, value: DataAlteracaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAlteracaoInicial.maxlength, id : DataAlteracaoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataAlteracaoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAlteracaoFinal.text, attr: { for: DataAlteracaoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataAlteracaoFinal.requiredClass, value: DataAlteracaoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAlteracaoFinal.maxlength, id : DataAlteracaoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataInicioVencimentoCertificado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicioVencimentoCertificado.text, attr: { for: DataInicioVencimentoCertificado.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicioVencimentoCertificado.requiredClass, value: DataInicioVencimentoCertificado.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicioVencimentoCertificado.maxlength, id : DataInicioVencimentoCertificado.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataFinalVencimentoCertificado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalVencimentoCertificado.text, attr: { for: DataFinalVencimentoCertificado.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalVencimentoCertificado.requiredClass, value: DataFinalVencimentoCertificado.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalVencimentoCertificado.maxlength, id : DataFinalVencimentoCertificado.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Bloqueado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Bloqueado.text, attr: { for: Bloqueado.id }"></label>
                                    <select class="form-control" data-bind="class: Bloqueado.requiredClass, options: Bloqueado.options, optionsText: 'text', optionsValue: 'value', value: Bloqueado.val, attr: { maxlength: Bloqueado.maxlength, id : Bloqueado.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-12 align-self-center" data-bind="visible: CertificadosVencidos.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: CertificadosVencidos.val, attr: { maxlength: CertificadosVencidos.maxlength, name: CertificadosVencidos.id, id: CertificadosVencidos.id }">
                                        <label class="custom-control-label" data-bind="text: CertificadosVencidos.text, attr: { for: CertificadosVencidos.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaTransportadores" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaTransportadores" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/transportadores"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("Transportadores.Transportadores").then(loadRelatorioTransportadores);
    });

</script>
