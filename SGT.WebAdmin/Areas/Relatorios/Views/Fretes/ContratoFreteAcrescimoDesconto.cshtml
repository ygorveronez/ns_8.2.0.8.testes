@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item">Fretes</li>
    <li class="breadcrumb-item active">Contrato Frete Acréscimo Desconto</li>
</ol>

<div class="subheader">
    <h1 class="subheader-title">
        <i class="subheader-icon fal fa-truck"></i> Contrato Frete Acréscimo Desconto
    </h1>
</div>

<div id="divConteudoRelatorio" class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-file-alt"></i>
                Relatórios
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutPesquisaContratoFreteAcrescimoDesconto">
                    <div class="row">
                        <div class="col-12 col-md-3" data-bind="visible: TipoRelatorio.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-3" data-bind="visible: NumCiot.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: NumCiot.text, attr: { for: NumCiot.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: NumCiot.requiredClass, value: NumCiot.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumCiot.maxlength, id : NumCiot.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: NumCiot.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-3" data-bind="visible: NumContratoFrete.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: NumContratoFrete.text, attr: { for: NumContratoFrete.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: NumContratoFrete.requiredClass, value: NumContratoFrete.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumContratoFrete.maxlength, id : NumContratoFrete.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: NumContratoFrete.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-3" data-bind="visible: NumCarga.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: NumCarga.text, attr: { for: NumCarga.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: NumCarga.requiredClass, value: NumCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumCarga.maxlength, id : NumCarga.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: NumCarga.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: DataInicialLancamento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataInicialLancamento.text"></label>
                                <input type="text" class="form-control" data-bind="class: DataInicialLancamento.requiredClass, value: DataInicialLancamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialLancamento.maxlength, id : DataInicialLancamento.id}" />
                            </div>
                        </div>
                        <div class="col-12 col-md-2" data-bind="visible: DataFinalLancamento.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFinalLancamento.text"></label>
                                <input type="text" class="form-control" data-bind="class: DataFinalLancamento.requiredClass, value: DataFinalLancamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalLancamento.maxlength, id : DataFinalLancamento.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: Terceiro.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Terceiro.text, attr: { for: Terceiro.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Terceiro.requiredClass, value: Terceiro.val, valueUpdate: 'afterkeydown', attr: { maxlength: Terceiro.maxlength, id : Terceiro.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Terceiro.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="col-12 col-md-4" data-bind="visible: Justificativa.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Justificativa.text, attr: { for: Justificativa.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Justificativa.requiredClass, value: Justificativa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Justificativa.maxlength, id : Justificativa.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Justificativa.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="row mb-3 mt-3">
                <!-- CRUD Configurações -->
                <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <!-- CRUD Filtro de Pesquisa -->
                <div id="knockoutCRUDFiltrosPesquisaContratoFreteAcrescimoDesconto" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">

                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>
                </div>
            </div>
            <!-- Configurações do Relatório -->
            <div id="divConfigRelatorio">
            </div>
            <!-- GRID de Resultados -->
            <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
            <!-- Botões Inferiores-->
            <div id="knockoutCRUDPesquisaContratoFreteAcrescimoDesconto" class="d-flex justify-content-center justify-content-md-end mt-3">
                <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                    <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                </button>
                <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                    <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/relatorios/scripts/contratoFreteAcrescimoDesconto"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadContratoFreteAcrescimoDesconto();
    });

</script>