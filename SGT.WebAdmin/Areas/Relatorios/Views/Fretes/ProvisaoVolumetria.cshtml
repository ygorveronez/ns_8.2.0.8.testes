@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Fretes</li>
        <li class="breadcrumb-item active">Provisão Volumetria</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Provisão e Volumetria
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaProvisaoVolumetria">
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoOperacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoOperacao.text, attr: { for: TipoOperacao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoOperacao.requiredClass, value: TipoOperacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacao.maxlength, id : TipoOperacao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Mercado.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Mercado.text, attr: { for: Mercado.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Mercado.requiredClass, value: Mercado.val, valueUpdate: 'afterkeydown', attr: { maxlength: Mercado.maxlength, id : Mercado.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Mercado.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEmissaoNFInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoNFInicio.text, attr: { for: DataEmissaoNFInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoNFInicio.requiredClass, value: DataEmissaoNFInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoNFInicio.maxlength, id : DataEmissaoNFInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataEmissaoNFFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataEmissaoNFFim.text, attr: { for: DataEmissaoNFFim.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataEmissaoNFFim.requiredClass, value: DataEmissaoNFFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoNFFim.maxlength, id : DataEmissaoNFFim.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataIntegracaoPagamentoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataIntegracaoPagamentoInicio.text, attr: { for: DataIntegracaoPagamentoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataIntegracaoPagamentoInicio.requiredClass, value: DataIntegracaoPagamentoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataIntegracaoPagamentoInicio.maxlength, id : DataIntegracaoPagamentoInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataIntegracaoPagamentoFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataIntegracaoPagamentoFim.text, attr: { for: DataIntegracaoPagamentoFim.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataIntegracaoPagamentoFim.requiredClass, value: DataIntegracaoPagamentoFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataIntegracaoPagamentoFim.maxlength, id : DataIntegracaoPagamentoFim.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataVencimentoInicio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoInicio.text, attr: { for: DataVencimentoInicio.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoInicio.requiredClass, value: DataVencimentoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoInicio.maxlength, id : DataVencimentoInicio.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataVencimentoFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoFim.text, attr: { for: DataVencimentoFim.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoFim.requiredClass, value: DataVencimentoFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoFim.maxlength, id : DataVencimentoFim.id}" />
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaProvisaoVolumetria" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaProvisaoVolumetria" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>


<script src="~/relatorios/scripts/provisaoVolumetria"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadProvisaoVolumetria();
    });

</script>
