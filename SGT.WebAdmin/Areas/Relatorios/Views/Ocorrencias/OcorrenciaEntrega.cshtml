@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Ocorrências</li>
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item active">Ocorrência</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            {Localize::Relatorios.Ocorrencias.Ocorrencia.RelatorioOcorrenciasEntrega}
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel" style="display: none;">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-search"></i>
                    {Localize::Relatorios.Ocorrencias.Ocorrencia.RelatorioOcorrenciasEntrega}
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaOcorrencia">
                        <div class="row">

                            <div class="col-12 col-md-3" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: DataSolicitacaoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataSolicitacaoInicial.text, attr: { for: DataSolicitacaoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataSolicitacaoInicial.requiredClass, value: DataSolicitacaoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataSolicitacaoInicial.maxlength, id : DataSolicitacaoInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: DataSolicitacaoFim.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataSolicitacaoFim.text, attr: { for: DataSolicitacaoFim.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataSolicitacaoFim.requiredClass, value: DataSolicitacaoFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataSolicitacaoFim.maxlength, id : DataSolicitacaoFim.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible : SituacaoOcorrencia.visible, css: SituacaoOcorrencia.cssClass">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoOcorrencia.text, attr: { for: SituacaoOcorrencia.id }"></label>
                                    <select class="form-control" data-bind="options: SituacaoOcorrencia.options, optionsText: 'text', optionsValue: 'value', selectedOptions: SituacaoOcorrencia.val, attr: { id : SituacaoOcorrencia.id}" multiple="multiple" data-actions-box="false" data-selected-text-format="count > 3" title="Todos"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible : SituacaoCancelamento.visible, css: SituacaoCancelamento.cssClass">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoCancelamento.text, attr: { for: SituacaoCancelamento.id }"></label>
                                    <select class="form-control" data-bind="options: SituacaoCancelamento.options, optionsText: 'text', optionsValue: 'value', selectedOptions: SituacaoCancelamento.val, attr: { id : SituacaoCancelamento.id}" multiple="multiple" data-actions-box="false" data-selected-text-format="count > 3" title="Todos"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Ocorrencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ocorrencia.text, attr: { for: Ocorrencia.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Ocorrencia.requiredClass, value: Ocorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: Ocorrencia.maxlength, id : Ocorrencia.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Ocorrencia.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: NumeroOcorrenciaInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroOcorrenciaInicial.text, attr: { for: NumeroOcorrenciaInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroOcorrenciaInicial.requiredClass, value: NumeroOcorrenciaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroOcorrenciaInicial.maxlength, id : NumeroOcorrenciaInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: NumeroOcorrenciaFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroOcorrenciaFinal.text, attr: { for: NumeroOcorrenciaFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroOcorrenciaFinal.requiredClass, value: NumeroOcorrenciaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroOcorrenciaFinal.maxlength, id : NumeroOcorrenciaFinal.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: TipoPessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoPessoa.text, attr: { for: TipoPessoa.id }"></label>
                                    <select class="form-control" data-bind="class: TipoPessoa.requiredClass, options: TipoPessoa.options, optionsText: 'text', optionsValue: 'value', value: TipoPessoa.val, attr: { maxlength: TipoPessoa.maxlength, id : TipoPessoa.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: GrupoPessoas.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoPessoas.text, attr: { for: GrupoPessoas.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: GrupoPessoas.requiredClass, value: GrupoPessoas.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoPessoas.maxlength, id : GrupoPessoas.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: GrupoPessoas.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: Pessoa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Pessoa.text, attr: { for: Pessoa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Pessoa.requiredClass, value: Pessoa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pessoa.maxlength, id : Pessoa.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Pessoa.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: GrupoOcorrencia.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoOcorrencia.text, attr: { for: GrupoOcorrencia.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: GrupoOcorrencia.requiredClass, value: GrupoOcorrencia.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoOcorrencia.maxlength, id : GrupoOcorrencia.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: GrupoOcorrencia.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                                <div class="col-12 col-md-3" data-bind="visible: CargaAgrupada.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CargaAgrupada.text, attr: { for: CargaAgrupada.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: CargaAgrupada.requiredClass, value: CargaAgrupada.val, valueUpdate: 'afterkeydown', attr: { maxlength: CargaAgrupada.maxlength, id : CargaAgrupada.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: NumeroNotaFiscal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NumeroNotaFiscal.text, attr: { for: NumeroNotaFiscal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: NumeroNotaFiscal.requiredClass, value: NumeroNotaFiscal.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroNotaFiscal.maxlength, id : NumeroNotaFiscal.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-3" data-bind="visible: Veiculo.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-3" data-bind="visible: Motorista.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Motorista.text, attr: { for: Motorista.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: Motorista.requiredClass, value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Motorista.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 col-md-3" data-bind="visible: TransportadorCarga.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: TransportadorCarga.text, attr: { for: TransportadorCarga.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: TransportadorCarga.requiredClass, value: TransportadorCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: TransportadorCarga.maxlength, id : TransportadorCarga.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TransportadorCarga.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-4" data-bind="visible: TipoOperacaoCarga.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: TipoOperacaoCarga.text, attr: { for: TipoOperacaoCarga.id }"></label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-bind="css: TipoOperacaoCarga.requiredClass, value: TipoOperacaoCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoOperacaoCarga.maxlength, id : TipoOperacaoCarga.id}" />
                                            <div class="input-group-append">
                                                <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoOperacaoCarga.idBtnSearch}" type="button" aria-label="Buscar">
                                                    <i class="fal fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaOcorrencia" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>

                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

                <div id="knockoutCRUDPesquisaOcorrencia" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/ocorrenciaEntrega"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources("Relatorios.Ocorrencias.Ocorrencia").then(LoadOcorrenciaEntrega);
    });

</script>
