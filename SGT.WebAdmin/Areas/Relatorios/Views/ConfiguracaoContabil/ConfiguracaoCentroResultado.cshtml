@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Configuração Contábil</li>
        <li class="breadcrumb-item active">Configuração Centro Resultado</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Configuração Centro de Resultado
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaConfiguracaoCentroResultado">
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: Remetente.visible">
