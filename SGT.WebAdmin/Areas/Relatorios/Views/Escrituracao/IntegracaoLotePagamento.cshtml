@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Escrituração</li>
        <li class="breadcrumb-item active">Integração Lote Pagamento</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Integração Lote Pagamento
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaIntegracaoLotePagamento">
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: NumeroPagamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroPagamento.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroPagamento.requiredClass, value: NumeroPagamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroPagamento.maxlength, id : NumeroPagamento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: CTe.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CTe.text, attr: { for: CTe.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: CTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: CTe.maxlength, id : CTe.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: CTe.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: SituacaoPagamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoPagamento.text, attr: { for: SituacaoPagamento.id }"></label>
                                    <select class="form-control" data-bind="options: SituacaoPagamento.options, optionsText: 'text', optionsValue: 'value', value: SituacaoPagamento.val, attr: { maxlength: SituacaoPagamento.maxlength, id : SituacaoPagamento.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: SituacaoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoIntegracao.text, attr: { for: SituacaoIntegracao.id }"></label>
                                    <select class="form-control" data-bind="options: SituacaoIntegracao.options, optionsText: 'text', optionsValue: 'value', value: SituacaoIntegracao.val, attr: { maxlength: SituacaoIntegracao.maxlength, id : SituacaoIntegracao.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: SituacaoCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoCarga.text, attr: { for: SituacaoCarga.id }"></label>
                                    <select class="form-control" data-bind="options: SituacaoCarga.options, optionsText: 'text', optionsValue: 'value', value: SituacaoCarga.val, attr: { maxlength: SituacaoCarga.maxlength, id : SituacaoCarga.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataInicialEmissaoDocumento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialEmissaoDocumento.text, attr: { for: DataInicialEmissaoDocumento.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicialEmissaoDocumento.requiredClass, value: DataInicialEmissaoDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialEmissaoDocumento.maxlength, id : DataInicialEmissaoDocumento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinalEmissaoDocumento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalEmissaoDocumento.text, attr: { for: DataFinalEmissaoDocumento.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalEmissaoDocumento.requiredClass, value: DataFinalEmissaoDocumento.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalEmissaoDocumento.maxlength, id : DataFinalEmissaoDocumento.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: ExibirUltimoRegistroQuandoExistirProtocoloCTeDuplicado.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: ExibirUltimoRegistroQuandoExistirProtocoloCTeDuplicado.val, attr: { maxlength: ExibirUltimoRegistroQuandoExistirProtocoloCTeDuplicado.maxlength, name: ExibirUltimoRegistroQuandoExistirProtocoloCTeDuplicado.id, id: ExibirUltimoRegistroQuandoExistirProtocoloCTeDuplicado.id }">
                                        <label class="custom-control-label" data-bind="text: ExibirUltimoRegistroQuandoExistirProtocoloCTeDuplicado.text, attr: { for: ExibirUltimoRegistroQuandoExistirProtocoloCTeDuplicado.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-2">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-2 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaIntegracaoLotePagamento" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaIntegracaoLotePagamento" class="d-flex justify-content-center justify-content-md-end mt-2">
                    <button class="btn btn-success me-2" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/integracaoLotePagamento"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadIntegracaoLotePagamento();
    });

</script>
