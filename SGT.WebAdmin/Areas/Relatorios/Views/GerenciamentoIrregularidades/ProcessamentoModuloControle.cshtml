@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Gerenciamento Irregularidades</li>
        <li class="breadcrumb-item active">Processamento Módulo Controle</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório do Processamento Módulo Controle
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaProcessamentoModuloControle">
                        <div class="row">

                            <div class="col-12 col-md-2" data-bind="visible: DataInicialEmissao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialEmissao.text, attr: { for: DataInicialEmissao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicialEmissao.requiredClass, value: DataInicialEmissao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialEmissao.maxlength, id : DataInicialEmissao.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataFinalEmissao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalEmissao.text, attr: { for: DataFinalEmissao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalEmissao.requiredClass, value: DataFinalEmissao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalEmissao.maxlength, id : DataFinalEmissao.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataInicialIrregularidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicialIrregularidade.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataInicialIrregularidade.requiredClass, value: DataInicialIrregularidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicialIrregularidade.maxlength, id : DataInicialIrregularidade.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataFinalIrregularidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinalIrregularidade.text"></label>
                                    <input type="text" class="form-control" data-bind="class: DataFinalIrregularidade.requiredClass, value: DataFinalIrregularidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinalIrregularidade.maxlength, id : DataFinalIrregularidade.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: NumeroCTe.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCTe.text"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroCTe.requiredClass, value: NumeroCTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCTe.maxlength, id : NumeroCTe.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: SerieCTe.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SerieCTe.text"></label>
                                    <input type="text" class="form-control" data-bind="class: SerieCTe.requiredClass, value: SerieCTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: SerieCTe.maxlength, id : SerieCTe.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Situacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Situacao.text, attr: { for: Situacao.id }"></label>
                                    <select class="form-control" data-bind="class: Situacao.requiredClass, options: Situacao.options, optionsText: 'text', optionsValue: 'value', value: Situacao.val, attr: { maxlength: Situacao.maxlength, id : Situacao.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-3" data-bind="visible: Processo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Processo.text, attr: { for: Processo.id }"></label>
                                    <select class="form-control" data-bind="class: Processo.requiredClass, options: Processo.options, optionsText: 'text', optionsValue: 'value', value: Processo.val, attr: { maxlength: Processo.maxlength, id : Processo.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: EstornoMIRO.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: EstornoMIRO.val, attr: { maxlength: EstornoMIRO.maxlength, name: EstornoMIRO.id, id: EstornoMIRO.id }">
                                        <label class="custom-control-label" data-bind="text: EstornoMIRO.text, attr: { for: EstornoMIRO.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                            <div class="col-12 col-md-4" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Irregularidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Irregularidade.text, attr: { for: Irregularidade.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Irregularidade.requiredClass, value: Irregularidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Irregularidade.maxlength, id : Irregularidade.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Irregularidade.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Carga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Portifolio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Portifolio.text, attr: { for: Portifolio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Portifolio.requiredClass, value: Portifolio.val, valueUpdate: 'afterkeydown', attr: { maxlength: Portifolio.maxlength, id : Portifolio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Portifolio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Setor.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Setor.text, attr: { for: Setor.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Setor.requiredClass, value: Setor.val, valueUpdate: 'afterkeydown', attr: { maxlength: Setor.maxlength, id : Setor.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Setor.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaProcessamentoModuloControle" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaProcessamentoModuloControle" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>


<script src="~/relatorios/scripts/processamentoModuloControle"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadProcessamentoModuloControle();
    });

</script>
