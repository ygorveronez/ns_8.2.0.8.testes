@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatorios</li>
        <li class="breadcrumb-item">Canhotos</li>
        <li class="breadcrumb-item active">Historico Movimentação Canhoto</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-file-alt"></i> Relatório de Historico de Movimentação de Canhotos
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatório de Historico de Movimentação de Canhotos
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaHistoricoMovimentacaoCanhoto">
                        <div class="row">
                            <div class="col-12 col-md-3" data-bind="visible: NumeroCanhoto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCanhoto.text, attr: { for: NumeroCanhoto.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: NumeroCanhoto.requiredClass, value: NumeroCanhoto.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCanhoto.maxlength, id : NumeroCanhoto.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Emitente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Emitente.text, attr: { for: Emitente.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Emitente.requiredClass, value: Emitente.val, valueUpdate: 'afterkeydown', attr: { maxlength: Emitente.maxlength, id : Emitente.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Emitente.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataUpload.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataUpload.text, attr: { for: DataUpload.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataUpload.requiredClass, value: DataUpload.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataUpload.maxlength, id : DataUpload.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataAprovacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAprovacao.text, attr: { for: DataAprovacao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataAprovacao.requiredClass, value: DataAprovacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAprovacao.maxlength, id : DataAprovacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataRejeicao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataRejeicao.text, attr: { for: DataRejeicao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataRejeicao.requiredClass, value: DataRejeicao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRejeicao.maxlength, id : DataRejeicao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataReversao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataReversao.text, attr: { for: DataReversao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataReversao.requiredClass, value: DataReversao.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataReversao.maxlength, id : DataReversao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataRecebimentoFisico.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataRecebimentoFisico.text, attr: { for: DataRecebimentoFisico.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataRecebimentoFisico.requiredClass, value: DataRecebimentoFisico.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRecebimentoFisico.maxlength, id : DataRecebimentoFisico.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataConfirmacaoEntrega.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataConfirmacaoEntrega.text, attr: { for: DataConfirmacaoEntrega.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataConfirmacaoEntrega.requiredClass, value: DataConfirmacaoEntrega.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataConfirmacaoEntrega.maxlength, id : DataConfirmacaoEntrega.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: MotivoRejeicao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: MotivoRejeicao.text, attr: { for: MotivoRejeicao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: MotivoRejeicao.requiredClass, value: MotivoRejeicao.val, valueUpdate: 'afterkeydown', attr: { maxlength: MotivoRejeicao.maxlength, id : MotivoRejeicao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: MotivoRejeicao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Usuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Usuario.text, attr: { for: Usuario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Usuario.requiredClass, value: Usuario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Usuario.maxlength, id : Usuario.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Usuario.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="divConfigRelatorio">
                    </div>

                    <div class="row mb-2">
                        <div id="knockoutCRUDFiltrosPesquisaHistoricoMovimentacaoCanhoto" class="col-6 col-sm d-flex justify-content-center justify-content-md-end">
                            <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                                <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                            </button>
                        </div>
                        <div id="knockoutCRUDConfiguracaoRelatorio">
                            <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                                <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                            </button>
                        </div>
                    </div>
                    <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>

                    <footer class="no-padding">
                        <div id="knockoutCRUDBaixarRelatorio" class="d-flex justify-content-center justify-content-md-end mt-2">
                            <button class="btn btn-success me-2" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                                <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                            </button>
                            <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                                <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                            </button>
                        </div>
                    </footer>
                </form>
            </div>
        </div>
    </div>
</section>



















<script src="~/relatorios/scripts/movimentacaoCanhoto"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadHistoricoMovimentacaoCanhoto();
    });

</script>
