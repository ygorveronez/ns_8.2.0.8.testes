@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Usuarios</li>
        <li class="breadcrumb-item active">Usuario</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Usuário
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaUsuario">
                        <div class="row">
                            <!-- FILTROS -->
                            <div class="col-12 col-md-3" data-bind="visible: DataCadastroInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCadastroInicial.text, attr: { for: DataCadastroInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCadastroInicial.requiredClass, value: DataCadastroInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCadastroInicial.maxlength, id : DataCadastroInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataCadastroFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCadastroFinal.text, attr: { for: DataCadastroFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataCadastroFinal.requiredClass, value: DataCadastroFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCadastroFinal.maxlength, id : DataCadastroFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: UltimoAcessoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: UltimoAcessoInicial.text, attr: { for: UltimoAcessoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: UltimoAcessoInicial.requiredClass, value: UltimoAcessoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: UltimoAcessoInicial.maxlength, id : UltimoAcessoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: UltimoAcessoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: UltimoAcessoFinal.text, attr: { for: UltimoAcessoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: UltimoAcessoFinal.requiredClass, value: UltimoAcessoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: UltimoAcessoFinal.maxlength, id : UltimoAcessoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Operador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Operador.text, attr: { for: Operador.id }"></label>
                                    <select class="form-control" data-bind="class: Operador.requiredClass, options: Operador.options, optionsText: 'text', optionsValue: 'value', value: Operador.val, attr: { maxlength: Operador.maxlength, id : Operador.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: AcessoSistema.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: AcessoSistema.text, attr: { for: AcessoSistema.id }"></label>
                                    <select class="form-control" data-bind="class: AcessoSistema.requiredClass, options: AcessoSistema.options, optionsText: 'text', optionsValue: 'value', value: AcessoSistema.val, attr: { maxlength: AcessoSistema.maxlength, id : AcessoSistema.id}"></select>
                                </div>
                            </div>
                            @*<section data-bind="visible: Ambiente.visible" class="col col-xs-12 col-sm-12 col-md-3">
                                       <label class="label"><b data-bind="text: Ambiente.text"></b></label>
                                       <label class="select">
                                           <select data-bind="options: Ambiente.options, optionsText: 'text', optionsValue: 'value', value: Ambiente.val, attr: { id : Ambiente.id}"></select>
                                           <i></i>
                                       </label>
                                </section>*@
                            <div class="col-12 col-md-3" data-bind="visible: SituacaoColaborador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: SituacaoColaborador.text, attr: { for: SituacaoColaborador.id }"></label>
                                    <select class="form-control" data-bind="class: SituacaoColaborador.requiredClass, options: SituacaoColaborador.options, optionsText: 'text', optionsValue: 'value', value: SituacaoColaborador.val, attr: { maxlength: SituacaoColaborador.maxlength, id : SituacaoColaborador.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Localidade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Localidade.text, attr: { for: Localidade.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Localidade.requiredClass, value: Localidade.val, valueUpdate: 'afterkeydown', attr: { maxlength: Localidade.maxlength, id : Localidade.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Localidade.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: PerfilAcesso.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: PerfilAcesso.text, attr: { for: PerfilAcesso.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: PerfilAcesso.requiredClass, value: PerfilAcesso.val, valueUpdate: 'afterkeydown', attr: { maxlength: PerfilAcesso.maxlength, id : PerfilAcesso.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: PerfilAcesso.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                            <div class="col-12 col-md-3" data-bind="visible: Aposentadoria.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Aposentadoria.text, attr: { for: Aposentadoria.id }"></label>
                                    <select class="form-control" data-bind="class: Aposentadoria.requiredClass, options: Aposentadoria.options, optionsText: 'text', optionsValue: 'value', value: Aposentadoria.val, attr: { maxlength: Aposentadoria.maxlength, id : Aposentadoria.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: TipoUsuario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoUsuario.text, attr: { for: TipoUsuario.id }"></label>
                                    <select class="form-control" data-bind="class: TipoUsuario.requiredClass, options: TipoUsuario.options, optionsText: 'text', optionsValue: 'value', value: TipoUsuario.val, attr: { maxlength: TipoUsuario.maxlength, id : TipoUsuario.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Ambiente.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Ambiente.text, attr: { for: Ambiente.id }"></label>
                                    <select class="form-control" data-bind="class: Ambiente.requiredClass, options: Ambiente.options, optionsText: 'text', optionsValue: 'value', value: Ambiente.val, attr: { maxlength: Ambiente.maxlength, id : Ambiente.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: SomenteUsuariosAtivo.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: SomenteUsuariosAtivo.val, attr: { maxlength: SomenteUsuariosAtivo.maxlength, name: SomenteUsuariosAtivo.id, id: SomenteUsuariosAtivo.id }">
                                        <label class="custom-control-label" data-bind="text: SomenteUsuariosAtivo.text, attr: { for: SomenteUsuariosAtivo.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaUsuario" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaUsuario" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>





<script src="~/relatorios/scripts/usuarios"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioUsuarios();
    });

</script>
