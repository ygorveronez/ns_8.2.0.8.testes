@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Integrações</li>
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item active">Pedido Aguardando Integração</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Pedidos aguardando integração
    </h1>
</div>
<div class="panel" id="divConteudoRelatorio">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-file-alt"></i>
                Relatórios
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutPesquisaPedidoAguardandoIntegracao">
                <div class="row">
                    <div class="col-12 col-md-6" data-bind="visible: Pesquisa.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Pesquisa.text, attr: { for: Pesquisa.id }"></label>
                            <input type="text" class="form-control" data-bind="class: Pesquisa.requiredClass, value: Pesquisa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Pesquisa.maxlength, id : Pesquisa.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataInicio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataInicio.text, attr: { for: DataInicio.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataInicio.requiredClass, value: DataInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicio.maxlength, id : DataInicio.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataFim.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataFim.text, attr: { for: DataFim.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataFim.requiredClass, value: DataFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFim.maxlength, id : DataFim.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: SituacaoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: SituacaoIntegracao.text, attr: { for: SituacaoIntegracao.id }"></label>
                            <select class="form-control" data-bind="class: SituacaoIntegracao.requiredClass, options: SituacaoIntegracao.options, optionsText: 'text', optionsValue: 'value', value: SituacaoIntegracao.val, attr: { maxlength: SituacaoIntegracao.maxlength, id : SituacaoIntegracao.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: TipoIntegracao.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoIntegracao.text, attr: { for: TipoIntegracao.id }"></label>
                            <select class="form-control" data-bind="class: TipoIntegracao.requiredClass, options: TipoIntegracao.options, optionsText: 'text', optionsValue: 'value', value: TipoIntegracao.val, attr: { maxlength: TipoIntegracao.maxlength, id : TipoIntegracao.id}"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataEmbarqueInicio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataEmbarqueInicio.text, attr: { for: DataEmbarqueInicio.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataEmbarqueInicio.requiredClass, value: DataEmbarqueInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmbarqueInicio.maxlength, id : DataEmbarqueInicio.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: DataEmbarqueFim.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataEmbarqueFim.text, attr: { for: DataEmbarqueFim.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataEmbarqueFim.requiredClass, value: DataEmbarqueFim.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmbarqueFim.maxlength, id : DataEmbarqueFim.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3" data-bind="visible: TransIDBusca.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TransIDBusca.text, attr: { for: TransIDBusca.id }"></label>
                            <input type="text" class="form-control" data-bind="class: TransIDBusca.requiredClass, value: TransIDBusca.val, valueUpdate: 'afterkeydown', attr: { maxlength: TransIDBusca.maxlength, id : TransIDBusca.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-2" data-bind="visible: top.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: top.text, attr: { for: top.id }"></label>
                            <input type="text" class="form-control" data-bind="class: top.requiredClass, enable: top.enable, value: top.val, valueUpdate: 'afterkeydown', attr: { maxlength: top.maxlength, id : top.id}" />
                        </div>
                    </div>
                </div>
            </form>
        </div>

        <div id="divConfigRelatorio"></div>

        <div class="row mb-2 me-2">
            <div id="knockoutCRUDFiltrosPesquisaPedidoAguardandoIntegracao" class="col-12 d-flex justify-content-md-end">
                <button class="btn btn-primary btnPesquisarFiltroPesquisa" style="margin-right:10px" data-bind="click: BuscaManual.eventClick, attr: { id: BuscaManual.id}" type="button">
                    <i class="fal fa-search"></i><span data-bind="text: BuscaManual.text"></span>
                </button>
                <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                    <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                </button>

            </div>
        </div>
        <table width="100%" class="table table-bordered table-hover table-condensed table-striped" data-bind="visible: !ConsultaManual.val()" id="gridPreviewConsultaManual" cellspacing="0"></table>

        <table width="100%" class="table table-bordered table-hover table-condensed table-striped" data-bind="visible: ConsultaManual.val()" id="gridPreviewRelatorio" cellspacing="0"></table>

    </div>
</div>

<div id="divModalHistoricoIntegracaoPedidoAgIntegracao" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutDelegarSelecionados" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Histórico de Integração</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table id="tblHistoricoIntegracaoPedidoAgIntegracao" class="table table-bordered table-hover" width="100%" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>

<div id="divModalNumeroNFChaveDeAcesso" class="modal fade" role="dialog" aria-hidden="true">
    <div id="knockoutDelegarSelecionados" class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Numero de NF e Chave de Acesso</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                    <i class="fal fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <table id="tblNumeroNFChaveDeAcesso" class="table table-bordered table-hover" width="100%" cellspacing="0"></table>
            </div>
        </div>
    </div>
</div>


<script src="~/relatorios/scripts/pedidoAguardandoIntegracao"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioPedidoAguardandoIntegracao();
    });

</script>

