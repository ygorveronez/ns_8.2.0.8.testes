@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item">Integrações</li>
    <li class="breadcrumb-item active">Indicador Integração CT-e</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        Relatório de Pagamentos/Escrituração de CT-e
    </h1>
</div>
<div class="panel" id="divConteudoRelatorio">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-file-alt"></i>
                Relatórios
            </h3>
        </div>
        <div class="panel-content">
            <form id="knockoutPesquisaIndicadorIntegracaoCTe">
                <div class="row">

                    <div class="col-12 col-md-6" data-bind="visible: TipoRelatorio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: DataEmissaoInicio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataEmissaoInicio.text, attr: { for: DataEmissaoInicio.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataEmissaoInicio.requiredClass, value: DataEmissaoInicio.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoInicio.maxlength, id : DataEmissaoInicio.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: DataEmissaoLimite.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataEmissaoLimite.text, attr: { for: DataEmissaoLimite.id }"></label>
                            <input type="text" class="form-control" data-bind="class: DataEmissaoLimite.requiredClass, value: DataEmissaoLimite.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEmissaoLimite.maxlength, id : DataEmissaoLimite.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: CodigoCargaEmbarcador.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: CodigoCargaEmbarcador.text, attr: { for: CodigoCargaEmbarcador.id }"></label>
                            <input type="text" class="form-control" data-bind="class: CodigoCargaEmbarcador.requiredClass, value: CodigoCargaEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCargaEmbarcador.maxlength, id : CodigoCargaEmbarcador.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: NumeroCTe.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: NumeroCTe.text, attr: { for: NumeroCTe.id }"></label>
                            <input type="text" class="form-control" data-bind="class: NumeroCTe.requiredClass, value: NumeroCTe.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCTe.maxlength, id : NumeroCTe.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-6" data-bind="visible: Transportador.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-6" data-bind="visible: Filial.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                            <div class="input-group">
                                <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: Integrado1.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Integrado1.text, attr: { for: Integrado1.id }"></label>
                            <select class="form-control" data-bind="class: Integrado1.requiredClass, options: Integrado1.options, optionsText: 'text', optionsValue: 'value', value: Integrado1.val, attr: { maxlength: Integrado1.maxlength, id : Integrado1.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: Integrado2.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Integrado2.text, attr: { for: Integrado2.id }"></label>
                            <select class="form-control" data-bind="class: Integrado2.requiredClass, options: Integrado2.options, optionsText: 'text', optionsValue: 'value', value: Integrado2.val, attr: { maxlength: Integrado2.maxlength, id : Integrado2.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: Integrado3.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Integrado3.text, attr: { for: Integrado3.id }"></label>
                            <select class="form-control" data-bind="class: Integrado3.requiredClass, options: Integrado3.options, optionsText: 'text', optionsValue: 'value', value: Integrado3.val, attr: { maxlength: Integrado3.maxlength, id : Integrado3.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: Integrado4.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Integrado4.text, attr: { for: Integrado4.id }"></label>
                            <select class="form-control" data-bind="class: Integrado4.requiredClass, options: Integrado4.options, optionsText: 'text', optionsValue: 'value', value: Integrado4.val, attr: { maxlength: Integrado4.maxlength, id : Integrado4.id}"></select>
                        </div>
                    </div>

                    <div class="col-12 col-md-3" data-bind="visible: Integrado5.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Integrado5.text, attr: { for: Integrado5.id }"></label>
                            <select class="form-control" data-bind="class: Integrado5.requiredClass, options: Integrado5.options, optionsText: 'text', optionsValue: 'value', value: Integrado5.val, attr: { maxlength: Integrado5.maxlength, id : Integrado5.id}"></select>
                        </div>
                    </div>
                </div>
            </form>
            <div class="row mb-3">
                <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <div id="knockoutCRUDFiltrosPesquisaIndicadorIntegracaoCTe" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>

                </div>
            </div>

            <div id="divConfigRelatorio" class="mt-3"></div>

            <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

            <div id="knockoutCRUDPesquisaIndicadorIntegracaoCTe" class="d-flex justify-content-center justify-content-md-end mt-3">
                <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                    <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                </button>
                <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                    <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>



<script src="~/relatorios/scripts/indicadorIntegracaoCTe"></script>

<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadIndicadorIntegracaoCTe();
    });

</script>
