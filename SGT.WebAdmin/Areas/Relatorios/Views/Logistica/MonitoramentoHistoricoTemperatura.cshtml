@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatório</li>
        <li class="breadcrumb-item">Logistica</li>
        <li class="breadcrumb-item active">Monitoramento Histórico Temperatura</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-truck"></i> Relatório de Histórico de Temperatura
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaMonitoramentoHistoricoTemperatura">
                        <div class="row">

                            <div class="col-12 col-md-2" data-bind="visible: DataInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataInicial.text, attr: { for: DataInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: DataInicial.requiredClass, value: DataInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataFinal.text, attr: { for: DataFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataCriacaoCargaInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCriacaoCargaInicial.text, attr: { for: DataCriacaoCargaInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataCriacaoCargaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCriacaoCargaInicial.maxlength, id : DataCriacaoCargaInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: DataCriacaoCargaFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataCriacaoCargaFinal.text, attr: { for: DataCriacaoCargaFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataCriacaoCargaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataCriacaoCargaFinal.maxlength, id : DataCriacaoCargaFinal.id}" />
                                </div>
                            </div>
                            <div class="col-6 col-md-2 align-self-md-center" data-bind="visible: DuranteMonitoramento.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: DuranteMonitoramento.val, attr: { maxlength: DuranteMonitoramento.maxlength, name: DuranteMonitoramento.id, id: DuranteMonitoramento.id }">
                                        <label class="custom-control-label" data-bind="text: DuranteMonitoramento.text, attr: { for: DuranteMonitoramento.id }"></label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: NumeroCarga.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: NumeroCarga.text, attr: { for: NumeroCarga.id }"></label>
                                    <input type="text" class="form-control" data-bind="css: NumeroCarga.requiredClass, class: NumeroCarga.requiredClass, value: NumeroCarga.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroCarga.maxlength, id : NumeroCarga.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Transportador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Veiculo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Veiculo.text, attr: { for: Veiculo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Veiculo.requiredClass, value: Veiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Veiculo.maxlength, id : Veiculo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Veiculo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: StatusMonitoramento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: StatusMonitoramento.text, attr: { for: StatusMonitoramento.id }"></label>
                                    <select class="form-control" data-bind="class: StatusMonitoramento.requiredClass, options: StatusMonitoramento.options, optionsText: 'text', optionsValue: 'value', value: StatusMonitoramento.val, attr: { maxlength: StatusMonitoramento.maxlength, id : StatusMonitoramento.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-1" data-bind="visible: EntregasRealizadas .visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EntregasRealizadas.text, attr: { for: EntregasRealizadas.id }"></label>
                                    <select class="form-control" data-bind="class: EntregasRealizadas.requiredClass, options: EntregasRealizadas.options, optionsText: 'text', optionsValue: 'value', value: EntregasRealizadas.val, attr: { maxlength: EntregasRealizadas.maxlength, id : EntregasRealizadas.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Filial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Filial.text, attr: { for: Filial.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Filial.requiredClass, value: Filial.val, valueUpdate: 'afterkeydown', attr: { maxlength: Filial.maxlength, id : Filial.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Filial.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: ForaFaixa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ForaFaixa.text, attr: { for: ForaFaixa.id }"></label>
                                    <select class="form-control" data-bind="class: ForaFaixa.requiredClass, options: ForaFaixa.options, optionsText: 'text', optionsValue: 'value', value: ForaFaixa.val, attr: { maxlength: ForaFaixa.maxlength, id : ForaFaixa.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: FaixaTemperatura.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: FaixaTemperatura.text, attr: { for: FaixaTemperatura.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: FaixaTemperatura.requiredClass, value: FaixaTemperatura.val, valueUpdate: 'afterkeydown', attr: { maxlength: FaixaTemperatura.maxlength, id : FaixaTemperatura.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: FaixaTemperatura.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: StatusViagem.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: StatusViagem.text, attr: { for: StatusViagem.id }"></label>
                                    <select class="form-control" data-bind="class: StatusViagem.requiredClass, options: StatusViagem.options, optionsText: 'text', optionsValue: 'value', selectedOptions: StatusViagem.val, attr: { maxlength: StatusViagem.maxlength, id : StatusViagem.id}" multiple="multiple"></select>
                                </div>
                            </div>

                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaMonitoramentoHistoricoTemperatura" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaMonitoramentoHistoricoTemperatura" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/monitoramentoHistoricoTemperatura"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadLocalizationResources(["Logistica.Monitoramento"]).then(LoadMonitoramentoHistoricoTemperatura);
    });

</script>
