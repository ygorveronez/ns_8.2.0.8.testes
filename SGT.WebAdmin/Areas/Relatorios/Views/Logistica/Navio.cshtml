@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Logística</li>
        <li class="breadcrumb-item active"> Navio </li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Navios
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaNavio">
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id }"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-5" data-bind="visible: CodigoIntegracao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoIntegracao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoIntegracao.requiredClass, value: CodigoIntegracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIntegracao.maxlength, id : CodigoIntegracao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-5" data-bind="visible: CodigoEmbarcacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoEmbarcacao.text"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoEmbarcacao.requiredClass, value: CodigoEmbarcacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoEmbarcacao.maxlength, id : CodigoEmbarcacao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: TipoEmbarcacao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoEmbarcacao.text, attr: { for: TipoEmbarcacao.id }"></label>
                                    <select class="form-control" multiple data-bind="options: TipoEmbarcacao.options, optionsText: 'text', optionsValue: 'value', selectedOptions: TipoEmbarcacao.val, attr: { id : TipoEmbarcacao.id}"></select>
                                </div>
                            </div>

                            <div class="row" data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                                <div class="col-12 col-md-6" data-bind="visible: CodigoIrin.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CodigoIrin.text"></label>
                                        <input type="text" class="form-control" data-bind="class: CodigoIrin.requiredClass, value: CodigoIrin.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIrin.maxlength, id : CodigoIrin.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: CodigoDocumentacao.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CodigoDocumentacao.text"></label>
                                        <input type="text" class="form-control" data-bind="class: CodigoDocumentacao.requiredClass, value: CodigoDocumentacao.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoDocumentacao.maxlength, id : CodigoDocumentacao.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: CodigoIMO.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: CodigoIMO.text"></label>
                                        <input type="text" class="form-control" data-bind="class: CodigoIMO.requiredClass, value: CodigoIMO.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoIMO.maxlength, id : CodigoIMO.id}" />
                                    </div>
                                </div>
                                <div class="col-12 col-md-6" data-bind="visible: NavioID.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: NavioID.text"></label>
                                        <input type="text" class="form-control" data-bind="class: NavioID.requiredClass, value: NavioID.val, valueUpdate: 'afterkeydown', attr: { maxlength: NavioID.maxlength, id : NavioID.id}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaNavio" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                            <i data-bind="attr: { class: BuscaAvancada.icon}" class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                        </button>
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaNavio" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>




<script src="~/relatorios/scripts/navio"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadNavio();
    });

</script>
