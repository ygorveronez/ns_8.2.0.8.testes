@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Logística</li>
    <li class="breadcrumb-item">Relatórios</li>
    <li class="breadcrumb-item active">Guarita TMS</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i class="subheader-icon fal fa-truck"></i> Relatório de Controle de Tempo de Viagem
    </h1>
</div>
<div id="divConteudoRelatorio" class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-file-alt"></i>
                Relatório
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div id="knockoutPesquisaControleTempoViagem">
                    <div class="row">
                        <div class="col-12 col-md-4" data-bind="visible: NumeroNota.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: NumeroNota.text, attr: { for: NumeroNota.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: NumeroNota.requiredClass, value: NumeroNota.val, valueUpdate: 'afterkeydown', attr: { maxlength: NumeroNota.maxlength, id : NumeroNota.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: NumeroNota.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: Carga.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Carga.text, attr: { for: Carga.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Carga.requiredClass, value: Carga.val, valueUpdate: 'afterkeydown', attr: { maxlength: Carga.maxlength, id : Carga.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Carga.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: Destino.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Destino.text, attr: { for: Destino.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Destino.requiredClass, value: Destino.val, valueUpdate: 'afterkeydown', attr: { maxlength: Destino.maxlength, id : Destino.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Destino.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-4" data-bind="visible: Transportador.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: Transportador.text, attr: { for: Transportador.id }"></label>
                                <div class="input-group">
                                    <input type="text" class="form-control" data-bind="css: Transportador.requiredClass, value: Transportador.val, valueUpdate: 'afterkeydown', attr: { maxlength: Transportador.maxlength, id : Transportador.id}" />
                                    <div class="input-group-append">
                                        <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Transportador.idBtnSearch}" type="button" aria-label="Buscar">
                                            <i class="fal fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataFaturaInicial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFaturaInicial.text, attr: { for: DataFaturaInicial.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataFaturaInicial.requiredClass, value: DataFaturaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFaturaInicial.maxlength, id : DataFaturaInicial.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: DataFaturaFinal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: DataFaturaFinal.text, attr: { for: DataFaturaFinal.id }"></label>
                                <input type="text" class="form-control" data-bind="class: DataFaturaFinal.requiredClass, value: DataFaturaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataFaturaFinal.maxlength, id : DataFaturaFinal.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: PrevisaoEntregaInicial.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: PrevisaoEntregaInicial.text, attr: { for: PrevisaoEntregaInicial.id }"></label>
                                <input type="text" class="form-control" data-bind="class: PrevisaoEntregaInicial.requiredClass, value: PrevisaoEntregaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: PrevisaoEntregaInicial.maxlength, id : PrevisaoEntregaInicial.id}" />
                            </div>
                        </div>

                        <div class="col-12 col-md-2" data-bind="visible: PrevisaoEntregaFinal.visible">
                            <div class="form-group">
                                <label class="form-label" data-bind="text: PrevisaoEntregaFinal.text, attr: { for: PrevisaoEntregaFinal.id }"></label>
                                <input type="text" class="form-control" data-bind="class: PrevisaoEntregaFinal.requiredClass, value: PrevisaoEntregaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: PrevisaoEntregaFinal.maxlength, id : PrevisaoEntregaFinal.id}" />
                            </div>
                        </div>

                        <div data-bind="id: Visible.idFade, fadeVisible: Visible.visibleFade">
                            <div class="row">

                                <div class="col-12 col-md-2" data-bind="visible: DataEntregaRealInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataEntregaRealInicial.text, attr: { for: DataEntregaRealInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataEntregaRealInicial.requiredClass, value: DataEntregaRealInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEntregaRealInicial.maxlength, id : DataEntregaRealInicial.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DataEntregaRealFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataEntregaRealFinal.text, attr: { for: DataEntregaRealFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataEntregaRealFinal.requiredClass, value: DataEntregaRealFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataEntregaRealFinal.maxlength, id : DataEntregaRealFinal.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DataRetornoComprovanteInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataRetornoComprovanteInicial.text, attr: { for: DataRetornoComprovanteInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataRetornoComprovanteInicial.requiredClass, value: DataRetornoComprovanteInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRetornoComprovanteInicial.maxlength, id : DataRetornoComprovanteInicial.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DataRetornoComprovanteFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DataRetornoComprovanteFinal.text, attr: { for: DataRetornoComprovanteFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DataRetornoComprovanteFinal.requiredClass, value: DataRetornoComprovanteFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataRetornoComprovanteFinal.maxlength, id : DataRetornoComprovanteFinal.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: TempoViagem.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: TempoViagem.text, attr: { for: TempoViagem.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: TempoViagem.requiredClass, value: TempoViagem.val, valueUpdate: 'afterkeydown', attr: { maxlength: TempoViagem.maxlength, id : TempoViagem.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DiasRetornoComprovante.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DiasRetornoComprovante.text, attr: { for: DiasRetornoComprovante.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DiasRetornoComprovante.requiredClass, value: DiasRetornoComprovante.val, valueUpdate: 'afterkeydown', attr: { maxlength: DiasRetornoComprovante.maxlength, id : DiasRetornoComprovante.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: DocumentoVenda.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: DocumentoVenda.text, attr: { for: DocumentoVenda.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: DocumentoVenda.requiredClass, value: DocumentoVenda.val, valueUpdate: 'afterkeydown', attr: { maxlength: DocumentoVenda.maxlength, id : DocumentoVenda.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: Performance.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: Performance.text, attr: { for: Performance.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: Performance.requiredClass, value: Performance.val, valueUpdate: 'afterkeydown', attr: { maxlength: Performance.maxlength, id : Performance.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: ValorNotaInicial.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: ValorNotaInicial.text, attr: { for: ValorNotaInicial.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: ValorNotaInicial.requiredClass, value: ValorNotaInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorNotaInicial.maxlength, id : ValorNotaInicial.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-2" data-bind="visible: ValorNotaFinal.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: ValorNotaFinal.text, attr: { for: ValorNotaFinal.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: ValorNotaFinal.requiredClass, value: ValorNotaFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: ValorNotaFinal.maxlength, id : ValorNotaFinal.id}" />
                                    </div>
                                </div>

                                <div class="col-12 col-md-4" data-bind="visible: RazaoSocialDestinatario.visible">
                                    <div class="form-group">
                                        <label class="form-label" data-bind="text: RazaoSocialDestinatario.text, attr: { for: RazaoSocialDestinatario.id }"></label>
                                        <input type="text" class="form-control" data-bind="class: RazaoSocialDestinatario.requiredClass, value: RazaoSocialDestinatario.val, valueUpdate: 'afterkeydown', attr: { maxlength: RazaoSocialDestinatario.maxlength, id : RazaoSocialDestinatario.id}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </form>
            <div class="row mb-3">
                <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <div id="knockoutCRUDFiltrosPesquisaControleTempoViagem" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                    <button class="btn btn-primary me-3 btnPesquisarFiltroPesquisa" data-bind="click: BuscaAvancada.eventClick, attr: { id: BuscaAvancada.id}" type="button">
                        <i class="fal fa-plus"></i><span data-bind="text: BuscaAvancada.text"></span>
                    </button>
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>

                </div>
            </div>

            <div id="divConfigRelatorio"></div>

            <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>

            <div id="knockoutCRUDPesquisaControleTempoViagem" class="d-flex justify-content-center justify-content-md-end mt-3">
                <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                    <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                </button>
                <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                    <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/relatorios/scripts/ControleTempoViagem"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadControleTempoViagem();
    });

</script>
