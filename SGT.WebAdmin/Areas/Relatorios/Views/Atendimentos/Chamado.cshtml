@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Atendimentos</li>
        <li class="breadcrumb-item active">Chamado</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-truck"></i> Relatório de Chamados
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Pesquisar Chamados
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaChamados">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: DataChamadoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataChamadoInicial.text, attr: { for: DataChamadoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataChamadoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataChamadoInicial.maxlength, id : DataChamadoInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataChamadoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataChamadoFinal.text, attr: { for: DataChamadoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataChamadoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataChamadoFinal.maxlength, id : DataChamadoFinal.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataAtendimentoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAtendimentoInicial.text, attr: { for: DataAtendimentoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataAtendimentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAtendimentoInicial.maxlength, id : DataAtendimentoInicial.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: DataAtendimentoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataAtendimentoFinal.text, attr: { for: DataAtendimentoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: DataAtendimentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataAtendimentoFinal.maxlength, id : DataAtendimentoFinal.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Prioridade.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Prioridade.text, attr: { for: Prioridade.id }"></label>
                                    <select class="form-control" data-bind="options: Prioridade.options, optionsText: 'text', optionsValue: 'value', value: Prioridade.val, attr: { id : Prioridade.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { id : Status.id}"></select>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Titulo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Titulo.text, attr: { for: Titulo.id }"></label>
                                    <input type="text" class="form-control" data-bind="value: Titulo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Titulo.maxlength, id : Titulo.id}" />
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Tela.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Tela.text, attr: { for: Tela.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Tela.val, valueUpdate: 'afterkeydown', attr: { maxlength: Tela.maxlength, id : Tela.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Tela.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Modulo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Modulo.text, attr: { for: Modulo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Modulo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Modulo.maxlength, id : Modulo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Modulo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Sistema.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Sistema.text, attr: { for: Sistema.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Sistema.val, valueUpdate: 'afterkeydown', attr: { maxlength: Sistema.maxlength, id : Sistema.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Sistema.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Tipo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Tipo.text, attr: { for: Tipo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Tipo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Tipo.maxlength, id : Tipo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Tipo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-4" data-bind="visible: Funcionario.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Funcionario.text, attr: { for: Funcionario.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Funcionario.val, valueUpdate: 'afterkeydown', attr: { maxlength: Funcionario.maxlength, id : Funcionario.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Funcionario.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: Empresa.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Empresa.text, attr: { for: Empresa.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Empresa.val, valueUpdate: 'afterkeydown', attr: { maxlength: Empresa.maxlength, id : Empresa.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Empresa.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: EmpresaFilho.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: EmpresaFilho.text, attr: { for: EmpresaFilho.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: EmpresaFilho.val, valueUpdate: 'afterkeydown', attr: { maxlength: EmpresaFilho.maxlength, id : EmpresaFilho.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: EmpresaFilho.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12 col-md-6" data-bind="visible: Solicitante.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Solicitante.text, attr: { for: Solicitante.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: Solicitante.val, valueUpdate: 'afterkeydown', attr: { maxlength: Solicitante.maxlength, id : Solicitante.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Solicitante.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                               <div class="col-12 col-md-6" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>



                        </div>
                    </div>
                    <div id="divConfigRelatorio">
                    </div>

                </form>
                <div class="row mb-3 mt-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaChamados" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>

                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaChamados" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/chamados"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioChamados();
    });

</script>