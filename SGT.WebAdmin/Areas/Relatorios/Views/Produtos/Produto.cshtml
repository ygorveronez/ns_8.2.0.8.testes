@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">Produtos</li>
        <li class="breadcrumb-item active">Produto</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Produtos
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaProduto">
                        <div class="row">
                            <div class="col-12 col-md-2" data-bind="visible: CodigoNCM.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoNCM.text, attr: { for: CodigoNCM.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoNCM.requiredClass, value: CodigoNCM.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoNCM.maxlength, id : CodigoNCM.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CodigoCEST.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoCEST.text, attr: { for: CodigoCEST.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoCEST.requiredClass, value: CodigoCEST.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoCEST.maxlength, id : CodigoCEST.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: CodigoBarrasEAN.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoBarrasEAN.text, attr: { for: CodigoBarrasEAN.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoBarrasEAN.requiredClass, value: CodigoBarrasEAN.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoBarrasEAN.maxlength, id : CodigoBarrasEAN.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-2" data-bind="visible: Status.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Status.text, attr: { for: Status.id }"></label>
                                    <select class="form-control" data-bind="class: Status.requiredClass, options: Status.options, optionsText: 'text', optionsValue: 'value', value: Status.val, attr: { maxlength: Status.maxlength, id : Status.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: CategoriaProduto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CategoriaProduto.text, attr: { for: CategoriaProduto.id }"></label>
                                    <select class="form-control" data-bind="class: CategoriaProduto.requiredClass, options: CategoriaProduto.options, optionsText: 'text', optionsValue: 'value', value: CategoriaProduto.val, attr: { maxlength: CategoriaProduto.maxlength, id : CategoriaProduto.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Produto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Produto.text, attr: { for: Produto.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Produto.requiredClass, value: Produto.val, valueUpdate: 'afterkeydown', attr: { maxlength: Produto.maxlength, id : Produto.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Produto.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Grupo.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Grupo.text, attr: { for: Grupo.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Grupo.requiredClass, value: Grupo.val, valueUpdate: 'afterkeydown', attr: { maxlength: Grupo.maxlength, id : Grupo.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Grupo.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Marca.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Marca.text, attr: { for: Marca.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Marca.requiredClass, value: Marca.val, valueUpdate: 'afterkeydown', attr: { maxlength: Marca.maxlength, id : Marca.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Marca.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: LocalArmazenamento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: LocalArmazenamento.text, attr: { for: LocalArmazenamento.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: LocalArmazenamento.requiredClass, value: LocalArmazenamento.val, valueUpdate: 'afterkeydown', attr: { maxlength: LocalArmazenamento.maxlength, id : LocalArmazenamento.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: LocalArmazenamento.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: GrupoImposto.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: GrupoImposto.text, attr: { for: GrupoImposto.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: GrupoImposto.requiredClass, value: GrupoImposto.val, valueUpdate: 'afterkeydown', attr: { maxlength: GrupoImposto.maxlength, id : GrupoImposto.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: GrupoImposto.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </form>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaProduto" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div id="divConfigRelatorio"></div>
                <table id="gridPreviewRelatorio" class="table table-bordered table-hover table-condensed table-striped" width="100%" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaProduto" class="d-flex justify-content-center justify-content-md-end mt-3">
                    <button class="btn btn-default me-3" data-bind="click: GerarEtiqueta.eventClick, attr: { id: GerarEtiqueta.id}" type="button">
                        <i class="fal fa-tag"></i><span data-bind="text: GerarEtiqueta.text"></span>
                    </button>
                    <button class="btn btn-success me-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/relatorios/scripts/produto"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        LoadProduto();
    });

</script>
