@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage

<section id="widget-grid" class="">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">Relatórios</li>
        <li class="breadcrumb-item">WMS</li>
        <li class="breadcrumb-item active">Saldo Armazenamento</li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            Relatório de Saldo de Local de Armazenamento
        </h1>
    </div>
    <div id="divConteudoRelatorio" class="panel">
        <div class="panel-container">
            <div class="panel-hdr">
                <h3>
                    <i class="fal fa-file-alt"></i>
                    Relatórios
                </h3>
            </div>
            <div class="panel-content">
                <form>
                    <div id="knockoutPesquisaSaldoArmazenamento">
                        <div class="row">
                            <div class="col-12 col-md-4" data-bind="visible: ProdutoEmbarcador.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: ProdutoEmbarcador.text, attr: { for: ProdutoEmbarcador.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: ProdutoEmbarcador.requiredClass, value: ProdutoEmbarcador.val, valueUpdate: 'afterkeydown', attr: { maxlength: ProdutoEmbarcador.maxlength, id : ProdutoEmbarcador.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: ProdutoEmbarcador.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Deposito.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Deposito.text, attr: { for: Deposito.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Deposito.requiredClass, value: Deposito.val, valueUpdate: 'afterkeydown', attr: { maxlength: Deposito.maxlength, id : Deposito.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Deposito.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-4" data-bind="visible: Bloco.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Bloco.text, attr: { for: Bloco.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Bloco.requiredClass, value: Bloco.val, valueUpdate: 'afterkeydown', attr: { maxlength: Bloco.maxlength, id : Bloco.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Bloco.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Rua.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Rua.text, attr: { for: Rua.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Rua.requiredClass, value: Rua.val, valueUpdate: 'afterkeydown', attr: { maxlength: Rua.maxlength, id : Rua.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Rua.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: Posicao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Posicao.text, attr: { for: Posicao.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: Posicao.requiredClass, value: Posicao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Posicao.maxlength, id : Posicao.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: Posicao.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: CodigoBarras.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: CodigoBarras.text, attr: { for: CodigoBarras.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: CodigoBarras.requiredClass, value: CodigoBarras.val, valueUpdate: 'afterkeydown', attr: { maxlength: CodigoBarras.maxlength, id : CodigoBarras.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: Descricao.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: Descricao.text, attr: { for: Descricao.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: Descricao.requiredClass, value: Descricao.val, valueUpdate: 'afterkeydown', attr: { maxlength: Descricao.maxlength, id : Descricao.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataVencimentoInicial.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoInicial.text, attr: { for: DataVencimentoInicial.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoInicial.requiredClass, value: DataVencimentoInicial.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoInicial.maxlength, id : DataVencimentoInicial.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-3" data-bind="visible: DataVencimentoFinal.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: DataVencimentoFinal.text, attr: { for: DataVencimentoFinal.id }"></label>
                                    <input type="text" class="form-control" data-bind="class: DataVencimentoFinal.requiredClass, value: DataVencimentoFinal.val, valueUpdate: 'afterkeydown', attr: { maxlength: DataVencimentoFinal.maxlength, id : DataVencimentoFinal.id}" />
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoRecebimento.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRecebimento.text, attr: { for: TipoRecebimento.id }"></label>
                                    <select class="form-control" data-bind="class: TipoRecebimento.requiredClass, options: TipoRecebimento.options, optionsText: 'text', optionsValue: 'value', value: TipoRecebimento.val, attr: { maxlength: TipoRecebimento.maxlength, id : TipoRecebimento.id}"></select>
                                </div>
                            </div>
                            <div class="col-12 col-md-6" data-bind="visible: TipoRelatorio.visible">
                                <div class="form-group">
                                    <label class="form-label" data-bind="text: TipoRelatorio.text, attr: { for: TipoRelatorio.id }"></label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-bind="css: TipoRelatorio.requiredClass, value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary waves-effect waves-themed" data-bind="attr: { id: TipoRelatorio.idBtnSearch}" type="button" aria-label="Buscar">
                                                <i class="fal fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-12" data-bind="visible: SaldoDisponivel.visible">
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" data-bind="checked: SaldoDisponivel.val, attr: { maxlength: SaldoDisponivel.maxlength, name: SaldoDisponivel.id, id: SaldoDisponivel.id }">
                                        <label class="custom-control-label" data-bind="text: SaldoDisponivel.text, attr: { for: SaldoDisponivel.id }"></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div id="knockoutCRUDConfiguracaoRelatorio" class="d-flex justify-content-start">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                        <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                    </button>
                </div>
                <div id="knockoutCRUDFiltrosPesquisaSaldoArmazenamento" class="d-flex justify-content-end">
                    <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                        <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                    </button>
                </div>
                <div id="divConfigRelatorio"></div>
                <table width="100%" class="table table-bordered table-hover table-condensed table-striped" id="gridPreviewRelatorio" cellspacing="0"></table>
                <div id="knockoutCRUDPesquisaSaldoArmazenamento" class="d-flex  justify-content-md-end mt-3">
                    <button class="btn btn-success mx-2" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i class="fal fa-file-excel"></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                    <button class="btn btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i class="fal fa-file-pdf"></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>
            </div>
        </div>
</section>
<script src="~/relatorios/scripts/saldoArmazenamento"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioSaldoArmazenamento();
    });

</script>

