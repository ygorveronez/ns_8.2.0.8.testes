@inherits Microsoft.AspNetCore.Mvc.Razor.RazorPage


<ol class="breadcrumb page-breadcrumb">
    <li class="breadcrumb-item">Relatorios</li>
    <li class="breadcrumb-item">Acerto Viagem</li>
    <li class="breadcrumb-item active">Resultado Acerto Viagem</li>
</ol>
<div class="subheader">
    <h1 class="subheader-title">
        <i></i> Resultado Acerto Viagem
    </h1>
</div>



<div class="panel">
    <div class="panel-container">
        <div class="panel-hdr">
            <h3>
                <i class="fal fa-edit"></i>
                Relatório Resultado Acerto Viagem
            </h3>
        </div>
        <div class="panel-content">
            <form>
                <div class="row" id="knockoutPesquisaResultadoAcertoViagem">
                    <div class="col-12 col-md-3">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataInicial.text"></label>
                            <input type="text" class="form-control" data-bind="value: DataInicial.val, attr: { maxlength: DataInicial.maxlength, id : DataInicial.id}" />
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: DataFinal.text"></label>
                            <input type="text" class="form-control" data-bind="value: DataFinal.val, attr: { maxlength: DataFinal.maxlength, id : DataFinal.id}" />
                        </div>
                    </div>

                    <div class="col-12 col-md-6">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: Motorista.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: Motorista.val, valueUpdate: 'afterkeydown', attr: { maxlength: Motorista.maxlength, id : Motorista.id}, class: Motorista.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: Motorista.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-6">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: GrupoPessoa.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="text: GrupoPessoa.text , class: GrupoPessoa.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: GrupoPessoa.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-6">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: ModeloVeiculo.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: ModeloVeiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: ModeloVeiculo.maxlength, id : ModeloVeiculo.id},  class: ModeloVeiculo.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: ModeloVeiculo.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-6">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: VeiculoTracao.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: VeiculoTracao.val, valueUpdate: 'afterkeydown', attr: { maxlength: VeiculoTracao.maxlength, id : VeiculoTracao.id},  class: VeiculoTracao.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: VeiculoTracao.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-6">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: VeiculoReboque.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: VeiculoReboque.val, valueUpdate: 'afterkeydown', attr: { maxlength: VeiculoReboque.maxlength, id : VeiculoReboque.id},class: VeiculoReboque.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: VeiculoReboque.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12 col-md-6" data-bind="visible: SegmentoVeiculo.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: SegmentoVeiculo.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: SegmentoVeiculo.val, valueUpdate: 'afterkeydown', attr: { maxlength: SegmentoVeiculo.maxlength, id : SegmentoVeiculo.id},class: SegmentoVeiculo.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: SegmentoVeiculo.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-6" data-bind="visible: TipoRelatorio.visible">
                        <div class="form-group">
                            <label class="form-label" data-bind="text: TipoRelatorio.text"></label>
                            <div class="input-group">
                                <input type="text" data-bind="value: TipoRelatorio.val, valueUpdate: 'afterkeydown', attr: { maxlength: TipoRelatorio.maxlength, id : TipoRelatorio.id}, class: TipoRelatorio.requiredClass" />
                                <div class="input-group-append">
                                    <button class="btn btn-primary waves-effect waves-themed is-invalid" type="button" data-bind="attr: { id: TipoRelatorio.idBtnSearch}">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="divConfigRelatorio">
                </div>
                <div class="row mb-3">
                    <div id="knockoutCRUDConfiguracaoRelatorio" class="col-12 col-md-6 mb-3 mb-md-0">
                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="enable: ExibirConfiguracoes.enable, click: ExibirConfiguracoes.eventClick, attr: { id: ExibirConfiguracoes.id}" type="button">
                            <i data-bind="css: ExibirConfiguracoes.icon"></i><span data-bind="text: ExibirConfiguracoes.text"></span>
                        </button>
                    </div>
                    <div id="knockoutCRUDFiltrosPesquisaResultadoAcertoViagem" class="col-12 col-md-6 d-flex justify-content-center justify-content-md-end">

                        <button class="btn btn-primary btnPesquisarFiltroPesquisa" data-bind="click: Preview.eventClick, attr: { id: Preview.id}" type="button">
                            <i class="fal fa-search"></i><span data-bind="text: Preview.text"></span>
                        </button>
                    </div>
                </div>
                <div class="my-2">
                    <table width="100%" class="table table-bordered table-hover table-condensed " id="gridPreviewRelatorio" cellspacing="0"></table>
                </div>
            </form>
            <footer class="d-flex align-items-center justify-content-end mb-3 " id="knockoutCRUDPesquisaResultadoAcertoViagem">
                <div>
                    <button class="btn btn-primary waves-effect waves-themed btn-success mx-3" data-bind="click: GerarRelatorioExcel.eventClick, attr: { id: GerarRelatorioExcel.id}" type="button">
                        <i></i><span data-bind="text: GerarRelatorioExcel.text"></span>
                    </button>
                </div>
                <div>
                    <button class="btn btn-primary waves-effect waves-themed btn-success" data-bind="click: GerarRelatorioPDF.eventClick, attr: { id: GerarRelatorioPDF.id}" type="button">
                        <i></i><span data-bind="text: GerarRelatorioPDF.text"></span>
                    </button>
                </div>

            </footer>
        </div>
    </div>
</div>






<script src="~/relatorios/scripts/resultadoAcertoViagem"></script>
<script type="text/javascript">

    pageSetUp();

    $(document).ready(function () {
        loadRelatorioResultadoAcertoViagem();
    });

</script>

