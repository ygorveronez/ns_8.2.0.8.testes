<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site.master.cs" Inherits="EmissaoCTe.WebApp.Site" %>

<%@ Import Namespace="System.Web.Optimization" %>

<!DOCTYPE html>
<html>
<head runat="server">
    <title></title>
    <link rel="shortcut icon" href="Images/favicon.ico" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%: Styles.Render("~/styles/bootstrap",
                      "~/styles/site",
                      "~/bundle/styles/smartMenu") %>
    <%: Scripts.Render("~/bundle/scripts/jquery", 
                       "~/bundle/scripts/bootstrap",
                       "~/bundle/scripts/bootbox",
                       "~/bundle/scripts/globalize",
                       "~/bundle/scripts/smartMenu",
                       "~/bundle/scripts/auditoria") %>
    <script type="text/javascript">
        function ObterPath() {
            var p = "";
            if (document.location.pathname.split("/").length > 1) {
                var paths = document.location.pathname.split("/");
                for (var i = 0; (paths.length - 1) > i; i++) {
                    if (paths[i] != "") {
                        p += "/" + paths[i];
                    }
                }
            }
            return p;
        }

        var _CONFIGURACAO_AUDITORIA = false;
        $(document).ready(function () {
            _CONFIGURACAO_AUDITORIA = "#<%: hddConfiguracaoAuditoria.ClientID %>";
        });
    </script>
    <style>
        @media screen and (min-width: 1024px) {
            #divModalAuditoria .modal-dialog, #divModalDetalhesAuditoria .modal-dialog {
                right: auto;
                width: 1000px;
                padding-top: 30px;
                padding-bottom: 30px;
            }

            #divModalAuditoria .modal-content, #divModalDetalhesAuditoria .modal-content {
                -webkit-box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
                box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
            }
        }

        @media screen and (min-width: 1560px) {
            #divModalAuditoria .modal-dialog, #divModalDetalhesAuditoria .modal-dialog {
                right: auto;
                width: 1550px;
                padding-top: 30px;
                padding-bottom: 30px;
            }

            #divModalAuditoria .modal-content, #divModalDetalhesAuditoria .modal-content {
                -webkit-box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
                box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
            }
        }
    </style>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <meta name="description" content="Emissão de CT-e Online" />
</head>
