        <script type="text/javascript">
            $(document).ready(function () {
                $('[data-toggle="tooltip"]').tooltip({ delay: { show: 500, hide: 200 }, animation: true });
                $("#imgLogo").attr("src", ObterPath() + "/Empresa/ObterLogoDaEmpresa");
            });
