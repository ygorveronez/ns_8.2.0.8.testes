						},
						{
							"name": "vesselOwnership",
							"type": "string",
							"doc": "Denotes the actual vessel ownership who is responsible to manage the vessel. Example: Owned, Foreign Feeder, Time Charter"
						},
						{
							"name": "vesselStatus",
							"type": "string",
