					"fields": [
						{
							"name": "imoNumber",
							"type": [
								"null",
								"string"
							],
							"doc": "The International Maritime Organization (IMO) number refers to a unique identifier for vessels/ships. It consist of the unique seven-digit numbers, assigned under the International Convention for the Safety of Life at Sea (SOLAS). Example:9315874"
						},
						{
							"name": "vesselTypeCode",
							"type": "string",
							"doc": "Display vessel type code. Example : CON "
						},
						{
							"name": "vesselTypeName",
							"type": "string",
